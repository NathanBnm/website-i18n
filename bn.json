{
    "acc-opt": "\u0985\u09ad\u09bf\u0997\u09ae\u09cd\u09af\u09a4\u09be \u0985\u09aa\u09b6\u09a8",
    "acc-opt-but": "\u0985\u09cd\u09af\u09be\u0995\u09cd\u09b8\u09c7\u09b8\u09af\u09cb\u0997\u09cd\u09af\u09a4\u09be\u09b0 \u09ac\u09bf\u0995\u09b2\u09cd\u09aa\u0997\u09c1\u09b2\u09bf \u09b8\u09ae\u09cd\u09aa\u09b0\u09cd\u0995\u09c7 \u0986\u09b0\u0993 \u099c\u09be\u09a8\u09c1\u09a8",
    "acc-opt-text": "\u0986\u09aa\u09a8\u09bf \u0995\u09bf \u09a6\u09c3\u09b7\u09cd\u099f\u09bf \u09aa\u09cd\u09b0\u09a4\u09bf\u09ac\u09a8\u09cd\u09a7\u09c0 \u09ac\u09be \u0985\u09a8\u09cd\u09af\u09a5\u09be\u09af\u09bc \u098f\u0987\u099a\u0995\u09cd\u09af\u09be\u09aa\u099a\u0997\u09c1\u09b2\u09bf \u09b8\u09ae\u09cd\u09aa\u09c2\u09b0\u09cd\u09a3 \u0995\u09b0\u09a4\u09c7 \u0985\u0995\u09cd\u09b7\u09ae?",
    "accessibility-link": "\u0985\u09ad\u09bf\u0997\u09ae\u09cd\u09af\u09a4\u09be",
    "appreciate-in": "\u0986\u09ae\u09b0\u09be \u0986\u09aa\u09a8\u09be\u09b0 \u0987\u09a8\u09aa\u09c1\u099f \u09aa\u09cd\u09b0\u09b6\u0982\u09b8\u09be \u0995\u09b0\u09bf\u0964 \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u09af\u09c7\u0995\u09cb\u09a8 \u09b8\u09ae\u09b8\u09cd\u09af\u09be \u099c\u09be\u09a8\u09c1\u09a8 \u098f\u09ac\u0982 \u0986\u09ae\u09b0\u09be \u098f\u0997\u09c1\u09b2\u09bf \u09a6\u09cd\u09b0\u09c1\u09a4 \u09b8\u09ae\u09be\u09a7\u09be\u09a8 \u0995\u09b0\u09ac\u0964",
    "fb-but": "\u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u09aa\u09cd\u09b0\u09a4\u09bf\u0995\u09cd\u09b0\u09bf\u09af\u09bc\u09be \u09aa\u09be\u09a0\u09be\u09a8",
    "feedback-text": "\u0986\u09aa\u09a8\u09bf \u09b8\u09ac\u09c7\u09ae\u09be\u09a4\u09cd\u09b0 \u09a6\u09c7\u0996\u09c7\u099b\u09c7\u09a8 \u098f\u0987\u099a\u09b8\u09bf\u09cd\u09af\u09be\u09aa\u09cd\u099f\u09be \u09b8\u09ae\u09cd\u09aa\u09b0\u09cd\u0995\u09c7 \u09aa\u09cd\u09b0\u09a4\u09bf\u0995\u09cd\u09b0\u09bf\u09af\u09bc\u09be \u099c\u09be\u09a8\u09be\u09a8",
    "home-ent-1": "\u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u098f\u09a8\u09cd\u099f\u09be\u09b0\u09aa\u09cd\u09b0\u09be\u0987\u099c \u09b8\u09c1\u09b0\u0995\u09cd\u09b7\u09be \u0985\u09ab\u09be\u09b0 \u0996\u09c1\u0981\u099c\u099b\u09c7\u09a8?",
    "home-ent-2": "\u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u09a8\u09a4\u09c1\u09a8 \u0989\u09a4\u09cd\u09b8\u09b0\u09cd\u0997\u09c0\u0995\u09c3\u09a4 \u09b8\u09be\u0987\u099f: \u09ac\u099f\u09b8\u099f\u09aa \u09a1\u099f\u0995\u09ae \u09a6\u09c7\u0996\u09c1\u09a8",
    "home-intro-body": "\u09ac\u099f\u0997\u09c1\u09b2\u09bf \u09b0\u09be\u0996\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u0986\u09aa\u09a8\u09bf \u0995\u09bf \u0995\u09cd\u09af\u09be\u09aa\u099a\u09be \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09c7\u09a8? hCaptcha \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0\u09b0 \u0997\u09cb\u09aa\u09a8\u09c0\u09af\u09bc\u09a4\u09be \u09b0\u0995\u09cd\u09b7\u09be \u0995\u09b0\u09c7, \u0993\u09af\u09bc\u09c7\u09ac\u09b8\u09be\u0987\u099f\u0997\u09c1\u09b2\u09bf\u0995\u09c7 \u09aa\u09c1\u09b0\u09b8\u09cd\u0995\u09c3\u09a4 \u0995\u09b0\u09c7 \u098f\u09ac\u0982 \u09b8\u0982\u09b8\u09cd\u09a5\u09be\u0997\u09c1\u09b2\u09bf \u09a4\u09be\u09a6\u09c7\u09b0 \u09a1\u09c7\u099f\u09be \u09b2\u09c7\u09ac\u09c7\u09b2 \u09aa\u09c7\u09a4\u09c7 \u09b8\u09b9\u09be\u09af\u09bc\u09a4\u09be \u0995\u09b0\u09c7\u0964 \u098f\u099f\u09bf \u09b0\u09bf\u09aa\u09cd\u09af\u09be\u099a\u099a\u098f\u09b0 \u099c\u09a8\u09cd\u09af \u098f\u0995\u099f\u09bf \u09a1\u09cd\u09b0\u09aa-\u0987\u09a8 \u09aa\u09cd\u09b0\u09a4\u09bf\u09b8\u09cd\u09a5\u09be\u09aa\u09a8: \u0986\u09aa\u09a8\u09bf \u0995\u09af\u09bc\u09c7\u0995 \u09ae\u09bf\u09a8\u09bf\u099f\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09b8\u09cd\u09af\u09c1\u0987\u099a \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7\u09a8\u0964",
    "home-intro-head": "\u0986\u09b0\u0993 \u09ac\u099f \u09ac\u09a8\u09cd\u09a7 \u0995\u09b0\u09c1\u09a8\u0964 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0\u09b0 \u0997\u09cb\u09aa\u09a8\u09c0\u09af\u09bc\u09a4\u09be \u09b0\u0995\u09cd\u09b7\u09be \u0995\u09b0\u09be \u09b6\u09c1\u09b0\u09c1 \u0995\u09b0\u09c1\u09a8\u0964",
    "home-main-1": "\u09ac\u09cd\u09af\u0995\u09cd\u09a4\u09bf\u0997\u09a4. \u09a8\u09bf\u09b0\u09be\u09aa\u09a6. \u09a6\u09cd\u09b0\u09c1\u09a4\u0964 \u098f\u09ac\u0982 \u0986\u09ae\u09b0\u09be \u098f\u099f\u09bf \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u0986\u09aa\u09a8\u09be\u0995\u09c7 \u0985\u09b0\u09cd\u09a5 \u09aa\u09cd\u09b0\u09a6\u09be\u09a8 \u0995\u09b0\u09bf\u0964",
    "home-main-2": "\u0997\u09cb\u09aa\u09a8\u09c0\u09af\u09bc\u09a4\u09be \u0995\u09c7\u09a8\u09cd\u09a6\u09cd\u09b0\u09c0\u09ad\u09c2\u09a4",
    "home-main-2c": "\u09ac\u09bf\u099c\u09cd\u099e\u09be\u09aa\u09a8 \u09a8\u09c7\u099f\u0993\u09af\u09bc\u09be\u09b0\u09cd\u0995\u0997\u09c1\u09b2\u09bf \u0986\u09aa\u09a8\u09be\u09b0 \u09a6\u09b0\u09cd\u09b6\u0995\u09a6\u09c7\u09b0 \u09aa\u09a3\u09cd\u09af \u09b9\u09bf\u09b8\u09be\u09ac\u09c7 \u09a6\u09c7\u0996\u09c7 see HCaptcha- \u098f\u09b0 \u099c\u09a8\u09cd\u09af \u09aa\u09a3\u09cd\u09af\u099f\u09bf \u09a4\u09be\u09a6\u09c7\u09b0 \u09b8\u09b0\u09ac\u09b0\u09be\u09b9 \u0995\u09b0\u09be \u0995\u09be\u099c\u0964 \u0986\u09aa\u09a8\u09be\u09b0 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0\u09b0\u09be \u0995\u09be\u09b0\u09be \u09a4\u09be \u0986\u09ae\u09b0\u09be \u09af\u09a4\u09cd\u09a8 \u09a8\u09bf\u0987 \u09a8\u09be, \u0995\u09c7\u09ac\u09b2 \u09a4\u09be\u09b0\u09be \u0995\u09c0\u09ad\u09be\u09ac\u09c7 \u09a6\u09c2\u09b7\u09bf\u09a4 \u09b9\u09ac\u09c7\u0964 \u0986\u09ae\u09b0\u09be \u0997\u09cb\u09aa\u09a8\u09c0\u09af\u09bc\u09a4\u09be \u09aa\u09be\u09b8\u09c7\u09b0 \u09ae\u09a4\u09cb \u0989\u09a6\u09cd\u09af\u09cb\u0997\u0997\u09c1\u09b2\u09bf\u0995\u09c7 \u09b8\u09ae\u09b0\u09cd\u09a5\u09a8 \u0995\u09b0\u09bf \u098f\u09ac\u0982 \u0997\u09cb\u09aa\u09a8\u09c0\u09af\u09bc\u09a4\u09be \u098f\u09ac\u0982 \u09b8\u09c1\u09b0\u0995\u09cd\u09b7\u09be\u09b0 \u09ad\u09be\u09b0\u09b8\u09be\u09ae\u09cd\u09af \u09ac\u099c\u09be\u09af\u09bc \u09b0\u09be\u0996\u09a4\u09c7 \u0995\u09a0\u09cb\u09b0 \u09aa\u09b0\u09bf\u09b6\u09cd\u09b0\u09ae \u0995\u09b0\u09bf\u0964",
    "home-main-3": "\u09b8\u09c1\u09b0\u0995\u09cd\u09b7\u09be \u09aa\u09cd\u09b0\u09a5\u09ae",
    "home-main-3c": "\u09af\u09a5\u09be\u09b0\u09cd\u09a5\u09a4\u09be \u0986\u09aa\u09a8\u09be\u09b0 \u0986\u0997\u09cd\u09b0\u09b9 \u098f\u09ac\u0982 \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7\u0964 hCaptcha \u09aa\u09cd\u09b0\u0995\u09be\u09b6\u0995-\u0995\u09c7\u09a8\u09cd\u09a6\u09cd\u09b0\u09bf\u0995\u0964 \u0986\u09ae\u09b0\u09be \u0986\u09aa\u09a8\u09be\u0995\u09c7 \u09aa\u09cd\u09b0\u0995\u09c3\u09a4 \u09a1\u09c7\u099f\u09be \u09a6\u09c7\u0993\u09af\u09bc\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u0995\u09be\u099c \u0995\u09b0\u09bf \u098f\u09ac\u0982 \u0986\u09aa\u09a8\u09be\u0995\u09c7 \u098f\u099f\u09bf \u0995\u09c0\u09ad\u09be\u09ac\u09c7 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09ac\u09c7\u09a8 \u09a4\u09be \u09b8\u09bf\u09a6\u09cd\u09a7\u09be\u09a8\u09cd\u09a4 \u09a8\u09bf\u09a4\u09c7 \u09a6\u09bf\u0987\u0964 \u0985\u09a8\u09cd\u09af\u09be\u09a8\u09cd\u09af \u0995\u09cd\u09af\u09be\u09aa\u099a\u09be \u09b8\u09b0\u09ac\u09b0\u09be\u09b9 \u0995\u09b0\u09c7\u099b\u09c7\u09a8",
    "home-main-3c2": "\u09ac\u09bf\u099c\u09cd\u099e\u09be\u09aa\u09a8 \u09b8\u0982\u09b8\u09cd\u09a5\u09be\u0964 \u098f\u09b0 \u0985\u09b0\u09cd\u09a5 \u09b9'\u09b2 \u09a4\u09be\u09b0\u09be \u09af\u09c7 \u09b8\u09ae\u09b8\u09cd\u09a4 \u09ac\u099f \u09ac\u09cd\u09b2\u0995 \u0995\u09b0\u09c7 \u09a4\u09be \u09a4\u09be\u09a6\u09c7\u09b0 \u0986\u09af\u09bc \u0995\u09ae\u09be\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7\u0964",
    "home-main-4": "\u0986\u09b0\u0993 \u09ad\u09be\u09b2 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0\u09b0 \u0985\u09ad\u09bf\u099c\u09cd\u099e\u09a4\u09be",
    "home-main-4c": "\u09aa\u09b0\u09bf\u09b6\u09c0\u09b2\u09bf\u09a4 \u09ae\u09c7\u09b6\u09bf\u09a8 \u09b2\u09be\u09b0\u09cd\u09a8\u09bf\u0982 \u09ae\u09a1\u09c7\u09b2\u0997\u09c1\u09b2\u09bf \u09ae\u09be\u09a8\u09ac\u09bf\u0995\u09a4\u09be \u09a8\u09bf\u09b0\u09cd\u09a7\u09be\u09b0\u09a3\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u098f\u0987\u099a\u09b8\u09bf\u09cd\u09af\u09be\u09aa\u09cd\u099f\u09be \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09c7\u0964 \u098f\u099f\u09bf \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0\u09a6\u09c7\u09b0 \u0995\u09be\u099b \u09a5\u09c7\u0995\u09c7 \u0995\u09ae \u09ac\u09bf\u09b0\u0995\u09cd\u09a4\u09bf\u0995\u09b0 \u0995\u09be\u099c\u0997\u09c1\u09b2\u09bf\u09b0 \u09aa\u09cd\u09b0\u09af\u09bc\u09cb\u099c\u09a8 \u098f\u09ac\u0982 \u098f\u099f\u09bf \u0995\u09b0\u09a4\u09c7 \u0997\u09cd\u09b0\u09be\u09b9\u0995\u09a6\u09c7\u09b0 \u09b2\u09c7\u09ac\u09c7\u09b2\u09af\u09c1\u0995\u09cd\u09a4 \u0997\u09cd\u09b0\u09be\u09b9\u0995\u09a6\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u09a6\u09b0\u0995\u09be\u09b0\u09c0 \u09b2\u09c7\u09ac\u09c7\u09b2\u09af\u09c1\u0995\u09cd\u09a4 \u09a1\u09c7\u099f\u09be \u09a4\u09c8\u09b0\u09bf \u0995\u09b0\u09be\u09b0 \u09b8\u09ae\u09af\u09bc \u09a4\u09be\u09a6\u09c7\u09b0 \u09b8\u09ae\u09af\u09bc \u0995\u09ae \u09a6\u09c7\u09af\u09bc\u0964",
    "home-main-plans": "hCaptcha \u0995\u09cb\u09a8\u0993 \u0986\u0995\u09be\u09b0\u09c7\u09b0 \u09aa\u09cd\u09b0\u0995\u09be\u09b6\u0995\u09a6\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u09ac\u09bf\u09a8\u09be\u09ae\u09c2\u09b2\u09cd\u09af\u09c7 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u09af\u09cb\u0997\u09cd\u09af\u0964 \u09a4\u09ac\u09c7, \u0986\u09b0\u0993 \u09a8\u09bf\u09af\u09bc\u09a8\u09cd\u09a4\u09cd\u09b0\u09a3\u09c7\u09b0 \u09aa\u09cd\u09b0\u09af\u09bc\u09cb\u099c\u09a8 \u098f\u09ae\u09a8 \u0989\u09a6\u09cd\u09af\u09cb\u0997 \u098f\u09ac\u0982 \u09aa\u09cd\u09b2\u09cd\u09af\u09be\u099f\u09ab\u09b0\u09cd\u09ae\u0997\u09c1\u09b2\u09bf\u09b0 \u099c\u09a8\u09cd\u09af, \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u098f\u09a8\u09cd\u099f\u09be\u09b0\u09aa\u09cd\u09b0\u09be\u0987\u099c \u09b8\u09ae\u09be\u09a7\u09be\u09a8 \u0989\u09aa\u09b2\u09ac\u09cd\u09a7\u0964",
    "home-main-plans2": "hCaptcha \u098f\u09a8\u09cd\u099f\u09be\u09b0\u09aa\u09cd\u09b0\u09be\u0987\u099c \u09b8\u09ae\u09be\u09a7\u09be\u09a8 \u0997\u09cd\u09b0\u09b9\u09c7\u09b0 \u0995\u09af\u09bc\u09c7\u0995\u099f\u09bf \u09ac\u09c3\u09b9\u09a4\u09cd\u09a4\u09ae \u09b8\u0982\u09b8\u09cd\u09a5\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u0995\u09af\u09bc\u09c7\u0995 \u09ae\u09bf\u09b2\u09bf\u09af\u09bc\u09a8 \u0993\u09af\u09bc\u09c7\u09ac\u09b8\u09be\u0987\u099f \u099c\u09c1\u09a1\u09bc\u09c7 \u0995\u09af\u09bc\u09c7\u0995 \u09ae\u09bf\u09b2\u09bf\u09af\u09bc\u09a8 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0\u0995\u09c7 \u09b8\u09c1\u09b0\u0995\u09cd\u09b7\u09be \u09a6\u09c7\u09af\u09bc\u0964 \u0986\u09b0\u0993 \u09a4\u09a5\u09cd\u09af\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u09ac\u09be \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u09b8\u09be\u09a5\u09c7 \u09af\u09cb\u0997\u09be\u09af\u09cb\u0997 \u0995\u09b0\u09c1\u09a8",
    "home-main-plans3": "hCaptcha \u098f\u09a8\u09cd\u099f\u09be\u09b0\u09aa\u09cd\u09b0\u09be\u0987\u099c \u09b8\u09ae\u09be\u09a7\u09be\u09a8 \u0997\u09cd\u09b0\u09b9\u09c7\u09b0 \u0995\u09af\u09bc\u09c7\u0995\u099f\u09bf \u09ac\u09c3\u09b9\u09a4\u09cd\u09a4\u09ae \u09b8\u0982\u09b8\u09cd\u09a5\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u0995\u09af\u09bc\u09c7\u0995 \u09ae\u09bf\u09b2\u09bf\u09af\u09bc\u09a8 \u0993\u09af\u09bc\u09c7\u09ac\u09b8\u09be\u0987\u099f \u099c\u09c1\u09a1\u09bc\u09c7 \u0995\u09af\u09bc\u09c7\u0995 \u09ae\u09bf\u09b2\u09bf\u09af\u09bc\u09a8 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0\u0995\u09c7 \u09b8\u09c1\u09b0\u0995\u09cd\u09b7\u09be \u09a6\u09c7\u09af\u09bc\u0964 \u0986\u09b0\u0993 \u09a4\u09a5\u09cd\u09af\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u09ac\u09be \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u09b8\u09be\u09a5\u09c7 \u09af\u09cb\u0997\u09be\u09af\u09cb\u0997 \u0995\u09b0\u09c1\u09a8",
    "home-main-plans4": "\u09ac\u09bf\u09b8\u09cd\u09a4\u09be\u09b0\u09bf\u09a4 \u099c\u09be\u09a8\u09be\u09b0 \u099c\u09a8\u09cd\u09af.",
    "learn-more-hcaptcha": "HCaptcha \u09b8\u09ae\u09cd\u09aa\u09b0\u09cd\u0995\u09c7 \u0986\u09b0\u0993 \u099c\u09be\u09a8\u09c1\u09a8",
    "page-acc-faq-1": "\u09aa\u09cd\u09b0\u09b6\u09cd\u09a8: \u098f\u0987\u099a\u09b8\u09bf\u09cd\u09af\u09be\u09aa\u09cd\u099f\u09be \u09ac\u09bf\u09ad\u09be\u0997 508 + \u09a1\u09be\u09ac\u09cd\u09b2\u09c1\u09b8\u09bf\u098f\u099c\u09bf 2.0 \u098f\u09b0 \u0985\u09a8\u09c1\u09ac\u09b0\u09cd\u09a4\u09c0 \u0995\u09bf? \u0989\u09a4\u09cd\u09a4\u09b0: \u0986\u09ae\u09b0\u09be \u098f\u099f\u09bf \u09ac\u09bf\u09b6\u09cd\u09ac\u09be\u09b8 \u0995\u09b0\u09bf: \u09af\u09c7 \u0995\u09cb\u09a8\u0993 \u09a7\u09b0\u09a3\u09c7\u09b0 \u09aa\u09cd\u09b0\u09a4\u09bf\u09ac\u09a8\u09cd\u09a7\u0995\u09a4\u09be \u09b8\u09b9 \u09b8\u09ae\u09b8\u09cd\u09a4 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0 \u09af\u09be\u09b0\u09be \u0993\u09af\u09bc\u09c7\u09ac \u09ac\u09cd\u09b0\u09be\u0989\u099c \u0995\u09b0\u09a4\u09c7 \u098f\u09ac\u0982 \u09ab\u09b0\u09cd\u09ae\u0997\u09c1\u09b2\u09bf\u09a4\u09c7 \u09aa\u09be\u09a0\u09cd\u09af \u09aa\u09cd\u09b0\u09ac\u09c7\u09b6 \u0995\u09b0\u09a4\u09c7 \u09b8\u0995\u09cd\u09b7\u09ae \u09b9\u09a8 \u09a4\u09be\u09b0\u09be \u09b0\u09c7\u099c\u09bf\u09b8\u09cd\u099f\u09cd\u09b0\u09c7\u09b6\u09a8 \u0995\u09b0\u09be\u09b0 \u09aa\u09b0\u09c7 \u098f\u0987\u099a\u09b8\u09bf\u09cd\u09af\u09be\u09aa\u09cd\u099f\u09be \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u09b8\u09c1\u09b0\u0995\u09cd\u09b7\u09bf\u09a4 \u09aa\u09b0\u09bf\u09b7\u09c7\u09ac\u09be\u0997\u09c1\u09b2\u09bf\u09a4\u09c7 \u0985\u09cd\u09af\u09be\u0995\u09cd\u09b8\u09c7\u09b8 \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09ac\u09c7\u09a8\u0964 \u09a4\u09ac\u09c7 \u098f\u099f\u09bf \u0986\u0987\u09a8\u09c0 \u09aa\u09b0\u09be\u09ae\u09b0\u09cd\u09b6 \u09a8\u09af\u09bc: \u0986\u09aa\u09a8\u09be\u09b0 \u09b8\u09cd\u09a5\u09be\u09aa\u09a8\u09be\u09b0 \u0995\u09cd\u09b7\u09c7\u09a4\u09cd\u09b0\u09c7 \u098f\u099f\u09bf \u09a8\u09bf\u09b6\u09cd\u099a\u09bf\u09a4 \u09b9\u0993\u09af\u09bc\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u0986\u09aa\u09a8\u09be\u09b0 \u09a8\u09bf\u09b0\u09cd\u09a6\u09bf\u09b7\u09cd\u099f \u09aa\u09cd\u09b0\u09af\u09bc\u09cb\u0997\u0995\u09c7 \u09ac\u09bf\u09ac\u09c7\u099a\u09a8\u09be \u0995\u09b0\u09c7 \u0986\u09aa\u09a8\u09be\u09b0 \u09a8\u09bf\u099c\u09b8\u09cd\u09ac \u09ae\u09c2\u09b2\u09cd\u09af\u09be\u09af\u09bc\u09a8 \u0995\u09b0\u09be \u0989\u099a\u09bf\u09a4\u0964",
    "page-acc-faq-2": "\u09aa\u09cd\u09b0\u09b6\u09cd\u09a8: \u0986\u09aa\u09a8\u09bf \u0995\u09bf \u0985\u09a1\u09bf\u0993\u09b0 \u09ae\u09a4\u09cb \u0985\u09a8\u09cd\u09af\u09be\u09a8\u09cd\u09af \u0985\u09cd\u09af\u09be\u0995\u09cd\u09b8\u09c7\u09b8\u09af\u09cb\u0997\u09cd\u09af\u09a4\u09be\u09b0 (\u098f 11y) \u09ac\u09bf\u0995\u09b2\u09cd\u09aa\u0997\u09c1\u09b2\u09bf\u09a4\u09c7 \u0995\u09be\u099c \u0995\u09b0\u099b\u09c7\u09a8? \u0989\u09a4\u09cd\u09a4\u09b0: \u09aa\u09c2\u09b0\u09cd\u09ac\u09c7 \u099c\u09a8\u09aa\u09cd\u09b0\u09bf\u09af\u09bc \u09ac\u09bf\u0995\u09b2\u09cd\u09aa\u0997\u09c1\u09b2\u09bf \u09af\u09c7\u09ae\u09a8 \u0985\u09a1\u09bf\u0993 \u0995\u09cd\u09af\u09be\u09aa\u099a\u09be\u09b8\u0997\u09c1\u09b2\u09bf \u0985\u09a8\u09c7\u0995 \u098f 11y \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0\u09a6\u09c7\u09b0 \u09b8\u09be\u09a5\u09c7 \u09ac\u09c8\u09b7\u09ae\u09cd\u09af\u09ae\u09c2\u09b2\u0995 \u0986\u099a\u09b0\u09a3 \u0995\u09b0\u09c7 \u098f\u09ac\u0982 \u0986\u09a7\u09c1\u09a8\u09bf\u0995 \u09ae\u09c7\u09b6\u09bf\u09a8 \u09b2\u09be\u09b0\u09cd\u09a8\u09bf\u0982 \u0995\u09cc\u09b6\u09b2\u0997\u09c1\u09b2\u09bf \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u09b8\u09b9\u099c\u09c7\u0987 \u09aa\u09b0\u09be\u099c\u09bf\u09a4 \u09b9\u09af\u09bc\u0964 \u098f\u099f\u09bf \u09ac\u09b0\u09cd\u09a4\u09ae\u09be\u09a8 \u0985\u09a1\u09bf\u0993 \u099a\u09cd\u09af\u09be\u09b2\u09c7\u099e\u09cd\u099c\u0997\u09c1\u09b2\u09bf\u0995\u09c7 \u0986\u09b0\u0993 \u09ac\u09c7\u09b6\u09bf \u0995\u09a0\u09bf\u09a8 \u09b9\u09af\u09bc\u09c7 \u0989\u09a0\u09a4\u09c7 \u09ac\u09be\u09a7\u09cd\u09af \u0995\u09b0\u09c7\u099b\u09c7, \u09b6\u09ac\u09cd\u09a6\u09c7\u09b0 \u09aa\u09cd\u09b0\u099a\u09b2\u09a8, \u09ac\u09bf\u099c\u09cb\u09a1\u09bc \u09b8\u09ae\u09af\u09bc, \u0985\u09b8\u09cd\u09ac\u09be\u09ad\u09be\u09ac\u09bf\u0995 \u09b6\u09ac\u09cd\u09a6\u09c7\u09b0 \u09b8\u0982\u09ae\u09bf\u09b6\u09cd\u09b0\u09a3 \u0987\u09a4\u09cd\u09af\u09be\u09a6\u09bf\u0995\u09c7 \u0986\u0995\u09cd\u09b0\u09ae\u09a3\u0995\u09be\u09b0\u09c0\u09a6\u09c7\u09b0 \u09aa\u09b0\u09be\u09b8\u09cd\u09a4 \u0995\u09b0\u09a4\u09c7\u0964 \u0986\u09ae\u09b0\u09be \u098f\u0987 \u09aa\u09a6\u09cd\u09a7\u09a4\u09bf\u09b0 \u09ac\u09bf\u09b7\u09af\u09bc\u09c7 \u0995\u09ae \u0989\u09a4\u09cd\u09b8\u09be\u09b9\u09c0 \u09ac\u09a8\u09be\u09ae\u0964 \u099a\u09cd\u09af\u09be\u09b2\u09c7\u099e\u09cd\u099c \u09aa\u09c1\u09b0\u09cb\u09aa\u09c1\u09b0\u09bf \u098f\u09a1\u09bc\u09bf\u09af\u09bc\u09c7 \u099a\u09b2\u09c7\u099b\u09bf, \u09a4\u09ac\u09c7 \u09af\u09a6\u09bf \u098f 11 \u09b8\u09ae\u09cd\u09aa\u09cd\u09b0\u09a6\u09be\u09af\u09bc\u09c7\u09b0 \u09b8\u09ae\u09cd\u09aa\u09cd\u09b0\u09a6\u09be\u09af\u09bc\u09c7\u09b0 \u0995\u09be\u099b \u09a5\u09c7\u0995\u09c7 \u099a\u09be\u09b9\u09bf\u09a6\u09be \u09a5\u09be\u0995\u09c7 \u09a4\u09ac\u09c7 \u09a4\u09be \u09ac\u09bf\u09ac\u09c7\u099a\u09a8\u09be \u0995\u09b0\u09ac\u0964 \u09a4\u09ac\u09c7 \u0985\u09cd\u09af\u09be\u0995\u09cd\u09b8\u09c7\u09b8\u09bf\u09ac\u09bf\u09b2\u09bf\u099f\u09bf \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u09c7\u09b0 \u0995\u09cd\u09b7\u09c7\u09a4\u09cd\u09b0\u09c7 \u09aa\u09cd\u09b0\u09be\u0987\u09ad\u09c7\u09b8\u09bf \u09aa\u09be\u09b8\u09c7 \u0986\u09ae\u09b0\u09be \u0996\u09c1\u09ac \u0986\u0997\u09cd\u09b0\u09b9\u09c0\u0964 \u0986\u09ae\u09b0\u09be \u09ac\u09bf\u09b6\u09cd\u09ac\u09be\u09b8 \u0995\u09b0\u09bf \u09af\u09c7 \u09aa\u09cd\u09b0\u09be\u0987\u09ad\u09c7\u09b8\u09bf \u09aa\u09be\u09b8 \u0987\u09b8\u09cd\u09af\u09c1 \u0995\u09b0\u09be\u09b0 \u09b8\u09be\u09a5\u09c7 \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u09ac\u09b0\u09cd\u09a4\u09ae\u09be\u09a8 \u098f 11-\u098f \u09aa\u09a6\u09cd\u09a7\u09a4\u09bf\u09b0 \u09b8\u09ae\u09a8\u09cd\u09ac\u09af\u09bc \u0995\u09b0\u09b2\u09c7 11 \u099c\u09a8 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0 \u09a8\u09bf\u09b0\u09be\u09aa\u09a6\u09c7 \u09ac\u09cd\u09b0\u09be\u0989\u099c \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09ac\u09c7\u09a8 \u098f\u09ac\u0982 \u09a4\u09be\u09a6\u09c7\u09b0 \u099f\u09cd\u09b0\u09be\u09ab\u09bf\u0995 \u0986\u09b0\u0993 \u09ac\u09cd\u09af\u0995\u09cd\u09a4\u09bf\u0997\u09a4 \u09ac\u09b2\u09c7 \u098f\u0987 \u09b8\u09c1\u09b0\u0995\u09cd\u09b7\u09bf\u09a4 \u09a5\u09be\u0995\u09ac\u09c7, \u09ac\u099f \u0985\u09aa\u09be\u09b0\u09c7\u099f\u09b0\u09a6\u09c7\u09b0 \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u0985\u09aa\u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09c7 \u09b8\u09c0\u09ae\u09be\u09ac\u09a6\u09cd\u09a7 \u0995\u09b0\u09be \u09b9\u09ac\u09c7 \u09af\u0996\u09a8 \u098f 11-\u098f \u09ac\u09bf\u0995\u09b2\u09cd\u09aa\u0997\u09c1\u09b2\u09bf \u09aa\u09be\u0993\u09af\u09bc\u09be \u09af\u09be\u09af\u09bc \u09a4\u0996\u09a8 \u0985\u09ac\u09b6\u09cd\u09af\u09ae\u09cd\u09ad\u09be\u09ac\u09c0\u09ad\u09be\u09ac\u09c7 \u0998\u099f\u09c7 occurs \u0986\u09ae\u09b0\u09be \u098f\u0987 \u09a8\u09a4\u09c1\u09a8 \u09aa\u09cd\u09b0\u09af\u09c1\u0995\u09cd\u09a4\u09bf\u099f\u09bf \u09ae\u09be\u09a8\u0995 \u0995\u09b0\u09c7 \u0986\u0987\u0987\u099f\u09bf\u098f\u09ab \u0993\u09af\u09bc\u09be\u09b0\u09cd\u0995\u09bf\u0982 \u0997\u09cd\u09b0\u09c1\u09aa\u09c7\u09b0 \u09b8\u0995\u09cd\u09b0\u09bf\u09af\u09bc \u0985\u0982\u09b6\u0997\u09cd\u09b0\u09b9\u09a3\u0995\u09be\u09b0\u09c0\u0964",
    "page-acc-faq-3": "\u09aa\u09cd\u09b0\u09b6\u09cd\u09a8: \u0997\u09cb\u09aa\u09a8\u09c0\u09af\u09bc\u09a4\u09be \u09b8\u09ae\u09cd\u09aa\u09b0\u09cd\u0995\u09c7 \u0995\u09c0? \u09b0\u09c7\u099c\u09bf\u09b8\u09cd\u099f\u09cd\u09b0\u09c7\u09b6\u09a8 \u0995\u09cb\u09a8\u0993 \u0989\u09aa\u09be\u09af\u09bc\u09c7 a11y \u09ac\u09cd\u09b0\u09be\u0989\u099c\u09bf\u0982 \u09a1\u09c7\u099f\u09be \u09aa\u09cd\u09b0\u0995\u09be\u09b6 \u0995\u09b0\u09c7? \u0986\u09aa\u09a8\u09bf \u0987\u09ae\u09c7\u09b2 \u09a6\u09bf\u09af\u09bc\u09c7 \u0995\u09bf \u0995\u09b0\u09ac\u09c7\u09a8? \u0989\u09a4\u09cd\u09a4\u09b0: hCaptcha \u0997\u09cd\u09b0\u09be\u0989\u09a8\u09cd\u09a1 \u0986\u09aa \u09a5\u09c7\u0995\u09c7 \u0997\u09cb\u09aa\u09a8\u09c0\u09af\u09bc\u09a4\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u09a1\u09bf\u099c\u09be\u0987\u09a8 \u0995\u09b0\u09be \u09b9\u09af\u09bc\u09c7\u099b\u09c7\u0964 \u09ac\u09bf\u099c\u09cd\u099e\u09be\u09aa\u09a8 \u09a8\u09c7\u099f\u0993\u09af\u09bc\u09be\u09b0\u09cd\u0995\u0997\u09c1\u09b2\u09bf\u09b0 \u09ae\u09be\u09b2\u09bf\u0995\u09be\u09a8\u09be\u09a7\u09c0\u09a8 \u09b0\u09c7\u0995\u09cd\u09af\u09be\u09aa\u099a\u09be\u098f\u09b0 \u09ae\u09a4\u09cb traditional\u09a4\u09bf\u09b9\u09cd\u09af\u0997\u09a4 \u09ac\u09bf\u0995\u09b2\u09cd\u09aa\u0997\u09c1\u09b2\u09bf\u09b0 \u099a\u09c7\u09af\u09bc\u09c7 \u098f\u099f\u09bf\u09b0 \u099a\u09c7\u09af\u09bc\u09c7 \u0986\u09b2\u09be\u09a6\u09be who \u0986\u09ae\u09b0\u09be \u098f 11 \u098f \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u09c7\u09b0 \u09b8\u09c1\u09ac\u09bf\u09a7\u09be\u09b0\u09cd\u09a5\u09c7 \u098f\u09ac\u0982 \u0985\u09aa\u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u09b0\u09cb\u09a7 \u0995\u09b0\u09be \u099b\u09be\u09a1\u09bc\u09be \u0985\u09a8\u09cd\u09af \u0995\u09cb\u09a8\u0993 \u0989\u09a6\u09cd\u09a6\u09c7\u09b6\u09cd\u09af\u09c7 \u0985\u09cd\u09af\u09be\u0995\u09cd\u09b8\u09c7\u09b8\u09af\u09cb\u0997\u09cd\u09af\u09a4\u09be\u09b0 \u0987\u09ae\u09c7\u09b2 \u09ac\u09be \u09a4\u09a5\u09cd\u09af \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09bf \u09a8\u09be\u0964 \u0986\u09ae\u09b0\u09be \u0995\u09c0\u09ad\u09be\u09ac\u09c7 \u09a1\u09c7\u099f\u09be \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09bf \u09b8\u09c7 \u09b8\u09ae\u09cd\u09aa\u09b0\u09cd\u0995\u09c7 \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u0997\u09cb\u09aa\u09a8\u09c0\u09af\u09bc\u09a4\u09be\u09b0 \u09a8\u09c0\u09a4\u09bf\u09ae\u09be\u09b2\u09be\u09b0 \u09ac\u09bf\u09b8\u09cd\u09a4\u09c3\u09a4 \u098f\u09ac\u0982 \u0985\u09a8\u09c1\u09ae\u09cb\u09a6\u09a8\u09ae\u09c2\u09b2\u0995 \u0989\u09a4\u09cd\u09a4\u09b0 \u09b0\u09af\u09bc\u09c7\u099b\u09c7 \u09a4\u09ac\u09c7 \u09b8\u0982\u0995\u09cd\u09b7\u09bf\u09aa\u09cd\u09a4 \u0989\u09a4\u09cd\u09a4\u09b0\u099f\u09bf \u09b9'\u09b2 \u0986\u09aa\u09a8\u09be\u09b0 \u09ac\u09cd\u09b0\u09be\u0989\u099c\u09bf\u0982 \u0987\u09a4\u09bf\u09b9\u09be\u09b8\u09c7\u09b0 \u098f\u0995\u099c\u09a8 \u09ac\u09cd\u09af\u0995\u09cd\u09a4\u09bf \u09b9\u09bf\u09b8\u09be\u09ac\u09c7 \u0986\u09aa\u09a8\u09be\u0995\u09c7 \u09af\u09c1\u0995\u09cd\u09a4 \u0995\u09b0\u09be\u09b0 \u0995\u09cb\u09a8\u0993 \u0986\u0997\u09cd\u09b0\u09b9 \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u09a8\u09c7\u0987 W \u0986\u09ae\u09b0\u09be \u09ac\u09b0\u09cd\u09a4\u09ae\u09be\u09a8\u09c7 \u0986\u09aa\u09a8\u09be\u09b0 \u0987\u09ae\u09c7\u09b2 \u09a0\u09bf\u0995\u09be\u09a8\u09be\u099f\u09bf \u09a6\u09cd\u09b0\u09c1\u09a4 \u09a4\u09cd\u09af\u09be\u0997 \u0995\u09b0\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u098f\u0995\u099f\u09bf \u0995\u09cd\u09b0\u09bf\u09aa\u09cd\u099f\u09cb\u0997\u09cd\u09b0\u09be\u09ab\u09bf\u0995 \u09b8\u09ae\u09be\u09a7\u09be\u09a8\u09c7\u0993 \u0995\u09be\u099c \u0995\u09b0\u099b\u09bf W \u098f\u0996\u09a8\u0993 \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u09aa\u09cd\u09b0\u09be\u0987\u09ad\u09c7\u09b8\u09bf \u09aa\u09be\u09b8 \u0995\u09be\u099c\u09c7\u09b0 \u09aa\u09b0\u09bf\u09aa\u09c2\u09b0\u0995, \u0985\u09aa\u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u09b0\u09cb\u09a7 \u0995\u09b0\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u0995\u09cd\u09b7\u09ae\u09a4\u09be \u09b0\u0995\u09cd\u09b7\u09be \u0995\u09b0\u09be\u09b0 \u09b8\u09ae\u09af\u09bc\u0964",
    "page-acc-link-1": "\u0986\u09aa\u09a8\u09bf \u09af\u09a6\u09bf \u0985\u09cd\u09af\u09be\u0995\u09cd\u09b8\u09c7\u09b8\u09af\u09cb\u0997\u09cd\u09af\u09a4\u09be\u09b0 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0 \u09b9\u09a8 \u09a4\u09ac\u09c7 \u09a6\u09af\u09bc\u09be \u0995\u09b0\u09c7 \u098f\u0996\u09be\u09a8\u09c7 \u09b8\u09be\u0987\u09a8 \u0986\u09aa \u0995\u09b0\u09c1\u09a8\u0964",
    "page-acc-subhead": "\u09b8\u09c1\u09b0\u0995\u09cd\u09b7\u09be \u09ac\u099c\u09be\u09af\u09bc \u09b0\u09c7\u0996\u09c7 \u09b8\u09ae\u09b8\u09cd\u09a4 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0\u09b0 \u099c\u09a8\u09cd\u09af \u09ac\u09bf\u099c\u09cb\u09a1\u09bc \u0985\u09cd\u09af\u09be\u0995\u09cd\u09b8\u09c7\u09b8 \u09a8\u09bf\u09b6\u09cd\u099a\u09bf\u09a4 \u0995\u09b0\u09be",
    "page-acc-summary": "\u09b8\u09be\u09b0\u09b8\u0982\u0995\u09cd\u09b7\u09c7\u09aa",
    "page-acc-summary-contents-1": "hCaptcha \u09ac\u099f\u0997\u09c1\u09b2\u09bf \u09b2\u09cb\u0995\u09a6\u09c7\u09b0 \u09a5\u09c7\u0995\u09c7 \u0986\u09b2\u09be\u09a6\u09be \u0995\u09b0\u09c7 \u09a4\u09be\u09a6\u09c7\u09b0 \u09a5\u09be\u09ae\u09be\u09a8\u09cb\u09b0 \u099c\u09a8\u09cd\u09af \u09a1\u09bf\u099c\u09be\u0987\u09a8 \u0995\u09b0\u09be \u09b9\u09af\u09bc\u09c7\u099b\u09c7\u0964 \u09ad\u09bf\u099c\u09cd\u09af\u09c1\u09af\u09bc\u09be\u09b2 \u09aa\u09b0\u09c0\u0995\u09cd\u09b7\u09be\u0997\u09c1\u09b2\u09bf \u098f\u099f\u09bf\u09b0 \u099c\u09a8\u09cd\u09af \u098f\u0995\u099f\u09bf \u09b8\u09c1\u09ac\u09bf\u09a7\u09be\u099c\u09a8\u0995 \u09b8\u09b0\u099e\u09cd\u099c\u09be\u09ae, \u09a4\u09ac\u09c7 \u09aa\u09cd\u09b0\u09a4\u09cd\u09af\u09c7\u0995\u09c7\u0987 \u099a\u09be\u0995\u09cd\u09b7\u09c1\u09b7 \u099a\u09cd\u09af\u09be\u09b2\u09c7\u099e\u09cd\u099c \u09b8\u09ae\u09be\u09a7\u09be\u09a8 \u0995\u09b0\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 \u09a8\u09be\u0964 \u098f\u0987 \u0995\u09be\u09b0\u09a3\u09c7 \u0986\u09ae\u09b0\u09be \u098f\u0995\u099f\u09bf \u09b8\u09be\u09a7\u09be\u09b0\u09a3, \u09ac\u09c7\u09a6\u09a8\u09be\u09ac\u09bf\u09b9\u09c0\u09a8 \u09ac\u09bf\u0995\u09b2\u09cd\u09aa\u09c7\u09b0 \u09a8\u0995\u09b6\u09be \u09a4\u09c8\u09b0\u09bf \u0995\u09b0\u09c7\u099b\u09bf \u09af\u09be\u09a4\u09c7 \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u09aa\u09b0\u09bf\u09b7\u09c7\u09ac\u09be \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09c7 \u09aa\u09cd\u09b0\u0995\u09be\u09b6\u0995\u09a6\u09c7\u09b0 \u09b8\u09ae\u09cd\u09aa\u09c2\u09b0\u09cd\u09a3 \u09ac\u09bf\u09ad\u09be\u0997 508 \u098f\u09ac\u0982 \u09a1\u09be\u09ac\u09cd\u09b2\u09c1\u09b8\u09bf\u098f\u099c\u09bf \u09e8.\u09e6 \u09b8\u09ae\u09cd\u09ae\u09a4\u09bf \u09b8\u09b9 \u09b8\u0995\u09b2\u09c7\u09b0 \u099c\u09a8\u09cd\u09af \u0985\u09cd\u09af\u09be\u0995\u09cd\u09b8\u09c7\u09b8\u09af\u09cb\u0997\u09cd\u09af\u09a4\u09be \u09b8\u0982\u09b0\u0995\u09cd\u09b7\u09a3 \u0995\u09b0\u09be \u09af\u09be\u09af\u09bc let",
    "page-acc-summary-contents-2": "\u098f\u099f\u09bf \u0995\u09c0\u09ad\u09be\u09ac\u09c7 \u0995\u09be\u099c \u0995\u09b0\u09c7: \u09aa\u09cd\u09b0\u09a5\u09ae\u09a4, \u0995\u09cb\u09a8\u0993 \u0985\u09cd\u09af\u09be\u0995\u09cd\u09b8\u09c7\u09b8\u09bf\u09ac\u09bf\u09b2\u09bf\u099f\u09bf \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0 \u098f\u0987 \u0987\u0989\u0986\u09b0\u098f\u09b2\u09c7 \u09b8\u09be\u0987\u09a8 \u0986\u09aa \u0995\u09b0\u09c7, \u09af\u09be \u098f\u0987\u099a\u09b8\u09bf\u09cd\u09af\u09be\u09aa\u099a\u09be \u0989\u0987\u099c\u09c7\u099f \u09a4\u09a5\u09cd\u09af \u09aa\u09c3\u09b7\u09cd\u09a0\u09be\u09a4\u09c7 \u09b2\u09bf\u0999\u09cd\u0995\u09af\u09c1\u0995\u09cd\u09a4\u0964 \u09a4\u09be\u09a6\u09c7\u09b0 \u098f\u0995\u099f\u09bf \u098f\u09a8\u0995\u09cd\u09b0\u09bf\u09aa\u09cd\u099f \u0995\u09b0\u09be \u0995\u09c1\u0995\u09bf \u09a6\u09c7\u0993\u09af\u09bc\u09be \u09b9\u09af\u09bc \u09af\u09be \u09aa\u09cd\u09b0\u09a4\u09bf\u09a6\u09bf\u09a8 \u0995\u09af\u09bc\u09c7\u0995\u09ac\u09be\u09b0 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09be \u09af\u09c7\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7 \u09a4\u09ac\u09c7 \u09b2\u0997\u0987\u09a8\u09c7\u09b0 \u09ae\u09be\u09a7\u09cd\u09af\u09ae\u09c7 \u09aa\u09cd\u09b0\u09a4\u09bf 24 \u0998\u09a8\u09cd\u099f\u09be \u0985\u09a8\u09cd\u09a4\u09b0 \u09b8\u09a4\u09c7\u099c \u0995\u09b0\u09a4\u09c7 \u09b9\u09ac\u09c7 hen",
    "page-acc-title": "hCaptcha \u0985\u09cd\u09af\u09be\u0995\u09cd\u09b8\u09c7\u09b8\u09bf\u09ac\u09bf\u09b2\u09bf\u099f\u09bf \u09a4\u09a5\u09cd\u09af",
    "page-acc-tr-body": "\u09aa\u09cd\u09b0\u09b6\u09cd\u09a8: \u0986\u09ae\u09bf \u0995\u09c1\u0995\u09bf \u09b8\u09c7\u099f \u0995\u09b0\u09be\u09b0 \u09aa\u09b0\u09c7\u0993 \u098f\u0995\u099f\u09bf \u099a\u09cd\u09af\u09be\u09b2\u09c7\u099e\u09cd\u099c \u09a6\u09c7\u0996\u099b\u09bf\u0964 \u098f\u09b0 \u0995\u09be\u09b0\u09a3 \u0995\u09c0? \u0989\u09a4\u09cd\u09a4\u09b0: \u098f\u099f\u09bf \u09b8\u09be\u09a7\u09be\u09b0\u09a3\u09a4 \u0986\u0995\u09cd\u09b0\u09ae\u09a3\u09be\u09a4\u09cd\u09ae\u0995 \u09ac\u09bf\u099c\u09cd\u099e\u09be\u09aa\u09a8 \u09ac\u09cd\u09b2\u0995\u09be\u09b0 \u09ac\u09be \u0985\u09cd\u09af\u09be\u09a8\u09cd\u099f\u09bf-\u0995\u09c1\u0995\u09bf \u098f\u0995\u09cd\u09b8\u099f\u09c7\u09a8\u09b6\u09be\u09a8 \u09ac\u09be \u0995\u09cd\u09b0\u09b8 \u09b8\u09be\u0987\u099f \u0995\u09c1\u0995\u09bf\u099c\u0995\u09c7 \u09ac\u09cd\u09b2\u0995 \u0995\u09b0\u09c7 \u098f\u09ae\u09a8 \u098f\u0995\u099f\u09bf \u09b8\u09c7\u099f\u09bf\u0982\u09b8 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u09c7\u09b0 \u0995\u09be\u09b0\u09a3\u09c7 \u0998\u099f\u09c7 \u09a5\u09be\u0995\u09c7, \u098f\u0987 \u0995\u09cd\u09b7\u09c7\u09a4\u09cd\u09b0\u09c7 hcaptcha.com \u098f\u09b0 \u099c\u09a8\u09cd\u09af \u098f\u0995\u099f\u09bf \u0995\u09c1\u0995\u09bf \u09af\u09be \u0986\u09b2\u09be\u09a6\u09be \u09b8\u09be\u0987\u099f\u09c7 \u098f\u0987\u099a\u09b8\u09bf\u09cd\u09af\u09be\u09aa\u09cd\u099f\u09be \u099c\u09be\u09ad\u09be\u09b8\u09cd\u0995\u09cd\u09b0\u09bf\u09aa\u09cd\u099f \u09a6\u09cd\u09ac\u09be\u09b0\u09be \u09b8\u09c7\u099f \u09ac\u09be \u099a\u09c7\u0995 \u0995\u09b0\u09be \u09a5\u09be\u0995\u09c7 \u09af\u09c7\u09ae\u09a8\u099f\u09bf \u0986\u09aa\u09a8\u09bf \u0998\u09c1\u09b0\u09c7 \u09a6\u09c7\u0996\u099b\u09c7\u09a8\u0964 hCaptcha \u0985\u09cd\u09af\u09be\u0995\u09cd\u09b8\u09c7\u09b8\u09bf\u09ac\u09bf\u09b2\u09bf\u099f\u09bf \u0995\u09c1\u0995\u09bf\u099c \u09b8\u09ae\u09b8\u09cd\u09a4 \u099c\u09a8\u09aa\u09cd\u09b0\u09bf\u09af\u09bc \u09ac\u09cd\u09b0\u09be\u0989\u099c\u09be\u09b0\u0997\u09c1\u09b2\u09bf \u098f\u09ac\u0982 \u09a4\u09be\u09a6\u09c7\u09b0 \u09ae\u09be\u09a8\u0995 \u09b8\u09c7\u099f\u09bf\u0982\u09b8 \u09b8\u09b9 \u0985\u09cd\u09af\u09be\u09a1 \u09ac\u09cd\u09b2\u0995\u09be\u09b0\u0997\u09c1\u09b2\u09bf\u09b0 \u09b8\u09be\u09a5\u09c7 \u0995\u09be\u099c \u0995\u09b0\u09c7, \u09a4\u09be\u0987 \u09b8\u09be\u09a7\u09be\u09b0\u09a3\u09a4 \u09ac\u09cd\u09af\u09b0\u09cd\u09a5\u09a4\u09be\u0997\u09c1\u09b2\u09bf \u0985\u09cd\u09af\u09be\u09a8\u09cd\u099f\u09bf-\u0985\u09cd\u09af\u09be\u09a1\u09ac\u09cd\u09b2\u0995 \u09b8\u09cd\u0995\u09cd\u09b0\u09bf\u09aa\u09cd\u099f\u0997\u09c1\u09b2\u09bf \u09ac\u09be \u09a8\u09bf\u09b0\u09cd\u09a6\u09bf\u09b7\u09cd\u099f \u09b8\u09be\u0987\u099f\u0997\u09c1\u09b2\u09bf\u0995\u09c7 \u09b2\u0995\u09cd\u09b7\u09cd\u09af\u09af\u09c1\u0995\u09cd\u09a4 \u0985\u09a8\u09c1\u09b0\u09c2\u09aa \u09a8\u09bf\u09af\u09bc\u09ae\u09c7\u09b0 \u0995\u09be\u09b0\u09a3\u09c7 \u09b9\u09af\u09bc\u0964 \u09b8\u09ae\u09be\u09a7\u09be\u09a8\u0997\u09c1\u09b2\u09bf: \u09e7. \u0986\u09aa\u09a8\u09be\u09b0 \u09ac\u09bf\u099c\u09cd\u099e\u09be\u09aa\u09a8 \u09ac\u09cd\u09b2\u0995\u09be\u09b0 \u09ac\u09be \u09ac\u09cd\u09b0\u09be\u0989\u099c\u09be\u09b0 \u09b8\u09c1\u09b0\u0995\u09cd\u09b7\u09be \u098f\u0995\u09cd\u09b8\u099f\u09c7\u09a8\u09b6\u09a8\u09c7 \u09b9\u09cb\u09af\u09bc\u09be\u0987\u099f\u09b2\u09bf\u09b8\u09cd\u099f hcaptcha.com \u098f\u09ac\u0982 * .hcaptcha.com \u0995\u09c1\u0995\u09bf\u099c\u0964 \u09e8. \u0986\u09aa\u09a8\u09bf \u09af\u09a6\u09bf \u09b8\u09be\u09b9\u09b8\u09c0 \u09ac\u09cd\u09b0\u09be\u0989\u099c\u09be\u09b0\u099f\u09bf \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u099b\u09c7\u09a8, \u09af\u09be (\u098f\u09aa\u09cd\u09b0\u09bf\u09b2 \u09e8\u09e6\u09e8\u09e6 \u0985\u09a8\u09c1\u09b8\u09be\u09b0\u09c7) \u0995\u09cb\u09a8\u0993 \u09a7\u09b0\u09a3\u09c7\u09b0 \u0995\u09c1\u0995\u09bf \u09b6\u09cd\u09ac\u09c7\u09a4 \u09a4\u09be\u09b2\u09bf\u0995\u09be \u09b0\u09af\u09bc\u09c7\u099b\u09c7 \u09ac\u09b2\u09c7 \u09ae\u09a8\u09c7 \u09b9\u09af\u09bc \u09a8\u09be, \u09a4\u09ac\u09c7 \u0985\u0997\u09cd\u09b0\u09be\u09a7\u09bf\u0995\u09be\u09b0 -> \u09b6\u09bf\u09b2\u09cd\u09a1\u09b8 -> \u0995\u09c1\u0995\u09bf\u0997\u09c1\u09b2\u09bf\u09a4\u09c7 \u09af\u09be\u09a8 \u098f\u09ac\u0982 \u09b8\u09ae\u09b8\u09cd\u09a4 \u0995\u09c1\u0995\u09bf\u099c\u0995\u09c7 \u09ae\u099e\u09cd\u099c\u09c1\u09b0 \u0995\u09b0\u09c1\u09a8 \u099a\u09af\u09bc\u09a8 \u0995\u09b0\u09c1\u09a8\u0964 \u09e9. \u0986\u09aa\u09a8\u09bf \u09af\u09a6\u09bf \u09b8\u09ae\u09cd\u09aa\u09cd\u09b0\u09a4\u09bf \u09aa\u09cd\u09b0\u0995\u09be\u09b6\u09bf\u09a4 \u0993\u098f\u09b8 \u098f\u0995\u09cd\u09b8 10.15 \u09ac\u09be \u0986\u0987\u0993\u098f\u09b8 13 \u09a4\u09c7 \u09b8\u09be\u09ab\u09be\u09b0\u09bf\u099f\u09bf\u09b0 \u0985\u09a4\u09bf \u09b8\u09be\u09ae\u09cd\u09aa\u09cd\u09b0\u09a4\u09bf\u0995\u09a4\u09ae \u09b8\u0982\u09b8\u09cd\u0995\u09b0\u09a3 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u099b\u09c7\u09a8 \u09a4\u09ac\u09c7 4, \u0985\u09cd\u09af\u09be\u09aa\u09b2 \u09b8\u09ac\u09c7\u09ae\u09be\u09a4\u09cd\u09b0 \u09a4\u09c3\u09a4\u09c0\u09af\u09bc \u09aa\u0995\u09cd\u09b7\u09c7\u09b0 \u0995\u09c1\u0995\u09bf\u099c \u09b8\u09ae\u09cd\u09aa\u09b0\u09cd\u0995\u09bf\u09a4 \u09b8\u09be\u09ab\u09be\u09b0\u09bf\u099f\u09bf\u09b0 \u0986\u099a\u09b0\u09a3 \u09aa\u09b0\u09bf\u09ac\u09b0\u09cd\u09a4\u09a8 \u0995\u09b0\u09c7\u099b\u09c7 \u098f\u09ac\u0982 \u09b8\u09c7\u0997\u09c1\u09b2\u09bf \u09a1\u09bf\u09ab\u09b2\u09cd\u099f\u09b0\u09c2\u09aa\u09c7 \u0985\u09ac\u09b0\u09c1\u09a6\u09cd\u09a7 \u0995\u09b0\u09c7\u099b\u09c7\u0964 \u0986\u09ae\u09b0\u09be \u098f\u0995\u099f\u09bf \u09b8\u09ae\u09be\u09a7\u09be\u09a8 \u09ac\u09be\u09b8\u09cd\u09a4\u09ac\u09be\u09af\u09bc\u09a8 \u0995\u09b0\u099b\u09bf, \u09a4\u09ac\u09c7 \u098f\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7 \u09a6\u09af\u09bc\u09be \u0995\u09b0\u09c7 \u09b8\u09be\u09ab\u09be\u09b0\u09bf \u09aa\u099b\u09a8\u09cd\u09a6\u09b8\u09ae\u09c2\u09b9, \u0997\u09cb\u09aa\u09a8\u09c0\u09af\u09bc\u09a4\u09be \u09ac\u09bf\u09ad\u09be\u0997 \u098f\u09ac\u0982 \u0993\u09af\u09bc\u09c7\u09ac\u09b8\u09be\u0987\u099f \u099f\u09cd\u09b0\u09cd\u09af\u09be\u0995\u09bf\u0982\u099f\u09bf \u099a\u09c7\u0995 \u0995\u09b0\u09c1\u09a8: \u0985\u09cd\u09af\u09be\u0995\u09cd\u09b8\u09c7\u09b8\u09af\u09cb\u0997\u09cd\u09af\u09a4\u09be \u0995\u09c1\u0995\u09bf\u0995\u09c7 \u09aa\u09cd\u09b0\u09a4\u09cd\u09af\u09be\u09b6\u09be \u0985\u09a8\u09c1\u09af\u09be\u09af\u09bc\u09c0 \u0995\u09be\u099c \u0995\u09b0\u09a4\u09c7 \u09b8\u0995\u09cd\u09b7\u09ae \u0995\u09b0\u09a4\u09c7 \u0995\u09cd\u09b0\u09b8-\u09b8\u09be\u0987\u099f \u099f\u09cd\u09b0\u09cd\u09af\u09be\u0995\u09bf\u0982 \u09aa\u09cd\u09b0\u09a4\u09bf\u09b0\u09cb\u09a7 \u0995\u09b0\u09c1\u09a8\u0964 \u09aa\u09cd\u09b0\u09b6\u09cd\u09a8: \u0986\u09ae\u09bf \u098f\u0995\u09be\u09a7\u09bf\u0995 \u09a1\u09bf\u09ad\u09be\u0987\u09b8 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09bf\u0964 \u0986\u09ae\u09be\u09b0 \u0995\u09bf \u098f\u0995\u09be\u09a7\u09bf\u0995\u09ac\u09be\u09b0 \u09b8\u09be\u0987\u09a8 \u0986\u09aa \u0995\u09b0\u09be \u09a6\u09b0\u0995\u09be\u09b0? \u0989\u09a4\u09cd\u09a4\u09b0: \u09a8\u09be, \u09a6\u09af\u09bc\u09be \u0995\u09b0\u09c7 \u0995\u09c1\u0995\u09bf \u09b8\u09c7\u099f \u0995\u09b0\u09a4\u09c7 \u0986\u09aa\u09a8\u09bf \u09af\u09c7 \u09aa\u09cd\u09b0\u09a4\u09bf\u099f\u09bf \u09a1\u09bf\u09ad\u09be\u0987\u09b8 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09c7\u09a8 \u09a4\u09be\u09a4\u09c7 \u098f\u0995\u0987 \u0987\u09ae\u09c7\u09b2 \u09b2\u0997\u0987\u09a8 \u09b2\u09bf\u0999\u09cd\u0995\u099f\u09bf \u0995\u09cd\u09b2\u09bf\u0995 \u0995\u09b0\u09c1\u09a8\u0964",
    "page-acc-tr-title": "\u0985\u09cd\u09af\u09be\u0995\u09cd\u09b8\u09c7\u09b8\u09af\u09cb\u0997\u09cd\u09af\u09a4\u09be\u09b0 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0\u09a6\u09c7\u09b0 \u099c\u09a8\u09cd\u09af: \u09aa\u09cd\u09b0\u09b6\u09cd\u09a8\u09cb\u09a4\u09cd\u09a4\u09b0 \u0993 \u09b8\u09ae\u09b8\u09cd\u09af\u09be \u09b8\u09ae\u09be\u09a7\u09be\u09a8\u09c7\u09b0 \u0997\u09be\u0987\u09a1",
    "priv-hc": "\u0997\u09cb\u09aa\u09a8\u09c0\u09af\u09bc\u09a4\u09be \u0995\u09c7\u09a8\u09cd\u09a6\u09cd\u09b0\u09c0\u09ad\u09c2\u09a4",
    "priv-hc-text": "\u09ac\u09bf\u099c\u09cd\u099e\u09be\u09aa\u09a8 \u09a8\u09c7\u099f\u0993\u09af\u09bc\u09be\u09b0\u09cd\u0995\u0997\u09c1\u09b2\u09bf \u0986\u09aa\u09a8\u09be\u0995\u09c7 \u09a4\u09be\u09a6\u09c7\u09b0 \u09aa\u09a3\u09cd\u09af \u09b9\u09bf\u09b8\u09be\u09ac\u09c7 \u09a6\u09c7\u0996\u09ac\u09c7\u0964 \u0986\u09ae\u09b0\u09be \u09a8\u09be\u0964 \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0\u0995\u09be\u09b0\u09c0\u09b0\u09be \u0995\u09c7, \u0986\u09ae\u09b0\u09be \u0995\u09c0\u09ad\u09be\u09ac\u09c7 \u09a4\u09be\u09a6\u09c7\u09b0 \u09a6\u09c2\u09b7\u09bf\u09a4 \u09b9\u09a4\u09c7 \u09aa\u09be\u09b0\u09bf \u09a4\u09be\u09b0 \u09b8\u09ae\u09cd\u09ad\u09be\u09ac\u09a8\u09be \u0995\u09c7\u09ac\u09b2 care \u0986\u09ae\u09b0\u09be \u0997\u09cb\u09aa\u09a8\u09c0\u09af\u09bc\u09a4\u09be \u09aa\u09be\u09b8\u09c7\u09b0 \u09ae\u09a4\u09cb \u0989\u09a6\u09cd\u09af\u09cb\u0997\u0997\u09c1\u09b2\u09bf\u0995\u09c7 \u09b8\u09ae\u09b0\u09cd\u09a5\u09a8 \u0995\u09b0\u09bf \u098f\u09ac\u0982 \u0997\u09cb\u09aa\u09a8\u09c0\u09af\u09bc\u09a4\u09be \u098f\u09ac\u0982 \u09b8\u09c1\u09b0\u0995\u09cd\u09b7\u09be\u09b0 \u09ad\u09be\u09b0\u09b8\u09be\u09ae\u09cd\u09af \u09ac\u099c\u09be\u09af\u09bc \u09b0\u09be\u0996\u09a4\u09c7 \u0995\u09a0\u09cb\u09b0 \u09aa\u09b0\u09bf\u09b6\u09cd\u09b0\u09ae \u0995\u09b0\u09bf\u0964",
    "sec-hc": "\u09b8\u09c1\u09b0\u0995\u09cd\u09b7\u09be \u09aa\u09cd\u09b0\u09a5\u09ae",
    "sec-hc-text": "hCaptcha \u09aa\u09cd\u09b0\u0995\u09be\u09b6\u0995-\u0995\u09c7\u09a8\u09cd\u09a6\u09cd\u09b0\u09bf\u0995\u0964 \u0986\u09ae\u09b0\u09be \u09b8\u09be\u0987\u099f\u0997\u09c1\u09b2\u09bf\u0995\u09c7 \u0986\u09b8\u09b2 \u09a1\u09c7\u099f\u09be \u09a6\u09c7\u0993\u09af\u09bc\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u0995\u09be\u099c \u0995\u09b0\u09bf \u098f\u09ac\u0982 \u0995\u09c0\u09ad\u09be\u09ac\u09c7 \u098f\u099f\u09bf \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09ac \u09b8\u09c7 \u09ac\u09bf\u09b7\u09af\u09bc\u09c7 \u09a4\u09be\u09a6\u09c7\u09b0 \u09b8\u09bf\u09a6\u09cd\u09a7\u09be\u09a8\u09cd\u09a4 \u09a8\u09bf\u09a4\u09c7 \u09a6\u09bf\u09a8\u0964 \u09af\u09a5\u09be\u09b0\u09cd\u09a5\u09a4\u09be \u0986\u09aa\u09a8\u09be\u09b0 \u0986\u0997\u09cd\u09b0\u09b9 \u098f\u09ac\u0982 \u0986\u09ae\u09be\u09a6\u09c7\u09b0 \u09ae\u09a7\u09cd\u09af\u09c7\u0964 \u0985\u09a8\u09cd\u09af\u09be\u09a8\u09cd\u09af \u0995\u09cd\u09af\u09be\u09aa\u099a\u09be \u09ac\u09bf\u099c\u09cd\u099e\u09be\u09aa\u09a8 \u09b8\u0982\u09b8\u09cd\u09a5\u09be\u0997\u09c1\u09b2\u09bf \u09b8\u09b0\u09ac\u09b0\u09be\u09b9 \u0995\u09b0\u09c7\u0964 \u09a4\u09be\u09b0\u09be \u09af\u09c7 \u09aa\u09cd\u09b0\u09a4\u09bf\u099f\u09bf \u09ac\u099f \u09ac\u09cd\u09b2\u0995 \u0995\u09b0\u09c7 \u09a4\u09be \u09a4\u09be\u09a6\u09c7\u09b0 \u0986\u09af\u09bc \u0995\u09ae\u09be\u09a4\u09c7 \u09aa\u09be\u09b0\u09c7\u0964",
    "what-is-hcaptcha": "\u098f\u0987\u099a \u0995\u09cd\u09af\u09be\u09aa\u099a\u09be \u0995\u09c0?",
    "what-is-hcaptcha-text": "hCaptcha \u0986\u09aa\u09a8\u09be\u09b0 \u09aa\u09cd\u09b0\u09bf\u09af\u09bc \u0993\u09af\u09bc\u09c7\u09ac \u09aa\u09b0\u09bf\u09b7\u09c7\u09ac\u09be\u09a6\u09bf\u0997\u09c1\u09b2\u09bf \u09ae\u09be\u09a8\u09c1\u09b7\u09c7\u09b0 \u09aa\u0995\u09cd\u09b7\u09c7 \u09b8\u09b9\u099c \u098f\u09ac\u0982 \u09ae\u09c7\u09b6\u09bf\u09a8\u0997\u09c1\u09b2\u09bf\u09b0 \u09aa\u0995\u09cd\u09b7\u09c7 \u0995\u09a0\u09bf\u09a8 \u098f\u09ae\u09a8 \u09b8\u09b9\u099c \u09aa\u09cd\u09b0\u09b6\u09cd\u09a8 \u099c\u09bf\u099c\u09cd\u099e\u09be\u09b8\u09be \u0995\u09b0\u09c7 \u09ac\u099f, \u09b8\u09cd\u09aa\u09cd\u09af\u09be\u09ae \u098f\u09ac\u0982 \u0985\u09aa\u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u09a5\u09c7\u0995\u09c7 \u09b0\u0995\u09cd\u09b7\u09be \u0995\u09b0\u09a4\u09c7 \u09b8\u09b9\u09be\u09af\u09bc\u09a4\u09be \u0995\u09b0\u09c7\u0964",
    "why-hc": "\u098f\u0987\u099a \u09b8\u09bf \u0995\u09cd\u09af\u09be\u09aa\u099a\u09be \u0995\u09c7\u09a8?",
    "why-hc-text": "\u09aa\u09cd\u09b0\u09a4\u09bf\u09ac\u09be\u09b0 \u0986\u09aa\u09a8\u09bf \u09af\u0996\u09a8 \u0995\u09cb\u09a8\u0993 \u099a\u09cd\u09af\u09be\u09b2\u09c7\u099e\u09cd\u099c\u09c7\u09b0 \u099c\u09ac\u09be\u09ac \u09a6\u09c7\u09a8, \u0986\u09ae\u09b0\u09be \u09ae\u09be\u09a8\u09c1\u09b7 \u09ac\u09be \u09ae\u09c7\u09b6\u09bf\u09a8 \u0995\u09bf\u09a8\u09be \u09a4\u09be \u09a8\u09bf\u09b0\u09cd\u09a7\u09be\u09b0\u09a3 \u0995\u09b0\u09be\u09b0 \u099c\u09a8\u09cd\u09af \u0986\u09ae\u09b0\u09be \u09aa\u09b0\u09bf\u09b6\u09c0\u09b2\u09bf\u09a4 \u09ae\u09a1\u09c7\u09b2\u0997\u09c1\u09b2\u09bf \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09bf\u0964 \u09a4\u09be\u09b0\u09aa\u09b0\u09c7 \u09af\u09c7 \u0993\u09af\u09bc\u09c7\u09ac\u09b8\u09be\u0987\u099f\u099f\u09bf \u0986\u09aa\u09a8\u09bf \u09aa\u09b0\u09bf\u09a6\u09b0\u09cd\u09b6\u09a8 \u0995\u09b0\u09c7\u099b\u09c7\u09a8 \u09b8\u09c7\u0997\u09c1\u09b2\u09bf \u09a4\u09be\u09a6\u09c7\u09b0 \u09ab\u09b2\u09be\u09ab\u09b2\u0997\u09c1\u09b2\u09bf\u09a4\u09c7 \u09ac\u099f \u09b0\u09c7\u0996\u09c7 \u09a8\u09bf\u099c\u09c7\u09b0 \u0985\u09ad\u09bf\u099c\u09cd\u099e\u09a4\u09be \u0989\u09a8\u09cd\u09a8\u09a4 \u0995\u09b0\u09a4\u09c7 \u098f\u0987 \u09ab\u09b2\u09be\u09ab\u09b2\u099f\u09bf \u09ac\u09cd\u09af\u09ac\u09b9\u09be\u09b0 \u0995\u09b0\u09c7\u0964"
}