{
    "acc-opt": "\u0631\u0633\u0627\u0626\u064a \u062c\u0627 \u0627\u062e\u062a\u064a\u0627\u0631",
    "acc-opt-but": "\u0631\u0633\u0627\u0626\u064a \u062c\u064a \u0622\u067e\u0634\u0646 \u0628\u0627\u0628\u062a \u0648\u068c\u064a\u06aa \u0627\u06bb\u0648",
    "acc-opt-text": "\u0687\u0627 \u062a\u0648\u0647\u0627\u0646 \u0628\u0635\u0631\u064a \u0645\u0639\u0630\u0648\u0631 \u0622\u0647\u064a\u0648 \u064a\u0627 \u067b\u064a \u0635\u0648\u0631\u062a \u06fe \u0627\u064a\u0687 \u0633\u064a\u067e\u0686\u0627 \u0645\u06aa\u0645\u0644 \u0646\u067f\u0627 \u06aa\u0631\u064a \u0633\u06af\u0647\u0648\u061f",
    "accessibility-link": "\u067e\u0647\u0686",
    "appreciate-in": "\u0627\u0633\u0627\u0646 \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u0631\u0627\u0621\u0650 \u062c\u0648 \u0642\u062f\u0631 \u06aa\u0631\u064a\u0648\u0646 \u067f\u0627. \u0627\u0686\u0648 \u062a\u0647 \u06aa\u0646\u0647\u0646 \u0628\u0647 \u0645\u0633\u0626\u0644\u064a \u06a9\u064a and\u0627\u06bb\u0648\u0646 \u06fd \u0627\u0633\u0627\u0646 \u0627\u0646\u0647\u0646 \u06a9\u064a \u062a\u06aa\u0699\u0648 \u062d\u0644 \u06aa\u0646\u062f\u0627\u0633\u064a\u0646.",
    "fb-but": "\u0627\u0633\u0627\u0646 \u06a9\u064a \u067e\u0646\u0647\u0646\u062c\u064a \u0631\u0627\u0621\u0650 \u0645\u0648\u06aa\u0644\u064a\u0648",
    "feedback-text": "\u062a\u0648\u0647\u0627\u0646 \u062c\u064a\u06aa\u0648 \u0635\u0631\u0641 \u068f\u067a\u0648 \u0622\u0647\u064a \u0627\u0646\u0647\u064a hCaptch \u062a\u064a \u0645\u0648\u067d \u068f\u064a\u0648",
    "home-ent-1": "\u0627\u0633\u0627\u0646 \u062c\u064a \u0627\u0646\u067d\u0631\u067e\u0631\u0627\u0626\u0632 \u0633\u064a\u06aa\u064a\u0648\u0631\u067d\u064a \u0622\u0687\u0648\u0646 \u06b3\u0648\u0644\u064a \u0631\u0647\u064a\u0627 \u0622\u0647\u064a\u0648\u061f",
    "home-ent-2": "\u0627\u0633\u0627\u0646 \u062c\u064a \u0646\u0626\u064a\u0646 \u0648\u0642\u0641 \u06aa\u064a\u0644 \u0633\u0627\u0626\u064a\u067d \u062a\u064a \u0648\u0683\u0648: BotStop.com",
    "home-intro-body": "\u0687\u0627 \u062a\u0648\u0647\u0627\u0646 \u0628\u0648\u067d\u0633 \u067b\u0627\u0647\u0631 \u0631\u06a9\u06bb \u0644\u0627\u0621\u0650 \u06aa\u064a\u067e\u0686\u0627 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0646\u062f\u0627 \u0622\u0647\u064a\u0648\u061f hCaptcha \u0635\u0627\u0631\u0641 \u062c\u064a \u0631\u0627\u0632\u062f\u0627\u0631\u064a \u06a9\u064a \u062a\u062d\u0641\u0638 \u068f\u064a\u0646\u062f\u0648 \u0622\u0647\u064a \u060c \u0648\u064a\u0628 \u0633\u0627\u0626\u067d\u0633 \u06a9\u064a \u0627\u0646\u0639\u0627\u0645 \u068f\u064a\u0646\u062f\u0648 \u0622\u0647\u064a \u06fd \u06aa\u0645\u067e\u0646\u064a\u0646 \u06a9\u064a \u067e\u0646\u0647\u0646\u062c\u0648 \u068a\u064a\u067d\u0627 \u0644\u064a\u0628\u0644 \u062d\u0627\u0635\u0644 \u06aa\u0631\u06bb \u06fe \u0645\u062f\u062f \u06aa\u0646\u062f\u0648 \u0622\u0647\u064a. \u0627\u0647\u0648 reCAPTCHA \u062c\u0648 \u0647\u06aa drop-in \u0645\u062a\u0628\u0627\u062f\u0644 \u0622\u0647\u064a: \u062a\u0648\u0647\u0627\u0646 \u06aa\u062c\u0647\u0647 \u0645\u0646\u067d\u0646 \u0627\u0646\u062f\u0631 \u0633\u0648\u0626\u0686 \u06aa\u0631\u064a \u0633\u06af\u0647\u0648 \u067f\u0627.",
    "home-intro-head": "\u0648\u068c\u064a\u06aa \u0628\u0648\u067d\u0646 \u06a9\u064a \u0631\u0648\u06aa\u064a\u0648. \u0635\u0627\u0631\u0641 \u062c\u064a \u0631\u0627\u0632\u062f\u0627\u0631\u064a \u062c\u064a \u062d\u0641\u0627\u0638\u062a \u06aa\u0631\u06bb \u0634\u0631\u0648\u0639 \u06aa\u0631\u064a\u0648.",
    "home-main-1": "\u0646\u062c\u064a. \u0645\u062d\u0641\u0648\u0638 \u062a\u064a\u0632. \u06fd \u0627\u0633\u0627\u0646 \u062a\u0648\u0647\u0627\u0646 \u06a9\u064a \u0647\u0646 \u06a9\u064a \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0631\u06bb \u062c\u0648 \u0645\u0639\u0627\u0648\u0636\u0648 \u0627\u062f\u0627 \u06aa\u0631\u064a\u0648\u0646 \u067f\u0627.",
    "home-main-2": "\u0631\u0627\u0632\u062f\u0627\u0631\u064a \u0645\u0631\u06aa\u0648\u0632 \u067f\u064a \u0648\u0626\u064a",
    "home-main-2c": "\u0627\u0634\u062a\u0647\u0627\u0631\u0633\u0627\u0632\u064a \u0646\u064a\u067d \u0648\u0631\u06aa\u0646 \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u0634\u064a\u0646 \u06a9\u064a \u0634\u064a\u0626\u0631 \u0637\u0648\u0631 \u068f\u0633\u064a \u067f\u0648. hCaptcha \u0644\u0627\u0621\u0650 \u0627\u06be\u0648 \u06aa\u0645 \u0622\u06be\u064a \u062c\u064a\u06aa\u0648 \u0627\u06be\u064a \u0645\u0647\u064a\u0627 \u06aa\u0646 \u067f\u0627. \u0627\u0633\u0627\u0646 \u06a9\u064a \u067e\u0631\u0648\u0627\u0647 \u0646\u0627\u0647\u064a \u062a\u0647 \u062a\u0648\u0647\u0627\u0646 \u062c\u0627 \u0635\u0627\u0631\u0641 \u06aa\u064a\u0631 \u0622\u0647\u0646 \u060c \u0635\u0631\u0641 \u0627\u0646 \u06b3\u0627\u0644\u0647\u0647 \u062c\u0648 \u062a\u0647 \u0627\u0647\u064a \u0634\u0627\u064a\u062f \u062e\u0631\u0627\u0628 \u0647\u0648\u0646\u062f\u0627. \u0627\u0633\u0627\u0646 \u067e\u0648\u0626\u0644\u06b3\u0646 \u0648\u0627\u0646\u06af\u0631 \u0631\u0627\u0632\u062f\u0627\u0631\u064a \u067e\u0627\u0633 \u060c \u06fd \u0631\u0627\u0632\u062f\u0627\u0631\u064a \u06fd \u062a\u062d\u0641\u0638 \u06a9\u064a \u062a\u0648\u0627\u0632\u0646 \u068f\u064a\u06bb \u0644\u0627\u0621\u0650 \u0633\u062e\u062a \u0645\u062d\u0646\u062a \u0633\u0627\u0646 \u062a\u0639\u0627\u0648\u0646 \u06aa\u0631\u064a\u0648\u0646 \u067f\u0627.",
    "home-main-3": "\u0633\u064a\u06aa\u064a\u0648\u0631\u067d\u064a \u067e\u0647\u0631\u064a\u0646",
    "home-main-3c": "\u062f\u0631\u0633\u062a\u064a \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u0645\u0641\u0627\u062f \u06fd \u0627\u0633\u0627\u0646 \u06fe \u0622\u0647\u064a. hCaptcha \u0646\u0627\u0634\u0631 \u062a\u064a \u0645\u0631\u06a9\u0648\u0632 \u0622\u06be\u064a. \u0627\u0633\u0627\u0646 \u06aa\u0645 \u06aa\u0631\u064a\u0648\u0646 \u067f\u0627 \u062a\u0648\u0647\u0627\u0646 \u06a9\u064a \u0627\u0635\u0644\u064a \u068a\u064a\u067d\u0627 \u068f\u064a\u06bb \u06fd \u062a\u0648\u0647\u0627\u0646 \u06a9\u064a \u0641\u064a\u0635\u0644\u0648 \u06aa\u0631\u06bb \u068f\u064a\u0648 \u062a\u0647 \u062a\u0648\u0647\u0627\u0646 \u0627\u0646 \u06a9\u064a \u06aa\u064a\u0626\u0646 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0631\u064a\u0648\u0646. \u067b\u064a\u0627 \u06aa\u064a\u067e\u0686\u0627 \u0637\u0631\u0641\u0627\u0646 \u0645\u0647\u064a\u0627 \u06aa\u064a\u0627 \u0648\u064a\u0627 \u0622\u0647\u0646",
    "home-main-3c2": "\u0627\u0634\u062a\u0647\u0627\u0631\u064a \u06aa\u0645\u067e\u0646\u064a\u0648\u0646. \u0645\u0637\u0644\u0628 \u0647\u0631 \u0628\u0648\u067d \u062c\u0646\u0647\u0646 \u06a9\u064a \u0627\u0646\u0647\u0646 \u0628\u0644\u0627\u06aa \u06aa\u064a\u0648 \u0622\u0647\u064a \u0627\u0646\u0647\u0646 \u062c\u064a \u0622\u0645\u062f\u0646\u064a \u06af\u0647\u067d \u067f\u064a \u0633\u06af\u0647\u064a \u067f\u064a.",
    "home-main-4": "\u0628\u0647\u062a\u0631 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0646\u062f\u0699 \u062c\u0648 \u062a\u062c\u0631\u0628\u0648",
    "home-main-4c": "\u0627\u0646\u0633\u0627\u0646\u064a\u062a \u06a9\u064a \u0637\u0626\u064a \u06aa\u0631\u06bb \u0644\u0627\u0621\u0650 \u0646\u0647\u0627\u064a\u062a \u0633\u064a\u067e\u0686\u0627 \u0637\u0631\u0641\u0627\u0646 \u062c\u062f\u064a\u062f \u062a\u0631\u064a\u0646 \u0633\u06a9\u064a\u0627 \u062c\u064a \u0633\u06a9\u064a\u0627 \u062c\u0627 \u0645\u0627\u068a\u0644 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u064a\u0627 \u0648\u064a\u0646\u062f\u0627 \u0622\u0647\u0646. \u0627\u0646\u0647\u064a \u0627\u0633\u0627\u0646 \u06a9\u064a \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u0635\u0627\u0631\u0641\u064a\u0646 \u06a9\u0627\u0646 \u06af\u0647\u067d \u062a\u06aa\u0644\u064a\u0641 \u0648\u0627\u0631\u0648 \u06aa\u0645 \u062c\u064a \u0636\u0631\u0648\u0631\u062a \u067e\u0648\u064a \u067f\u064a \u06fd \u0627\u0626\u064a\u0646 \u06aa\u0631\u06bb \u06fe \u067e\u0646\u0647\u0646\u062c\u0648 \u0648\u0642\u062a \u06af\u0647\u067d \u06aa take\u064a \u067f\u0648 \u060c \u062c\u068f\u0647\u0646 \u062a\u0647 \u06af\u0631\u0627\u0647\u06aa\u0646 \u062c\u064a \u0644\u064a\u0628\u0644\u0646\u06af \u0644\u0627\u0621\u0650 \u0627\u0683\u0627 \u062a\u0627\u0626\u064a\u0646 \u06aa\u0627\u0631\u0627\u0626\u062a\u0648 label\u0631\u0646\u062f\u0699 \u068a\u064a\u067d\u0627 \u062a\u064a\u0627\u0631 \u06aa\u064a\u0648 \u0648\u064a\u0646\u062f\u0648 \u0622\u0647\u064a.",
    "home-main-plans": "hCaptcha \u06aa\u0646\u0647\u0646 \u0628\u0647 \u0633\u0627\u0626\u064a\u0632 \u062c\u064a \u067e\u0628\u0644\u0634\u0631\u0646 \u0644\u0627\u0621\u0650 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u0644\u0627\u0621\u0650 \u0645\u0641\u062a \u0622\u0647\u064a. \u062a\u0646\u0647\u0646 \u0647\u0648\u0646\u062f\u064a \u060c \u0627\u062f\u0627\u0631\u0646 \u06fd \u067e\u0644\u064a\u067d \u0641\u0627\u0631\u0645\u0632 \u0644\u0627\u0621\u0650 \u062c\u064a\u06aa\u064a \u0648\u068c\u064a\u06aa \u0636\u0627\u0628\u0637\u064a \u062c\u064a \u0636\u0631\u0648\u0631\u062a \u0622\u0647\u064a \u060c \u0627\u0646\u0647\u0646 \u0644\u0627\u0621\u0650 \u0627\u0633\u0627\u0646 \u062c\u0627 \u0627\u062f\u0627\u0631\u0627 \u062d\u0644 \u0645\u0648\u062c\u0648\u062f \u0622\u0647\u0646.",
    "home-main-plans2": "hCaptcha \u0627\u0646\u0679\u0631\u067e\u0631\u0627\u0626\u0632 \u062d\u0644 \u0633\u064a\u0627\u0631\u0648\u064a \u062c\u064a \u06aa\u062c\u0647\u0647 \u0648\u068f\u064a\u0646 \u06aa\u0645\u067e\u0646\u064a\u0646 \u0644\u0627\u0621\u0650 \u0644\u06a9\u0646 \u062c\u064a \u0648\u064a\u0628 \u0633\u0627\u0626\u064a\u067d\u0646 \u062c\u064a \u0644\u06a9\u0646 \u062a\u0627\u0626\u064a\u0646 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0646\u062f\u0699\u0646 \u062c\u064a \u062d\u0641\u0627\u0638\u062a \u06aa\u0646 \u067f\u0627. \u0645\u0647\u0631\u0628\u0627\u0646\u064a \u06aa\u0631\u064a \u0648\u068c\u064a\u06aa \u0645\u0639\u0644\u0648\u0645\u0627\u062a \u0644\u0627\u0621\u0650 \u0627\u0633\u0627\u0646 \u0633\u0627\u0646 \u0631\u0627\u0628\u0637\u0648 \u06aa\u064a\u0648 \u060c \u064a\u0627 \u062f\u0648\u0631\u0648 \u06aa\u0631\u064a\u0648",
    "home-main-plans3": "hCaptcha \u0627\u0646\u0679\u0631\u067e\u0631\u0627\u0626\u0632 \u062d\u0644 \u0633\u064a\u0627\u0631\u0648\u064a \u062c\u064a \u06aa\u062c\u0647\u0647 \u0648\u068f\u064a\u0646 \u06aa\u0645\u067e\u0646\u064a\u0646 \u0644\u0627\u0621\u0650 \u0644\u06a9\u0646 \u062c\u064a \u0648\u064a\u0628 \u0633\u0627\u0626\u064a\u067d\u0646 \u062c\u064a \u0644\u06a9\u0646 \u062a\u0627\u0626\u064a\u0646 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0646\u062f\u0699\u0646 \u062c\u064a \u062d\u0641\u0627\u0638\u062a \u06aa\u0646 \u067f\u0627. \u0645\u0647\u0631\u0628\u0627\u0646\u064a \u06aa\u0631\u064a \u0648\u068c\u064a\u06aa \u0645\u0639\u0644\u0648\u0645\u0627\u062a \u0644\u0627\u0621\u0650 \u0627\u0633\u0627\u0646 \u0633\u0627\u0646 \u0631\u0627\u0628\u0637\u0648 \u06aa\u064a\u0648 \u060c \u064a\u0627 \u062f\u0648\u0631\u0648 \u06aa\u0631\u064a\u0648",
    "home-main-plans4": "\u062a\u0641\u0635\u064a\u0644 \u0644\u0627\u0621\u0650.",
    "learn-more-hcaptcha": "\u0648\u068c\u064a\u06aa h\u0627\u06bb hCaptcha \u0628\u0627\u0628\u062a",
    "page-acc-faq-1": "\u0633\u0648\u0627\u0644: \u0687\u0627 hCaptcha \u0633\u064a\u06aa\u0634\u0646 508 + WCAG 2.0 \u0645\u0637\u0627\u0628\u0642 \u0622\u0647\u064a\u061f \u062c\u0648\u0627\u0628: \u0627\u0633\u0627\u0646 \u0627\u0646\u0647\u064a \u062a\u064a \u064a\u0642\u064a\u0646 \u0631\u06a9\u0648\u0646 \u067f\u0627: \u0633\u0680\u0626\u064a \u0635\u0627\u0631\u0641 \u06aa\u0646\u0647\u0646 \u0628\u0647 \u0642\u0633\u0645 \u062c\u064a \u062a\u06aa\u0644\u064a\u0641 \u062c\u0648 \u0627\u0647\u064a \u0648\u064a\u0628 \u0628\u0631\u0627\u0626\u0648\u0632 \u06aa\u0631\u06bb \u06fd \u0641\u0627\u0631\u0645 \u062a\u064a \u0645\u062a\u0646 \u062f\u0627\u062e\u0644 \u06aa\u0631\u06bb \u062c\u064a \u0631\u062c\u0633\u067d\u0631\u064a\u0634\u0646 \u062a\u064a hCaptcha \u0637\u0631\u0641\u0627\u0646 \u0645\u062d\u0641\u0648\u0638 \u06aa\u064a\u0644 \u062e\u062f\u0645\u062a\u0646 \u062a\u0627\u0626\u064a\u0646 \u0631\u0633\u0627\u0626\u064a \u062d\u0627\u0635\u0644 \u06aa\u0631\u064a \u0633\u06af\u06be\u0646 \u067f\u0627. \u062a\u0646\u0647\u0646 \u0647\u0648\u0646\u062f\u064a \u060c \u0627\u0647\u0648 \u0642\u0627\u0646\u0648\u0646\u064a \u0635\u0644\u0627\u062d \u0646\u0647 \u0622\u0647\u064a: \u062a\u0648\u0647\u0627\u0646 \u06a9\u064a \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u067e\u0646\u0647\u0646\u062c\u064a \u06aa\u0627\u0631\u06aa\u0631\u062f\u06af\u064a \u062a\u064a \u068c\u064a\u0627\u0646 \u068f\u064a\u06bb \u06af\u0647\u0631\u062c\u064a \u060c \u0627\u0646\u0647\u064a \u06a9\u064a \u064a\u0642\u064a\u0646\u064a \u0628\u06bb\u0627\u0626\u06bb \u0644\u0627\u0621\u0650 \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u067e\u0646\u0647\u0646\u062c\u064a \u0645\u0631\u0636\u064a \u062c\u064a \u0635\u0648\u0631\u062a \u0647\u064a \u0622\u0647\u064a.",
    "page-acc-faq-2": "\u0633\u0648\u0627\u0644: \u0687\u0627 \u062a\u0648\u0647\u0627\u0646 \u0622\u068a\u064a\u0648 \u0648\u0627\u0646\u06af\u0631 \u067b\u064a\u0646 \u067e\u0647\u0686 (A11y) \u0627\u062e\u062a\u064a\u0627\u0631\u0646 \u062a\u064a \u06aa\u0645 \u06aa\u0631\u064a \u0631\u0647\u064a\u0627 \u0622\u0647\u064a\u0648\u061f \u062c\u0648\u0627\u0628: \u0622\u068a\u064a\u0648 \u06aa\u064a\u067e\u0686\u0627\u0633 \u0648\u0627\u0646\u06af\u0631 \u0627\u06b3\u0648\u06bb\u064a \u0645\u0634\u0647\u0648\u0631 \u0622\u067e\u0634\u0646 \u06aa\u064a\u062a\u0631\u0646 \u0626\u064a \u0627\u064a\u064a \u0635\u0627\u0631\u0641\u064a\u0646 \u062c\u064a \u062e\u0644\u0627\u0641 \u0641\u0631\u0642 \u06aa\u064a\u0648 \u06fd \u062c\u062f\u064a\u062f \u0645\u0634\u064a\u0646 \u0644\u0631\u0646\u0646\u06af \u067d\u064a\u06aa\u0646\u0627\u0644\u0627\u062c\u064a \u06a9\u064a \u0622\u0633\u0627\u0646\u064a \u0633\u0627\u0646 \u0634\u06aa\u0633\u062a \u068f\u0626\u064a \u0686\u06aa\u0627 \u0622\u0647\u0646. \u0627\u0647\u0648 \u0645\u0648\u062c\u0648\u062f\u0647 \u0622\u068a\u064a\u0648 \u0686\u0626\u0644\u064a\u0646\u062c\u0632 \u06a9\u064a \u0648\u068c\u064a\u06aa \u06fd \u0648\u068c\u064a\u06aa \u068f\u06a9\u064a\u0648 \u067f\u064a\u06bb \u060c \u0634\u0648\u0631 \u0645\u062a\u0639\u0627\u0631\u0641 \u06aa\u0631\u0627\u0626\u06bb \u060c \u0628\u064a \u0645\u062b\u0627\u0644 \u0648\u0642\u062a \u060c \u063a\u064a\u0631\u0645\u0639\u0645\u0648\u0644\u064a \u0644\u0641\u0638 \u062c\u0648 \u0645\u064a\u0644\u0627\u067e \u06fd \u0627\u0646\u0647\u064a \u062a\u064a \u062d\u0645\u0644\u0648 \u06aa\u0646\u062f\u0699\u0646 \u06a9\u064a \u0634\u06aa\u0633\u062a \u068f\u064a\u06bb \u062a\u064a \u0645\u062c\u0628\u0648\u0631 \u06aa\u064a\u0648 \u0622\u0647\u064a. \u0627\u0633\u0627\u0646 \u0627\u0646\u0647\u064a \u0637\u0631\u064a\u0642\u064a \u0633\u0627\u0646 \u0686\u0626\u0644\u064a\u0646\u062c \u06a9\u0627\u0646 \u0628\u0686\u06bb \u06a9\u0627\u0646 \u0628\u0686\u0627\u0621\u064f \u06fe \u062a\u0645\u0627\u0645 \u06af\u0647\u067d \u062d\u0648\u0635\u0644\u064a \u0648\u0627\u0631\u0648 \u0622\u0647\u064a\u0648\u0646 \u060c \u067e\u0631 \u0627\u0646\u0647\u064a \u062a\u064a \u063a\u0648\u0631 \u06aa\u0646\u062f\u0627\u0633\u064a\u0646 \u062c\u064a\u06aa\u068f\u0647\u0646 \u0627\u0626\u064a \u0628\u0631\u0627\u062f\u0631\u064a \u06a9\u0627\u0646 \u06aa\u0627 \u0637\u0644\u0628 \u0622\u0647\u064a. \u062a\u0646\u0647\u0646 \u0647\u0648\u0646\u062f\u064a \u060c \u0627\u0633\u0627\u0646 \u0631\u0633\u0627\u0626\u064a \u0648\u0627\u0631\u064a \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u062c\u064a \u06aa\u064a\u0633 \u0644\u0627\u0621\u0650 \u0631\u0627\u0632\u062f\u0627\u0631\u064a \u067e\u0627\u0633 \u0628\u0627\u0628\u062a \u062a\u0645\u0627\u0645 \u06af\u0647\u06bb\u064a \u062f\u0644\u0686\u0633\u067e\u064a \u0631\u06a9\u0648\u0646 \u067f\u0627. \u0627\u0633\u0627\u0646 \u064a\u0642\u064a\u0646 \u0631\u06a9\u0648\u0646 \u067f\u0627 \u0631\u0627\u0632\u062f\u0627\u0631\u064a \u067e\u0627\u0633 \u062c\u0627\u0631\u064a \u06aa\u0631\u06bb \u0633\u0627\u0646 \u0627\u0633\u0627\u0646 \u062c\u064a \u0647\u0627\u06bb\u0648\u06aa\u064a \u0627\u064a\u064a \u062d\u06aa\u0645\u062a \u0639\u0645\u0644\u064a \u06a9\u064a \u06af\u068f \u06aa\u0631\u06bb \u060c \u0647\u06aa \u0680\u064a\u0631\u0648 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0646\u062f\u0699 \u06a9\u064a \u0645\u062d\u0641\u0648\u0638 \u0637\u0648\u0631 \u062a\u064a \u0628\u0631\u0627\u0624\u0632 \u06aa\u0631\u06bb \u062c\u064a \u0627\u062c\u0627\u0632\u062a \u068f\u064a\u0646 \u067f\u064a\u0648\u0646 \u060c \u0645\u062d\u0641\u0648\u0638 \u0637\u0648\u0631 \u062a\u064a \u0627\u0646\u0647\u064a in\u0627\u06bb \u06fe \u062a\u0647 \u0627\u0646\u0647\u0646 \u062c\u064a \u067d\u0631\u0626\u0641\u06aa \u0648\u068c\u064a\u06aa \u0646\u062c\u064a \u0622\u0647\u064a \u060c \u062c\u068f\u0647\u0646 \u062a\u0647 \u0628\u0648\u067d \u0622\u067e\u0631\u064a\u067d\u0631\u0632 \u062c\u064a \u063a\u0644\u0637 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u064a \u0645\u062d\u062f\u0648\u062f \u06aa\u0631\u06bb \u062c\u0648 \u0646\u0627\u06af\u0632\u064a\u0631 \u0637\u0648\u0631 \u062a\u064a \u067f\u0626\u064a \u067f\u0648 \u062c\u068f\u0647\u0646 a11y \u0627\u062e\u062a\u064a\u0627\u0631 \u0645\u0648\u062c\u0648\u062f \u0622\u0647\u0646. \u0627\u0633\u0627\u0646 \u0627\u0646\u0647\u064a \u062c\u062f\u064a\u062f \u067d\u064a\u06aa\u0646\u0627\u0644\u0627\u062c\u064a \u06a9\u064a \u0645\u0639\u064a\u0627\u0631\u064a \u0628\u06bb\u0627\u0626\u06bb \u0648\u0627\u0631\u064a IETF \u06aa\u0645 \u06aa\u0646\u062f\u0699 \u06af\u0631\u0648\u0647\u0647 \u06fe \u0633\u0631\u06af\u0631\u0645 \u068c\u0631\u064a\u0648\u0646 \u0622\u0647\u064a\u0648\u0646.",
    "page-acc-faq-3": "\u0633\u0648\u0627\u0644: \u0631\u0627\u0632\u062f\u0627\u0631\u064a \u0628\u0627\u0628\u062a \u0687\u0627\u061f \u0687\u0627 \u0631\u062c\u0633\u067d\u0631\u064a\u0634\u0646 \u06aa\u062c\u06be \u067e\u0627\u0633\u064a \u062a\u064a \u0628\u0631\u0627\u0626\u0648\u0632\u0646\u06af a11y \u068a\u064a\u067d\u0627 \u06a9\u064a \u0628\u064a \u0646\u0642\u0627\u0628 \u06aa\u0631\u064a \u067f\u064a\u061f \u062a\u0648\u0647\u0627\u0646 \u0627\u064a \u0645\u064a\u0644 \u0633\u0627\u0646 \u0687\u0627 \u06aa\u0646\u062f\u0627 \u0622\u0647\u064a\u0648\u061f \u062c\u0648\u0627\u0628: hCaptcha \u0631\u0627\u0632\u062f\u0627\u0631\u064a \u0644\u0627\u0621\u0650 \u068a\u064a\u0632\u0627\u0626\u064a\u0646 \u06a9\u0627\u0646 \u067a\u0627\u0647\u064a\u0648 \u0648\u064a\u0648 \u0622\u0647\u064a. \u0627\u0647\u0648 \u0631\u0648\u0627\u064a\u062a\u064a \u0627\u062e\u062a\u064a\u0627\u0631\u064a\u0646 \u06a9\u0627\u0646 \u0628\u0644\u06aa\u0644 \u0645\u062e\u062a\u0644\u0641 \u0622\u0647\u064a \u060c reCAPTCHA \u062c\u064a\u06aa\u064a \u0627\u0634\u062a\u0647\u0627\u0631 \u0646\u064a\u067d \u0648\u0631\u06aa \u062c\u0627 \u0645\u0627\u0644\u06aa \u0622\u0647\u0646 \u060c \u062c\u064a\u06aa\u064a \u062a\u0648\u0647\u0627\u0646 \u06a9\u064a \u0648\u064a\u0628 \u06fd \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u0627\u0635\u0644\u064a \u0634\u0646\u0627\u062e\u062a \u0633\u0627\u0646 \u062c\u0699\u064a\u0644 \u0631\u06a9\u06bb \u062c\u064a \u062a\u0631\u063a\u064a\u0628 \u068f\u064a\u0646 \u067f\u064a\u0648\u0646. \u0627\u0633\u0627\u0646 \u0631\u0633\u0627\u0626\u064a \u062c\u064a \u0627\u064a \u0645\u064a\u0644\u0648\u0646 \u064a\u0627 \u0645\u0639\u0644\u0648\u0645\u0627\u062a \u06aa\u068f\u0647\u0646 \u0628\u0647 \u06aa\u0646\u0647\u0646 \u067b\u0626\u064a \u0645\u0642\u0635\u062f \u0644\u0627\u0621\u0650 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0631\u06bb \u06fd \u0633\u0647\u0648\u0644\u062a \u06a9\u064a \u0631\u0648\u06aa\u06bb \u06a9\u0627\u0646 \u0633\u0648\u0627\u0621\u0650 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0648\u0646\u0647 \u06aa\u0646\u062f\u0627 \u0622\u0647\u064a\u0648\u0646. \u0627\u0633\u0627\u0646 \u062c\u064a \u0631\u0627\u0632\u062f\u0627\u0631\u064a \u067e\u0627\u0644\u064a\u0633\u064a \u062c\u0627 \u062c\u0627\u0645\u0639 \u06fd \u0645\u0633\u062a\u0646\u062f \u062c\u0648\u0627\u0628 \u0622\u0647\u0646 \u062a\u0647 \u0627\u0633\u0627\u0646 \u068a\u064a\u067d\u0627 \u06aa\u064a\u0626\u0646 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0631\u064a\u0648\u0646 \u067f\u0627 \u060c \u067e\u0631 \u0645\u062e\u062a\u0635\u0631 \u062c\u0648\u0627\u0628 \u0627\u0647\u0648 \u0622\u0647\u064a \u062a\u0647 \u0627\u0633\u0627\u0646 \u06a9\u064a \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u0628\u0631\u0627\u0624\u0632\u0646\u06af \u062a\u0627\u0631\u064a\u062e \u0633\u0627\u0646 \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u06af\u068f\u064a\u0644 \u0648\u0627\u0628\u0633\u062a\u06af\u064a \u06fe \u06aa\u0627 \u0628\u0647 \u062f\u0644\u0686\u0633\u067e\u064a \u0646\u0627\u0647\u064a. \u0627\u0633\u0627\u0646 \u067e\u06bb \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u0627\u064a \u0645\u064a\u0644 \u067e\u062a\u064a \u06a9\u064a \u062c\u0644\u062f\u064a \u0631\u062f \u06aa\u0631\u06bb \u0644\u0627\u0621\u0650 \u0647\u0646 \u0648\u0642\u062a \u0647\u06aa \u06aa\u0631\u067e\u067d\u067d\u0648\u06af\u0631\u0627\u0641\u06aa \u062d\u0644 \u062a\u064a \u06aa\u0645 \u06aa\u0631\u064a \u0631\u0647\u064a\u0627 \u0622\u0647\u064a\u0648\u0646. \u0627\u0633\u0627\u0646 \u062c\u064a \u0631\u0627\u0632\u062f\u0627\u0631\u064a \u067e\u0627\u0633 \u06aa\u0645 \u06a9\u064a \u0645\u06aa\u0645\u0644 \u06aa\u0646\u062f\u064a \u060c \u063a\u0644\u0637\u064a\u0646 \u06a9\u064a \u0631\u0648\u06aa\u06bb \u062c\u064a \u0642\u0627\u0628\u0644\u064a\u062a \u06a9\u064a \u0627\u0683\u0627\u0646 \u0628\u0647 \u0645\u062d\u0641\u0648\u0638 \u0631\u06a9\u0648\u0646 \u067f\u0627.",
    "page-acc-link-1": "\u062c\u064a\u06aa\u068f\u0647\u0646 \u062a\u0648\u0647\u0627\u0646 \u0631\u0633\u0627\u0626\u064a \u0648\u0627\u0631\u064a \u0635\u0627\u0631\u0641 \u0622\u0647\u064a\u0648 \u060c \u0645\u0647\u0631\u0628\u0627\u0646\u064a \u06aa\u0631\u064a \u0647\u062a\u064a \u0633\u0627\u0626\u0646 \u0627\u067e \u06aa\u0631\u064a\u0648.",
    "page-acc-subhead": "\u0633\u064a\u06aa\u064a\u0648\u0631\u067d\u064a \u06a9\u064a \u0628\u0631\u0642\u0631\u0627\u0631 \u0631\u06a9\u06bb \u062f\u0648\u0631\u0627\u0646 \u0633\u0680\u0646\u064a \u0635\u0627\u0631\u0641\u0646 \u0644\u0627\u0621\u0650 \u0628\u064a \u067e\u0647\u0686 \u0631\u0633\u0627\u0626\u064a \u06a9\u064a \u064a\u0642\u064a\u0646\u064a \u0628\u06bb\u0627\u064a\u0648",
    "page-acc-summary": "\u062e\u0644\u0627\u0635\u0648",
    "page-acc-summary-contents-1": "hCaptcha \u068a\u0632\u0627\u0626\u064a\u0646 \u0645\u0627\u06bb\u0647\u0646 \u06a9\u064a \u068c\u0627\u0631 \u068c\u0627\u0631 \u06aa\u0631\u064a \u0628\u0648\u067d\u0633 \u0631\u0648\u06aa\u06bb \u0644\u0627\u0621\u0650 \u067a\u0627\u0647\u064a\u0627 \u0648\u064a\u0627 \u0622\u0647\u0646. \u0628\u0635\u0631\u064a \u067d\u064a\u0633\u067d \u0627\u0646\u0647\u064a \u0644\u0627\u0621\u0650 \u0647\u06aa \u0622\u0633\u0627\u0646 \u0627\u0648\u0632\u0627\u0631 \u0622\u0647\u064a \u067e\u0631 \u0647\u0631 \u0645\u0627\u06bb\u0647\u0648 \u0647\u06aa \u0628\u0635\u0631\u064a \u0686\u0626\u0644\u064a\u0646\u062c \u062d\u0644 \u0646\u0647 \u06aa\u0631\u064a \u0633\u06af\u0647\u0646\u062f\u0648 \u0622\u0647\u064a. \u0627\u0646\u0647\u064a \u0644\u0627\u0621\u0650 \u0627\u0633\u0627\u0646 \u067a\u0647\u064a\u0644 \u0647\u06aa \u0622\u0633\u0627\u0646 \u060c \u062f\u0631\u062f \u0645\u0646\u062f \u0645\u062a\u0628\u0627\u062f\u0644 \u067a\u0627\u0647\u064a\u0648 \u0622\u0647\u064a \u067e\u0628\u0644\u0634\u0631\u0632 \u06a9\u064a \u0627\u0633\u0627\u0646 \u062c\u064a \u062e\u062f\u0645\u062a \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0646\u062f\u064a \u0633\u0680\u0646\u064a \u062a\u0627\u0626\u064a\u0646 \u0631\u0633\u0627\u0626\u064a \u062c\u064a \u062d\u0641\u0627\u0638\u062a \u0645\u06aa\u0645\u0644 \u0633\u064a\u06aa\u0634\u0646 508 \u06fd WCAG 2.0 \u0645\u0637\u0627\u0628\u0642.",
    "page-acc-summary-contents-2": "\u0627\u0647\u0648 \u06aa\u064a\u0626\u0646 \u06aa\u0645 \u06aa\u0631\u064a \u067f\u0648: \u067e\u0647\u0631\u064a\u0646 \u060c \u0647\u06aa \u067e\u0647\u0686 \u0648\u0627\u0631\u0648 \u0635\u0627\u0631\u0641 \u064a\u0648 \u0622\u0631 \u0627\u064a\u0644 \u062a\u064a \u0633\u0627\u0626\u0646 \u0627\u067e \u067f\u064a\u0648 \u060c \u062c\u064a\u06aa\u0648 hCaptcha widget \u0645\u0639\u0644\u0648\u0645\u0627\u062a \u067e\u064a\u062c \u06fe \u06b3\u0646 is\u064a\u0644 \u0622\u0647\u064a. \u0627\u0646\u0647\u0646 \u06a9\u064a \u0647\u06aa \u06aa\u0631\u064a\u068a\u067d \u06aa\u0648\u06aa\u064a \u068f\u0646\u064a \u0648\u0626\u064a \u0622\u0647\u064a \u062c\u064a\u06aa\u0627 \u0647\u0631 \u068f\u064a\u0646\u0647\u0646 \u06aa\u064a\u062a\u0631\u0627\u0626\u064a \u0680\u064a\u0631\u0627 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u067f\u064a \u0633\u06af\u0647\u064a \u067f\u064a \u060c \u067e\u0631 \u0644\u0627\u06af \u0627\u0646 \u067f\u064a\u06bb \u062c\u064a \u0647\u0631 24 \u06aa\u0644\u0627\u06aa \u06a9\u064a \u0631\u064a\u0641\u0631\u064a\u0634 \u067f\u064a\u06bb\u0648 \u067e\u0648\u0646\u062f\u0648. \u062c\u068f\u0647\u0646 \u0647\u06aa \u0686\u0626\u0644\u064a\u0646\u062c \u06aa\u0646\u0647\u0646 \u0631\u0633\u0627\u0626\u064a \u062c\u064a \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0646\u062f\u0699 \u0635\u0627\u0631\u0641 \u06a9\u064a hCaptcha \u0633\u0631\u0648\u0633 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0646\u062f\u064a \u067e\u064a\u0634 \u06aa\u064a\u0648 \u0648\u064a\u0646\u062f\u0648 \u0622\u0647\u064a \u060c \u0627\u0647\u064a \u067e\u0627\u06bb\u0645\u0631\u0627\u062f\u0648 \u06af\u0630\u0631\u064a \u0648\u064a\u0646\u062f\u0627.",
    "page-acc-title": "hCaptcha \u0631\u0633\u0627\u0626\u064a \u062c\u064a Information\u0627\u06bb",
    "page-acc-tr-body": "\u0633\u0648\u0627\u0644: \u0622\u0626\u0648\u0646 \u06aa\u0648\u06aa\u064a\u0632 \u06a9\u064a \u0633\u064a\u067d \u06aa\u0631\u06bb \u06a9\u0627\u0646\u067e\u0648\u0621\u0650 \u0627\u0683\u0627 \u0628\u0647 \u0647\u06aa \u0686\u0626\u0644\u064a\u0646\u062c \u068f\u0633\u064a \u0631\u0647\u064a\u0648 \u0622\u0647\u064a\u0627\u0646. \u0627\u0647\u0648 \u06aa\u0647\u0699\u0648 \u0633\u0628\u0628 \u0622\u0647\u064a\u061f \u062c\u0648\u0627\u0628: \u0627\u0647\u0648 \u0639\u0627\u0645 \u0637\u0648\u0631 \u062a\u064a \u062c\u0627\u0631\u062d\u064a\u062a \u0648\u0627\u0631\u064a \u0627\u0634\u062a\u0647\u0627\u0631 \u0628\u0627\u0632 \u06a9\u064a \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0631\u06bb \u0648\u0627\u0631\u064a \u06aa\u0648\u06aa \u064a\u0627 \u0627\u064a\u0646\u067d\u064a \u06aa\u0648\u06aa\u064a \u0648\u0627\u068c \u062c\u064a \u06aa\u0631\u064a \u060c \u064a\u0627 \u0647\u06aa \u0633\u064a\u067d\u0646\u06af \u062c\u064a\u06aa\u0627 \u06aa\u0631\u0627\u0633 \u0633\u0627\u0626\u064a\u067d \u06aa\u0648\u06aa\u064a\u0632 \u06a9\u064a \u0628\u0644\u0627\u06aa \u06aa\u0631\u064a \u067f\u064a \u060c \u0627\u0646\u0647\u064a \u0635\u0648\u0631\u062a \u06fe hcaptcha.com \u0644\u0627\u0621\u0650 \u06aa\u0648\u06aa\u064a\u0632 \u062c\u064a\u06aa\u0627 \u0645\u062e\u062a\u0644\u0641 \u0633\u0627\u0626\u064a\u067d \u062a\u064a hCaptcha \u062c\u0627\u0648\u0627 \u0627\u0633\u06aa\u0631\u067e\u067d \u06a9\u064a \u0633\u064a\u067d \u064a\u0627 \u0686\u064a\u06aa \u067f\u064a\u0644 \u0622\u0647\u064a. \u0648\u0627\u0646\u06af\u0631 \u060c \u062c\u064a\u06aa\u0648 \u062a\u0648\u0647\u0627\u0646 \u062f\u0648\u0631\u0648 \u06aa\u064a\u0648 \u067e\u064a\u0648 \u0648\u0683\u064a. hCaptcha \u0631\u0633\u0627\u0626\u064a \u0648\u0627\u0631\u0648 \u06aa\u0648\u06aa\u064a\u0632 \u0633\u0680\u0646\u064a \u0645\u0639\u064a\u0627\u0631\u064a \u0628\u0631\u0627\u0624\u0632\u0631\u0632 \u06fd \u0627\u0634\u062a\u0647\u0627\u0631\u0646 \u062c\u064a \u0628\u0644\u0627\u06aa\u0631\u0646 \u06a9\u064a \u0627\u0646\u0647\u0646 \u062c\u064a \u0645\u0639\u064a\u0627\u0631\u064a \u0633\u064a\u067d\u0646\u06af\u0646 \u0633\u0627\u0646 \u06aa\u0645 \u06aa\u0646\u062f\u064a\u0648\u0646 \u0622\u0647\u0646 \u060c \u062a\u0646\u0647\u0646 \u06aa\u0631\u064a \u0639\u0627\u0645 \u0637\u0648\u0631 \u062a\u064a \u0646\u0627\u06aa\u0627\u0645\u064a\u0648\u0646 \u0636\u062f \u0645\u062e\u0627\u0644\u0641 \u0627\u0634\u062a\u0647\u0627\u0631 \u0645\u062e\u0627\u0644\u0641 \u0627\u0633\u06aa\u0631\u067e\u067d \u064a\u0627 \u062e\u0627\u0635 \u0633\u0627\u0626\u064a\u067d\u0646 \u06a9\u064a \u0647\u062f\u0641 \u068f\u064a\u06bb \u0648\u0627\u0631\u064a \u0642\u0627\u0639\u062f\u0646 \u062c\u064a \u0633\u0628\u0628 \u0647\u0648\u0646\u062f\u064a\u0648\u0646 \u0622\u0647\u0646. \u062d\u0644: 1. \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u0627\u064a\u068a \u0628\u0644\u0627\u06aa\u0631 \u064a\u0627 \u0628\u0631\u0627\u0626\u0648\u0632\u0631 \u0633\u064a\u06aa\u064a\u0648\u0631\u067d\u064a \u062a\u0648\u0633\u064a\u0639 \u06fe \u0648\u0627\u0626\u064a\u067d \u0644\u0633\u067d hcaptcha.com \u06fd * .hcaptcha.com \u06aa\u0648\u06aa\u064a\u0632. 2. \u062c\u064a\u06aa\u068f\u0647\u0646 \u062a\u0648\u0647\u0627\u0646 \u0628\u0647\u0627\u062f\u0631 \u0628\u0631\u0627\u0626\u0648\u0632\u0631 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0631\u064a \u0631\u0647\u064a\u0627 \u0622\u0647\u064a\u0648 \u060c \u062c\u0646\u0647\u0646 \u06fe (\u0627\u067e\u0631\u064a\u0644 2020 \u062a\u0627\u0626\u064a\u0646) \u06aa\u0627\u06aa\u064a \u0641\u0647\u0631\u0633\u062a \u068f\u0646\u0644 \u06aa\u0646\u0647\u0646 \u0628\u0647 \u0642\u0633\u0645 \u062c\u064a \u0634\u064a\u0621\u064e \u0638\u0627\u0647\u0631 \u0646\u0647 \u067f\u064a \u0622\u0647\u064a \u060c \u062a\u0631\u062c\u064a\u062d\u0627\u062a \u068f\u0627\u0646\u0647\u0646 \u0648\u0683\u0648 -> \u0634\u064a\u0644\u068a\u0633 -> \u06aa\u0648\u06aa\u064a\u0632 \u06fd \u0633\u0680 \u06aa\u0648\u06aa\u064a\u0632 \u062c\u064a \u0627\u062c\u0627\u0632\u062a \u068f\u064a\u0648. 3. \u062c\u064a\u06aa\u068f\u0647\u0646 \u062a\u0648\u0647\u0627\u0646 \u064a\u0627 \u062a\u0647 \u062a\u0627\u0632\u0648 \u0622\u0632\u0627\u062f \u067f\u064a\u0644 \u0627\u0648 \u0627\u064a\u0633 \u0627\u064a\u06aa\u0633 10.15 \u064a\u0627 \u0622\u0626\u064a \u0627\u0648 \u0627\u064a\u0633 13. \u062a\u064a \u0633\u0641\u0627\u0631\u064a \u062c\u0648 \u062a\u0645\u0627\u0645 \u06af\u0647\u06bb\u0648 \u0648\u0631\u0632\u0646 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0631\u064a \u0631\u0647\u064a\u0627 \u0622\u0647\u064a\u0648 \u060c \u0627\u064a\u067e\u0644 \u0635\u0631\u0641 \u067d\u0626\u064a\u0646 \u067e\u0627\u0631\u067d\u064a \u062c\u064a \u06aa\u0648\u06aa\u064a\u0632 \u0633\u0627\u0646 \u0644\u0627\u06b3\u0627\u067e\u064a\u0644 \u0633\u0641\u0627\u0631\u064a \u062c\u064a \u0631\u0648\u064a\u064a \u06a9\u064a \u062a\u0628\u062f\u064a\u0644 \u06aa\u064a\u0648 \u0622\u0647\u064a \u060c \u0627\u0646\u0647\u0646 \u0633\u0680\u0646\u064a \u06a9\u064a \u068a\u0641\u0627\u0644\u067d \u0637\u0648\u0631 \u0628\u0644\u0627\u06aa \u06aa\u0631\u064a \u0687\u068f\u064a\u0648 \u0622\u0647\u064a. \u0627\u0633\u0627\u0646 \u0647\u06aa \u062d\u0644 \u062a\u064a \u0639\u0645\u0644 \u06aa\u0631\u064a \u0631\u0647\u064a\u0627 \u0622\u0647\u064a\u0648\u0646 \u060c \u067e\u0631 \u0627\u0646 \u062f\u0648\u0631\u0627\u0646 \u0645\u0647\u0631\u0628\u0627\u0646\u064a \u06aa\u0631\u064a \u0633\u0641\u0627\u0631\u064a \u062c\u0648\u0646 \u062a\u0631\u062c\u064a\u062d\u0648\u0646 \u060c \u0631\u0627\u0632\u062f\u0627\u0631\u064a \u0633\u064a\u06aa\u0634\u0646 \u062c\u0648 \u062f\u0648\u0631\u0648 \u06aa\u0631\u064a\u0648 \u060c \u06fd \u0648\u064a\u0628 \u0633\u0627\u0626\u064a\u067d \u062c\u064a \u067d\u0631\u064a\u06aa \u06a9\u064a \u0686\u064a\u06aa \u06aa\u0631\u064a\u0648: \u06aa\u0631\u0627\u0633 \u0633\u0627\u0626\u064a\u067d \u067d\u0631\u064a\u06aa\u0646\u06af \u06a9\u064a \u0631\u0648\u06aa\u064a\u0648 \u062a\u0647 \u0631\u0633\u0627\u0626\u064a \u062c\u064a \u06aa\u0648\u06aa\u064a \u06a9\u064a \u062a\u0648\u0642\u0639 \u0637\u0648\u0631 \u06aa\u0645 \u06aa\u0631\u06bb \u0644\u0627\u0621\u0650 \u0641\u0639\u0627\u0644 \u06aa\u064a\u0648 \u0648\u0683\u064a. \u0633\u0648\u0627\u0644: \u0622\u0626\u0648\u0646 \u06aa\u064a\u062a\u0631\u0627\u0626\u064a \u068a\u064a\u0648\u0627\u0626\u0633 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0646\u062f\u0648 \u0622\u0647\u064a\u0627\u0646. \u0687\u0627 \u0645\u0648\u0646\u06a9\u064a \u06aa\u064a\u062a\u0631\u0627\u0626\u064a \u0680\u064a\u0631\u0627 \u0633\u0627\u0626\u0646 \u0627\u067e \u06aa\u0631\u06bb \u062c\u064a \u0636\u0631\u0648\u0631\u062a \u0622\u06be\u064a\u061f \u062c\u0648\u0627\u0628: \u0646\u0647 \u060c \u0645\u0647\u0631\u0628\u0627\u0646\u064a \u06aa\u0631\u064a \u06aa\u0648\u06aa\u064a \u0633\u064a\u067d \u06aa\u0631\u06bb \u0644\u0627\u0621\u0650 \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u0647\u0631 \u068a\u064a\u0648\u0627\u0626\u0633 \u062a\u064a \u062a\u0648\u0647\u0627\u0646 \u068f\u0627\u0646\u0647\u0646 \u0645\u0648\u06aa\u0644\u064a\u0644 \u0633\u0627\u06b3\u064a\u0627 \u0627\u064a \u0645\u064a\u0644 \u0644\u0627\u06af \u0627\u0646 \u0644\u0646\u06aa \u06aa\u0644\u06aa \u06aa\u0631\u064a\u0648.",
    "page-acc-tr-title": "\u0631\u0633\u0627\u0626\u064a \u062c\u064a \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0646\u062f\u0699\u0646 \u0644\u0627\u0621\u0650: \u0633\u0648\u0627\u0644 \u06fd \u067b\u067b\u0631 \u06fd \u0645\u0633\u0626\u0644\u0627 \u062d\u0644 \u06aa\u0631\u06bb \u0648\u0627\u0631\u064a \u06af\u0627\u0626\u064a\u068a",
    "priv-hc": "\u0631\u0627\u0632\u062f\u0627\u0631\u064a \u0645\u0631\u06aa\u0648\u0632 \u067f\u064a \u0648\u0626\u064a",
    "priv-hc-text": "\u0627\u0634\u062a\u0647\u0627\u0631 \u062c\u0627 \u0646\u064a\u067d \u0648\u0631\u06aa \u062a\u0648\u0647\u0627\u0646 \u06a9\u064a \u0627\u0646\u0647\u0646 \u062c\u064a \u067e\u0631\u0627\u068a\u06aa\u067d \u0637\u0648\u0631 \u068f\u0633\u0646\u062f\u0627 \u0622\u0647\u0646. \u0627\u0633\u0627\u0646 \u0646\u067f\u0627 \u06aa\u0631\u064a\u0648\u0646. \u0627\u0633\u0627\u0646 \u06a9\u064a \u067e\u0631\u0648\u0627\u0647 \u0646\u0627\u0647\u064a \u062a\u0647 \u06aa\u0647\u0699\u0627 \u0635\u0627\u0631\u0641 \u0622\u0647\u0646 \u060c \u0635\u0631\u0641 \u0627\u0647\u0648 \u06aa\u064a\u0626\u0646 \u0645\u0645\u06aa\u0646 \u0622\u0647\u064a \u062a\u0647 \u0627\u0647\u064a \u0628\u062f\u0646\u0627\u0645 \u0647\u0648\u0646\u062f\u0627. \u0627\u0633\u0627\u0646 \u067e\u0648\u0626\u0644\u06b3\u0646 \u0648\u0627\u0646\u06af\u0631 \u0631\u0627\u0632\u062f\u0627\u0631\u064a \u067e\u0627\u0633 \u060c \u06fd \u0631\u0627\u0632\u062f\u0627\u0631\u064a \u06fd \u0633\u064a\u06aa\u064a\u0648\u0631\u067d\u064a \u06a9\u064a \u0645\u062a\u0648\u0627\u0632\u0646 \u06aa\u0631\u06bb \u0644\u0627\u0621\u0650 \u0645\u062d\u0646\u062a \u0633\u0627\u0646 \u062a\u0639\u0627\u0648\u0646 \u06aa\u0631\u064a\u0648\u0646 \u067f\u0627.",
    "sec-hc": "\u0633\u064a\u06aa\u064a\u0648\u0631\u067d\u064a \u067e\u0647\u0631\u064a\u0646",
    "sec-hc-text": "hCaptcha \u0646\u0627\u0634\u0631 \u062a\u064a \u0645\u0631\u06a9\u0648\u0632 \u0622\u06be\u064a. \u0627\u0633\u0627\u0646 \u0633\u0627\u0626\u064a\u067d\u0646 \u06a9\u064a \u0627\u0635\u0644\u064a \u068a\u064a\u067d\u0627 \u068f\u064a\u06bb \u0644\u0627\u0621\u0650 \u06aa\u0645 \u06aa\u0631\u064a\u0648\u0646 \u067f\u0627 \u06fd \u0627\u0646\u0647\u0646 \u06a9\u064a \u0627\u0647\u0648 \u0641\u064a\u0635\u0644\u0648 \u06aa\u0631\u06bb \u068f\u064a\u0648 \u062a\u0647. \u062f\u0631\u0633\u062a\u064a \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u0645\u0641\u0627\u062f \u06fd \u0627\u0633\u0627\u0646 \u06fe \u0622\u0647\u064a. \u067b\u064a\u0648\u0646 \u06aa\u064a\u067e\u0686\u064a\u0648\u0646 \u0645\u0634\u0647\u0648\u0631\u064a \u0627\u062f\u0627\u0631\u0646 \u0637\u0631\u0641\u0627\u0646 \u0645\u0647\u064a\u0627 \u06aa\u064a\u0648\u0646 \u0648\u064a\u0646\u062f\u064a\u0648\u0646 \u0622\u0647\u0646. \u0627\u0647\u064a \u0647\u0631 \u0628\u067d\u0648 \u0628\u0644\u0627\u06aa \u0627\u0646\u0647\u0646 \u062c\u064a \u0622\u0645\u062f\u0646\u064a \u06af\u0647\u067d\u0627\u0626\u064a \u0633\u06af\u0647\u064a \u067f\u064a.",
    "what-is-hcaptcha": "hCaptcha \u0687\u0627 \u0622\u0647\u064a\u061f",
    "what-is-hcaptcha-text": "hCaptcha \u0645\u062f\u062f \u06aa\u0646\u062f\u0648 \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u067e\u0633\u0646\u062f\u064a\u062f\u0647 \u0648\u064a\u0628 \u0633\u0631\u0648\u0633\u0632 \u0628\u0648\u067d \u060c \u0627\u0633\u067e\u0627\u0645 \u06fd \u063a\u0644\u0637 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u0627\u0646 \u067b\u0627\u0647\u0631 \u0633\u0627\u062f\u064a \u0633\u0648\u0627\u0644 \u067e\u0687\u06bb \u0633\u0627\u0646 \u062c\u064a\u06aa\u064a \u0627\u0646\u0633\u0627\u0646\u0646 \u0644\u0627\u0621\u0650 \u0622\u0633\u0627\u0646 \u06fd \u0645\u0634\u064a\u0646\u0646 \u0644\u0627\u0621\u0650 \u0645\u0634\u06aa\u0644 \u0647\u0648\u0646\u062f\u0627.",
    "why-hc": "\u0687\u0648 hCaptcha\u061f",
    "why-hc-text": "\u0647\u0631 \u0648\u0642\u062a \u062a\u0648\u0647\u0627\u0646 \u0647\u06aa \u0686\u0626\u0644\u064a\u0646\u062c \u062c\u0648 \u062c\u0648\u0627\u0628 \u068f\u064a\u0646\u062f\u0627\u0633\u064a\u0646 \u060c \u0627\u0633\u0627\u0646 \u0627\u0647\u0648 \u0645\u0639\u0644\u0648\u0645 \u06aa\u0631\u06bb \u0644\u0627\u0621\u0650 \u062c\u062f\u064a\u062f \u0645\u0627\u068a\u0644 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0646\u062f\u0627 \u0622\u0647\u064a\u0648\u0646 \u062a\u0647 \u062a\u0648\u0647\u0627\u0646 \u0627\u0646\u0633\u0627\u0646 \u0622\u0647\u064a\u0648 \u064a\u0627 \u0647\u06aa \u0645\u0634\u064a\u0646. \u062c\u0646\u0647\u0646 \u0648\u064a\u0628 \u0633\u0627\u0626\u064a\u067d \u062a\u064a \u062a\u0648\u0647\u0627\u0646 \u0648\u064a\u0627 \u0622\u0647\u064a\u0648 \u0627\u0647\u0627 \u0646\u062a\u064a\u062c\u0648 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06aa\u0631\u064a \u067f\u064a \u062a\u0648\u0647\u0627\u0646 \u062c\u064a \u0633\u0627\u0626\u064a\u067d \u062a\u064a \u0627\u0646\u0647\u064a \u062c\u0648 \u062a\u062c\u0631\u0628\u0648 \u0628\u0647\u062a\u0631 \u06aa\u0631\u06bb \u06fe \u0627\u0646\u0647\u064a \u062c\u064a \u0645\u062f\u062f \u06aa\u0631\u06bb \u0633\u0627\u0646."
}