{
    "acc-opt": "\u06af\u0632\u06cc\u0646\u0647 \u0647\u0627\u06cc \u062f\u0633\u062a\u0631\u0633\u06cc",
    "acc-opt-but": "\u062f\u0631\u0628\u0627\u0631\u0647 \u06af\u0632\u06cc\u0646\u0647 \u0647\u0627\u06cc \u062f\u0633\u062a\u0631\u0633\u06cc \u0628\u06cc\u0634\u062a\u0631 \u0628\u06cc\u0627\u0645\u0648\u0632\u06cc\u062f",
    "acc-opt-text": "\u0622\u06cc\u0627 \u0634\u0645\u0627 \u0627\u0632 \u0644\u062d\u0627\u0638 \u0628\u06cc\u0646\u0627\u06cc\u06cc \u0627\u062e\u062a\u0644\u0627\u0644 \u062f\u0627\u0631\u06cc\u062f \u06cc\u0627 \u062f\u0631 \u063a\u06cc\u0631 \u0627\u06cc\u0646 \u0635\u0648\u0631\u062a \u0642\u0627\u062f\u0631 \u0628\u0647 \u062a\u06a9\u0645\u06cc\u0644 hCaptchas \u0646\u06cc\u0633\u062a\u06cc\u062f\u061f",
    "accessibility-link": "\u062f\u0633\u062a\u0631\u0633\u06cc",
    "appreciate-in": "\u0645\u0627 \u0627\u0632 \u0648\u0631\u0648\u062f\u06cc \u0634\u0645\u0627 \u0642\u062f\u0631\u062f\u0627\u0646\u06cc \u0645\u06cc \u06a9\u0646\u06cc\u0645 \u0628\u0647 \u0645\u0627 \u0627\u0637\u0644\u0627\u0639 \u062f\u0647\u06cc\u062f \u0648 \u0645\u0627 \u0628\u0647 \u0633\u0631\u0639\u062a \u0622\u0646\u0647\u0627 \u0631\u0627 \u062d\u0644 \u062e\u0648\u0627\u0647\u06cc\u0645 \u06a9\u0631\u062f.",
    "fb-but": "\u0628\u0631\u0627\u06cc \u0645\u0627 \u0628\u0627\u0632\u062e\u0648\u0631\u062f \u0627\u0631\u0633\u0627\u0644 \u06a9\u0646\u06cc\u062f",
    "feedback-text": "\u0628\u0627\u0632\u062e\u0648\u0631\u062f\u06cc \u062f\u0631\u0628\u0627\u0631\u0647 hCaptcha \u06a9\u0647 \u062a\u0627\u0632\u0647 \u062f\u06cc\u062f\u06cc\u062f \u0627\u0631\u0627\u0626\u0647 \u062f\u0647\u06cc\u062f",
    "home-ent-1": "\u0628\u0647 \u062f\u0646\u0628\u0627\u0644 \u0627\u0631\u0627\u0626\u0647 \u062e\u062f\u0645\u0627\u062a \u0627\u0645\u0646\u06cc\u062a\u06cc \u0633\u0627\u0632\u0645\u0627\u0646\u06cc \u0645\u0627 \u0647\u0633\u062a\u06cc\u062f\u061f",
    "home-ent-2": "\u0627\u0632 \u0633\u0627\u06cc\u062a \u0627\u062e\u062a\u0635\u0627\u0635\u06cc \u062c\u062f\u06cc\u062f \u0645\u0627: BotStop.com \u0628\u0627\u0632\u062f\u06cc\u062f \u06a9\u0646\u06cc\u062f",
    "home-intro-body": "\u0622\u06cc\u0627 \u0628\u0631\u0627\u06cc \u0646\u06af\u0647 \u062f\u0627\u0634\u062a\u0646 \u0631\u0628\u0627\u062a\u0647\u0627 \u0627\u0632 captcha \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0645\u06cc \u06a9\u0646\u06cc\u062f\u061f hCaptcha \u0627\u0632 \u062d\u0631\u06cc\u0645 \u0634\u062e\u0635\u06cc \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 \u0645\u062d\u0627\u0641\u0638\u062a \u0645\u06cc \u06a9\u0646\u062f \u060c \u0628\u0647 \u0648\u0628 \u0633\u0627\u06cc\u062a \u0647\u0627 \u067e\u0627\u062f\u0627\u0634 \u0645\u06cc \u062f\u0647\u062f \u0648 \u0628\u0647 \u0634\u0631\u06a9\u062a \u0647\u0627 \u06a9\u0645\u06a9 \u0645\u06cc \u06a9\u0646\u062f \u062a\u0627 \u062f\u0627\u062f\u0647 \u0647\u0627\u06cc \u062e\u0648\u062f \u0631\u0627 \u0628\u0631\u0686\u0633\u0628 \u06af\u0630\u0627\u0631\u06cc \u06a9\u0646\u0646\u062f. \u0627\u06cc\u0646 \u06cc\u06a9 \u062c\u0627\u06cc\u06af\u0632\u06cc\u0646 \u0642\u0637\u0631\u0647 \u0628\u0631\u0627\u06cc reCAPTCHA \u0627\u0633\u062a: \u0634\u0645\u0627 \u0645\u06cc \u062a\u0648\u0627\u0646\u06cc\u062f \u062f\u0631 \u0639\u0631\u0636 \u0686\u0646\u062f \u062f\u0642\u06cc\u0642\u0647 \u062a\u063a\u06cc\u06cc\u0631 \u062f\u0647\u06cc\u062f.",
    "home-intro-head": "\u062a\u0648\u0642\u0641 \u0631\u0628\u0627\u062a \u0647\u0627\u06cc \u0628\u06cc\u0634\u062a\u0631. \u0634\u0631\u0648\u0639 \u0628\u0647 \u0645\u062d\u0627\u0641\u0638\u062a \u0627\u0632 \u062d\u0631\u06cc\u0645 \u062e\u0635\u0648\u0635\u06cc \u06a9\u0627\u0631\u0628\u0631.",
    "home-main-1": "\u062e\u0635\u0648\u0635\u06cc. \u0627\u0645\u0646 \u0627\u0633\u062a. \u0633\u0631\u06cc\u0639\u062a\u0631 \u0648 \u0645\u0627 \u0628\u0647 \u0634\u0645\u0627 \u067e\u0631\u062f\u0627\u062e\u062a \u0645\u06cc \u06a9\u0646\u06cc\u0645 \u062a\u0627 \u0627\u0632 \u0622\u0646 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u06a9\u0646\u06cc\u062f",
    "home-main-2": "\u062a\u0645\u0631\u06a9\u0632 \u062d\u0631\u06cc\u0645 \u062e\u0635\u0648\u0635\u06cc",
    "home-main-2c": "\u0634\u0628\u06a9\u0647 \u0647\u0627\u06cc \u062a\u0628\u0644\u06cc\u063a\u0627\u062a\u06cc \u0628\u0627\u0632\u062f\u06cc\u062f \u06a9\u0646\u0646\u062f\u06af\u0627\u0646 \u0634\u0645\u0627 \u0631\u0627 \u0628\u0647 \u0639\u0646\u0648\u0627\u0646 \u0645\u062d\u0635\u0648\u0644 \u0645\u06cc \u0628\u06cc\u0646\u0646\u062f. \u0628\u0631\u0627\u06cc hCaptcha \u0645\u062d\u0635\u0648\u0644 \u06a9\u0627\u0631\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0622\u0646\u0647\u0627 \u0627\u0631\u0627\u0626\u0647 \u0645\u06cc \u062f\u0647\u0646\u062f. \u0645\u0627 \u0627\u0647\u0645\u06cc\u062a\u06cc \u0646\u0645\u06cc \u062f\u0647\u06cc\u0645 \u06a9\u0647 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 \u0634\u0645\u0627 \u0686\u0647 \u06a9\u0633\u0627\u0646\u06cc \u0647\u0633\u062a\u0646\u062f \u060c \u0641\u0642\u0637 \u0686\u0642\u062f\u0631 \u0627\u062d\u062a\u0645\u0627\u0644 \u062f\u0627\u0631\u0646\u062f \u0622\u0646\u0647\u0627 \u0628\u062f\u062e\u0648\u0627\u0647 \u0628\u0627\u0634\u0646\u062f. \u0645\u0627 \u0627\u0632 \u0627\u0628\u062a\u06a9\u0627\u0631\u0627\u062a\u06cc \u0645\u0627\u0646\u0646\u062f \u06af\u0630\u0631\u06af\u0627\u0647 \u062d\u0631\u06cc\u0645 \u062e\u0635\u0648\u0635\u06cc \u067e\u0634\u062a\u06cc\u0628\u0627\u0646\u06cc \u0645\u06cc \u06a9\u0646\u06cc\u0645 \u060c \u0648 \u0628\u0631\u0627\u06cc \u062a\u0639\u0627\u062f\u0644 \u062d\u0641\u0638 \u062d\u0631\u06cc\u0645 \u062e\u0635\u0648\u0635\u06cc \u0648 \u0627\u0645\u0646\u06cc\u062a \u0628\u0633\u06cc\u0627\u0631 \u0633\u062e\u062a \u062a\u0644\u0627\u0634 \u0645\u06cc \u06a9\u0646\u06cc\u0645.",
    "home-main-3": "\u0627\u0645\u0646\u06cc\u062a \u0627\u0648\u0644",
    "home-main-3c": "\u062f\u0642\u062a \u0628\u0647 \u0646\u0641\u0639 \u0634\u0645\u0627 \u0648 \u0645\u0627\u0633\u062a. hCaptcha \u0628\u0627 \u0645\u062d\u0648\u0631\u06cc\u062a \u0646\u0627\u0634\u0631 \u0627\u0633\u062a. \u0645\u0627 \u062f\u0631 \u062a\u0644\u0627\u0634\u06cc\u0645 \u062a\u0627 \u062f\u0627\u062f\u0647 \u0647\u0627\u06cc \u0648\u0627\u0642\u0639\u06cc \u0631\u0627 \u0628\u0647 \u0634\u0645\u0627 \u0627\u0631\u0627\u0626\u0647 \u062f\u0647\u06cc\u0645 \u0648 \u0628\u0647 \u0634\u0645\u0627 \u0627\u06cc\u0646 \u0627\u0645\u06a9\u0627\u0646 \u0631\u0627 \u0645\u06cc \u062f\u0647\u06cc\u0645 \u06a9\u0647 \u062f\u0631 \u0645\u0648\u0631\u062f \u0646\u062d\u0648\u0647 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 \u0622\u0646 \u062a\u0635\u0645\u06cc\u0645 \u0628\u06af\u06cc\u0631\u06cc\u062f. \u0633\u0627\u06cc\u0631 \u0627\u0633\u06cc\u0631\u0647\u0627 \u062a\u0648\u0633\u0637",
    "home-main-3c2": "\u0634\u0631\u06a9\u062a \u0647\u0627\u06cc \u062a\u0628\u0644\u06cc\u063a\u0627\u062a\u06cc \u0627\u06cc\u0646 \u0628\u062f\u0627\u0646 \u0645\u0639\u0646\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0647\u0631 \u0631\u0628\u0627\u062a \u06a9\u0647 \u0645\u0633\u062f\u0648\u062f \u0645\u06cc \u06a9\u0646\u062f \u0645\u0645\u06a9\u0646 \u0627\u0633\u062a \u062f\u0631\u0622\u0645\u062f \u0622\u0646\u0647\u0627 \u0631\u0627 \u06a9\u0627\u0647\u0634 \u062f\u0647\u062f.",
    "home-main-4": "\u062a\u062c\u0631\u0628\u0647 \u06a9\u0627\u0631\u0628\u0631\u06cc \u0628\u0647\u062a\u0631",
    "home-main-4c": "\u0645\u062f\u0644\u0647\u0627\u06cc \u06cc\u0627\u062f\u06af\u06cc\u0631\u06cc \u067e\u06cc\u0634\u0631\u0641\u062a\u0647 \u0645\u0627\u0634\u06cc\u0646 \u062a\u0648\u0633\u0637 hCaptcha \u0628\u0631\u0627\u06cc \u062a\u0639\u06cc\u06cc\u0646 \u0628\u0634\u0631\u06cc\u062a \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0645\u06cc \u0634\u0648\u062f. \u0627\u06cc\u0646 \u0628\u0647 \u0645\u0627 \u0627\u0645\u06a9\u0627\u0646 \u0645\u06cc \u062f\u0647\u062f \u062a\u0627 \u06a9\u0627\u0631\u0647\u0627\u06cc \u0634\u0645\u0627 \u0631\u0627 \u06a9\u0645\u062a\u0631 \u0622\u0632\u0627\u0631\u062f\u0647\u0646\u062f\u0647 \u06a9\u0646\u06cc\u0645 \u0648 \u0627\u0632 \u0627\u06cc\u0646 \u0637\u0631\u06cc\u0642 \u06a9\u0645\u062a\u0631 \u0627\u0632 \u0648\u0642\u062a \u062e\u0648\u062f \u0631\u0627 \u0635\u0631\u0641 \u06a9\u0646\u06cc\u0645 \u060c \u062f\u0631 \u062d\u0627\u0644\u06cc \u06a9\u0647 \u0647\u0646\u0648\u0632 \u0647\u0645 \u062f\u0627\u062f\u0647 \u0647\u0627\u06cc \u062f\u0627\u0631\u0627\u06cc \u0628\u0631\u0686\u0633\u0628 \u0645\u0641\u06cc\u062f \u0631\u0627 \u0628\u0631\u0627\u06cc \u0628\u0631\u0686\u0633\u0628 \u0632\u062f\u0646 \u0628\u0647 \u0645\u0634\u062a\u0631\u06cc\u0627\u0646 \u062a\u0648\u0644\u06cc\u062f \u0645\u06cc \u06a9\u0646\u06cc\u0645.",
    "home-main-plans": "hCaptcha \u0628\u0631\u0627\u06cc \u0646\u0627\u0634\u0631\u0627\u0646 \u0627\u0632 \u0647\u0631 \u0627\u0646\u062f\u0627\u0632\u0647 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0631\u0627\u06cc\u06af\u0627\u0646 \u0627\u0633\u062a. \u0628\u0627 \u0627\u06cc\u0646 \u062d\u0627\u0644 \u060c \u0628\u0631\u0627\u06cc \u0634\u0631\u06a9\u062a \u0647\u0627 \u0648 \u0633\u06cc\u0633\u062a\u0645 \u0639\u0627\u0645\u0644 \u0647\u0627\u06cc\u06cc \u06a9\u0647 \u0628\u0647 \u06a9\u0646\u062a\u0631\u0644 \u0628\u06cc\u0634\u062a\u0631\u06cc \u0646\u06cc\u0627\u0632 \u062f\u0627\u0631\u0646\u062f \u060c \u0631\u0627\u0647 \u062d\u0644 \u0647\u0627\u06cc \u0633\u0627\u0632\u0645\u0627\u0646\u06cc \u0645\u0627 \u062f\u0631 \u062f\u0633\u062a\u0631\u0633 \u0627\u0633\u062a.",
    "home-main-plans2": "\u0631\u0627\u0647 \u062d\u0644 \u0647\u0627\u06cc \u0634\u0631\u06a9\u062a hCaptcha \u0645\u06cc\u0644\u06cc\u0648\u0646 \u0647\u0627 \u06a9\u0627\u0631\u0628\u0631 \u0631\u0627 \u062f\u0631 \u0628\u06cc\u0646 \u062f\u0647 \u0647\u0627 \u0645\u06cc\u0644\u06cc\u0648\u0646 \u0648\u0628 \u0633\u0627\u06cc\u062a \u0628\u0631\u0627\u06cc \u0628\u0631\u062e\u06cc \u0627\u0632 \u0628\u0632\u0631\u06af\u062a\u0631\u06cc\u0646 \u0634\u0631\u06a9\u062a \u0647\u0627\u06cc \u06a9\u0631\u0647 \u0632\u0645\u06cc\u0646 \u0645\u062d\u0627\u0641\u0638\u062a \u0645\u06cc \u06a9\u0646\u062f. \u0644\u0637\u0641\u0627 \u0628\u0631\u0627\u06cc \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0628\u06cc\u0634\u062a\u0631 \u0628\u0627 \u0645\u0627 \u062a\u0645\u0627\u0633 \u0628\u06af\u06cc\u0631\u06cc\u062f \u06cc\u0627 \u0628\u0627\u0632\u062f\u06cc\u062f \u06a9\u0646\u06cc\u062f",
    "home-main-plans3": "\u0631\u0627\u0647 \u062d\u0644 \u0647\u0627\u06cc \u0634\u0631\u06a9\u062a hCaptcha \u0645\u06cc\u0644\u06cc\u0648\u0646 \u0647\u0627 \u06a9\u0627\u0631\u0628\u0631 \u0631\u0627 \u062f\u0631 \u0628\u06cc\u0646 \u062f\u0647 \u0647\u0627 \u0645\u06cc\u0644\u06cc\u0648\u0646 \u0648\u0628 \u0633\u0627\u06cc\u062a \u0628\u0631\u0627\u06cc \u0628\u0631\u062e\u06cc \u0627\u0632 \u0628\u0632\u0631\u06af\u062a\u0631\u06cc\u0646 \u0634\u0631\u06a9\u062a \u0647\u0627\u06cc \u06a9\u0631\u0647 \u0632\u0645\u06cc\u0646 \u0645\u062d\u0627\u0641\u0638\u062a \u0645\u06cc \u06a9\u0646\u062f. \u0644\u0637\u0641\u0627 \u0628\u0631\u0627\u06cc \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0628\u06cc\u0634\u062a\u0631 \u0628\u0627 \u0645\u0627 \u062a\u0645\u0627\u0633 \u0628\u06af\u06cc\u0631\u06cc\u062f \u06cc\u0627 \u0628\u0627\u0632\u062f\u06cc\u062f \u06a9\u0646\u06cc\u062f",
    "home-main-plans4": "\u0628\u0631\u0627\u06cc \u062c\u0632\u0626\u06cc\u0627\u062a \u0628\u06cc\u0634\u062a\u0631",
    "learn-more-hcaptcha": "\u062f\u0631\u0628\u0627\u0631\u0647 hCaptcha \u0628\u06cc\u0634\u062a\u0631 \u0628\u062f\u0627\u0646\u06cc\u062f",
    "page-acc-faq-1": "\u0633\u0648\u0627\u0644: \u0622\u06cc\u0627 hCaptcha \u0628\u062e\u0634 508 + WCAG 2.0 \u0633\u0627\u0632\u06af\u0627\u0631 \u0627\u0633\u062a\u061f \u067e\u0627\u0633\u062e: \u0645\u0627 \u0686\u0646\u06cc\u0646 \u0627\u0639\u062a\u0642\u0627\u062f \u062f\u0627\u0631\u06cc\u0645: \u06a9\u0644\u06cc\u0647 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 \u0628\u0627 \u0647\u0631 \u0646\u0648\u0639 \u0627\u062e\u062a\u0644\u0627\u0644 \u06a9\u0647 \u0642\u0627\u062f\u0631 \u0628\u0647 \u0645\u0631\u0648\u0631 \u0648\u0628 \u0647\u0633\u062a\u0646\u062f \u0648 \u0645\u062a\u0646 \u0631\u0627 \u062f\u0631 \u0641\u0631\u0645 \u0647\u0627 \u0648\u0627\u0631\u062f \u0645\u06cc \u06a9\u0646\u0646\u062f \u060c \u0645\u06cc \u062a\u0648\u0627\u0646\u0646\u062f \u067e\u0633 \u0627\u0632 \u062b\u0628\u062a \u0646\u0627\u0645 \u0628\u0647 \u0633\u0631\u0648\u06cc\u0633 \u0647\u0627\u06cc \u0645\u062d\u0627\u0641\u0638\u062a \u0634\u062f\u0647 \u062a\u0648\u0633\u0637 hCaptcha \u062f\u0633\u062a\u0631\u0633\u06cc \u067e\u06cc\u062f\u0627 \u06a9\u0646\u0646\u062f. \u0627\u0645\u0627 \u060c \u0627\u06cc\u0646 \u0645\u0634\u0627\u0648\u0631\u0647 \u062d\u0642\u0648\u0642\u06cc \u0646\u06cc\u0633\u062a: \u0634\u0645\u0627 \u0628\u0627\u06cc\u062f \u0627\u0631\u0632\u06cc\u0627\u0628\u06cc \u062e\u0648\u062f \u0631\u0627 \u0628\u0627 \u062f\u0631 \u0646\u0638\u0631 \u06af\u0631\u0641\u062a\u0646 \u0627\u062c\u0631\u0627\u06cc \u062e\u0627\u0635 \u062e\u0648\u062f \u0627\u0646\u062c\u0627\u0645 \u062f\u0647\u06cc\u062f \u062a\u0627 \u0627\u0637\u0645\u06cc\u0646\u0627\u0646 \u062d\u0627\u0635\u0644 \u0634\u0648\u062f \u06a9\u0647 \u0627\u06cc\u0646 \u0645\u0648\u0631\u062f \u0628\u0631\u0627\u06cc \u0627\u0633\u062a\u0642\u0631\u0627\u0631 \u0634\u0645\u0627\u0633\u062a.",
    "page-acc-faq-2": "\u0633\u0648\u0627\u0644: \u0622\u06cc\u0627 \u0634\u0645\u0627 \u0631\u0648\u06cc \u06af\u0632\u06cc\u0646\u0647 \u0647\u0627\u06cc \u062f\u0633\u062a\u0631\u0633\u06cc \u062f\u06cc\u06af\u0631\u06cc (a11y) \u0645\u0627\u0646\u0646\u062f \u0635\u062f\u0627 \u06a9\u0627\u0631 \u0645\u06cc \u06a9\u0646\u06cc\u062f\u061f \u067e\u0627\u0633\u062e: \u0642\u0628\u0644\u0627\u064b \u06af\u0632\u06cc\u0646\u0647 \u0647\u0627\u06cc \u0645\u062d\u0628\u0648\u0628 \u0645\u0627\u0646\u0646\u062f captchas \u0647\u0627\u06cc \u0635\u0648\u062a\u06cc \u0628\u0627 \u0628\u0633\u06cc\u0627\u0631\u06cc \u0627\u0632 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 a11y \u062a\u0628\u0639\u06cc\u0636 \u0642\u0627\u0626\u0644 \u0645\u06cc \u0634\u0648\u0646\u062f \u0648 \u0628\u0647 \u0631\u0627\u062d\u062a\u06cc \u062a\u0648\u0633\u0637 \u062a\u06a9\u0646\u06cc\u06a9 \u0647\u0627\u06cc \u0645\u062f\u0631\u0646 \u06cc\u0627\u062f\u06af\u06cc\u0631\u06cc \u0645\u0627\u0634\u06cc\u0646 \u0634\u06a9\u0633\u062a \u0645\u06cc \u062e\u0648\u0631\u0646\u062f. \u0627\u06cc\u0646 \u0627\u0645\u0631 \u0628\u0627\u0639\u062b \u0634\u062f\u0647 \u0627\u0633\u062a \u06a9\u0647 \u0686\u0627\u0644\u0634\u0647\u0627\u06cc \u0635\u0648\u062a\u06cc \u06a9\u0646\u0648\u0646\u06cc \u0628\u06cc\u0634\u062a\u0631 \u0648 \u0633\u062e\u062a \u062a\u0631 \u0634\u0648\u0646\u062f \u060c \u0645\u0639\u0631\u0641\u06cc \u0633\u0631 \u0648 \u0635\u062f\u0627 \u060c \u0632\u0645\u0627\u0646 \u0639\u062c\u06cc\u0628 \u060c \u062a\u0631\u06a9\u06cc\u0628 \u06a9\u0644\u0645\u0627\u062a \u063a\u06cc\u0631\u0645\u0639\u0645\u0648\u0644 \u0648 \u063a\u06cc\u0631\u0647 \u0628\u0631\u0627\u06cc \u0634\u06a9\u0633\u062a \u0645\u0647\u0627\u062c\u0645\u0627\u0646. \u0628\u0646\u0627\u0628\u0631\u0627\u06cc\u0646 \u060c \u062f\u0631 \u0645\u0642\u0627\u0628\u0644 \u0627\u062c\u062a\u0646\u0627\u0628 \u0627\u0632 \u0686\u0627\u0644\u0634 \u062f\u0631 \u06a9\u0644 \u060c \u06a9\u0645\u062a\u0631 \u0627\u0632 \u0627\u06cc\u0646 \u0631\u0648\u06cc\u06a9\u0631\u062f \u0645\u0634\u062a\u0627\u0642 \u0647\u0633\u062a\u06cc\u0645 \u060c \u0627\u0645\u0627 \u062f\u0631 \u0635\u0648\u0631\u062a \u062a\u0642\u0627\u0636\u0627\u06cc \u062c\u0627\u0645\u0639\u0647 a11y \u060c \u0622\u0646 \u0631\u0627 \u062f\u0631 \u0646\u0638\u0631 \u062e\u0648\u0627\u0647\u06cc\u0645 \u06af\u0631\u0641\u062a. \u0628\u0627 \u0627\u06cc\u0646 \u062d\u0627\u0644 \u060c \u0645\u0627 \u0628\u0647 Privacy Pass \u0628\u0631\u0627\u06cc \u0645\u0648\u0631\u062f \u0627\u0633\u062a\u0641\u0627\u062f\u0647 Accessibility \u0628\u0633\u06cc\u0627\u0631 \u0639\u0644\u0627\u0642\u0647 \u0645\u0646\u062f \u0647\u0633\u062a\u06cc\u0645. \u0645\u0627 \u0645\u0639\u062a\u0642\u062f\u06cc\u0645 \u062a\u0644\u0641\u06cc\u0642 \u0631\u0648\u06cc\u06a9\u0631\u062f a11y \u0641\u0639\u0644\u06cc \u0645\u0627 \u0628\u0627 \u0635\u062f\u0648\u0631 \u06af\u0630\u0631\u0646\u0627\u0645\u0647 \u062d\u0631\u06cc\u0645 \u062e\u0635\u0648\u0635\u06cc \u060c \u0628\u0647 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 a11y \u0627\u0645\u06a9\u0627\u0646 \u0645\u06cc \u062f\u0647\u062f \u0628\u0627 \u062e\u06cc\u0627\u0644 \u0631\u0627\u062d\u062a \u0648 \u0645\u0637\u0645\u0626\u0646 \u0628\u0627 \u0622\u06af\u0627\u0647\u06cc \u0627\u0632 \u0627\u06cc\u0646\u06a9\u0647 \u062a\u0631\u0627\u0641\u06cc\u06a9 \u0622\u0646\u0647\u0627 \u062e\u0635\u0648\u0635\u06cc \u062a\u0631 \u0627\u0633\u062a \u060c \u0645\u0631\u0648\u0631 \u06a9\u0646\u0646\u062f \u060c \u062f\u0631 \u062d\u0627\u0644\u06cc \u06a9\u0647 \u0633\u0648\u0621\u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u062a\u0648\u0633\u0637 \u0627\u067e\u0631\u0627\u062a\u0648\u0631\u0647\u0627\u06cc \u0631\u0628\u0627\u062a \u0631\u0627 \u0645\u062d\u062f\u0648\u062f \u0645\u06cc \u06a9\u0646\u0646\u062f \u060c \u06a9\u0647 \u0628\u0647 \u0646\u0627\u0686\u0627\u0631 \u062f\u0631 \u0635\u0648\u0631\u062a \u0645\u0648\u062c\u0648\u062f \u0628\u0648\u062f\u0646 \u06af\u0632\u06cc\u0646\u0647 \u0647\u0627\u06cc a11y \u0627\u062a\u0641\u0627\u0642 \u0645\u06cc \u0627\u0641\u062a\u062f. \u0645\u0627 \u0634\u0631\u06a9\u062a \u06a9\u0646\u0646\u062f\u06af\u0627\u0646 \u0641\u0639\u0627\u0644 \u062f\u0631 \u06af\u0631\u0648\u0647 \u06a9\u0627\u0631\u06cc IETF \u0647\u0633\u062a\u06cc\u0645 \u06a9\u0647 \u0627\u06cc\u0646 \u0641\u0646\u0627\u0648\u0631\u06cc \u062c\u062f\u06cc\u062f \u0631\u0627 \u0627\u0633\u062a\u0627\u0646\u062f\u0627\u0631\u062f \u0633\u0627\u0632\u06cc \u0645\u06cc \u06a9\u0646\u062f.",
    "page-acc-faq-3": "\u0633\u0624\u0627\u0644: \u062f\u0631\u0628\u0627\u0631\u0647 \u062d\u0631\u06cc\u0645 \u062e\u0635\u0648\u0635\u06cc \u0686\u06cc\u0633\u062a\u061f \u0622\u06cc\u0627 \u062b\u0628\u062a \u0646\u0627\u0645 \u062f\u0627\u062f\u0647 \u0647\u0627\u06cc \u0645\u0631\u0648\u0631\u06af\u0631 a11y \u0631\u0627 \u0628\u0647 \u0646\u0648\u0639\u06cc \u0646\u0634\u0627\u0646 \u0645\u06cc \u062f\u0647\u062f\u061f \u0628\u0627 \u0627\u06cc\u0645\u06cc\u0644 \u0686\u0647 \u0645\u06cc \u06a9\u0646\u06cc\u062f\u061f \u067e\u0627\u0633\u062e: hCaptcha \u0628\u0631\u0627\u06cc \u062d\u0641\u0638 \u062d\u0631\u06cc\u0645 \u062e\u0635\u0648\u0635\u06cc \u0627\u0632 \u0632\u0645\u06cc\u0646 \u0628\u0647 \u0628\u0627\u0644\u0627 \u0637\u0631\u0627\u062d\u06cc \u0634\u062f\u0647 \u0627\u0633\u062a. \u0627\u06cc\u0646 \u0628\u0633\u06cc\u0627\u0631 \u0645\u062a\u0641\u0627\u0648\u062a \u0627\u0632 \u06af\u0632\u06cc\u0646\u0647 \u0647\u0627\u06cc \u0633\u0646\u062a\u06cc \u0645\u0627\u0646\u0646\u062f reCAPTCHA \u0645\u062a\u0639\u0644\u0642 \u0628\u0647 \u0634\u0628\u06a9\u0647 \u0647\u0627\u06cc \u062a\u0628\u0644\u06cc\u063a\u0627\u062a\u06cc \u0627\u0633\u062a \u060c \u06a9\u0647 \u0627\u0646\u06af\u06cc\u0632\u0647 \u0627\u06cc \u0628\u0631\u0627\u06cc \u067e\u06cc\u06af\u06cc\u0631\u06cc \u0634\u0645\u0627 \u062f\u0631 \u0648\u0628 \u0648 \u067e\u06cc\u0648\u0646\u062f \u062f\u0627\u062f\u0646 \u0634\u0645\u0627 \u0628\u0627 \u0647\u0648\u06cc\u062a \u0648\u0627\u0642\u0639\u06cc \u062f\u0627\u0631\u0646\u062f. \u0645\u0627 \u0647\u0631\u06af\u0632 \u0627\u0632 \u0627\u06cc\u0645\u06cc\u0644 \u06cc\u0627 \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u062f\u0633\u062a\u06cc\u0627\u0628\u06cc \u0628\u0647 \u0645\u0646\u0638\u0648\u0631 \u062a\u0633\u0647\u06cc\u0644 \u062f\u0631 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 a11y \u0648 \u062c\u0644\u0648\u06af\u06cc\u0631\u06cc \u0627\u0632 \u0633\u0648\u0621\u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u060c \u0628\u0647 \u0645\u0646\u0638\u0648\u0631 \u062f\u06cc\u06af\u0631\u06cc \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0646\u0645\u06cc \u06a9\u0646\u06cc\u0645. \u062e\u0637 \u0645\u0634\u06cc \u0631\u0627\u0632\u062f\u0627\u0631\u06cc \u0645\u0627 \u062f\u0631 \u0645\u0648\u0631\u062f \u0646\u062d\u0648\u0647 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 \u062f\u0627\u062f\u0647 \u0647\u0627 \u062f\u0627\u0631\u0627\u06cc \u067e\u0627\u0633\u062e \u0647\u0627\u06cc \u062c\u0627\u0645\u0639 \u0648 \u0645\u0639\u062a\u0628\u0631\u06cc \u0627\u0633\u062a \u060c \u0627\u0645\u0627 \u067e\u0627\u0633\u062e \u06a9\u0648\u062a\u0627\u0647 \u0627\u06cc\u0646 \u0627\u0633\u062a \u06a9\u0647 \u0645\u0627 \u0647\u06cc\u0686 \u0639\u0644\u0627\u0642\u0647 \u0627\u06cc \u0646\u062f\u0627\u0631\u06cc\u0645 \u06a9\u0647 \u0634\u0645\u0627 \u0631\u0627 \u0628\u0647 \u0639\u0646\u0648\u0627\u0646 \u0634\u062e\u0635\u06cc \u0628\u0627 \u0633\u0627\u0628\u0642\u0647 \u0645\u0631\u0648\u0631 \u062e\u0648\u062f \u0645\u0631\u062a\u0628\u0637 \u06a9\u0646\u06cc\u0645. \u0645\u0627 \u0647\u0645\u0686\u0646\u06cc\u0646 \u062f\u0631 \u062d\u0627\u0644 \u062d\u0627\u0636\u0631 \u062f\u0631 \u062d\u0627\u0644 \u06a9\u0627\u0631 \u0628\u0631 \u0631\u0648\u06cc \u06cc\u06a9 \u0631\u0627\u0647 \u062d\u0644 \u0631\u0645\u0632\u0646\u06af\u0627\u0631\u06cc \u0647\u0633\u062a\u06cc\u0645 \u062a\u0627 \u0628\u0647 \u0633\u0631\u0639\u062a \u0622\u062f\u0631\u0633 \u0627\u06cc\u0645\u06cc\u0644 \u062e\u0648\u062f \u0631\u0627 \u062f\u0648\u0631 \u0628\u06cc\u0646\u062f\u0627\u0632\u06cc\u062f. \u062f\u0631 \u062d\u0627\u0644\u06cc \u06a9\u0647 \u0647\u0646\u0648\u0632 \u062a\u0648\u0627\u0646\u0627\u06cc\u06cc \u0645\u0627 \u0628\u0631\u0627\u06cc \u062c\u0644\u0648\u06af\u06cc\u0631\u06cc \u0627\u0632 \u0633\u0648\u0621\u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0631\u0627 \u062d\u0641\u0638 \u0645\u06cc \u06a9\u0646\u06cc\u0645 \u060c \u0627\u06cc\u0646 \u06a9\u0627\u0631 \u0628\u0627\u0639\u062b \u0645\u06cc \u0634\u0648\u062f \u06a9\u0647 \u06a9\u0627\u0631\u0646\u0627\u0645\u0647 \u06af\u0630\u0631\u0646\u0627\u0645\u0647 \u0645\u0627 \u0631\u0627 \u0646\u06cc\u0632 \u062d\u0641\u0638 \u06a9\u0646\u062f.",
    "page-acc-link-1": "\u0627\u06af\u0631 \u06a9\u0627\u0631\u0628\u0631 \u0642\u0627\u0628\u0644 \u062f\u0633\u062a\u0631\u0633\u06cc \u0647\u0633\u062a\u06cc\u062f \u060c \u0644\u0637\u0641\u0627\u064b \u0627\u0632 \u0627\u06cc\u0646\u062c\u0627 \u0648\u0627\u0631\u062f \u0634\u0648\u06cc\u062f",
    "page-acc-subhead": "\u0627\u0637\u0645\u06cc\u0646\u0627\u0646 \u0627\u0632 \u062f\u0633\u062a\u0631\u0633\u06cc \u06cc\u06a9\u067e\u0627\u0631\u0686\u0647 \u0628\u0631\u0627\u06cc \u0647\u0645\u0647 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 \u0636\u0645\u0646 \u062d\u0641\u0638 \u0627\u0645\u0646\u06cc\u062a",
    "page-acc-summary": "\u062e\u0644\u0627\u0635\u0647",
    "page-acc-summary-contents-1": "hCaptcha \u0628\u0647 \u06af\u0648\u0646\u0647 \u0627\u06cc \u0637\u0631\u0627\u062d\u06cc \u0634\u062f\u0647 \u0627\u0633\u062a \u06a9\u0647 \u0628\u0627 \u0645\u062a\u0645\u0627\u06cc\u0632 \u06a9\u0631\u062f\u0646 \u0622\u0646\u0647\u0627 \u0627\u0632 \u0645\u0631\u062f\u0645 \u060c \u0631\u0628\u0627\u062a \u0647\u0627 \u0631\u0627 \u0645\u062a\u0648\u0642\u0641 \u0645\u06cc \u06a9\u0646\u062f. \u062a\u0633\u062a \u0647\u0627\u06cc \u0628\u0635\u0631\u06cc \u0627\u0628\u0632\u0627\u0631\u06cc \u0645\u0646\u0627\u0633\u0628 \u0628\u0631\u0627\u06cc \u0627\u06cc\u0646 \u06a9\u0627\u0631 \u0647\u0633\u062a\u0646\u062f \u060c \u0627\u0645\u0627 \u0647\u0645\u0647 \u0646\u0645\u06cc \u062a\u0648\u0627\u0646\u0646\u062f \u06cc\u06a9 \u0686\u0627\u0644\u0634 \u0628\u0635\u0631\u06cc \u0631\u0627 \u062d\u0644 \u06a9\u0646\u0646\u062f. \u0628\u0647 \u0647\u0645\u06cc\u0646 \u062f\u0644\u06cc\u0644 \u0645\u0627 \u06cc\u06a9 \u06af\u0632\u06cc\u0646\u0647 \u062c\u0627\u06cc\u06af\u0632\u06cc\u0646 \u0633\u0627\u062f\u0647 \u0648 \u0628\u062f\u0648\u0646 \u062f\u0631\u062f \u0631\u0627 \u0627\u06cc\u062c\u0627\u062f \u06a9\u0631\u062f\u0647 \u0627\u06cc\u0645 \u062a\u0627 \u0646\u0627\u0634\u0631\u0627\u0646 \u0628\u0627 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 \u062e\u062f\u0645\u0627\u062a \u062e\u0648\u062f \u0628\u062a\u0648\u0627\u0646\u0646\u062f \u062f\u0633\u062a\u0631\u0633\u06cc \u0628\u0647 \u0647\u0645\u0647 \u0631\u0627 \u0628\u0627 \u0631\u0639\u0627\u06cc\u062a \u06a9\u0627\u0645\u0644 \u0628\u062e\u0634 508 \u0648 WCAG 2.0 \u062d\u0641\u0638 \u06a9\u0646\u0646\u062f.",
    "page-acc-summary-contents-2": "\u0646\u062d\u0648\u0647 \u06a9\u0627\u0631: \u0627\u0628\u062a\u062f\u0627 \u060c \u06a9\u0627\u0631\u0628\u0631 \u0642\u0627\u0628\u0644 \u062f\u0633\u062a\u0631\u0633\u06cc \u062f\u0631 \u0627\u06cc\u0646 URL \u062b\u0628\u062a \u0646\u0627\u0645 \u0645\u06cc \u06a9\u0646\u062f \u060c \u06a9\u0647 \u062f\u0631 \u0635\u0641\u062d\u0647 \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0648\u06cc\u062c\u062a hCaptcha \u067e\u06cc\u0648\u0646\u062f \u062f\u0627\u0631\u062f. \u0628\u0647 \u0622\u0646\u0647\u0627 \u06a9\u0648\u06a9\u06cc \u0631\u0645\u0632\u06af\u0630\u0627\u0631\u06cc \u0634\u062f\u0647 \u062f\u0627\u062f\u0647 \u0645\u06cc \u0634\u0648\u062f \u06a9\u0647 \u0645\u06cc \u062a\u0648\u0627\u0646\u062f \u0686\u0646\u062f\u06cc\u0646 \u0628\u0627\u0631 \u062f\u0631 \u0631\u0648\u0632 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0634\u0648\u062f \u060c \u0627\u0645\u0627 \u0628\u0627\u06cc\u062f \u0647\u0631 24 \u0633\u0627\u0639\u062a \u06cc\u06a9\u0628\u0627\u0631 \u0627\u0632 \u0637\u0631\u06cc\u0642 \u0648\u0631\u0648\u062f \u0628\u0647 \u0633\u0627\u06cc\u062a \u062a\u062c\u062f\u06cc\u062f \u0634\u0648\u062f. \u0648\u0642\u062a\u06cc \u06cc\u06a9 \u0686\u0627\u0644\u0634 \u0628\u0631\u0627\u06cc \u0647\u0631 \u06a9\u0627\u0631\u0628\u0631 \u0628\u0627 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 \u0633\u0631\u0648\u06cc\u0633 hCaptcha \u062f\u0631 \u0647\u0631 \u0633\u0627\u06cc\u062a\u06cc \u0627\u0631\u0627\u0626\u0647 \u0645\u06cc \u0634\u0648\u062f \u060c \u0628\u0647 \u0637\u0648\u0631 \u062e\u0648\u062f\u06a9\u0627\u0631 \u0639\u0628\u0648\u0631 \u0645\u06cc \u06a9\u0646\u0646\u062f.",
    "page-acc-title": "\u0627\u0637\u0644\u0627\u0639\u0627\u062a \u062f\u0633\u062a\u06cc\u0627\u0628\u06cc hCaptcha",
    "page-acc-tr-body": "\u0633\u0648\u0627\u0644: \u0645\u0646 \u0647\u0646\u0648\u0632 \u0647\u0645 \u0628\u0639\u062f \u0627\u0632 \u062a\u0646\u0638\u06cc\u0645 \u06a9\u0648\u06a9\u06cc \u0634\u0627\u0647\u062f \u06cc\u06a9 \u0686\u0627\u0644\u0634 \u0647\u0633\u062a\u0645. \u062f\u0644\u06cc\u0644\u0634 \u0686\u06cc \u0627\u0633\u062a\u061f \u067e\u0627\u0633\u062e: \u0627\u06cc\u0646 \u0628\u0647 \u0637\u0648\u0631 \u0645\u0639\u0645\u0648\u0644 \u0628\u0647 \u062f\u0644\u06cc\u0644 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 \u0645\u0633\u062f\u0648\u062f \u06a9\u0646\u0646\u062f\u0647 \u062a\u0628\u0644\u06cc\u063a\u0627\u062a\u06cc \u062a\u0647\u0627\u062c\u0645\u06cc \u06cc\u0627 \u0627\u0641\u0632\u0648\u0646\u0647 \u0636\u062f \u06a9\u0648\u06a9\u06cc \u06cc\u0627 \u062a\u0646\u0638\u06cc\u0645\u0627\u062a\u06cc \u0627\u0633\u062a \u06a9\u0647 \u06a9\u0648\u06a9\u06cc \u0647\u0627\u06cc \u0633\u0637\u062d \u0633\u0627\u06cc\u062a \u0631\u0627 \u0645\u0633\u062f\u0648\u062f \u0645\u06cc \u06a9\u0646\u062f \u060c \u062f\u0631 \u0627\u06cc\u0646 \u062d\u0627\u0644\u062a \u06cc\u06a9 \u06a9\u0648\u06a9\u06cc \u0628\u0631\u0627\u06cc hcaptcha.com \u06a9\u0647 \u062a\u0648\u0633\u0637 hCaptcha JavaScript \u062f\u0631 \u06cc\u06a9 \u0633\u0627\u06cc\u062a \u062f\u06cc\u06af\u0631 \u062a\u0646\u0638\u06cc\u0645 \u0648 \u0628\u0631\u0631\u0633\u06cc \u0634\u062f\u0647 \u0627\u0633\u062a. \u0645\u0627\u0646\u0646\u062f \u06a9\u0633\u06cc \u06a9\u0647 \u0628\u0627\u0632\u062f\u06cc\u062f \u0645\u06cc \u06a9\u0646\u06cc\u062f \u06a9\u0648\u06a9\u06cc \u0647\u0627\u06cc \u062f\u0633\u062a\u0631\u0633\u06cc hCaptcha \u0628\u0627 \u06a9\u0644\u06cc\u0647 \u0645\u0631\u0648\u0631\u06af\u0631\u0647\u0627\u06cc \u0645\u062d\u0628\u0648\u0628 \u0648 \u0645\u0633\u062f\u0648\u062f \u06a9\u0646\u0646\u062f\u0647 \u0647\u0627\u06cc \u062a\u0628\u0644\u06cc\u063a\u0627\u062a\u06cc \u0628\u0627 \u062a\u0646\u0638\u06cc\u0645\u0627\u062a \u0627\u0633\u062a\u0627\u0646\u062f\u0627\u0631\u062f \u062e\u0648\u062f \u06a9\u0627\u0631 \u0645\u06cc \u06a9\u0646\u0646\u062f \u060c \u0628\u0646\u0627\u0628\u0631\u0627\u06cc\u0646 \u0628\u0647 \u0637\u0648\u0631 \u0645\u0639\u0645\u0648\u0644 \u062e\u0631\u0627\u0628\u06cc \u0647\u0627 \u0628\u0647 \u062f\u0644\u06cc\u0644 \u0627\u0633\u06a9\u0631\u06cc\u067e\u062a \u0647\u0627\u06cc \u0636\u062f \u0636\u062f adblock \u06cc\u0627 \u0642\u0648\u0627\u0646\u06cc\u0646 \u0645\u0634\u0627\u0628\u0647\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0633\u0627\u06cc\u062a \u0647\u0627\u06cc \u062e\u0627\u0635 \u0631\u0627 \u0647\u062f\u0641 \u0642\u0631\u0627\u0631 \u0645\u06cc \u062f\u0647\u062f. \u0631\u0627\u0647 \u062d\u0644 \u0647\u0627: 1. \u06a9\u0648\u06a9\u06cc \u0647\u0627\u06cc \u0644\u06cc\u0633\u062a \u0633\u0641\u06cc\u062f hcaptcha.com \u0648 * .hcaptcha.com \u062f\u0631 \u0645\u0633\u062f\u0648\u062f \u06a9\u0646\u0646\u062f\u0647 \u062a\u0628\u0644\u06cc\u063a\u0627\u062a \u06cc\u0627 \u067e\u0633\u0648\u0646\u062f \u0627\u0645\u0646\u06cc\u062a\u06cc \u0645\u0631\u0648\u0631\u06af\u0631 \u0634\u0645\u0627. 2. \u0627\u06af\u0631 \u0627\u0632 \u0645\u0631\u0648\u0631\u06af\u0631 Brave \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0645\u06cc \u06a9\u0646\u06cc\u062f \u060c \u06a9\u0647 \u0628\u0647 \u0646\u0638\u0631 \u0646\u0645\u06cc \u0631\u0633\u062f (\u0627\u0632 \u0622\u0648\u0631\u06cc\u0644 2020) \u0647\u0631 \u0646\u0648\u0639 \u0644\u06cc\u0633\u062a \u0633\u0641\u06cc\u062f \u0644\u06cc\u0633\u062a \u06a9\u0648\u06a9\u06cc \u0647\u0627 \u0631\u0627 \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u062f \u060c \u0628\u0647 Preferences -> Shields -> Cookies \u0628\u0631\u0648\u06cc\u062f \u0648 All All All Cookies \u0631\u0627 \u0627\u0646\u062a\u062e\u0627\u0628 \u06a9\u0646\u06cc\u062f. 3. \u0627\u06af\u0631 \u0627\u0632 \u062c\u062f\u06cc\u062f\u062a\u0631\u06cc\u0646 \u0646\u0633\u062e\u0647 Safari \u062f\u0631 OS OS 10.15 \u06cc\u0627 iOS 13. 4 \u06a9\u0647 \u0627\u062e\u06cc\u0631\u0627\u064b \u0645\u0646\u062a\u0634\u0631 \u0634\u062f\u0647 \u0627\u0633\u062a \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0645\u06cc \u06a9\u0646\u06cc\u062f \u060c \u0627\u067e\u0644 \u0628\u0647 \u062a\u0627\u0632\u06af\u06cc \u0631\u0641\u062a\u0627\u0631 Safari \u0645\u0631\u0628\u0648\u0637 \u0628\u0647 \u06a9\u0648\u06a9\u06cc \u0647\u0627\u06cc \u0634\u062e\u0635 \u062b\u0627\u0644\u062b \u0631\u0627 \u062a\u063a\u06cc\u06cc\u0631 \u062f\u0627\u062f\u0647 \u0627\u0633\u062a \u0648 \u0647\u0645\u0647 \u0622\u0646\u0647\u0627 \u0631\u0627 \u0628\u0647 \u0637\u0648\u0631 \u067e\u06cc\u0634 \u0641\u0631\u0636 \u0645\u0633\u062f\u0648\u062f \u06a9\u0631\u062f\u0647 \u0627\u0633\u062a. \u0645\u0627 \u062f\u0631 \u062d\u0627\u0644 \u067e\u06cc\u0627\u062f\u0647 \u0633\u0627\u0632\u06cc \u0631\u0627\u0647 \u062d\u0644 \u0647\u0633\u062a\u06cc\u0645 \u060c \u0627\u0645\u0627 \u062f\u0631 \u0627\u06cc\u0646 \u0636\u0645\u0646 \u0644\u0637\u0641\u0627\u064b \u0627\u0632 \u062a\u0646\u0638\u06cc\u0645\u0627\u062a \u0628\u0631\u06af\u0632\u06cc\u062f\u0647 \u0633\u0627\u0641\u0627\u0631\u06cc \u060c \u0628\u062e\u0634 \u062d\u0641\u0638 \u062d\u0631\u06cc\u0645 \u062e\u0635\u0648\u0635\u06cc \u0648 \u0639\u06cc\u0628 \u06cc\u0627\u0628\u06cc \u0631\u062f\u06cc\u0627\u0628\u06cc \u0648\u0628 \u0633\u0627\u06cc\u062a \u062f\u06cc\u062f\u0646 \u06a9\u0646\u06cc\u062f: \u0627\u0632 \u0631\u062f\u06cc\u0627\u0628\u06cc \u0648\u0628 \u0633\u0627\u06cc\u062a \u062c\u0644\u0648\u06af\u06cc\u0631\u06cc \u06a9\u0646\u06cc\u062f \u062a\u0627 \u0639\u0645\u0644\u06a9\u0631\u062f \u06a9\u0648\u06a9\u06cc \u062f\u0631 \u062f\u0633\u062a\u0631\u0633 \u0647\u0645\u0627\u0646\u0637\u0648\u0631 \u06a9\u0647 \u067e\u06cc\u0634 \u0628\u06cc\u0646\u06cc \u0634\u062f\u0647 \u0627\u0633\u062a \u0641\u0639\u0627\u0644 \u0634\u0648\u062f. \u0633\u0648\u0627\u0644: \u0645\u0646 \u0627\u0632 \u0686\u0646\u062f\u06cc\u0646 \u062f\u0633\u062a\u06af\u0627\u0647 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0645\u06cc \u06a9\u0646\u0645. \u0622\u06cc\u0627 \u0628\u0627\u06cc\u062f \u0686\u0646\u062f\u06cc\u0646 \u0628\u0627\u0631 \u062b\u0628\u062a \u0646\u0627\u0645 \u06a9\u0646\u0645\u061f \u067e\u0627\u0633\u062e: \u062e\u06cc\u0631 \u060c \u0644\u0637\u0641\u0627\u064b \u0631\u0648\u06cc \u0647\u0645\u0627\u0646 \u067e\u06cc\u0648\u0646\u062f \u0648\u0631\u0648\u062f \u0628\u0647 \u0627\u06cc\u0645\u06cc\u0644 \u06a9\u0647 \u0628\u0631\u0627\u06cc \u062a\u0646\u0638\u06cc\u0645 \u06a9\u0648\u06a9\u06cc \u062f\u0631 \u0647\u0631 \u062f\u0633\u062a\u06af\u0627\u0647 \u0645\u0648\u0631\u062f \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0628\u0631\u0627\u06cc \u0634\u0645\u0627 \u0627\u0631\u0633\u0627\u0644 \u0634\u062f\u0647 \u060c \u06a9\u0644\u06cc\u06a9 \u06a9\u0646\u06cc\u062f.",
    "page-acc-tr-title": "\u0628\u0631\u0627\u06cc \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 \u0642\u0627\u0628\u0644 \u062f\u0633\u062a\u0631\u0633\u06cc: \u067e\u0631\u0633\u0634 \u0648 \u067e\u0627\u0633\u062e \u0648 \u0631\u0627\u0647\u0646\u0645\u0627\u06cc \u0639\u06cc\u0628 \u06cc\u0627\u0628\u06cc",
    "priv-hc": "\u0645\u062d\u0631\u0645\u0627\u0646\u0647 \u0645\u062a\u0645\u0631\u06a9\u0632 \u0634\u062f",
    "priv-hc-text": "\u0634\u0628\u06a9\u0647 \u0647\u0627\u06cc \u062a\u0628\u0644\u06cc\u063a\u0627\u062a\u06cc \u0634\u0645\u0627 \u0631\u0627 \u0628\u0647 \u0639\u0646\u0648\u0627\u0646 \u0645\u062d\u0635\u0648\u0644 \u062e\u0648\u062f \u0645\u06cc \u0628\u06cc\u0646\u0646\u062f. \u0645\u0627 \u0646\u0645\u06cc \u06a9\u0646\u06cc\u0645 \u0645\u0627 \u0627\u0647\u0645\u06cc\u062a\u06cc \u0646\u0645\u06cc \u062f\u0647\u06cc\u0645 \u06a9\u0647 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 \u0686\u0647 \u06a9\u0633\u0627\u0646\u06cc \u0647\u0633\u062a\u0646\u062f \u060c \u0641\u0642\u0637 \u0686\u0642\u062f\u0631 \u0627\u062d\u062a\u0645\u0627\u0644 \u062f\u0627\u0631\u0646\u062f \u0622\u0646\u0647\u0627 \u0628\u062f\u062e\u0648\u0627\u0647 \u0628\u0627\u0634\u0646\u062f. \u0645\u0627 \u0627\u0632 \u0627\u0628\u062a\u06a9\u0627\u0631\u0627\u062a\u06cc \u0645\u0627\u0646\u0646\u062f \u06af\u0630\u0631\u06af\u0627\u0647 \u062d\u0631\u06cc\u0645 \u062e\u0635\u0648\u0635\u06cc \u067e\u0634\u062a\u06cc\u0628\u0627\u0646\u06cc \u0645\u06cc \u06a9\u0646\u06cc\u0645 \u060c \u0648 \u0628\u0647 \u0633\u062e\u062a\u06cc \u06a9\u0627\u0631 \u0645\u06cc \u06a9\u0646\u06cc\u0645 \u062a\u0627 \u062d\u0641\u0638 \u062d\u0631\u06cc\u0645 \u062e\u0635\u0648\u0635\u06cc \u0648 \u0627\u0645\u0646\u06cc\u062a \u0628\u0631\u0642\u0631\u0627\u0631 \u0634\u0648\u062f.",
    "sec-hc": "\u0627\u0645\u0646\u06cc\u062a \u0627\u0648\u0644",
    "sec-hc-text": "hCaptcha \u0628\u0627 \u0645\u062d\u0648\u0631\u06cc\u062a \u0646\u0627\u0634\u0631 \u0627\u0633\u062a. \u0645\u0627 \u062f\u0631 \u062a\u0644\u0627\u0634\u06cc\u0645 \u062f\u0627\u062f\u0647 \u0647\u0627\u06cc \u0648\u0627\u0642\u0639\u06cc \u0628\u0647 \u0633\u0627\u06cc\u062a \u0647\u0627 \u0628\u062f\u0647\u06cc\u0645 \u0648 \u0628\u0647 \u0622\u0646\u0647\u0627 \u0627\u062c\u0627\u0632\u0647 \u062f\u0647\u06cc\u0645 \u06a9\u0647 \u0686\u06af\u0648\u0646\u0647 \u0627\u0632 \u0622\u0646 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u06a9\u0646\u0646\u062f. \u062f\u0642\u062a \u0628\u0647 \u0646\u0641\u0639 \u0634\u0645\u0627 \u0648 \u0645\u0627\u0633\u062a. \u0633\u0627\u06cc\u0631 \u06a9\u067e\u0686\u0631\u0647\u0627 \u062a\u0648\u0633\u0637 \u0634\u0631\u06a9\u062a\u0647\u0627\u06cc \u062a\u0628\u0644\u06cc\u063a\u0627\u062a\u06cc \u062a\u0647\u06cc\u0647 \u0645\u06cc \u0634\u0648\u0646\u062f. \u0647\u0631 \u0631\u0628\u0627\u062a \u06a9\u0647 \u0645\u0627\u0646\u0639 \u0622\u0646 \u0645\u06cc \u0634\u0648\u062f \u0645\u06cc \u062a\u0648\u0627\u0646\u062f \u062f\u0631\u0622\u0645\u062f \u062e\u0648\u062f \u0631\u0627 \u06a9\u0627\u0647\u0634 \u062f\u0647\u062f.",
    "what-is-hcaptcha": "hCaptcha \u0686\u06cc\u0633\u062a\u061f",
    "what-is-hcaptcha-text": "hCaptcha \u0628\u0627 \u067e\u0631\u0633\u06cc\u062f\u0646 \u0633\u0624\u0627\u0644\u0627\u062a \u0633\u0627\u062f\u0647 \u0627\u06cc \u06a9\u0647 \u0628\u0631\u0627\u06cc \u0627\u0646\u0633\u0627\u0646 \u0622\u0633\u0627\u0646 \u0627\u0633\u062a \u0648 \u0628\u0631\u0627\u06cc \u0645\u0627\u0634\u06cc\u0646 \u0647\u0627 \u062f\u0634\u0648\u0627\u0631 \u0627\u0633\u062a \u060c \u0628\u0647 \u0633\u0631\u0648\u06cc\u0633 \u0647\u0627\u06cc \u0648\u0628 \u0645\u0648\u0631\u062f \u0639\u0644\u0627\u0642\u0647 \u0634\u0645\u0627 \u06a9\u0645\u06a9 \u0645\u06cc \u06a9\u0646\u062f \u062a\u0627 \u0627\u0632 \u0631\u0628\u0627\u062a \u0647\u0627 \u060c \u0627\u0633\u067e\u0645 \u0647\u0627 \u0648 \u0633\u0648\u0621 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0647\u0627 \u062e\u0648\u062f\u062f\u0627\u0631\u06cc \u06a9\u0646\u0646\u062f.",
    "why-hc": "\u0686\u0631\u0627 hCaptcha\u061f",
    "why-hc-text": "\u0647\u0631 \u0628\u0627\u0631 \u06a9\u0647 \u0628\u0647 \u06cc\u06a9 \u0686\u0627\u0644\u0634 \u067e\u0627\u0633\u062e \u0645\u06cc \u062f\u0647\u06cc\u062f \u060c \u0627\u0632 \u0645\u062f\u0644 \u0647\u0627\u06cc \u067e\u06cc\u0634\u0631\u0641\u062a\u0647 \u0628\u0631\u0627\u06cc \u062a\u0634\u062e\u06cc\u0635 \u0627\u06cc\u0646\u06a9\u0647 \u0634\u0645\u0627 \u0627\u0646\u0633\u0627\u0646 \u0647\u0633\u062a\u06cc\u062f \u06cc\u0627 \u06cc\u06a9 \u0645\u0627\u0634\u06cc\u0646 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0645\u06cc \u06a9\u0646\u06cc\u062f. \u0648\u0628 \u0633\u0627\u06cc\u062a\u06cc \u06a9\u0647 \u0628\u0627\u0632\u062f\u06cc\u062f \u06a9\u0631\u062f\u0647 \u0627\u06cc\u062f \u0627\u0632 \u0627\u06cc\u0646 \u0646\u062a\u06cc\u062c\u0647 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0645\u06cc \u06a9\u0646\u062f \u062a\u0627 \u0628\u0627 \u0646\u06af\u0647 \u062f\u0627\u0634\u062a\u0646 \u0631\u0628\u0627\u062a\u0647\u0627 \u0628\u0647 \u0628\u0647\u0628\u0648\u062f \u062a\u062c\u0631\u0628\u0647 \u0634\u0645\u0627 \u062f\u0631 \u0633\u0627\u06cc\u062a \u0622\u0646\u0647\u0627 \u06a9\u0645\u06a9 \u06a9\u0646\u062f."
}