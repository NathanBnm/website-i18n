{
    "acc-opt": "\u062e\u064a\u0627\u0631\u0627\u062a \u0627\u0644\u062f\u062e\u0648\u0644",
    "acc-opt-but": "\u062a\u0639\u0631\u0641 \u0639\u0644\u0649 \u0627\u0644\u0645\u0632\u064a\u062f \u062d\u0648\u0644 \u062e\u064a\u0627\u0631\u0627\u062a \u0625\u0645\u0643\u0627\u0646\u064a\u0629 \u0627\u0644\u0648\u0635\u0648\u0644",
    "acc-opt-text": "\u0647\u0644 \u0623\u0646\u062a \u0636\u0639\u064a\u0641 \u0627\u0644\u0628\u0635\u0631 \u0623\u0648 \u063a\u064a\u0631 \u0642\u0627\u062f\u0631 \u0639\u0644\u0649 \u0625\u0643\u0645\u0627\u0644 hCaptchas\u061f",
    "accessibility-link": "\u0625\u0645\u0643\u0627\u0646\u064a\u0629 \u0627\u0644\u0648\u0635\u0648\u0644",
    "appreciate-in": "\u0646\u062d\u0646 \u0646\u0642\u062f\u0631 \u0645\u0633\u0627\u0647\u0645\u062a\u0643. \u0623\u062e\u0628\u0631\u0646\u0627 \u0628\u0623\u064a \u0645\u0634\u0627\u0643\u0644 \u0648\u0633\u0646\u0642\u0648\u0645 \u0628\u062d\u0644\u0647\u0627 \u0628\u0633\u0631\u0639\u0629.",
    "fb-but": "\u0623\u0631\u0633\u0644 \u0644\u0646\u0627 \u0631\u062f\u0648\u062f \u0627\u0644\u0641\u0639\u0644",
    "feedback-text": "\u0642\u062f\u0651\u0645 \u062a\u0639\u0644\u064a\u0642\u0627\u062a \u0639\u0644\u0649 hCaptcha \u0627\u0644\u0630\u064a \u0631\u0623\u064a\u062a\u0647 \u0644\u0644\u062a\u0648",
    "home-ent-1": "\u0647\u0644 \u062a\u0628\u062d\u062b \u0639\u0646 \u0639\u0631\u0648\u0636 \u0623\u0645\u0646 \u0627\u0644\u0645\u0624\u0633\u0633\u0627\u062a \u0644\u062f\u064a\u0646\u0627\u061f",
    "home-ent-2": "\u0642\u0645 \u0628\u0632\u064a\u0627\u0631\u0629 \u0645\u0648\u0642\u0639\u0646\u0627 \u0627\u0644\u062c\u062f\u064a\u062f \u0627\u0644\u0645\u062e\u0635\u0635: BotStop.com",
    "home-intro-body": "\u0647\u0644 \u062a\u0633\u062a\u062e\u062f\u0645 \u0643\u0644\u0645\u0629 \u0627\u0644\u062a\u062d\u0642\u0642 \u0644\u0625\u0628\u0639\u0627\u062f \u0628\u0631\u0627\u0645\u062c \u0627\u0644\u0631\u0648\u0628\u0648\u062a\u061f \u062a\u062d\u0645\u064a hCaptcha \u062e\u0635\u0648\u0635\u064a\u0629 \u0627\u0644\u0645\u0633\u062a\u062e\u062f\u0645 \u060c \u0648\u062a\u0643\u0627\u0641\u0626 \u0645\u0648\u0627\u0642\u0639 \u0627\u0644\u0648\u064a\u0628 \u060c \u0648\u062a\u0633\u0627\u0639\u062f \u0627\u0644\u0634\u0631\u0643\u0627\u062a \u0639\u0644\u0649 \u062a\u0635\u0646\u064a\u0641 \u0628\u064a\u0627\u0646\u0627\u062a\u0647\u0627. \u0625\u0646\u0647 \u0628\u062f\u064a\u0644 \u0639\u0646 reCAPTCHA: \u064a\u0645\u0643\u0646\u0643 \u0627\u0644\u062a\u0628\u062f\u064a\u0644 \u062e\u0644\u0627\u0644 \u062f\u0642\u0627\u0626\u0642.",
    "home-intro-head": "\u0623\u0648\u0642\u0641 \u0627\u0644\u0645\u0632\u064a\u062f \u0645\u0646 \u0628\u0631\u0627\u0645\u062c \u0627\u0644\u062a\u062a\u0628\u064f\u0651\u0639. \u0627\u0628\u062f\u0623 \u0641\u064a \u062d\u0645\u0627\u064a\u0629 \u062e\u0635\u0648\u0635\u064a\u0629 \u0627\u0644\u0645\u0633\u062a\u062e\u062f\u0645.",
    "home-main-1": "\u0646\u0634\u0631. \u0622\u0645\u0646. \u0628\u0633\u0631\u0639\u0629. \u0648\u0646\u062d\u0646 \u0646\u062f\u0641\u0639 \u0644\u0643 \u0644\u0627\u0633\u062a\u062e\u062f\u0627\u0645\u0647\u0627.",
    "home-main-2": "\u062a\u0631\u0643\u0632 \u0627\u0644\u062e\u0635\u0648\u0635\u064a\u0629",
    "home-main-2c": "\u062a\u0631\u0649 \u0634\u0628\u0643\u0627\u062a \u0627\u0644\u0625\u0639\u0644\u0627\u0646\u0627\u062a \u0632\u0648\u0627\u0631\u0643 \u0643\u0645\u0646\u062a\u062c. \u0628\u0627\u0644\u0646\u0633\u0628\u0629 \u0644\u0640 hCaptcha \u060c \u0627\u0644\u0645\u0646\u062a\u062c \u0647\u0648 \u0627\u0644\u0639\u0645\u0644 \u0627\u0644\u0630\u064a \u064a\u0642\u062f\u0645\u0648\u0646\u0647. \u0646\u062d\u0646 \u0644\u0627 \u0646\u0647\u062a\u0645 \u0628\u0645\u0646 \u064a\u0643\u0648\u0646 \u0627\u0644\u0645\u0633\u062a\u062e\u062f\u0645\u0648\u0646 \u0644\u062f\u064a\u0643 \u060c \u0641\u0642\u0637 \u0645\u062f\u0649 \u0627\u062d\u062a\u0645\u0627\u0644\u064a\u0629 \u0623\u0646 \u064a\u0643\u0648\u0646\u0648\u0627 \u0636\u0627\u0631\u064a\u0646. \u0646\u062d\u0646 \u0646\u062f\u0639\u0645 \u0645\u0628\u0627\u062f\u0631\u0627\u062a \u0645\u062b\u0644 Privacy Pass \u060c \u0648\u0646\u0639\u0645\u0644 \u0628\u062c\u062f \u0644\u062a\u062d\u0642\u064a\u0642 \u0627\u0644\u062a\u0648\u0627\u0632\u0646 \u0628\u064a\u0646 \u0627\u0644\u062e\u0635\u0648\u0635\u064a\u0629 \u0648\u0627\u0644\u0623\u0645\u0627\u0646.",
    "home-main-3": "\u0627\u0644\u0623\u0645\u0646 \u0623\u0648\u0644\u0627\u064b",
    "home-main-3c": "\u0627\u0644\u062f\u0642\u0629 \u0641\u064a \u0645\u0635\u0644\u062d\u062a\u0643\u0645 \u0648\u0645\u0635\u0644\u062d\u062a\u0646\u0627. \u064a\u0631\u0643\u0632 hCaptcha \u0639\u0644\u0649 \u0627\u0644\u0646\u0627\u0634\u0631. \u0646\u062d\u0646 \u0646\u0639\u0645\u0644 \u0639\u0644\u0649 \u0625\u0639\u0637\u0627\u0626\u0643 \u0628\u064a\u0627\u0646\u0627\u062a \u062d\u0642\u064a\u0642\u064a\u0629 \u0648\u0646\u062a\u064a\u062d \u0644\u0643 \u062a\u062d\u062f\u064a\u062f \u0643\u064a\u0641\u064a\u0629 \u0627\u0633\u062a\u062e\u062f\u0627\u0645\u0647\u0627. \u064a\u062a\u0645 \u062a\u0648\u0641\u064a\u0631 captchas \u0623\u062e\u0631\u0649 \u0645\u0646 \u0642\u0628\u0644",
    "home-main-3c2": "\u0634\u0631\u0643\u0627\u062a \u0627\u0644\u0625\u0639\u0644\u0627\u0646. \u0648\u0647\u0630\u0627 \u064a\u0639\u0646\u064a \u0623\u0646 \u0643\u0644 \u0631\u0648\u0628\u0648\u062a \u064a\u062d\u0638\u0631\u0647 \u0642\u062f \u064a\u0642\u0644\u0644 \u0645\u0646 \u0625\u064a\u0631\u0627\u062f\u0627\u062a\u0647.",
    "home-main-4": "\u062a\u062c\u0631\u0628\u0629 \u0645\u0633\u062a\u062e\u062f\u0645 \u0623\u0641\u0636\u0644",
    "home-main-4c": "\u062a\u0633\u062a\u062e\u062f\u0645 hCaptcha \u0646\u0645\u0627\u0630\u062c \u0627\u0644\u062a\u0639\u0644\u0645 \u0627\u0644\u0622\u0644\u064a \u0627\u0644\u0645\u062a\u0637\u0648\u0631\u0629 \u0644\u062a\u062d\u062f\u064a\u062f \u0627\u0644\u0625\u0646\u0633\u0627\u0646\u064a\u0629. \u064a\u062a\u064a\u062d \u0644\u0646\u0627 \u0630\u0644\u0643 \u0637\u0644\u0628 \u0645\u0647\u0627\u0645 \u0623\u0642\u0644 \u0625\u0632\u0639\u0627\u062c\u064b\u0627 \u0645\u0646 \u0627\u0644\u0645\u0633\u062a\u062e\u062f\u0645\u064a\u0646 \u0648\u0627\u0633\u062a\u063a\u0631\u0627\u0642 \u0648\u0642\u062a \u0623\u0642\u0644 \u0641\u064a \u0627\u0644\u0642\u064a\u0627\u0645 \u0628\u0630\u0644\u0643 \u060c \u0645\u0639 \u0627\u0644\u0627\u0633\u062a\u0645\u0631\u0627\u0631 \u0641\u064a \u0625\u0646\u062a\u0627\u062c \u0628\u064a\u0627\u0646\u0627\u062a \u0645\u0641\u064a\u062f\u0629 \u0645\u0635\u0646\u0641\u0629 \u0644\u062a\u0635\u0646\u064a\u0641 \u0627\u0644\u0639\u0645\u0644\u0627\u0621.",
    "home-main-plans": "hCaptcha \u0645\u062c\u0627\u0646\u064a \u0644\u0644\u0627\u0633\u062a\u062e\u062f\u0627\u0645 \u0644\u0644\u0646\u0627\u0634\u0631\u064a\u0646 \u0645\u0646 \u0623\u064a \u062d\u062c\u0645. \u0648\u0645\u0639 \u0630\u0644\u0643 \u060c \u0628\u0627\u0644\u0646\u0633\u0628\u0629 \u0644\u0644\u0634\u0631\u0643\u0627\u062a \u0648\u0627\u0644\u0645\u0646\u0635\u0627\u062a \u0627\u0644\u062a\u064a \u062a\u062d\u062a\u0627\u062c \u0625\u0644\u0649 \u0645\u0632\u064a\u062f \u0645\u0646 \u0627\u0644\u062a\u062d\u0643\u0645 \u060c \u062a\u062a\u0648\u0641\u0631 \u062d\u0644\u0648\u0644 \u0627\u0644\u0645\u0624\u0633\u0633\u0629 \u0644\u062f\u064a\u0646\u0627.",
    "home-main-plans2": "\u062a\u062d\u0645\u064a \u062d\u0644\u0648\u0644 hCaptcha \u0644\u0644\u0645\u0624\u0633\u0633\u0627\u062a \u0627\u0644\u0645\u0644\u0627\u064a\u064a\u0646 \u0645\u0646 \u0627\u0644\u0645\u0633\u062a\u062e\u062f\u0645\u064a\u0646 \u0639\u0628\u0631 \u0639\u0634\u0631\u0627\u062a \u0627\u0644\u0645\u0644\u0627\u064a\u064a\u0646 \u0645\u0646 \u0645\u0648\u0627\u0642\u0639 \u0627\u0644\u0648\u064a\u0628 \u0644\u0628\u0639\u0636 \u0623\u0643\u0628\u0631 \u0627\u0644\u0634\u0631\u0643\u0627\u062a \u0641\u064a \u0627\u0644\u0639\u0627\u0644\u0645. \u064a\u0631\u062c\u0649 \u0627\u0644\u0627\u062a\u0635\u0627\u0644 \u0628\u0646\u0627 \u0644\u0645\u0632\u064a\u062f \u0645\u0646 \u0627\u0644\u0645\u0639\u0644\u0648\u0645\u0627\u062a \u060c \u0623\u0648 \u0632\u064a\u0627\u0631\u0629",
    "home-main-plans3": "\u062a\u062d\u0645\u064a \u062d\u0644\u0648\u0644 hCaptcha \u0644\u0644\u0645\u0624\u0633\u0633\u0627\u062a \u0627\u0644\u0645\u0644\u0627\u064a\u064a\u0646 \u0645\u0646 \u0627\u0644\u0645\u0633\u062a\u062e\u062f\u0645\u064a\u0646 \u0639\u0628\u0631 \u0639\u0634\u0631\u0627\u062a \u0627\u0644\u0645\u0644\u0627\u064a\u064a\u0646 \u0645\u0646 \u0645\u0648\u0627\u0642\u0639 \u0627\u0644\u0648\u064a\u0628 \u0644\u0628\u0639\u0636 \u0623\u0643\u0628\u0631 \u0627\u0644\u0634\u0631\u0643\u0627\u062a \u0641\u064a \u0627\u0644\u0639\u0627\u0644\u0645. \u064a\u0631\u062c\u0649 \u0627\u0644\u0627\u062a\u0635\u0627\u0644 \u0628\u0646\u0627 \u0644\u0645\u0632\u064a\u062f \u0645\u0646 \u0627\u0644\u0645\u0639\u0644\u0648\u0645\u0627\u062a \u060c \u0623\u0648 \u0632\u064a\u0627\u0631\u0629",
    "home-main-plans4": "\u0644\u0644\u062a\u0641\u0627\u0635\u064a\u0644.",
    "learn-more-hcaptcha": "\u062a\u0639\u0631\u0641 \u0639\u0644\u0649 \u0627\u0644\u0645\u0632\u064a\u062f \u062d\u0648\u0644 hCaptcha",
    "page-acc-faq-1": "\u0627\u0644\u0633\u0624\u0627\u0644: \u0647\u0644 \u064a\u062a\u0648\u0627\u0641\u0642 \u0627\u0644\u0642\u0633\u0645 hCaptcha 508 + WCAG 2.0\u061f \u0627\u0644\u0625\u062c\u0627\u0628\u0629: \u0646\u062d\u0646 \u0646\u0639\u062a\u0642\u062f \u0630\u0644\u0643: \u064a\u0645\u0643\u0646 \u0644\u062c\u0645\u064a\u0639 \u0627\u0644\u0645\u0633\u062a\u062e\u062f\u0645\u064a\u0646 \u0627\u0644\u0630\u064a\u0646 \u064a\u0639\u0627\u0646\u0648\u0646 \u0645\u0646 \u0623\u064a \u0634\u0643\u0644 \u0645\u0646 \u0623\u0634\u0643\u0627\u0644 \u0627\u0644\u0625\u0639\u0627\u0642\u0629 \u0627\u0644\u0642\u0627\u062f\u0631\u064a\u0646 \u0639\u0644\u0649 \u062a\u0635\u0641\u062d \u0627\u0644\u0648\u064a\u0628 \u0648\u0625\u062f\u062e\u0627\u0644 \u0646\u0635 \u0641\u064a \u0627\u0644\u0646\u0645\u0627\u0630\u062c \u0627\u0644\u0648\u0635\u0648\u0644 \u0625\u0644\u0649 \u0627\u0644\u062e\u062f\u0645\u0627\u062a \u0627\u0644\u0645\u062d\u0645\u064a\u0629 \u0628\u0648\u0627\u0633\u0637\u0629 hCaptcha \u0639\u0646\u062f \u0627\u0644\u062a\u0633\u062c\u064a\u0644. \u0648\u0645\u0639 \u0630\u0644\u0643 \u060c \u0647\u0630\u0647 \u0644\u064a\u0633\u062a \u0646\u0635\u064a\u062d\u0629 \u0642\u0627\u0646\u0648\u0646\u064a\u0629: \u064a\u062c\u0628 \u0639\u0644\u064a\u0643 \u0625\u062c\u0631\u0627\u0621 \u062a\u0642\u064a\u064a\u0645\u0643 \u0627\u0644\u062e\u0627\u0635 \u060c \u0645\u0639 \u0645\u0631\u0627\u0639\u0627\u0629 \u0627\u0644\u062a\u0646\u0641\u064a\u0630 \u0627\u0644\u062e\u0627\u0635 \u0628\u0643 \u0644\u0644\u062a\u0623\u0643\u062f \u0645\u0646 \u0623\u0646 \u0647\u0630\u0627 \u0647\u0648 \u0627\u0644\u062d\u0627\u0644 \u0628\u0627\u0644\u0646\u0633\u0628\u0629 \u0644\u0646\u0634\u0631\u0643.",
    "page-acc-faq-2": "\u0627\u0644\u0633\u0624\u0627\u0644: \u0647\u0644 \u062a\u0639\u0645\u0644 \u0639\u0644\u0649 \u062e\u064a\u0627\u0631\u0627\u062a \u0623\u062e\u0631\u0649 \u0644\u0625\u0645\u0643\u0627\u0646\u064a\u0629 \u0627\u0644\u0648\u0635\u0648\u0644 (a11y) \u060c \u0645\u062b\u0644 \u0627\u0644\u0635\u0648\u062a\u061f \u0627\u0644\u0625\u062c\u0627\u0628\u0629: \u0627\u0644\u062e\u064a\u0627\u0631\u0627\u062a \u0627\u0644\u0634\u0627\u0626\u0639\u0629 \u0633\u0627\u0628\u0642\u064b\u0627 \u0645\u062b\u0644 captchas \u0627\u0644\u0635\u0648\u062a\u064a\u0629 \u062a\u0645\u064a\u0632 \u0636\u062f \u0627\u0644\u0639\u062f\u064a\u062f \u0645\u0646 \u0645\u0633\u062a\u062e\u062f\u0645\u064a a11y \u0648\u064a\u0645\u0643\u0646 \u0647\u0632\u064a\u0645\u062a\u0647\u0627 \u0628\u0633\u0647\u0648\u0644\u0629 \u0645\u0646 \u062e\u0644\u0627\u0644 \u062a\u0642\u0646\u064a\u0627\u062a \u0627\u0644\u062a\u0639\u0644\u0645 \u0627\u0644\u0622\u0644\u064a \u0627\u0644\u062d\u062f\u064a\u062b\u0629. \u0648\u0642\u062f \u0623\u062c\u0628\u0631 \u0647\u0630\u0627 \u0627\u0644\u062a\u062d\u062f\u064a\u0627\u062a \u0627\u0644\u0635\u0648\u062a\u064a\u0629 \u0627\u0644\u062d\u0627\u0644\u064a\u0629 \u0639\u0644\u0649 \u0623\u0646 \u062a\u0635\u0628\u062d \u0623\u0643\u062b\u0631 \u0635\u0639\u0648\u0628\u0629 \u060c \u0648\u0625\u062f\u062e\u0627\u0644 \u0636\u0648\u0636\u0627\u0621 \u060c \u0648\u062a\u0648\u0642\u064a\u062a \u063a\u0631\u064a\u0628 \u060c \u0648\u062a\u0631\u0643\u064a\u0628\u0627\u062a \u0643\u0644\u0645\u0627\u062a \u063a\u064a\u0631 \u0639\u0627\u062f\u064a\u0629 \u060c \u0648\u0645\u0627 \u0625\u0644\u0649 \u0630\u0644\u0643 \u0644\u0647\u0632\u064a\u0645\u0629 \u0627\u0644\u0645\u0647\u0627\u062c\u0645\u064a\u0646. \u0648\u0628\u0627\u0644\u062a\u0627\u0644\u064a \u060c \u0641\u0625\u0646\u0646\u0627 \u0623\u0642\u0644 \u062d\u0645\u0627\u0633\u0629 \u062a\u062c\u0627\u0647 \u0647\u0630\u0627 \u0627\u0644\u0646\u0647\u062c \u0645\u0642\u0627\u0628\u0644 \u062a\u062c\u0646\u0628 \u0627\u0644\u062a\u062d\u062f\u064a \u062a\u0645\u0627\u0645\u064b\u0627 \u060c \u0648\u0644\u0643\u0646\u0646\u0627 \u0633\u0646\u0623\u062e\u0630\u0647 \u0641\u064a \u0627\u0644\u0627\u0639\u062a\u0628\u0627\u0631 \u0625\u0630\u0627 \u0643\u0627\u0646 \u0647\u0646\u0627\u0643 \u0637\u0644\u0628 \u0645\u0646 \u0645\u062c\u062a\u0645\u0639 a11y. \u0648\u0645\u0639 \u0630\u0644\u0643 \u060c \u0646\u062d\u0646 \u0645\u0647\u062a\u0645\u0648\u0646 \u062c\u062f\u064b\u0627 \u0628\u0628\u0637\u0627\u0642\u0629 \u0627\u0644\u062e\u0635\u0648\u0635\u064a\u0629 \u0644\u062d\u0627\u0644\u0629 \u0627\u0633\u062a\u062e\u062f\u0627\u0645 \u0625\u0645\u0643\u0627\u0646\u064a\u0629 \u0627\u0644\u0648\u0635\u0648\u0644. \u0646\u0639\u062a\u0642\u062f \u0623\u0646 \u0627\u0644\u062c\u0645\u0639 \u0628\u064a\u0646 \u0646\u0647\u062c\u0646\u0627 \u0627\u0644\u062d\u0627\u0644\u064a \u0644\u0640 a11y \u0648\u0625\u0635\u062f\u0627\u0631 Privacy Pass \u0633\u064a\u0633\u0645\u062d \u0644\u0645\u0633\u062a\u062e\u062f\u0645\u064a a11y \u0628\u0627\u0644\u062a\u0635\u0641\u062d \u0628\u0623\u0645\u0627\u0646 \u060c \u0645\u0639 \u0627\u0644\u0639\u0644\u0645 \u0628\u0623\u0646 \u062d\u0631\u0643\u0629 \u0627\u0644\u0645\u0631\u0648\u0631 \u0627\u0644\u062e\u0627\u0635\u0629 \u0628\u0647\u0645 \u0623\u0643\u062b\u0631 \u062e\u0635\u0648\u0635\u064a\u0629 \u060c \u0645\u0639 \u062a\u0642\u064a\u064a\u062f \u0625\u0633\u0627\u0621\u0629 \u0627\u0633\u062a\u062e\u062f\u0627\u0645 \u0645\u0634\u063a\u0644\u064a \u0627\u0644\u0631\u0648\u0628\u0648\u062a\u0627\u062a \u0627\u0644\u062a\u064a \u062a\u062d\u062f\u062b \u062d\u062a\u0645\u064b\u0627 \u0639\u0646\u062f \u062a\u0648\u0641\u0631 \u062e\u064a\u0627\u0631\u0627\u062a a11y. \u0646\u062d\u0646 \u0645\u0634\u0627\u0631\u0643\u0648\u0646 \u0646\u0634\u0637\u0627\u0621 \u0641\u064a \u0645\u062c\u0645\u0648\u0639\u0629 \u0639\u0645\u0644 IETF \u0627\u0644\u062a\u064a \u062a\u0648\u062d\u062f \u0647\u0630\u0647 \u0627\u0644\u062a\u0642\u0646\u064a\u0629 \u0627\u0644\u062c\u062f\u064a\u062f\u0629.",
    "page-acc-faq-3": "\u0633\u0624\u0627\u0644: \u0645\u0627\u0630\u0627 \u0639\u0646 \u0627\u0644\u062e\u0635\u0648\u0635\u064a\u0629\u061f \u0647\u0644 \u064a\u0643\u0634\u0641 \u0627\u0644\u062a\u0633\u062c\u064a\u0644 \u0639\u0646 \u0628\u064a\u0627\u0646\u0627\u062a \u0627\u0644\u062a\u0635\u0641\u062d \u0628\u0637\u0631\u064a\u0642\u0629 \u0645\u0627\u061f \u0645\u0627\u0630\u0627 \u062a\u0641\u0639\u0644 \u0628\u0627\u0644\u0628\u0631\u064a\u062f \u0627\u0644\u0627\u0644\u0643\u062a\u0631\u0648\u0646\u064a\u061f \u0627\u0644\u0625\u062c\u0627\u0628\u0629: \u062a\u0645 \u062a\u0635\u0645\u064a\u0645 hCaptcha \u0644\u0644\u062e\u0635\u0648\u0635\u064a\u0629 \u0645\u0646 \u0627\u0644\u0623\u0644\u0641 \u0625\u0644\u0649 \u0627\u0644\u064a\u0627\u0621. \u0625\u0646\u0647 \u0645\u062e\u062a\u0644\u0641 \u062a\u0645\u0627\u0645\u064b\u0627 \u0639\u0646 \u0627\u0644\u062e\u064a\u0627\u0631\u0627\u062a \u0627\u0644\u062a\u0642\u0644\u064a\u062f\u064a\u0629 \u0645\u062b\u0644 reCAPTCHA \u0627\u0644\u062a\u064a \u062a\u0645\u062a\u0644\u0643\u0647\u0627 \u0634\u0628\u0643\u0627\u062a \u0627\u0644\u0625\u0639\u0644\u0627\u0646\u0627\u062a \u060c \u0648\u0627\u0644\u062a\u064a \u0644\u062f\u064a\u0647\u0627 \u062d\u0627\u0641\u0632 \u0644\u062a\u062a\u0628\u0639\u0643 \u0639\u0628\u0631 \u0627\u0644\u0648\u064a\u0628 \u0648\u0631\u0628\u0637\u0643 \u0628\u0647\u0648\u064a\u0629 \u062d\u0642\u064a\u0642\u064a\u0629. \u0646\u062d\u0646 \u0644\u0627 \u0646\u0633\u062a\u062e\u062f\u0645 \u0631\u0633\u0627\u0626\u0644 \u0627\u0644\u0628\u0631\u064a\u062f \u0627\u0644\u0625\u0644\u0643\u062a\u0631\u0648\u0646\u064a \u0623\u0648 \u0645\u0639\u0644\u0648\u0645\u0627\u062a \u0625\u0645\u0643\u0627\u0646\u064a\u0629 \u0627\u0644\u0648\u0635\u0648\u0644 \u0645\u0637\u0644\u0642\u064b\u0627 \u0644\u0623\u064a \u063a\u0631\u0636 \u0628\u062e\u0644\u0627\u0641 \u062a\u0633\u0647\u064a\u0644 \u0627\u0633\u062a\u062e\u062f\u0627\u0645 a11y \u0648\u0645\u0646\u0639 \u0625\u0633\u0627\u0621\u0629 \u0627\u0644\u0627\u0633\u062a\u062e\u062f\u0627\u0645. \u062a\u062d\u062a\u0648\u064a \u0633\u064a\u0627\u0633\u0629 \u0627\u0644\u062e\u0635\u0648\u0635\u064a\u0629 \u0627\u0644\u062e\u0627\u0635\u0629 \u0628\u0646\u0627 \u0639\u0644\u0649 \u0625\u062c\u0627\u0628\u0627\u062a \u0634\u0627\u0645\u0644\u0629 \u0648\u0645\u0648\u062b\u0648\u0642\u0629 \u062d\u0648\u0644 \u0643\u064a\u0641\u064a\u0629 \u0627\u0633\u062a\u062e\u062f\u0627\u0645\u0646\u0627 \u0644\u0644\u0628\u064a\u0627\u0646\u0627\u062a \u060c \u0648\u0644\u0643\u0646 \u0627\u0644\u0625\u062c\u0627\u0628\u0629 \u0627\u0644\u0645\u062e\u062a\u0635\u0631\u0629 \u0647\u064a \u0623\u0646\u0646\u0627 \u0644\u0627 \u0646\u0631\u063a\u0628 \u0641\u064a \u0631\u0628\u0637\u0643 \u0643\u0634\u062e\u0635 \u0628\u0633\u062c\u0644 \u0627\u0644\u062a\u0635\u0641\u062d \u0627\u0644\u062e\u0627\u0635 \u0628\u0643 \u060c \u0643\u0645\u0627 \u0623\u0646\u0646\u0627 \u0646\u0639\u0645\u0644 \u062d\u0627\u0644\u064a\u064b\u0627 \u0639\u0644\u0649 \u062d\u0644 \u062a\u0634\u0641\u064a\u0631 \u0644\u062a\u062c\u0627\u0647\u0644 \u0639\u0646\u0648\u0627\u0646 \u0628\u0631\u064a\u062f\u0643 \u0627\u0644\u0625\u0644\u0643\u062a\u0631\u0648\u0646\u064a \u0628\u0633\u0631\u0639\u0629 \u0645\u0639 \u0627\u0644\u062d\u0641\u0627\u0638 \u0639\u0644\u0649 \u0642\u062f\u0631\u062a\u0646\u0627 \u0639\u0644\u0649 \u0645\u0646\u0639 \u0625\u0633\u0627\u0621\u0629 \u0627\u0644\u0627\u0633\u062a\u062e\u062f\u0627\u0645 \u060c \u0648\u0627\u0633\u062a\u0643\u0645\u0627\u0644 \u0639\u0645\u0644 Privacy Pass.",
    "page-acc-link-1": "\u0625\u0630\u0627 \u0643\u0646\u062a \u0645\u0646 \u0645\u0633\u062a\u062e\u062f\u0645\u064a \u0625\u0645\u0643\u0627\u0646\u064a\u0629 \u0627\u0644\u0648\u0635\u0648\u0644 \u060c \u064a\u0631\u062c\u0649 \u0627\u0644\u0627\u0634\u062a\u0631\u0627\u0643 \u0647\u0646\u0627.",
    "page-acc-subhead": "\u0636\u0645\u0627\u0646 \u0627\u0644\u0648\u0635\u0648\u0644 \u0627\u0644\u0633\u0644\u0633 \u0644\u062c\u0645\u064a\u0639 \u0627\u0644\u0645\u0633\u062a\u062e\u062f\u0645\u064a\u0646 \u0645\u0639 \u0627\u0644\u062d\u0641\u0627\u0638 \u0639\u0644\u0649 \u0627\u0644\u0623\u0645\u0646",
    "page-acc-summary": "\u0645\u0644\u062e\u0635",
    "page-acc-summary-contents-1": "\u062a\u0645 \u062a\u0635\u0645\u064a\u0645 hCaptcha \u0644\u0625\u064a\u0642\u0627\u0641 \u0628\u0631\u0627\u0645\u062c \u0627\u0644\u062a\u062a\u0628\u064f\u0651\u0639 \u0645\u0646 \u062e\u0644\u0627\u0644 \u062a\u0645\u064a\u064a\u0632\u0647\u0627 \u0639\u0646 \u0627\u0644\u0623\u0634\u062e\u0627\u0635. \u062a\u0639\u062f \u0627\u0644\u0627\u062e\u062a\u0628\u0627\u0631\u0627\u062a \u0627\u0644\u0645\u0631\u0626\u064a\u0629 \u0623\u062f\u0627\u0629 \u0645\u0646\u0627\u0633\u0628\u0629 \u0644\u0630\u0644\u0643 \u060c \u0648\u0644\u0643\u0646 \u0644\u0627 \u064a\u0645\u0643\u0646 \u0644\u0623\u064a \u0634\u062e\u0635 \u062d\u0644 \u0627\u0644\u062a\u062d\u062f\u064a \u0627\u0644\u0628\u0635\u0631\u064a. \u0644\u0647\u0630\u0627 \u0627\u0644\u0633\u0628\u0628 \u060c \u0635\u0645\u0645\u0646\u0627 \u0628\u062f\u064a\u0644\u0627\u064b \u0628\u0633\u064a\u0637\u064b\u0627 \u0648\u063a\u064a\u0631 \u0645\u0624\u0644\u0645 \u0644\u0644\u0633\u0645\u0627\u062d \u0644\u0644\u0646\u0627\u0634\u0631\u064a\u0646 \u0627\u0644\u0630\u064a\u0646 \u064a\u0633\u062a\u062e\u062f\u0645\u0648\u0646 \u062e\u062f\u0645\u062a\u0646\u0627 \u0628\u0627\u0644\u062d\u0641\u0627\u0638 \u0639\u0644\u0649 \u0625\u0645\u0643\u0627\u0646\u064a\u0629 \u0627\u0644\u0648\u0635\u0648\u0644 \u0644\u0644\u062c\u0645\u064a\u0639 \u0645\u0639 \u0627\u0644\u0627\u0645\u062a\u062b\u0627\u0644 \u0627\u0644\u0643\u0627\u0645\u0644 \u0644\u0644\u0642\u0633\u0645 508 \u0648 WCAG 2.0.",
    "page-acc-summary-contents-2": "\u0643\u064a\u0641 \u064a\u0639\u0645\u0644: \u0623\u0648\u0644\u0627\u064b \u060c \u064a\u0642\u0648\u0645 \u0645\u0633\u062a\u062e\u062f\u0645 \u0625\u0645\u0643\u0627\u0646\u064a\u0629 \u0627\u0644\u0648\u0635\u0648\u0644 \u0628\u0627\u0644\u062a\u0633\u062c\u064a\u0644 \u0641\u064a \u0639\u0646\u0648\u0627\u0646 URL \u0647\u0630\u0627 \u060c \u0648\u0627\u0644\u0645\u0631\u062a\u0628\u0637 \u0641\u064a \u0635\u0641\u062d\u0629 \u0645\u0639\u0644\u0648\u0645\u0627\u062a \u0623\u062f\u0627\u0629 hCaptcha. \u064a\u062a\u0645 \u0645\u0646\u062d\u0647\u0645 \u0645\u0644\u0641 \u062a\u0639\u0631\u064a\u0641 \u0627\u0631\u062a\u0628\u0627\u0637 \u0645\u0634\u0641\u0631 \u064a\u0645\u0643\u0646 \u0627\u0633\u062a\u062e\u062f\u0627\u0645\u0647 \u0639\u062f\u0629 \u0645\u0631\u0627\u062a \u0641\u064a \u0627\u0644\u064a\u0648\u0645 \u060c \u0648\u0644\u0643\u0646 \u064a\u062c\u0628 \u062a\u062d\u062f\u064a\u062b\u0647 \u0643\u0644 24 \u0633\u0627\u0639\u0629 \u0639\u0628\u0631 \u062a\u0633\u062c\u064a\u0644 \u0627\u0644\u062f\u062e\u0648\u0644 \u060c \u0648\u0639\u0646\u062f\u0645\u0627 \u064a\u062a\u0645 \u062a\u0642\u062f\u064a\u0645 \u062a\u062d\u062f\u064a \u0644\u0645\u0633\u062a\u062e\u062f\u0645 \u064a\u0633\u0647\u0644 \u0627\u0644\u0648\u0635\u0648\u0644 \u0625\u0644\u064a\u0647 \u0639\u0644\u0649 \u0623\u064a \u0645\u0648\u0642\u0639 \u064a\u0633\u062a\u062e\u062f\u0645 \u062e\u062f\u0645\u0629 hCaptcha \u060c \u0641\u0633\u064a\u062a\u0645 \u062a\u0645\u0631\u064a\u0631\u0647 \u062a\u0644\u0642\u0627\u0626\u064a\u064b\u0627.",
    "page-acc-title": "\u0645\u0639\u0644\u0648\u0645\u0627\u062a \u0627\u0644\u0648\u0635\u0648\u0644 hCaptcha",
    "page-acc-tr-body": "\u0627\u0644\u0633\u0624\u0627\u0644: \u0645\u0627 \u0632\u0644\u062a \u0623\u0631\u0649 \u062a\u062d\u062f\u064a\u064b\u0627 \u0628\u0639\u062f \u0625\u0639\u062f\u0627\u062f \u0645\u0644\u0641 \u062a\u0639\u0631\u064a\u0641 \u0627\u0644\u0627\u0631\u062a\u0628\u0627\u0637. \u0644\u0645\u0627\u0630\u0627 \u064a\u062d\u062f\u062b \u0647\u0630\u0627\u061f \u0627\u0644\u0625\u062c\u0627\u0628\u0629: \u064a\u0631\u062c\u0639 \u0647\u0630\u0627 \u0639\u0627\u062f\u0629\u064b \u0625\u0644\u0649 \u0627\u0633\u062a\u062e\u062f\u0627\u0645 \u0623\u062f\u0627\u0629 \u062d\u0638\u0631 \u0627\u0644\u0625\u0639\u0644\u0627\u0646\u0627\u062a \u0627\u0644\u0642\u0648\u064a\u0629 \u0623\u0648 \u0645\u0644\u062d\u0642 \u0645\u0643\u0627\u0641\u062d\u0629 \u0645\u0644\u0641\u0627\u062a \u062a\u0639\u0631\u064a\u0641 \u0627\u0644\u0627\u0631\u062a\u0628\u0627\u0637 \u060c \u0623\u0648 \u0627\u0644\u0625\u0639\u062f\u0627\u062f \u0627\u0644\u0630\u064a \u064a\u062d\u0638\u0631 \u0645\u0644\u0641\u0627\u062a \u062a\u0639\u0631\u064a\u0641 \u0627\u0644\u0627\u0631\u062a\u0628\u0627\u0637 \u0639\u0628\u0631 \u0627\u0644\u0645\u0648\u0627\u0642\u0639 \u060c \u0648\u0641\u064a \u0647\u0630\u0647 \u0627\u0644\u062d\u0627\u0644\u0629 \u0645\u0644\u0641 \u062a\u0639\u0631\u064a\u0641 \u0627\u0644\u0627\u0631\u062a\u0628\u0627\u0637 \u0644\u0645\u0648\u0642\u0639 hcaptcha.com \u0627\u0644\u0630\u064a \u062a\u0645 \u062a\u0639\u064a\u064a\u0646\u0647 \u0623\u0648 \u0627\u0644\u062a\u062d\u0642\u0642 \u0645\u0646\u0647 \u0628\u0648\u0627\u0633\u0637\u0629 hCaptcha JavaScript \u0639\u0644\u0649 \u0645\u0648\u0642\u0639 \u0645\u062e\u062a\u0644\u0641 \u060c \u0645\u062b\u0644 \u062a\u0644\u0643 \u0627\u0644\u062a\u064a \u062a\u0632\u0648\u0631\u0647\u0627. \u062a\u0639\u0645\u0644 \u0645\u0644\u0641\u0627\u062a \u062a\u0639\u0631\u064a\u0641 \u0627\u0631\u062a\u0628\u0627\u0637 \u0625\u0645\u0643\u0627\u0646\u064a\u0629 \u0627\u0644\u0648\u0635\u0648\u0644 \u0639\u0644\u0649 hCaptcha \u0645\u0639 \u062c\u0645\u064a\u0639 \u0627\u0644\u0645\u062a\u0635\u0641\u062d\u0627\u062a \u0627\u0644\u0634\u0627\u0626\u0639\u0629 \u0648\u062d\u0627\u0635\u0631\u0627\u062a \u0627\u0644\u0625\u0639\u0644\u0627\u0646\u0627\u062a \u0645\u0639 \u0625\u0639\u062f\u0627\u062f\u0627\u062a\u0647\u0627 \u0627\u0644\u0642\u064a\u0627\u0633\u064a\u0629 \u060c \u0644\u0630\u0644\u0643 \u0639\u0627\u062f\u0629\u064b \u0645\u0627 \u064a\u0643\u0648\u0646 \u0627\u0644\u0641\u0634\u0644 \u0628\u0633\u0628\u0628 \u0627\u0644\u0628\u0631\u0627\u0645\u062c \u0627\u0644\u0646\u0635\u064a\u0629 \u0627\u0644\u0645\u0636\u0627\u062f\u0629 \u0644\u0645\u0636\u0627\u062f\u0627\u062a \u0627\u0644\u0625\u0639\u0644\u0627\u0646\u0627\u062a \u0623\u0648 \u0645\u062c\u0645\u0648\u0639\u0627\u062a \u0642\u0648\u0627\u0639\u062f \u0645\u0645\u0627\u062b\u0644\u0629 \u062a\u0633\u062a\u0647\u062f\u0641 \u0645\u0648\u0627\u0642\u0639 \u0645\u0639\u064a\u0646\u0629. \u0627\u0644\u062d\u0644\u0648\u0644: 1. Whitelist hcaptcha.com \u0648 * .hcaptcha.com \u0645\u0644\u0641\u0627\u062a \u062a\u0639\u0631\u064a\u0641 \u0627\u0644\u0627\u0631\u062a\u0628\u0627\u0637 \u0641\u064a \u0623\u062f\u0627\u0629 \u062d\u0638\u0631 \u0627\u0644\u0625\u0639\u0644\u0627\u0646\u0627\u062a \u0623\u0648 \u0645\u0644\u062d\u0642 \u0623\u0645\u0627\u0646 \u0627\u0644\u0645\u0633\u062a\u0639\u0631\u0636. 2. \u0625\u0630\u0627 \u0643\u0646\u062a \u062a\u0633\u062a\u062e\u062f\u0645 \u0645\u062a\u0635\u0641\u062d Brave \u060c \u0627\u0644\u0630\u064a \u0644\u0627 \u064a\u0628\u062f\u0648 \u0623\u0646\u0647 (\u0627\u0639\u062a\u0628\u0627\u0631\u064b\u0627 \u0645\u0646 \u0623\u0628\u0631\u064a\u0644 2020) \u064a\u062d\u062a\u0648\u064a \u0639\u0644\u0649 \u0623\u064a \u0646\u0648\u0639 \u0645\u0646 \u0627\u0644\u0642\u0627\u0626\u0645\u0629 \u0627\u0644\u0628\u064a\u0636\u0627\u0621 \u0644\u0645\u0644\u0641\u0627\u062a \u062a\u0639\u0631\u064a\u0641 \u0627\u0644\u0627\u0631\u062a\u0628\u0627\u0637 \u060c \u0641\u0627\u0646\u062a\u0642\u0644 \u0625\u0644\u0649 Preferences -> Shields -> Cookies \u0648\u0627\u062e\u062a\u0631 Allow All Cookies. 3. \u0625\u0630\u0627 \u0643\u0646\u062a \u062a\u0633\u062a\u062e\u062f\u0645 \u0623\u062d\u062f\u062b \u0625\u0635\u062f\u0627\u0631 \u0645\u0646 Safari \u0639\u0644\u0649 \u0646\u0638\u0627\u0645 \u0627\u0644\u062a\u0634\u063a\u064a\u0644 OS X 10.15 \u0627\u0644\u0630\u064a \u062a\u0645 \u0625\u0635\u062f\u0627\u0631\u0647 \u0645\u0624\u062e\u0631\u064b\u0627 \u0623\u0648 iOS 13. 4 \u060c \u0641\u0642\u062f \u063a\u064a\u0631\u062a Apple \u0644\u0644\u062a\u0648 \u0633\u0644\u0648\u0643 Safari \u0627\u0644\u0645\u0631\u062a\u0628\u0637 \u0628\u0645\u0644\u0641\u0627\u062a \u062a\u0639\u0631\u064a\u0641 \u0627\u0644\u0627\u0631\u062a\u0628\u0627\u0637 \u0644\u0644\u062c\u0647\u0627\u062a \u0627\u0644\u062e\u0627\u0631\u062c\u064a\u0629 \u060c \u0648\u062d\u0638\u0631\u0647\u0627 \u062c\u0645\u064a\u0639\u064b\u0627 \u0628\u0634\u0643\u0644 \u0627\u0641\u062a\u0631\u0627\u0636\u064a. \u0646\u062d\u0646 \u0646\u0646\u0641\u0630 \u062d\u0644\u0627\u064b \u060c \u0648\u0644\u0643\u0646 \u0641\u064a \u063a\u0636\u0648\u0646 \u0630\u0644\u0643 \u060c \u064a\u0631\u062c\u0649 \u0632\u064a\u0627\u0631\u0629 \u062a\u0641\u0636\u064a\u0644\u0627\u062a Safari \u0648\u0642\u0633\u0645 \u0627\u0644\u062e\u0635\u0648\u0635\u064a\u0629 \u0648\u0625\u0644\u063a\u0627\u0621 \u062a\u062d\u062f\u064a\u062f \u062a\u062a\u0628\u0639 \u0627\u0644\u0645\u0648\u0642\u0639: \u0645\u0646\u0639 \u0627\u0644\u062a\u062a\u0628\u0639 \u0639\u0628\u0631 \u0627\u0644\u0645\u0648\u0627\u0642\u0639 \u0644\u062a\u0645\u0643\u064a\u0646 \u0645\u0644\u0641 \u062a\u0639\u0631\u064a\u0641 \u0627\u0631\u062a\u0628\u0627\u0637 \u0625\u0645\u0643\u0627\u0646\u064a\u0629 \u0627\u0644\u0648\u0635\u0648\u0644 \u0644\u0644\u0639\u0645\u0644 \u0643\u0645\u0627 \u0647\u0648 \u0645\u062a\u0648\u0642\u0639. \u0627\u0644\u0633\u0624\u0627\u0644: \u0623\u0633\u062a\u062e\u062f\u0645 \u0623\u062c\u0647\u0632\u0629 \u0645\u062a\u0639\u062f\u062f\u0629. \u0647\u0644 \u0623\u062d\u062a\u0627\u062c \u0644\u0644\u062a\u0633\u062c\u064a\u0644 \u0639\u062f\u0629 \u0645\u0631\u0627\u062a\u061f \u0627\u0644\u0625\u062c\u0627\u0628\u0629: \u0644\u0627. \u0627\u0644\u0631\u062c\u0627\u0621 \u0627\u0644\u0646\u0642\u0631 \u0641\u0648\u0642 \u0646\u0641\u0633 \u0631\u0627\u0628\u0637 \u062a\u0633\u062c\u064a\u0644 \u0627\u0644\u062f\u062e\u0648\u0644 \u0625\u0644\u0649 \u0627\u0644\u0628\u0631\u064a\u062f \u0627\u0644\u0625\u0644\u0643\u062a\u0631\u0648\u0646\u064a \u0627\u0644\u0645\u0631\u0633\u0644 \u0625\u0644\u064a\u0643 \u0639\u0644\u0649 \u0643\u0644 \u062c\u0647\u0627\u0632 \u062a\u0633\u062a\u062e\u062f\u0645\u0647 \u0644\u062a\u0639\u064a\u064a\u0646 \u0645\u0644\u0641 \u062a\u0639\u0631\u064a\u0641 \u0627\u0644\u0627\u0631\u062a\u0628\u0627\u0637.",
    "page-acc-tr-title": "\u0628\u0627\u0644\u0646\u0633\u0628\u0629 \u0644\u0645\u0633\u062a\u062e\u062f\u0645\u064a \u0625\u0645\u0643\u0627\u0646\u064a\u0629 \u0627\u0644\u0648\u0635\u0648\u0644: \u0627\u0644\u0623\u0633\u0626\u0644\u0629 \u0648\u0627\u0644\u0623\u062c\u0648\u0628\u0629 \u0648\u062f\u0644\u064a\u0644 \u0627\u0633\u062a\u0643\u0634\u0627\u0641 \u0627\u0644\u0623\u062e\u0637\u0627\u0621 \u0648\u0625\u0635\u0644\u0627\u062d\u0647\u0627",
    "priv-hc": "\u062a\u0631\u0643\u0632 \u0627\u0644\u062e\u0635\u0648\u0635\u064a\u0629",
    "priv-hc-text": "\u0634\u0628\u0643\u0627\u062a \u0627\u0644\u0625\u0639\u0644\u0627\u0646\u0627\u062a \u062a\u0631\u0627\u0643 \u0643\u0645\u0646\u062a\u062c \u0644\u0647\u0627. \u0646\u062d\u0646 \u0644\u0627. \u0646\u062d\u0646 \u0644\u0627 \u0646\u0647\u062a\u0645 \u0645\u0646 \u0647\u0645 \u0627\u0644\u0645\u0633\u062a\u062e\u062f\u0645\u0648\u0646 \u060c \u0641\u0642\u0637 \u0645\u062f\u0649 \u0627\u062d\u062a\u0645\u0627\u0644\u064a\u0629 \u0623\u0646 \u064a\u0643\u0648\u0646\u0648\u0627 \u0636\u0627\u0631\u064a\u0646. \u0646\u062d\u0646 \u0646\u062f\u0639\u0645 \u0645\u0628\u0627\u062f\u0631\u0627\u062a \u0645\u062b\u0644 Privacy Pass \u060c \u0648\u0646\u0639\u0645\u0644 \u0628\u062c\u062f \u0644\u062a\u062d\u0642\u064a\u0642 \u0627\u0644\u062a\u0648\u0627\u0632\u0646 \u0628\u064a\u0646 \u0627\u0644\u062e\u0635\u0648\u0635\u064a\u0629 \u0648\u0627\u0644\u0623\u0645\u0627\u0646.",
    "sec-hc": "\u0627\u0644\u0623\u0645\u0646 \u0623\u0648\u0644\u0627\u064b",
    "sec-hc-text": "\u064a\u0631\u0643\u0632 hCaptcha \u0639\u0644\u0649 \u0627\u0644\u0646\u0627\u0634\u0631. \u0646\u062d\u0646 \u0646\u0639\u0645\u0644 \u0639\u0644\u0649 \u062a\u0648\u0641\u064a\u0631 \u0628\u064a\u0627\u0646\u0627\u062a \u062d\u0642\u064a\u0642\u064a\u0629 \u0644\u0644\u0645\u0648\u0627\u0642\u0639 \u0648\u0627\u0644\u0633\u0645\u0627\u062d \u0644\u0647\u0645 \u0628\u062a\u062d\u062f\u064a\u062f \u0643\u064a\u0641\u064a\u0629 \u0627\u0633\u062a\u062e\u062f\u0627\u0645\u0647\u0627. \u0627\u0644\u062f\u0642\u0629 \u0641\u064a \u0645\u0635\u0644\u062d\u062a\u0643\u0645 \u0648\u0645\u0635\u0644\u062d\u062a\u0646\u0627. \u064a\u062a\u0645 \u062a\u0648\u0641\u064a\u0631 captchas \u0623\u062e\u0631\u0649 \u0645\u0646 \u0642\u0628\u0644 \u0634\u0631\u0643\u0627\u062a \u0627\u0644\u0625\u0639\u0644\u0627\u0646. \u0643\u0644 \u0631\u0648\u0628\u0648\u062a \u064a\u062d\u062c\u0628\u0648\u0646\u0647 \u0642\u062f \u064a\u0642\u0644\u0644 \u0645\u0646 \u0625\u064a\u0631\u0627\u062f\u0627\u062a\u0647\u0645.",
    "what-is-hcaptcha": "\u0645\u0627 \u0647\u0648 \u0628\u0631\u0646\u0627\u0645\u062c hCaptcha\u061f",
    "what-is-hcaptcha-text": "\u064a\u0633\u0627\u0639\u062f hCaptcha \u062e\u062f\u0645\u0627\u062a \u0627\u0644\u0648\u064a\u0628 \u0627\u0644\u0645\u0641\u0636\u0644\u0629 \u0644\u062f\u064a\u0643 \u0639\u0644\u0649 \u0645\u0646\u0639 \u0628\u0631\u0627\u0645\u062c \u0627\u0644\u062a\u062a\u0628\u064f\u0651\u0639 \u0648\u0627\u0644\u0631\u0633\u0627\u0626\u0644 \u063a\u064a\u0631 \u0627\u0644\u0645\u0631\u063a\u0648\u0628 \u0641\u064a\u0647\u0627 \u0648\u0625\u0633\u0627\u0621\u0629 \u0627\u0644\u0627\u0633\u062a\u062e\u062f\u0627\u0645 \u0645\u0646 \u062e\u0644\u0627\u0644 \u0637\u0631\u062d \u0623\u0633\u0626\u0644\u0629 \u0628\u0633\u064a\u0637\u0629 \u0633\u0647\u0644\u0629 \u0639\u0644\u0649 \u0627\u0644\u0628\u0634\u0631 \u0648\u0635\u0639\u0628\u0629 \u0639\u0644\u0649 \u0627\u0644\u0623\u062c\u0647\u0632\u0629.",
    "why-hc": "\u0644\u0645\u0627\u0630\u0627 hCaptcha\u061f",
    "why-hc-text": "\u0641\u064a \u0643\u0644 \u0645\u0631\u0629 \u062a\u062c\u064a\u0628 \u0641\u064a\u0647\u0627 \u0639\u0644\u0649 \u0627\u0644\u062a\u062d\u062f\u064a \u060c \u0646\u0633\u062a\u062e\u062f\u0645 \u0646\u0645\u0627\u0630\u062c \u0645\u062a\u0637\u0648\u0631\u0629 \u0644\u0645\u0639\u0631\u0641\u0629 \u0645\u0627 \u0625\u0630\u0627 \u0643\u0646\u062a \u0625\u0646\u0633\u0627\u0646\u064b\u0627 \u0623\u0645 \u0622\u0644\u0629. \u064a\u0633\u062a\u062e\u062f\u0645 \u0645\u0648\u0642\u0639 \u0627\u0644\u0648\u064a\u0628 \u0627\u0644\u0630\u064a \u0632\u0631\u062a\u0647 \u0628\u0639\u062f \u0630\u0644\u0643 \u0647\u0630\u0647 \u0627\u0644\u0646\u062a\u064a\u062c\u0629 \u0644\u0644\u0645\u0633\u0627\u0639\u062f\u0629 \u0641\u064a \u062a\u062d\u0633\u064a\u0646 \u062a\u062c\u0631\u0628\u062a\u0643 \u0639\u0644\u0649 \u0645\u0648\u0642\u0639\u0647\u0645 \u0639\u0646 \u0637\u0631\u064a\u0642 \u0645\u0646\u0639 \u0628\u0631\u0627\u0645\u062c \u0627\u0644\u062a\u062a\u0628\u064f\u0651\u0639."
}