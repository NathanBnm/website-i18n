{
    "acc-opt": "\u041e\u043f\u0446\u0438\u0458\u0435 \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0447\u043d\u043e\u0441\u0442\u0438",
    "acc-opt-but": "\u0421\u0430\u0437\u043d\u0430\u0458\u0442\u0435 \u0432\u0438\u0448\u0435 \u043e \u043e\u043f\u0446\u0438\u0458\u0430\u043c\u0430 \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0447\u043d\u043e\u0441\u0442\u0438",
    "acc-opt-text": "\u0408\u0435\u0441\u0442\u0435 \u043b\u0438 \u0441\u043b\u0430\u0431\u043e\u0432\u0438\u0434\u043d\u0438 \u0438\u043b\u0438 \u043d\u0435 \u043c\u043e\u0436\u0435\u0442\u0435 \u0434\u0430 \u0438\u0441\u043f\u0443\u043d\u0438\u0442\u0435 \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430\u0441?",
    "accessibility-link": "\u041f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0447\u043d\u043e\u0441\u0442",
    "appreciate-in": "\u0426\u0435\u043d\u0438\u043c\u043e \u0432\u0430\u0448 \u0434\u043e\u043f\u0440\u0438\u043d\u043e\u0441. \u0408\u0430\u0432\u0438\u0442\u0435 \u043d\u0430\u043c \u0431\u0438\u043b\u043e \u043a\u043e\u0458\u0430 \u043f\u0438\u0442\u0430\u045a\u0430 \u0438 \u0431\u0440\u0437\u043e \u045b\u0435\u043c\u043e \u0438\u0445 \u0440\u0435\u0448\u0438\u0442\u0438.",
    "fb-but": "\u041f\u043e\u0448\u0430\u0459\u0438\u0442\u0435 \u043d\u0430\u043c \u043f\u043e\u0432\u0440\u0430\u0442\u043d\u0435 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0458\u0435",
    "feedback-text": "\u0414\u0430\u0458\u0442\u0435 \u043f\u043e\u0432\u0440\u0430\u0442\u043d\u0435 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0458\u0435 \u043e \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u043a\u043e\u0458\u0438 \u0441\u0442\u0435 \u0443\u043f\u0440\u0430\u0432\u043e \u0432\u0438\u0434\u0458\u0435\u043b\u0438",
    "home-ent-1": "\u0422\u0440\u0430\u0436\u0438\u0442\u0435 \u043d\u0430\u0448\u0435 \u0431\u0435\u0437\u0431\u0435\u0434\u043d\u043e\u0441\u043d\u0435 \u043f\u043e\u043d\u0443\u0434\u0435 \u043f\u0440\u0435\u0434\u0443\u0437\u0435\u045b\u0430?",
    "home-ent-2": "\u041f\u043e\u0441\u0435\u0442\u0438\u0442\u0435 \u043d\u0430\u0448 \u043d\u043e\u0432\u0438 \u043d\u0430\u043c\u0435\u043d\u0441\u043a\u0438 \u0441\u0430\u0458\u0442: \u0411\u043e\u0442\u0421\u0442\u043e\u043f.\u0446\u043e\u043c",
    "home-intro-body": "\u0414\u0430 \u043b\u0438 \u043a\u043e\u0440\u0438\u0441\u0442\u0438\u0442\u0435 \u0446\u0430\u043f\u0442\u0446\u0445\u0430 \u0434\u0430 \u043d\u0435 \u0431\u0438\u0441\u0442\u0435 \u0434\u0440\u0436\u0430\u043b\u0438 \u0440\u043e\u0431\u043e\u0442\u0430? \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u0448\u0442\u0438\u0442\u0438 \u043f\u0440\u0438\u0432\u0430\u0442\u043d\u043e\u0441\u0442 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u043a\u0430, \u043d\u0430\u0433\u0440\u0430\u0452\u0443\u0458\u0435 \u0432\u0435\u0431 \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u0435 \u0438 \u043f\u043e\u043c\u0430\u0436\u0435 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0458\u0430\u043c\u0430 \u0434\u0430 \u0434\u043e\u0431\u0438\u0458\u0443 \u043e\u0437\u043d\u0430\u043a\u0443 \u0441\u0432\u043e\u0458\u0438\u0445 \u043f\u043e\u0434\u0430\u0442\u0430\u043a\u0430. \u0422\u043e \u0458\u0435 \u0437\u0430\u043c\u0435\u043d\u0430 \u0437\u0430 \u0440\u0435\u0426\u0410\u041f\u0422\u0426\u0425\u0410: \u043c\u043e\u0436\u0435\u0442\u0435 \u0437\u0430\u043c\u0435\u043d\u0438\u0442\u0438 \u0443 \u0440\u043e\u043a\u0443 \u043e\u0434 \u043d\u0435\u043a\u043e\u043b\u0438\u043a\u043e \u043c\u0438\u043d\u0443\u0442\u0430.",
    "home-intro-head": "\u0417\u0430\u0443\u0441\u0442\u0430\u0432\u0438 \u0432\u0438\u0448\u0435 \u0440\u043e\u0431\u043e\u0442\u0430. \u0417\u0430\u043f\u043e\u0447\u043d\u0438\u0442\u0435 \u0437\u0430\u0448\u0442\u0438\u0442\u0443 \u043f\u0440\u0438\u0432\u0430\u0442\u043d\u043e\u0441\u0442\u0438 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u043a\u0430.",
    "home-main-1": "\u041f\u0440\u0438\u0432\u0430\u0442\u043d\u043e. \u0421\u0435\u0446\u0443\u0440\u0435. \u0411\u0440\u0436\u0435. \u0418 \u043c\u0438 \u0432\u0430\u043c \u043f\u043b\u0430\u045b\u0430\u043c\u043e \u0434\u0430 \u0433\u0430 \u043a\u043e\u0440\u0438\u0441\u0442\u0438\u0442\u0435.",
    "home-main-2": "\u041f\u0440\u0438\u0432\u0430\u0442\u043d\u043e\u0441\u0442 \u0444\u043e\u043a\u0443\u0441\u0438\u0440\u0430\u043d\u0430",
    "home-main-2c": "\u041e\u0433\u043b\u0430\u0441\u043d\u0435 \u043c\u0440\u0435\u0436\u0435 \u0432\u0438\u0434\u0435 \u0432\u0430\u0448\u0435 \u043f\u043e\u0441\u0435\u0442\u0438\u043e\u0446\u0435 \u043a\u0430\u043e \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434. \u0417\u0430 \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434 \u0458\u0435 \u043f\u043e\u0441\u0430\u043e \u043a\u043e\u0458\u0438 \u043f\u0440\u0443\u0436\u0430\u0458\u0443. \u041d\u0435 \u0437\u0430\u043d\u0438\u043c\u0430 \u043d\u0430\u0441 \u043a\u043e \u0441\u0443 \u0432\u0430\u0448\u0438 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u0446\u0438, \u0441\u0430\u043c\u043e \u043a\u043e\u043b\u0438\u043a\u043e \u0441\u0443 \u0432\u0435\u0440\u043e\u0432\u0430\u0442\u043d\u0438 \u0434\u0430 \u0431\u0443\u0434\u0443 \u0437\u043b\u043e\u043d\u0430\u043c\u0435\u0440\u043d\u0438. \u041f\u043e\u0434\u0440\u0436\u0430\u0432\u0430\u043c\u043e \u0438\u043d\u0438\u0446\u0438\u0458\u0430\u0442\u0438\u0432\u0435 \u043f\u043e\u043f\u0443\u0442 \u041f\u0440\u0438\u0432\u0430\u0446\u0438 \u041f\u0430\u0441\u0441-\u0430 \u0438 \u043d\u0430\u043f\u043e\u0440\u043d\u043e \u0440\u0430\u0434\u0438\u043c\u043e \u043d\u0430 \u0443\u0440\u0430\u0432\u043d\u043e\u0442\u0435\u0436\u0435\u045a\u0443 \u043f\u0440\u0438\u0432\u0430\u0442\u043d\u043e\u0441\u0442\u0438 \u0438 \u0441\u0438\u0433\u0443\u0440\u043d\u043e\u0441\u0442\u0438.",
    "home-main-3": "\u041f\u0440\u0432\u043e \u0431\u0435\u0437\u0431\u0435\u0434\u043d\u043e\u0441\u0442",
    "home-main-3c": "\u0422\u0430\u0447\u043d\u043e\u0441\u0442 \u0458\u0435 \u0443 \u0432\u0430\u0448\u0435\u043c \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u0443 \u0438 \u043d\u0430\u043c\u0430. \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u0458\u0435 \u0444\u043e\u043a\u0443\u0441\u0438\u0440\u0430\u043d \u043d\u0430 \u0438\u0437\u0434\u0430\u0432\u0430\u0447\u0430. \u0420\u0430\u0434\u0438\u043c\u043e \u043d\u0430 \u0442\u043e\u043c\u0435 \u0434\u0430 \u0432\u0430\u043c \u043f\u0440\u0443\u0436\u0438\u043c\u043e \u0441\u0442\u0432\u0430\u0440\u043d\u0435 \u043f\u043e\u0434\u0430\u0442\u043a\u0435 \u0438 \u043f\u0440\u0435\u043f\u0443\u0441\u0442\u0438\u043c\u043e \u0432\u0430\u043c \u0434\u0430 \u043e\u0434\u043b\u0443\u0447\u0438\u0442\u0435 \u043a\u0430\u043a\u043e \u045b\u0435\u0442\u0435 \u0438\u0445 \u043a\u043e\u0440\u0438\u0441\u0442\u0438\u0442\u0438. \u041e\u0441\u0442\u0430\u043b\u0435 \u043a\u0430\u043f\u0438\u0442\u0443\u043b\u0435 \u043f\u0440\u0443\u0436\u0430\u0458\u0443",
    "home-main-3c2": "\u0440\u0435\u043a\u043b\u0430\u043c\u043d\u0435 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0458\u0435. \u0422\u043e \u0437\u043d\u0430\u0447\u0438 \u0434\u0430 \u0441\u0432\u0430\u043a\u0438 \u0431\u043e\u0442 \u043a\u043e\u0458\u0438 \u0431\u043b\u043e\u043a\u0438\u0440\u0430\u0458\u0443 \u043c\u043e\u0436\u0435 \u0441\u043c\u0430\u045a\u0438\u0442\u0438 \u0441\u0432\u043e\u0458 \u043f\u0440\u0438\u0445\u043e\u0434.",
    "home-main-4": "\u0411\u043e\u0459\u0435 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u0447\u043a\u043e \u0438\u0441\u043a\u0443\u0441\u0442\u0432\u043e",
    "home-main-4c": "\u0425\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u043a\u043e\u0440\u0438\u0441\u0442\u0438 \u0441\u043e\u0444\u0438\u0441\u0442\u0438\u0446\u0438\u0440\u0430\u043d\u0435 \u043c\u043e\u0434\u0435\u043b\u0435 \u043c\u0430\u0448\u0438\u043d\u0441\u043a\u043e\u0433 \u0443\u0447\u0435\u045a\u0430 \u0437\u0430 \u043e\u0434\u0440\u0435\u0452\u0438\u0432\u0430\u045a\u0435 \u0447\u043e\u0432\u0435\u0447\u0430\u043d\u0441\u0442\u0432\u0430. \u0422\u043e \u043d\u0430\u043c \u043e\u043c\u043e\u0433\u0443\u045b\u0430\u0432\u0430 \u0434\u0430 \u043e\u0434 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u043a\u0430 \u0437\u0430\u0445\u0442\u0435\u0432\u0430\u043c\u043e \u043c\u0430\u045a\u0435 \u0434\u043e\u0441\u0430\u0434\u043d\u0435 \u0437\u0430\u0434\u0430\u0442\u043a\u0435 \u0438 \u0434\u0430 \u0438\u043c \u043e\u0434\u0443\u0437\u043c\u0435\u043c\u043e \u043c\u0430\u045a\u0435 \u0432\u0440\u0435\u043c\u0435\u043d\u0430, \u0430 \u043f\u0440\u0438\u0442\u043e\u043c \u0458\u043e\u0448 \u0443\u0432\u0435\u043a \u0434\u0430\u0458\u0435\u043c\u043e \u043a\u043e\u0440\u0438\u0441\u043d\u0435 \u043e\u0431\u0435\u043b\u0435\u0436\u0435\u043d\u0435 \u043f\u043e\u0434\u0430\u0442\u043a\u0435 \u0437\u0430 \u043e\u0431\u0435\u043b\u0435\u0436\u0430\u0432\u0430\u045a\u0435 \u043a\u0443\u043f\u0430\u0446\u0430.",
    "home-main-plans": "\u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u0441\u0435 \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u043e \u043a\u043e\u0440\u0438\u0441\u0442\u0438 \u0437\u0430 \u0438\u0437\u0434\u0430\u0432\u0430\u0447\u0435 \u0431\u0438\u043b\u043e \u043a\u043e\u0458\u0435 \u0432\u0435\u043b\u0438\u0447\u0438\u043d\u0435. \u041c\u0435\u0452\u0443\u0442\u0438\u043c, \u0437\u0430 \u043f\u0440\u0435\u0434\u0443\u0437\u0435\u045b\u0430 \u0438 \u043f\u043b\u0430\u0442\u0444\u043e\u0440\u043c\u0435 \u043a\u043e\u0458\u0438\u043c\u0430 \u0458\u0435 \u043f\u043e\u0442\u0440\u0435\u0431\u043d\u0430 \u0432\u0435\u045b\u0430 \u043a\u043e\u043d\u0442\u0440\u043e\u043b\u0430 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u0430 \u0441\u0443 \u043d\u0430\u0448\u0430 \u0440\u0435\u0448\u0435\u045a\u0430 \u0437\u0430 \u043f\u0440\u0435\u0434\u0443\u0437\u0435\u045b\u0430.",
    "home-main-plans2": "\u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u043f\u043e\u0441\u043b\u043e\u0432\u043d\u0430 \u0440\u0435\u0448\u0435\u045a\u0430 \u0448\u0442\u0438\u0442\u0435 \u043c\u0438\u043b\u0438\u043e\u043d\u0435 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u043a\u0430 \u043d\u0430 \u0434\u0435\u0441\u0435\u0442\u0438\u043d\u0435 \u043c\u0438\u043b\u0438\u043e\u043d\u0430 \u0432\u0435\u0431 \u043b\u043e\u043a\u0430\u0446\u0438\u0458\u0430 \u0437\u0430 \u043d\u0435\u043a\u0435 \u043e\u0434 \u043d\u0430\u0458\u0432\u0435\u045b\u0438\u0445 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0458\u0430 \u043d\u0430 \u043f\u043b\u0430\u043d\u0435\u0442\u0438. \u041c\u043e\u043b\u0438\u043c\u043e \u043a\u043e\u043d\u0442\u0430\u043a\u0442\u0438\u0440\u0430\u0458\u0442\u0435 \u043d\u0430\u0441 \u0437\u0430 \u0432\u0438\u0448\u0435 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0458\u0430 \u0438\u043b\u0438 \u043f\u043e\u0441\u0435\u0442\u0438\u0442\u0435",
    "home-main-plans3": "\u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u043f\u043e\u0441\u043b\u043e\u0432\u043d\u0430 \u0440\u0435\u0448\u0435\u045a\u0430 \u0448\u0442\u0438\u0442\u0435 \u043c\u0438\u043b\u0438\u043e\u043d\u0435 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u043a\u0430 \u043d\u0430 \u0434\u0435\u0441\u0435\u0442\u0438\u043d\u0435 \u043c\u0438\u043b\u0438\u043e\u043d\u0430 \u0432\u0435\u0431 \u043b\u043e\u043a\u0430\u0446\u0438\u0458\u0430 \u0437\u0430 \u043d\u0435\u043a\u0435 \u043e\u0434 \u043d\u0430\u0458\u0432\u0435\u045b\u0438\u0445 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0458\u0430 \u043d\u0430 \u043f\u043b\u0430\u043d\u0435\u0442\u0438. \u041c\u043e\u043b\u0438\u043c\u043e \u043a\u043e\u043d\u0442\u0430\u043a\u0442\u0438\u0440\u0430\u0458\u0442\u0435 \u043d\u0430\u0441 \u0437\u0430 \u0432\u0438\u0448\u0435 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0458\u0430 \u0438\u043b\u0438 \u043f\u043e\u0441\u0435\u0442\u0438\u0442\u0435",
    "home-main-plans4": "\u0437\u0430 \u0434\u0435\u0442\u0430\u0459\u0435.",
    "learn-more-hcaptcha": "\u0421\u0430\u0437\u043d\u0430\u0458\u0442\u0435 \u0432\u0438\u0448\u0435 \u043e \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430",
    "page-acc-faq-1": "\u041f\u0438\u0442\u0430\u045a\u0435: \u0414\u0430 \u043b\u0438 \u0458\u0435 \u0443\u0441\u043a\u043b\u0430\u0452\u0435\u043d \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u041e\u0434\u0435\u0459\u0430\u043a 508 + \u0412\u0426\u0410\u0413 2.0? \u041e\u0434\u0433\u043e\u0432\u043e\u0440: \u0412\u0435\u0440\u0443\u0458\u0435\u043c\u043e \u0443 \u0442\u043e: \u0441\u0432\u0438 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u0446\u0438 \u0441\u0430 \u0431\u0438\u043b\u043e \u043a\u043e\u0458\u0438\u043c \u043e\u0431\u043b\u0438\u043a\u043e\u043c \u043e\u0448\u0442\u0435\u045b\u0435\u045a\u0430 \u043a\u043e\u0458\u0438 \u0441\u0443 \u0443 \u043c\u043e\u0433\u0443\u045b\u043d\u043e\u0441\u0442\u0438 \u0434\u0430 \u043f\u0440\u0435\u0433\u043b\u0435\u0434\u0430\u0432\u0430\u0458\u0443 \u0438\u043d\u0442\u0435\u0440\u043d\u0435\u0442 \u0438 \u0443\u043d\u0435\u0441\u0443 \u0442\u0435\u043a\u0441\u0442 \u043d\u0430 \u043e\u0431\u0440\u0430\u0441\u0446\u0435 \u043c\u043e\u0433\u0443 \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0438\u0442\u0438 \u0443\u0441\u043b\u0443\u0433\u0430\u043c\u0430 \u0437\u0430\u0448\u0442\u0438\u045b\u0435\u043d\u0438\u043c \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u043d\u0430\u043a\u043e\u043d \u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0458\u0435. \u041c\u0435\u0452\u0443\u0442\u0438\u043c, \u043e\u0432\u043e \u043d\u0438\u0458\u0435 \u043f\u0440\u0430\u0432\u043d\u0438 \u0441\u0430\u0432\u0435\u0442: \u0442\u0440\u0435\u0431\u0430\u043b\u043e \u0431\u0438 \u0434\u0430 \u0438\u0437\u0432\u0440\u0448\u0438\u0442\u0435 \u0441\u043e\u043f\u0441\u0442\u0432\u0435\u043d\u0443 \u043f\u0440\u043e\u0446\u0435\u043d\u0443, \u0443\u0437\u0438\u043c\u0430\u0458\u0443\u045b\u0438 \u0443 \u043e\u0431\u0437\u0438\u0440 \u0432\u0430\u0448\u0443 \u043a\u043e\u043d\u043a\u0440\u0435\u0442\u043d\u0443 \u0438\u043c\u043f\u043b\u0435\u043c\u0435\u043d\u0442\u0430\u0446\u0438\u0458\u0443 \u043a\u0430\u043a\u043e \u0431\u0438\u0441\u0442\u0435 \u0431\u0438\u043b\u0438 \u0441\u0438\u0433\u0443\u0440\u043d\u0438 \u0434\u0430 \u0458\u0435 \u0442\u043e \u0441\u043b\u0443\u0447\u0430\u0458 \u0441\u0430 \u0432\u0430\u0448\u0438\u043c \u0440\u0430\u0437\u043c\u0435\u0448\u0442\u0430\u045a\u0435\u043c.",
    "page-acc-faq-2": "\u041f\u0438\u0442\u0430\u045a\u0435: \u0414\u0430 \u043b\u0438 \u0440\u0430\u0434\u0438\u0442\u0435 \u043d\u0430 \u0434\u0440\u0443\u0433\u0438\u043c \u043e\u043f\u0446\u0438\u0458\u0430\u043c\u0430 \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0447\u043d\u043e\u0441\u0442\u0438 (\u043011\u0438), \u043f\u043e\u043f\u0443\u0442 \u0437\u0432\u0443\u043a\u0430? \u041e\u0434\u0433\u043e\u0432\u043e\u0440: \u0420\u0430\u043d\u0438\u0458\u0435 \u043f\u043e\u043f\u0443\u043b\u0430\u0440\u043d\u0435 \u043e\u043f\u0446\u0438\u0458\u0435 \u043a\u0430\u043e \u0448\u0442\u043e \u0441\u0443 \u0430\u0443\u0434\u0438\u043e \u0446\u0430\u043f\u0442\u0446\u0445\u0430 \u0434\u0438\u0441\u043a\u0440\u0438\u043c\u0438\u043d\u0438\u0448\u0443 \u043c\u043d\u043e\u0433\u0435 \u043011\u0438 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u043a\u0435 \u0438 \u043b\u0430\u043a\u043e \u0438\u0445 \u043f\u043e\u0431\u0438\u0458\u0430\u0458\u0443 \u043c\u043e\u0434\u0435\u0440\u043d\u0435 \u0442\u0435\u0445\u043d\u0438\u043a\u0435 \u043c\u0430\u0448\u0438\u043d\u0441\u043a\u043e\u0433 \u0443\u0447\u0435\u045a\u0430. \u041e\u0432\u043e \u0458\u0435 \u043f\u0440\u0438\u0441\u0438\u043b\u0438\u043b\u043e \u0442\u0440\u0435\u043d\u0443\u0442\u043d\u0435 \u0430\u0443\u0434\u0438\u043e \u0438\u0437\u0430\u0437\u043e\u0432\u0435 \u0434\u0430 \u043f\u043e\u0441\u0442\u0430\u043d\u0443 \u0441\u0432\u0435 \u0442\u0435\u0436\u0438, \u0443\u0432\u043e\u0434\u0435\u045b\u0438 \u0431\u0443\u043a\u0443, \u0447\u0443\u0434\u043d\u043e \u0432\u0440\u0435\u043c\u0435, \u043d\u0435\u043e\u0431\u0438\u0447\u043d\u0435 \u043a\u043e\u043c\u0431\u0438\u043d\u0430\u0446\u0438\u0458\u0435 \u0440\u0435\u0447\u0438 \u0438 \u0442\u0430\u043a\u043e \u0434\u0430\u0459\u0435, \u0434\u0430 \u043f\u043e\u0440\u0430\u0437\u0435 \u043d\u0430\u043f\u0430\u0434\u0430\u0447\u0435. \u0421\u0442\u043e\u0433\u0430 \u0441\u043c\u043e \u043c\u0430\u045a\u0435 \u0435\u043d\u0442\u0443\u0437\u0438\u0458\u0430\u0441\u0442\u0438\u0447\u043d\u0438 \u0437\u0430 \u043e\u0432\u0430\u0458 \u043f\u0440\u0438\u0441\u0442\u0443\u043f \u043d\u0430\u0441\u0443\u043f\u0440\u043e\u0442 \u0442\u043e\u043c\u0435 \u0434\u0430 \u0443 \u043f\u043e\u0442\u043f\u0443\u043d\u043e\u0441\u0442\u0438 \u0438\u0437\u0431\u0435\u0433\u0430\u0432\u0430\u043c\u043e \u0438\u0437\u0430\u0437\u043e\u0432, \u0430\u043b\u0438 \u0440\u0430\u0437\u043c\u043e\u0442\u0440\u0438\u045b\u0435\u043c\u043e \u0433\u0430 \u0443\u043a\u043e\u043b\u0438\u043a\u043e \u043f\u043e\u0441\u0442\u043e\u0458\u0438 \u043f\u043e\u0442\u0440\u0430\u0436\u045a\u0430 \u043e\u0434 \u0437\u0430\u0458\u0435\u0434\u043d\u0438\u0446\u0435 \u041011\u0438. \u041c\u0435\u0452\u0443\u0442\u0438\u043c, \u0437\u0430\u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043e\u0432\u0430\u043d\u0438 \u0441\u043c\u043e \u0437\u0430 \u043f\u0440\u0438\u0432\u0430\u0442\u043d\u043e\u0441\u0442 \u0437\u0430 \u0441\u043b\u0443\u0447\u0430\u0458 \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0447\u043d\u043e\u0441\u0442\u0438. \u0412\u0458\u0435\u0440\u0443\u0458\u0435\u043c\u043e \u0434\u0430 \u045b\u0435 \u043a\u043e\u043c\u0431\u0438\u043d\u043e\u0432\u0430\u045a\u0435 \u043d\u0430\u0448\u0435\u0433 \u0442\u0440\u0435\u043d\u0443\u0442\u043d\u043e\u0433 \u043011\u0438 \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430 \u0441\u0430 \u0438\u0437\u0434\u0430\u0432\u0430\u045a\u0435\u043c \u041f\u0440\u0438\u0432\u0430\u0446\u0438 \u041f\u0430\u0441\u0441-\u0430 \u043e\u043c\u043e\u0433\u0443\u045b\u0438\u0442\u0438 \u043011\u0438 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u0446\u0438\u043c\u0430 \u0434\u0430 \u0431\u0435\u0437\u0431\u0435\u0434\u043d\u043e \u043f\u0440\u0435\u0433\u043b\u0435\u0434\u0430\u0432\u0430\u0458\u0443, \u0441\u0438\u0433\u0443\u0440\u043d\u0438 \u0443 \u0441\u0430\u0437\u043d\u0430\u045a\u0435 \u0434\u0430 \u0458\u0435 \u045a\u0438\u0445\u043e\u0432 \u0441\u0430\u043e\u0431\u0440\u0430\u045b\u0430\u0458 \u043f\u0440\u0438\u0432\u0430\u0442\u043d\u0438\u0458\u0438, \u0434\u043e\u043a \u043e\u0433\u0440\u0430\u043d\u0438\u0447\u0430\u0432\u0430 \u0437\u043b\u043e\u0443\u043f\u043e\u0442\u0440\u0435\u0431\u0443 \u043e\u0434 \u0441\u0442\u0440\u0430\u043d\u0435 \u0431\u043e\u0442 \u043e\u043f\u0435\u0440\u0430\u0442\u043e\u0440\u0430 \u043a\u043e\u0458\u0430 \u0441\u0435 \u043d\u0435\u043c\u0438\u043d\u043e\u0432\u043d\u043e \u0434\u043e\u0433\u0430\u0452\u0430 \u043a\u0430\u0434\u0430 \u0441\u0443 \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u0435 \u043011\u0438 \u043e\u043f\u0446\u0438\u0458\u0435. \u041c\u0438 \u0441\u043c\u043e \u0430\u043a\u0442\u0438\u0432\u043d\u0438 \u0443\u0447\u0435\u0441\u043d\u0438\u0446\u0438 \u0418\u0415\u0422\u0424 \u0440\u0430\u0434\u043d\u0435 \u0433\u0440\u0443\u043f\u0435 \u043a\u043e\u0458\u0430 \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0434\u0438\u0437\u0438\u0440\u0430 \u043e\u0432\u0443 \u043d\u043e\u0432\u0443 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u0458\u0443.",
    "page-acc-faq-3": "\u041f\u0438\u0442\u0430\u045a\u0435: \u0428\u0442\u0430 \u0458\u0435 \u0441\u0430 \u043f\u0440\u0438\u0432\u0430\u0442\u043d\u043e\u0448\u045b\u0443? \u0414\u0430 \u043b\u0438 \u0441\u0435 \u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0458\u0430 \u043d\u0430 \u043d\u0435\u043a\u0438 \u043d\u0430\u0447\u0438\u043d \u043e\u0442\u043a\u0440\u0438\u0432\u0430 \u043d\u0430 11 \u043f\u043e\u0434\u0430\u0442\u0430\u043a\u0430 \u043e \u043f\u0440\u0435\u0433\u043b\u0435\u0434\u0430\u0432\u0430\u045a\u0443? \u0428\u0442\u0430 \u0440\u0430\u0434\u0438\u0442\u0435 \u0441\u0430 \u0435-\u043c\u0430\u0438\u043b\u043e\u043c? \u041e\u0434\u0433\u043e\u0432\u043e\u0440: \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u0458\u0435 \u043e\u0441\u043c\u0438\u0448\u0459\u0435\u043d\u0430 \u0437\u0430 \u043f\u0440\u0438\u0432\u0430\u0442\u043d\u043e\u0441\u0442 \u043e\u0434 \u043f\u043e\u0447\u0435\u0442\u043a\u0430. \u0412\u0440\u043b\u043e \u0441\u0435 \u0440\u0430\u0437\u043b\u0438\u043a\u0443\u0458\u0435 \u043e\u0434 \u0442\u0440\u0430\u0434\u0438\u0446\u0438\u043e\u043d\u0430\u043b\u043d\u0438\u0445 \u043e\u043f\u0446\u0438\u0458\u0430 \u043f\u043e\u043f\u0443\u0442 \u0440\u0435\u0426\u0410\u041f\u0422\u0426\u0425\u0410 \u043a\u043e\u0458\u0435 \u0441\u0443 \u0443 \u0432\u043b\u0430\u0441\u043d\u0438\u0448\u0442\u0432\u0443 \u0440\u0435\u043a\u043b\u0430\u043c\u043d\u0438\u0445 \u043c\u0440\u0435\u0436\u0430, \u043a\u043e\u0458\u0435 \u0438\u043c\u0430\u0458\u0443 \u043f\u043e\u0434\u0441\u0442\u0438\u0446\u0430\u0458 \u0434\u0430 \u0432\u0430\u0441 \u043f\u0440\u0430\u0442\u0435 \u043f\u043e \u0432\u0435\u0431\u0443 \u0438 \u043f\u043e\u0432\u0435\u0437\u0443\u0458\u0443 \u0432\u0430\u0441 \u0441\u0430 \u0441\u0442\u0432\u0430\u0440\u043d\u0438\u043c \u0438\u0434\u0435\u043d\u0442\u0438\u0442\u0435\u0442\u043e\u043c. \u041d\u0438\u043a\u0430\u0434\u0430 \u043d\u0435 \u043a\u043e\u0440\u0438\u0441\u0442\u0438\u043c\u043e \u0435-\u043f\u043e\u0448\u0442\u0443 \u0438\u043b\u0438 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0458\u0435 \u043e \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0447\u043d\u043e\u0441\u0442\u0438 \u0443 \u0431\u0438\u043b\u043e \u043a\u043e\u0458\u0435 \u0434\u0440\u0443\u0433\u0435 \u0441\u0432\u0440\u0445\u0435 \u043e\u0441\u0438\u043c \u0437\u0430 \u043e\u043b\u0430\u043a\u0448\u0430\u0432\u0430\u045a\u0435 \u0443\u043f\u043e\u0442\u0440\u0435\u0431\u0435 \u0438 \u0441\u043f\u0440\u0435\u0447\u0430\u0432\u0430\u045a\u0435 \u0437\u043b\u043e\u0443\u043f\u043e\u0442\u0440\u0435\u0431\u0435. \u041d\u0430\u0448\u0430 \u043f\u043e\u043b\u0438\u0442\u0438\u043a\u0430 \u043f\u0440\u0438\u0432\u0430\u0442\u043d\u043e\u0441\u0442\u0438 \u0441\u0430\u0434\u0440\u0436\u0438 \u0441\u0432\u0435\u043e\u0431\u0443\u0445\u0432\u0430\u0442\u043d\u0435 \u0438 \u0430\u0443\u0442\u043e\u0440\u0438\u0442\u0430\u0442\u0438\u0432\u043d\u0435 \u043e\u0434\u0433\u043e\u0432\u043e\u0440\u0435 \u043e \u0442\u043e\u043c\u0435 \u043a\u0430\u043a\u043e \u043a\u043e\u0440\u0438\u0441\u0442\u0438\u043c\u043e \u043f\u043e\u0434\u0430\u0442\u043a\u0435, \u0430\u043b\u0438 \u043a\u0440\u0430\u0442\u0430\u043a \u043e\u0434\u0433\u043e\u0432\u043e\u0440 \u0458\u0435 \u0434\u0430 \u043d\u0435\u043c\u0430\u043c\u043e \u0438\u043d\u0442\u0435\u0440\u0435\u0441 \u0434\u0430 \u0432\u0430\u0441 \u043f\u043e\u0432\u0435\u0436\u0435\u043c\u043e \u043a\u0430\u043e \u043e\u0441\u043e\u0431\u0443 \u0441\u0430 \u0432\u0430\u0448\u043e\u043c \u0438\u0441\u0442\u043e\u0440\u0438\u0458\u043e\u043c \u043f\u0440\u0435\u0433\u043b\u0435\u0434\u0430\u0432\u0430\u045a\u0430. \u0422\u0440\u0435\u043d\u0443\u0442\u043d\u043e \u0442\u0430\u043a\u043e\u0452\u0435 \u0440\u0430\u0434\u0438\u043c\u043e \u043d\u0430 \u043a\u0440\u0438\u043f\u0442\u043e\u0433\u0440\u0430\u0444\u0441\u043a\u043e\u043c \u0440\u0435\u0448\u0435\u045a\u0443 \u0437\u0430 \u0431\u0440\u0437\u043e \u043e\u0434\u0431\u0430\u0446\u0438\u0432\u0430\u045a\u0435 \u0430\u0434\u0440\u0435\u0441\u0435 \u0435-\u043f\u043e\u0448\u0442\u0435. \u0434\u043e\u043a \u0458\u043e\u0448 \u0443\u0432\u0435\u043a \u0447\u0443\u0432\u0430\u043c\u043e \u043d\u0430\u0448\u0443 \u0441\u043f\u043e\u0441\u043e\u0431\u043d\u043e\u0441\u0442 \u0434\u0430 \u0441\u043f\u0440\u0435\u0447\u0438\u043c\u043e \u0437\u043b\u043e\u0443\u043f\u043e\u0442\u0440\u0435\u0431\u0443 \u0434\u043e\u043f\u0443\u045a\u0443\u0458\u0443\u045b\u0438 \u043d\u0430\u0448 \u0440\u0430\u0434 \u043e \u043f\u0440\u0438\u0432\u0430\u0442\u043d\u043e\u0441\u0442\u0438.",
    "page-acc-link-1": "\u0410\u043a\u043e \u0441\u0442\u0435 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u043a \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0447\u043d\u043e\u0441\u0442\u0438, \u043f\u0440\u0438\u0458\u0430\u0432\u0438\u0442\u0435 \u0441\u0435 \u043e\u0432\u0434\u0435.",
    "page-acc-subhead": "\u041e\u0431\u0435\u0437\u0431\u0435\u0452\u0438\u0432\u0430\u045a\u0435 \u043d\u0435\u043e\u043c\u0435\u0442\u0430\u043d\u043e\u0433 \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430 \u0437\u0430 \u0441\u0432\u0435 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u043a\u0435 \u0443\u0437 \u043e\u0434\u0440\u0436\u0430\u0432\u0430\u045a\u0435 \u0431\u0435\u0437\u0431\u0435\u0434\u043d\u043e\u0441\u0442\u0438",
    "page-acc-summary": "\u0420\u0435\u0437\u0438\u043c\u0435",
    "page-acc-summary-contents-1": "\u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u0458\u0435 \u0434\u0438\u0437\u0430\u0458\u043d\u0438\u0440\u0430\u043d\u0430 \u0437\u0430 \u0437\u0430\u0443\u0441\u0442\u0430\u0432\u0459\u0430\u045a\u0435 \u0431\u043e\u0442\u043e\u0432\u0430 \u0440\u0430\u0437\u043b\u0438\u043a\u0443\u0458\u0443\u045b\u0438 \u0438\u0445 \u043e\u0434 \u0459\u0443\u0434\u0438. \u0412\u0438\u0437\u0443\u0435\u043b\u043d\u0438 \u0442\u0435\u0441\u0442\u043e\u0432\u0438 \u0441\u0443 \u043f\u043e\u0433\u043e\u0434\u0430\u043d \u0430\u043b\u0430\u0442 \u0437\u0430 \u0442\u043e, \u0430\u043b\u0438 \u043d\u0435 \u043c\u043e\u0436\u0435 \u0441\u0432\u0430\u043a\u043e \u0434\u0430 \u0440\u0435\u0448\u0438 \u0432\u0438\u0437\u0443\u0435\u043b\u043d\u0438 \u0438\u0437\u0430\u0437\u043e\u0432. \u0418\u0437 \u0442\u043e\u0433 \u0440\u0430\u0437\u043b\u043e\u0433\u0430 \u0441\u043c\u043e \u0434\u0438\u0437\u0430\u0458\u043d\u0438\u0440\u0430\u043b\u0438 \u0458\u0435\u0434\u043d\u043e\u0441\u0442\u0430\u0432\u043d\u0443, \u0431\u0435\u0437\u0431\u043e\u043b\u043d\u0443 \u0430\u043b\u0442\u0435\u0440\u043d\u0430\u0442\u0438\u0432\u0443 \u043a\u043e\u0458\u043e\u043c \u0438\u0437\u0434\u0430\u0432\u0430\u0447\u0438\u043c\u0430 \u043a\u043e\u0458\u0438 \u043a\u043e\u0440\u0438\u0441\u0442\u0435 \u043d\u0430\u0448 \u0441\u0435\u0440\u0432\u0438\u0441 \u0441\u0430\u0447\u0443\u0432\u0430\u0458\u0443 \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0447\u043d\u043e\u0441\u0442 \u0437\u0430 \u0441\u0432\u0435, \u0443 \u043f\u043e\u0442\u043f\u0443\u043d\u043e\u0441\u0442\u0438 \u0441\u0430\u0433\u043b\u0430\u0441\u043d\u043e\u0441\u0442\u0438 \u0441\u0430 \u043e\u0434\u0435\u0459\u043a\u043e\u043c 508 \u0438 \u0412\u0426\u0410\u0413 2.0.",
    "page-acc-summary-contents-2": "\u041a\u0430\u043a\u043e \u0442\u043e \u0444\u0443\u043d\u043a\u0446\u0438\u043e\u043d\u0438\u0448\u0435: \u043f\u0440\u0432\u043e, \u043d\u0430 \u043e\u0432\u043e\u0458 \u0423\u0420\u041b \u0430\u0434\u0440\u0435\u0441\u0438 \u043f\u0440\u0438\u0458\u0430\u0432\u0459\u0443\u0458\u0435 \u0441\u0435 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u043a \u0437\u0430 \u043f\u0440\u0438\u0441\u0442\u0443\u043f \u043a\u043e\u0458\u0438 \u0458\u0435 \u043f\u043e\u0432\u0435\u0437\u0430\u043d \u043d\u0430 \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u0438\u043d\u0444\u043e \u0441\u0442\u0440\u0430\u043d\u0438\u0446\u0438 \u0432\u0438\u0434\u0433\u0435\u0442-\u0430. \u0414\u043e\u0431\u0438\u0432\u0430\u0458\u0443 \u0448\u0438\u0444\u0440\u0438\u0440\u0430\u043d\u0438 \u043a\u043e\u043b\u0430\u0447\u0438\u045b \u043a\u043e\u0458\u0438 \u0441\u0435 \u043c\u043e\u0433\u0443 \u043a\u043e\u0440\u0438\u0441\u0442\u0438\u0442\u0438 \u043d\u0435\u043a\u043e\u043b\u0438\u043a\u043e \u043f\u0443\u0442\u0430 \u0434\u043d\u0435\u0432\u043d\u043e, \u0430\u043b\u0438 \u043c\u043e\u0440\u0430\u0458\u0443 \u0441\u0435 \u043e\u0441\u0432\u0458\u0435\u0436\u0430\u0432\u0430\u0442\u0438 \u0441\u0432\u0430\u043a\u0430 24 \u0441\u0430\u0442\u0430 \u043f\u0443\u0442\u0435\u043c \u043f\u0440\u0438\u0458\u0430\u0432\u0435. \u041a\u0430\u0434\u0430 \u0441\u0435 \u0438\u0437\u0430\u0437\u043e\u0432 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u0438 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u043a\u0443 \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0447\u043d\u043e\u0441\u0442\u0438 \u043d\u0430 \u0431\u0438\u043b\u043e \u043a\u043e\u0458\u0435\u043c \u0432\u0435\u0431 \u043c\u0458\u0435\u0441\u0442\u0443 \u043f\u043e\u043c\u043e\u045b\u0443 \u0443\u0441\u043b\u0443\u0433\u0435 \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430, \u0430\u0443\u0442\u043e\u043c\u0430\u0442\u0441\u043a\u0438 \u045b\u0435 \u043f\u0440\u043e\u045b\u0438.",
    "page-acc-title": "\u0418\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0458\u0435 \u043e \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0447\u043d\u043e\u0441\u0442\u0438 \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430",
    "page-acc-tr-body": "\u041f\u0438\u0442\u0430\u045a\u0435: \u041d\u0430\u043a\u043e\u043d \u043f\u043e\u0441\u0442\u0430\u0432\u0459\u0430\u045a\u0430 \u043a\u043e\u043b\u0430\u0447\u0438\u045b\u0430 \u0438 \u0434\u0430\u0459\u0435 \u0432\u0438\u0434\u0438\u043c \u0438\u0437\u0430\u0437\u043e\u0432. \u0428\u0442\u0430 \u0442\u043e \u0443\u0437\u0440\u043e\u043a\u0443\u0458\u0435? \u041e\u0434\u0433\u043e\u0432\u043e\u0440: \u0422\u043e \u0441\u0435 \u043e\u0431\u0438\u0447\u043d\u043e \u0434\u0435\u0448\u0430\u0432\u0430 \u0443\u043f\u043e\u0442\u0440\u0435\u0431\u043e\u043c \u0430\u0433\u0440\u0435\u0441\u0438\u0432\u043d\u043e\u0433 \u0431\u043b\u043e\u043a\u0430\u0442\u043e\u0440\u0430 \u043e\u0433\u043b\u0430\u0441\u0430 \u0438\u043b\u0438 \u043f\u0440\u043e\u0448\u0438\u0440\u0435\u045a\u0430 \u043a\u043e\u043b\u0430\u0447\u0438\u045b\u0430 \u0438\u043b\u0438 \u043f\u043e\u0441\u0442\u0430\u0432\u043a\u0435 \u043a\u043e\u0458\u0430 \u0431\u043b\u043e\u043a\u0438\u0440\u0430 \u043a\u043e\u043b\u0430\u0447\u0438\u045b\u0435 \u043d\u0430 \u0440\u0430\u0437\u043b\u0438\u0447\u0438\u0442\u0438\u043c \u043b\u043e\u043a\u0430\u0446\u0438\u0458\u0430\u043c\u0430, \u0443 \u043e\u0432\u043e\u043c \u0441\u043b\u0443\u0447\u0430\u0458\u0443 \u043a\u043e\u043b\u0430\u0447\u0438\u045b \u0437\u0430 \u0445\u0446\u0430\u043f\u0442\u0446\u0445\u0430.\u0446\u043e\u043c \u043a\u043e\u0458\u0438 \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u0408\u0430\u0432\u0430\u0421\u0446\u0440\u0438\u043f\u0442 \u043f\u043e\u0441\u0442\u0430\u0432\u0459\u0430 \u0438\u043b\u0438 \u043f\u0440\u043e\u0432\u0458\u0435\u0440\u0430\u0432\u0430 \u043d\u0430 \u0434\u0440\u0443\u0433\u043e\u0458 \u0432\u0435\u0431 \u043b\u043e\u043a\u0430\u0446\u0438\u0458\u0438 , \u043f\u043e\u043f\u0443\u0442 \u043e\u043d\u0435 \u043a\u043e\u0458\u0443 \u043f\u043e\u0441\u0435\u045b\u0443\u0458\u0435\u0442\u0435. \u041a\u043e\u043b\u0430\u0447\u0438\u045b\u0438 \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0447\u043d\u043e\u0441\u0442\u0438 \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u0440\u0430\u0434\u0435 \u0441\u0430 \u0441\u0432\u0438\u043c \u043f\u043e\u043f\u0443\u043b\u0430\u0440\u043d\u0438\u043c \u043f\u0440\u0435\u0433\u043b\u0435\u0434\u0430\u0447\u0438\u043c\u0430 \u0438 \u0431\u043b\u043e\u043a\u0430\u0442\u043e\u0440\u0438\u043c\u0430 \u043e\u0433\u043b\u0430\u0441\u0430 \u0441\u0430 \u045a\u0438\u0445\u043e\u0432\u0438\u043c \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0434\u043d\u0438\u043c \u043f\u043e\u0434\u0435\u0448\u0430\u0432\u0430\u045a\u0438\u043c\u0430, \u0442\u0430\u043a\u043e \u0434\u0430 \u043e\u0431\u0438\u0447\u043d\u043e \u043a\u0432\u0430\u0440\u043e\u0432\u0438 \u043d\u0430\u0441\u0442\u0430\u0458\u0443 \u0437\u0431\u043e\u0433 \u0441\u043a\u0440\u0438\u043f\u0442\u0438 \u0430\u043d\u0442\u0438-\u0430\u0434\u0431\u043b\u043e\u0446\u043a \u0438\u043b\u0438 \u0441\u043b\u0438\u0447\u043d\u0438\u0445 \u0441\u043a\u0443\u043f\u043e\u0432\u0430 \u043f\u0440\u0430\u0432\u0438\u043b\u0430 \u043a\u043e\u0458\u0438 \u0446\u0438\u0459\u0430\u0458\u0443 \u043e\u0434\u0440\u0435\u0452\u0435\u043d\u0435 \u0432\u0435\u0431 \u043b\u043e\u043a\u0430\u0446\u0438\u0458\u0435. \u0420\u0435\u0448\u0435\u045a\u0430: 1. \u0412\u0445\u0438\u0442\u0435\u043b\u0438\u0441\u0442 \u043a\u043e\u043b\u0430\u0447\u0438\u045b\u0438 \u0445\u0446\u0430\u043f\u0442\u0446\u0445\u0430.\u0446\u043e\u043c \u0438 * .\u0445\u0446\u0430\u043f\u0442\u0446\u0445\u0430.\u0446\u043e\u043c \u0443 \u0431\u043b\u043e\u043a\u0430\u0442\u043e\u0440\u0443 \u043e\u0433\u043b\u0430\u0441\u0430 \u0438\u043b\u0438 \u0431\u0435\u0437\u0431\u0435\u0434\u043d\u043e\u0441\u043d\u043e\u043c \u043f\u0440\u043e\u0448\u0438\u0440\u0435\u045a\u0443 \u043f\u0440\u0435\u0433\u043b\u0435\u0434\u0430\u0447\u0430. 2. \u0410\u043a\u043e \u043a\u043e\u0440\u0438\u0441\u0442\u0438\u0442\u0435 \u043f\u0440\u0435\u0442\u0440\u0430\u0436\u0438\u0432\u0430\u0447 \u0411\u0440\u0430\u0432\u0435, \u0437\u0430 \u043a\u043e\u0458\u0438 \u0441\u0435 \u0447\u0438\u043d\u0438 \u0434\u0430 (\u043e\u0434 \u0430\u043f\u0440\u0438\u043b\u0430 2020. \u0433\u043e\u0434\u0438\u043d\u0435) \u043d\u0435\u043c\u0430 \u0431\u0438\u043b\u043e \u043a\u0430\u043a\u0430\u0432 \u0441\u043f\u0438\u0441\u0430\u043a \u043a\u043e\u043b\u0430\u0447\u0438\u045b\u0430, \u0438\u0434\u0438\u0442\u0435 \u043d\u0430 \u041f\u043e\u0441\u0442\u0430\u0432\u043a\u0435 -> \u0428\u0442\u0438\u0442\u043d\u0438\u0446\u0438 -> \u041a\u043e\u043b\u0430\u0447\u0438\u045b\u0438 \u0438 \u043e\u0434\u0430\u0431\u0435\u0440\u0438\u0442\u0435 \u0414\u043e\u0437\u0432\u043e\u043b\u0438 \u0441\u0432\u0435 \u043a\u043e\u043b\u0430\u0447\u0438\u045b\u0435. 3. \u0410\u043a\u043e \u043a\u043e\u0440\u0438\u0441\u0442\u0438\u0442\u0435 \u043d\u0430\u0458\u043d\u043e\u0432\u0438\u0458\u0443 \u0432\u0435\u0440\u0437\u0438\u0458\u0443 \u0421\u0430\u0444\u0430\u0440\u0438\u0458\u0430 \u043d\u0430 \u043d\u0435\u0434\u0430\u0432\u043d\u043e \u0438\u0437\u0434\u0430\u043d\u043e\u043c \u041e\u0421 \u041a\u0441 10.15 \u0438\u043b\u0438 \u0438\u041e\u0421 13. 4, \u0410\u043f\u043f\u043b\u0435 \u0458\u0435 \u0443\u043f\u0440\u0430\u0432\u043e \u043f\u0440\u043e\u043c\u0435\u043d\u0438\u043e \u043f\u043e\u043d\u0430\u0448\u0430\u045a\u0435 \u0421\u0430\u0444\u0430\u0440\u0438\u0458\u0430 \u0443 \u0432\u0435\u0437\u0438 \u0441\u0430 \u043a\u043e\u043b\u0430\u0447\u0438\u045b\u0438\u043c\u0430 \u0442\u0440\u0435\u045b\u0438\u0445 \u0441\u0442\u0440\u0430\u043d\u0430, \u0431\u043b\u043e\u043a\u0438\u0440\u0430\u0458\u0443\u045b\u0438 \u0438\u0445 \u0441\u0432\u0435 \u043f\u043e \u0434\u0435\u0444\u0430\u0443\u043b\u0442\u0443. \u0418\u043c\u043f\u043b\u0435\u043c\u0435\u043d\u0442\u0438\u0440\u0430\u043c\u043e \u0440\u0435\u0448\u0435\u045a\u0435, \u0430\u043b\u0438 \u0443 \u043c\u0435\u0452\u0443\u0432\u0440\u0435\u043c\u0435\u043d\u0443 \u043f\u043e\u0441\u0435\u0442\u0438\u0442\u0435 \u043f\u043e\u0434\u0435\u0448\u0430\u0432\u0430\u045a\u0430 \u0421\u0430\u0444\u0430\u0440\u0438\u0458\u0430, \u043f\u0440\u0438\u0432\u0430\u0442\u043d\u043e\u0441\u0442 \u0438 \u0443\u043a\u043b\u043e\u043d\u0438\u0442\u0435 \u043f\u043e\u0442\u0432\u0440\u0434\u043d\u0443 \u043e\u043f\u0446\u0438\u0458\u0443 \u043f\u0440\u0430\u045b\u0435\u045a\u0430 \u0432\u0435\u0431 \u043b\u043e\u043a\u0430\u0446\u0438\u0458\u0430: \u0441\u043f\u0440\u0435\u0447\u0438\u0442\u0435 \u043f\u0440\u0430\u045b\u0435\u045a\u0435 \u0432\u0435\u0431 \u043b\u043e\u043a\u0430\u0446\u0438\u0458\u0430 \u0434\u0430 \u0431\u0438 \u043a\u043e\u043b\u0430\u0447\u0438\u045b \u0437\u0430 \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0447\u043d\u043e\u0441\u0442 \u043c\u043e\u0433\u0430\u043e \u0434\u0430 \u0444\u0443\u043d\u043a\u0446\u0438\u043e\u043d\u0438\u0448\u0435 \u043e\u043d\u0430\u043a\u043e \u043a\u0430\u043a\u043e \u0441\u0435 \u043e\u0447\u0435\u043a\u0443\u0458\u0435. \u041f\u0438\u0442\u0430\u045a\u0435: \u041a\u043e\u0440\u0438\u0441\u0442\u0438\u043c \u0432\u0438\u0448\u0435 \u0443\u0440\u0435\u0452\u0430\u0458\u0430. \u0414\u0430 \u043b\u0438 \u0458\u0435 \u043f\u043e\u0442\u0440\u0435\u0431\u043d\u043e \u0434\u0430 \u0441\u0435 \u043f\u0440\u0438\u0458\u0430\u0432\u0438\u043c \u0432\u0438\u0448\u0435 \u043f\u0443\u0442\u0430? \u041e\u0434\u0433\u043e\u0432\u043e\u0440: \u041d\u0435. \u041a\u043b\u0438\u043a\u043d\u0438\u0442\u0435 \u0438\u0441\u0442\u0443 \u0432\u0435\u0437\u0443 \u0437\u0430 \u043f\u0440\u0438\u0458\u0430\u0432\u0443 \u043f\u0443\u0442\u0435\u043c \u0435-\u043f\u043e\u0448\u0442\u0435 \u043a\u043e\u0458\u0430 \u0432\u0430\u043c \u0458\u0435 \u043f\u043e\u0441\u043b\u0430\u043d\u0430 \u043d\u0430 \u0441\u0432\u0430\u043a\u043e\u043c \u0443\u0440\u0435\u0452\u0430\u0458\u0443 \u043a\u043e\u0458\u0438 \u043a\u043e\u0440\u0438\u0441\u0442\u0438\u0442\u0435 \u0434\u0430 \u0431\u0438\u0441\u0442\u0435 \u043f\u043e\u0441\u0442\u0430\u0432\u0438\u043b\u0438 \u043a\u043e\u043b\u0430\u0447\u0438\u045b.",
    "page-acc-tr-title": "\u0417\u0430 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u043a\u0435 \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0447\u043d\u043e\u0441\u0442\u0438: \u0412\u043e\u0434\u0438\u0447 \u0437\u0430 \u043f\u0438\u0442\u0430\u045a\u0430 \u0438 \u0440\u0435\u0448\u0430\u0432\u0430\u045a\u0435 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u0430",
    "priv-hc": "\u041f\u0440\u0438\u0432\u0430\u0442\u043d\u043e\u0441\u0442 \u0444\u043e\u043a\u0443\u0441\u0438\u0440\u0430\u043d\u0430",
    "priv-hc-text": "\u041e\u0433\u043b\u0430\u0441\u043d\u0435 \u043c\u0440\u0435\u0436\u0435 \u0432\u0438\u0434\u0435 \u0432\u0430\u0441 \u043a\u0430\u043e \u045a\u0438\u0445\u043e\u0432 \u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434. \u041c\u0438 \u043d\u0435. \u041d\u0435 \u0437\u0430\u043d\u0438\u043c\u0430 \u043d\u0430\u0441 \u043a\u043e \u0441\u0443 \u043a\u043e\u0440\u0438\u0441\u043d\u0438\u0446\u0438, \u0432\u0435\u045b \u0441\u0430\u043c\u043e \u043a\u043e\u043b\u0438\u043a\u043e \u045b\u0435 \u043e\u043d\u0438 \u0431\u0438\u0442\u0438 \u0437\u043b\u043e\u043d\u0430\u043c\u0435\u0440\u043d\u0438. \u041f\u043e\u0434\u0440\u0436\u0430\u0432\u0430\u043c\u043e \u0438\u043d\u0438\u0446\u0438\u0458\u0430\u0442\u0438\u0432\u0435 \u043f\u043e\u043f\u0443\u0442 \u041f\u0440\u0438\u0432\u0430\u0446\u0438 \u041f\u0430\u0441\u0441-\u0430 \u0438 \u043d\u0430\u043f\u043e\u0440\u043d\u043e \u0440\u0430\u0434\u0438\u043c\u043e \u043d\u0430 \u0443\u0440\u0430\u0432\u043d\u043e\u0442\u0435\u0436\u0435\u045a\u0443 \u043f\u0440\u0438\u0432\u0430\u0442\u043d\u043e\u0441\u0442\u0438 \u0438 \u0441\u0438\u0433\u0443\u0440\u043d\u043e\u0441\u0442\u0438.",
    "sec-hc": "\u041f\u0440\u0432\u043e \u0431\u0435\u0437\u0431\u0435\u0434\u043d\u043e\u0441\u0442",
    "sec-hc-text": "\u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u0458\u0435 \u0444\u043e\u043a\u0443\u0441\u0438\u0440\u0430\u043d \u043d\u0430 \u0438\u0437\u0434\u0430\u0432\u0430\u0447\u0430. \u0420\u0430\u0434\u0438\u043c\u043e \u043d\u0430 \u0442\u043e\u043c\u0435 \u0434\u0430 \u0432\u0435\u0431 \u043b\u043e\u043a\u0430\u0446\u0438\u0458\u0430\u043c\u0430 \u043f\u0440\u0443\u0436\u0438\u043c\u043e \u0441\u0442\u0432\u0430\u0440\u043d\u0435 \u043f\u043e\u0434\u0430\u0442\u043a\u0435 \u0438 \u043f\u0443\u0441\u0442\u0438\u043c\u043e \u0438\u0445 \u0434\u0430 \u043e\u0434\u043b\u0443\u0447\u0435 \u043a\u0430\u043a\u043e \u0434\u0430 \u0438\u0445 \u043a\u043e\u0440\u0438\u0441\u0442\u0435. \u0422\u0430\u0447\u043d\u043e\u0441\u0442 \u0458\u0435 \u0443 \u0432\u0430\u0448\u0435\u043c \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u0443 \u0438 \u043d\u0430\u043c\u0430. \u041e\u0441\u0442\u0430\u043b\u0435 \u043a\u0430\u043f\u0442\u043e\u043b\u0435 \u043f\u0440\u0443\u0436\u0430\u0458\u0443 \u0440\u0435\u043a\u043b\u0430\u043c\u043d\u0435 \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u0458\u0435. \u0421\u0432\u0430\u043a\u0438 \u0431\u043e\u0442 \u043a\u043e\u0458\u0438 \u0431\u043b\u043e\u043a\u0438\u0440\u0430\u0458\u0443 \u043c\u043e\u0436\u0435 \u0441\u043c\u0430\u045a\u0438\u0442\u0438 \u0441\u0432\u043e\u0458 \u043f\u0440\u0438\u0445\u043e\u0434.",
    "what-is-hcaptcha": "\u0428\u0442\u0430 \u0458\u0435 \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430?",
    "what-is-hcaptcha-text": "\u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430 \u043f\u043e\u043c\u0430\u0436\u0435 \u0432\u0430\u0448\u0438\u043c \u043e\u043c\u0438\u0459\u0435\u043d\u0438\u043c \u0432\u0435\u0431 \u0443\u0441\u043b\u0443\u0433\u0430\u043c\u0430 \u0434\u0430 \u0441\u043f\u0440\u0435\u0447\u0435 \u0431\u043e\u0442\u043e\u0432\u0435, \u043d\u0435\u0436\u0435\u0459\u0435\u043d\u0443 \u043f\u043e\u0448\u0442\u0443 \u0438 \u0437\u043b\u043e\u0443\u043f\u043e\u0442\u0440\u0435\u0431\u0443 \u043f\u043e\u0441\u0442\u0430\u0432\u0459\u0430\u045a\u0435\u043c \u0458\u0435\u0434\u043d\u043e\u0441\u0442\u0430\u0432\u043d\u0438\u0445 \u043f\u0438\u0442\u0430\u045a\u0430 \u043a\u043e\u0458\u0430 \u0441\u0443 \u0458\u0435\u0434\u043d\u043e\u0441\u0442\u0430\u0432\u043d\u0430 \u0437\u0430 \u0459\u0443\u0434\u0435 \u0438 \u0442\u0435\u0448\u043a\u0430 \u0437\u0430 \u043c\u0430\u0448\u0438\u043d\u0435.",
    "why-hc": "\u0417\u0430\u0448\u0442\u043e \u0445\u0426\u0430\u043f\u0442\u0446\u0445\u0430?",
    "why-hc-text": "\u0421\u0432\u0430\u043a\u0438 \u043f\u0443\u0442 \u043a\u0430\u0434\u0430 \u043e\u0434\u0433\u043e\u0432\u043e\u0440\u0438\u0442\u0435 \u043d\u0430 \u0438\u0437\u0430\u0437\u043e\u0432, \u043a\u043e\u0440\u0438\u0441\u0442\u0438\u043c\u043e \u0441\u043e\u0444\u0438\u0441\u0442\u0438\u0446\u0438\u0440\u0430\u043d\u0435 \u043c\u043e\u0434\u0435\u043b\u0435 \u043a\u0430\u043a\u043e \u0431\u0438\u0441\u043c\u043e \u0441\u0445\u0432\u0430\u0442\u0438\u043b\u0438 \u0434\u0430 \u043b\u0438 \u0441\u0442\u0435 \u0447\u043e\u0432\u0435\u043a \u0438\u043b\u0438 \u043c\u0430\u0448\u0438\u043d\u0430. \u0412\u0435\u0431 \u043b\u043e\u043a\u0430\u0446\u0438\u0458\u0430 \u043a\u043e\u0458\u0443 \u0441\u0442\u0435 \u043f\u043e\u0441\u0435\u0442\u0438\u043b\u0438 \u0442\u0430\u0434\u0430 \u043a\u043e\u0440\u0438\u0441\u0442\u0438 \u043e\u0432\u0430\u0458 \u0440\u0435\u0437\u0443\u043b\u0442\u0430\u0442 \u0434\u0430 \u0431\u0438 \u0432\u0430\u043c \u043f\u043e\u043c\u043e\u0433\u0430\u043e \u0434\u0430 \u043f\u043e\u0431\u043e\u0459\u0448\u0430\u0442\u0435 \u0432\u0430\u0448\u0435 \u0438\u0441\u043a\u0443\u0441\u0442\u0432\u043e \u043d\u0430 \u045a\u0438\u0445\u043e\u0432\u043e\u0458 \u0432\u0435\u0431 \u043b\u043e\u043a\u0430\u0446\u0438\u0458\u0438 \u0447\u0443\u0432\u0430\u045a\u0435\u043c \u0431\u043e\u0442\u043e\u0432\u0430."
}