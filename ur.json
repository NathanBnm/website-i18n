{
    "acc-opt": "\u0631\u0633\u0627\u0626\u06cc \u06a9\u06d2 \u0627\u062e\u062a\u06cc\u0627\u0631\u0627\u062a",
    "acc-opt-but": "\u0631\u0633\u0627\u0626\u06cc \u06a9\u06d2 \u0627\u062e\u062a\u06cc\u0627\u0631\u0627\u062a \u06a9\u06d2 \u0628\u0627\u0631\u06d2 \u0645\u06cc\u06ba \u0645\u0632\u06cc\u062f \u0645\u0639\u0644\u0648\u0645\u0627\u062a \u062d\u0627\u0635\u0644 \u06a9\u0631\u06cc\u06ba",
    "acc-opt-text": "\u06a9\u06cc\u0627 \u0622\u067e \u0636\u0639\u0641 \u0628\u0635\u0627\u0631\u062a \u06a9\u0627 \u0634\u06a9\u0627\u0631 \u06c1\u06cc\u06ba \u06cc\u0627 \u062f\u0648\u0633\u0631\u06cc \u0635\u0648\u0631\u062a \u0645\u06cc\u06ba HCaptchas \u06a9\u0648 \u0645\u06a9\u0645\u0644 \u06a9\u0631\u0646\u06d2 \u0633\u06d2 \u0642\u0627\u0635\u0631 \u06c1\u06cc\u06ba\u061f",
    "accessibility-link": "\u0631\u0633\u0627\u0626",
    "appreciate-in": "\u06c1\u0645 \u0622\u067e \u06a9\u06d2 \u0627\u0646 \u067e\u0679 \u06a9\u06cc \u062a\u0639\u0631\u06cc\u0641 \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4 \u06c1\u0645\u06cc\u06ba \u06a9\u0648\u0626\u06cc \u0645\u0633\u0626\u0644\u06c1 \u0628\u062a\u0627\u0626\u06cc\u06ba \u0627\u0648\u0631 \u06c1\u0645 \u0627\u0646\u06c1\u06cc\u06ba \u062c\u0644\u062f \u062d\u0644 \u06a9\u0631\u062f\u06cc\u06ba \u06af\u06d2\u06d4",
    "fb-but": "\u06c1\u0645\u06cc\u06ba \u0622\u0631\u0627\u0621 \u0628\u06be\u06cc\u062c\u06cc\u06ba",
    "feedback-text": "\u0622\u067e \u0646\u06d2 \u0627\u0628\u06be\u06cc \u062f\u06cc\u06a9\u06be\u0627 hCaptcha \u067e\u0631 \u0631\u0627\u0626\u06d2 \u062f\u06cc\u06ba",
    "home-ent-1": "\u06c1\u0645\u0627\u0631\u06d2 \u0627\u0646\u0679\u0631\u067e\u0631\u0627\u0626\u0632 \u0633\u06cc\u06a9\u06cc\u0648\u0631\u0679\u06cc \u06a9\u06cc \u067e\u06cc\u0634 \u06a9\u0634\u0648\u06ba \u06a9\u06cc \u062a\u0644\u0627\u0634 \u06c1\u06d2\u061f",
    "home-ent-2": "\u06c1\u0645\u0627\u0631\u06cc \u0646\u0626\u06cc \u0633\u0631\u0634\u0627\u0631 \u0633\u0627\u0626\u0679 \u062f\u06cc\u06a9\u06be\u06cc\u06ba: \u0628\u0648\u0679 \u0627\u0633\u0679\u0627\u067e \u0688\u0627\u0679 \u06a9\u0627\u0645",
    "home-intro-body": "\u06a9\u06cc\u0627 \u0622\u067e \u0628\u0648\u0679\u0633 \u0631\u06a9\u06be\u0646\u06d2 \u06a9\u06d2 \u0644\u0626\u06d2 \u06a9\u06cc\u067e\u0686\u0627 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba\u061f hCaptcha \u0635\u0627\u0631\u0641 \u06a9\u06cc \u0631\u0627\u0632\u062f\u0627\u0631\u06cc \u06a9\u06cc \u062d\u0641\u0627\u0638\u062a \u06a9\u0631\u062a\u0627 \u06c1\u06d2 \u060c \u0648\u06cc\u0628 \u0633\u0627\u0626\u0679\u0648\u06ba \u06a9\u0648 \u0627\u0646\u0639\u0627\u0645 \u062f\u06cc\u062a\u0627 \u06c1\u06d2 \u060c \u0627\u0648\u0631 \u06a9\u0645\u067e\u0646\u06cc\u0648\u06ba \u06a9\u0648 \u0627\u067e\u0646\u06d2 \u0688\u06cc\u0679\u0627 \u06a9\u0627 \u0644\u06cc\u0628\u0644 \u0644\u06af\u0627\u0646\u06d2 \u0645\u06cc\u06ba \u0645\u062f\u062f \u06a9\u0631\u062a\u0627 \u06c1\u06d2\u06d4 \u06cc\u06c1 \u0631\u06cc\u06a9\u0627\u0679\u0627 \u06a9\u06cc\u0644\u0626\u06d2 \u0688\u0631\u0627\u067e \u0627\u0646 \u0645\u062a\u0628\u0627\u062f\u0644 \u06c1\u06d2: \u0622\u067e \u0686\u0646\u062f \u0645\u0646\u0679 \u06a9\u06d2 \u0627\u0646\u062f\u0631 \u0627\u0646\u062f\u0631 \u0633\u0648\u0626\u0686 \u06a9\u0631\u0633\u06a9\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4",
    "home-intro-head": "\u0645\u0632\u06cc\u062f \u0628\u0648\u0679\u0633 \u0628\u0646\u062f \u06a9\u0631\u0648\u06d4 \u0635\u0627\u0631\u0641 \u06a9\u06cc \u0631\u0627\u0632\u062f\u0627\u0631\u06cc \u06a9\u06cc \u062d\u0641\u0627\u0638\u062a \u0634\u0631\u0648\u0639 \u06a9\u0631\u06cc\u06ba\u06d4",
    "home-main-1": "\u0646\u062c\u06cc. \u0645\u062d\u0641\u0648\u0638 \u062a\u06cc\u0632 \u0627\u0648\u0631 \u06c1\u0645 \u0622\u067e \u06a9\u0648 \u0627\u0633 \u06a9\u06d2 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u06cc\u0644\u0626\u06d2 \u0627\u062f\u0627\u0626\u06cc\u06af\u06cc \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4",
    "home-main-2": "\u0646\u062c\u06cc \u0645\u0639\u0644\u0648\u0645\u0627\u062a \u06a9\u06cc \u062d\u0641\u0627\u0638\u062a\u06cc",
    "home-main-2c": "\u0627\u0634\u062a\u06c1\u0627\u0631\u06cc \u0646\u06cc\u0679 \u0648\u0631\u06a9 \u0622\u067e \u06a9\u06d2 \u0632\u0627\u0626\u0631\u06cc\u0646 \u06a9\u0648 \u0628\u0637\u0648\u0631 \u0645\u0635\u0646\u0648\u0639\u06c1 \u062f\u06cc\u06a9\u06be\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4 HCaptcha \u06a9\u06d2 \u0644\u0626\u06d2 \u0648\u06c1 \u06a9\u0627\u0645 \u06c1\u0648\u062a\u0627 \u06c1\u06d2 \u062c\u0648 \u0648\u06c1 \u0641\u0631\u0627\u06c1\u0645 \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4 \u06c1\u0645\u06cc\u06ba \u0627\u0633 \u06a9\u06cc \u067e\u0631\u0648\u0627\u06c1 \u0646\u06c1\u06cc\u06ba \u06c1\u06d2 \u06a9\u06c1 \u0622\u067e \u06a9\u06d2 \u0635\u0627\u0631\u0641 \u06a9\u0648\u0646 \u06c1\u06cc\u06ba \u060c \u0635\u0631\u0641 \u0627\u0633 \u06a9\u06d2 \u06a9\u06c1 \u0648\u06c1 \u0628\u062f\u0639\u0646\u0648\u0627\u0646\u06cc \u06a9\u0627 \u0634\u06a9\u0627\u0631 \u06c1\u0648\u06ba\u06d4 \u06c1\u0645 \u067e\u0631\u0627\u0626\u06cc\u0648\u06cc\u0633\u06cc \u067e\u0627\u0633 \u062c\u06cc\u0633\u06d2 \u0627\u0642\u062f\u0627\u0645\u0627\u062a \u06a9\u06cc \u062d\u0645\u0627\u06cc\u062a \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba \u060c \u0627\u0648\u0631 \u0631\u0627\u0632\u062f\u0627\u0631\u06cc \u0627\u0648\u0631 \u0633\u0644\u0627\u0645\u062a\u06cc \u06a9\u0648 \u0645\u062a\u0648\u0627\u0632\u0646 \u0628\u0646\u0627\u0646\u06d2 \u06a9\u06d2 \u0644\u0626\u06d2 \u0633\u062e\u062a \u0645\u062d\u0646\u062a \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4",
    "home-main-3": "\u0633\u06cc\u06a9\u06cc\u0648\u0631\u0679\u06cc \u0633\u0628 \u0633\u06d2 \u067e\u06c1\u0644\u06d2",
    "home-main-3c": "\u062f\u0631\u0633\u062a\u06af\u06cc \u0622\u067e \u06a9\u06d2 \u0627\u0648\u0631 \u06c1\u0645\u0627\u0631\u06d2 \u0645\u0641\u0627\u062f \u0645\u06cc\u06ba \u06c1\u06d2\u06d4 hCaptcha \u0646\u0627\u0634\u0631 \u0645\u06cc\u06ba \u0645\u0631\u06a9\u0648\u0632 \u06c1\u06d2\u06d4 \u06c1\u0645 \u0622\u067e \u06a9\u0648 \u062d\u0642\u06cc\u0642\u06cc \u0627\u0639\u062f\u0627\u062f \u0648 \u0634\u0645\u0627\u0631 \u062f\u06cc\u0646\u06d2 \u06a9\u06d2 \u0644\u0626\u06d2 \u06a9\u0627\u0645 \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba \u0627\u0648\u0631 \u0622\u067e \u06a9\u0648 \u0641\u06cc\u0635\u0644\u06c1 \u06a9\u0631\u0646\u06d2 \u062f\u06cc\u062a\u06d2 \u06c1\u06cc\u06ba \u06a9\u06c1 \u0627\u0633\u06d2 \u06a9\u0633 \u0637\u0631\u062d \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u06cc\u0627 \u062c\u0627\u0626\u06d2\u06d4 \u062f\u0648\u0633\u0631\u06d2 \u06a9\u06cc\u067e\u0686\u0627 \u0628\u0630\u0631\u06cc\u0639\u06c1 \u0641\u0631\u0627\u06c1\u0645 \u06a9\u0631\u062f\u06c1 \u06c1\u06cc\u06ba",
    "home-main-3c2": "\u0627\u0634\u062a\u06c1\u0627\u0631\u06cc \u06a9\u0645\u067e\u0646\u06cc\u0627\u06ba\u06d4 \u0627\u0633 \u06a9\u0627 \u0645\u0637\u0644\u0628 \u06c1\u06d2 \u06a9\u06c1 \u06c1\u0631 \u062c\u0633 \u0628\u0648\u0679 \u06a9\u0648 \u0627\u0646\u06c1\u0648\u06ba \u0646\u06d2 \u0645\u0633\u062f\u0648\u062f \u06a9\u06cc\u0627 \u06c1\u06d2 \u0627\u0633 \u0633\u06d2 \u0627\u0646 \u06a9\u06cc \u0622\u0645\u062f\u0646\u06cc \u0645\u06cc\u06ba \u06a9\u0645\u06cc \u0622\u0633\u06a9\u062a\u06cc \u06c1\u06d2\u06d4",
    "home-main-4": "\u0628\u06c1\u062a\u0631 \u0635\u0627\u0631\u0641 \u06a9\u0627 \u062a\u062c\u0631\u0628\u06c1",
    "home-main-4c": "\u062c\u062f\u06cc\u062f \u062a\u0631\u06cc\u0646 \u0645\u0634\u06cc\u0646 \u0644\u0631\u0646\u0646\u06af \u0645\u0627\u0688\u0644\u0632 \u0627\u0646\u0633\u0627\u0646\u06cc\u062a \u06a9\u0627 \u062a\u0639\u06cc\u0646 \u06a9\u0631\u0646\u06d2 \u06a9\u06cc\u0644\u0626\u06d2 \u0627\u06cc\u0686 \u06a9\u06cc\u067e\u0686\u0627 \u06a9\u06d2 \u0630\u0631\u06cc\u0639\u06c1 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06c1\u0648\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4 \u0627\u0633 \u0633\u06d2 \u06c1\u0645\u06cc\u06ba \u0622\u067e \u06a9\u06d2 \u0635\u0627\u0631\u0641\u06cc\u0646 \u0633\u06d2 \u06a9\u0645 \u067e\u0631\u06cc\u0634\u0627\u0646 \u06a9\u0646 \u06a9\u0627\u0645\u0648\u06ba \u06a9\u06cc \u0636\u0631\u0648\u0631\u062a \u067e\u0691\u0633\u06a9\u062a\u06cc \u06c1\u06d2 \u0627\u0648\u0631 \u0627\u06cc\u0633\u0627 \u06a9\u0631\u0646\u06d2 \u0645\u06cc\u06ba \u0627\u0646 \u06a9\u0627 \u0628\u06c1\u062a \u06a9\u0645 \u0648\u0642\u062a \u0644\u06af\u062a\u0627 \u06c1\u06d2 \u060c \u062c\u0628\u06a9\u06c1 \u0627\u0628 \u0628\u06be\u06cc \u0635\u0627\u0631\u0641\u06cc\u0646 \u06a9\u0648 \u0644\u06cc\u0628\u0644 \u0644\u06af\u0627\u0646\u06d2 \u06a9\u06d2 \u0644 useful \u0645\u0641\u06cc\u062f \u0644\u06cc\u0628\u0644 \u0644\u06af\u0627 \u0688\u06cc\u0679\u0627 \u062a\u06cc\u0627\u0631 \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4",
    "home-main-plans": "hCaptcha \u06a9\u0633\u06cc \u0628\u06be\u06cc \u0633\u0627\u0626\u0632 \u06a9\u06d2 \u067e\u0628\u0644\u0634\u0631\u0648\u06ba \u06a9\u06d2 \u0644\u0626\u06d2 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u0631\u0646\u06d2 \u06a9\u06d2 \u0644\u0626\u06d2 \u0622\u0632\u0627\u062f \u06c1\u06d2\u06d4 \u062a\u0627\u06c1\u0645 \u060c \u0627\u06cc\u0633\u06d2 \u06a9\u0627\u0631\u0648\u0628\u0627\u0631\u06cc \u0627\u062f\u0627\u0631\u0648\u06ba \u0627\u0648\u0631 \u067e\u0644\u06cc\u0679 \u0641\u0627\u0631\u0645\u0632 \u06a9\u06d2 \u0644\u0626\u06d2 \u062c\u0646 \u06a9\u0648 \u0632\u06cc\u0627\u062f\u06c1 \u06a9\u0646\u0679\u0631\u0648\u0644 \u06a9\u06cc \u0636\u0631\u0648\u0631\u062a \u06c1\u06d2 \u060c \u06c1\u0645\u0627\u0631\u06d2 \u0627\u0646\u0679\u0631\u067e\u0631\u0627\u0626\u0632 \u062d\u0644 \u062f\u0633\u062a\u06cc\u0627\u0628 \u06c1\u06cc\u06ba\u06d4",
    "home-main-plans2": "hCaptcha \u0627\u0646\u0679\u0631\u067e\u0631\u0627\u0626\u0632 \u062d\u0644 \u06a9\u0631\u06c1 \u0627\u0631\u0636 \u06a9\u06cc \u0633\u0628 \u0633\u06d2 \u0628\u0691\u06cc \u06a9\u0645\u067e\u0646\u06cc\u0648\u06ba \u0645\u06cc\u06ba \u0633\u06d2 \u0644\u0627\u06a9\u06be\u0648\u06ba \u0635\u0627\u0631\u0641\u06cc\u0646 \u06a9\u0648 \u0644\u0627\u06a9\u06be\u0648\u06ba \u0648\u06cc\u0628 \u0633\u0627\u0626\u0679\u0648\u06ba \u06a9\u06cc \u062d\u0641\u0627\u0638\u062a \u06a9\u0631\u062a\u0627 \u06c1\u06d2\u06d4 \u0645\u0632\u06cc\u062f \u0645\u0639\u0644\u0648\u0645\u0627\u062a \u06a9\u06d2 \u0644\u0626\u06d2 \u06c1\u0645 \u0633\u06d2 \u0631\u0627\u0628\u0637\u06c1 \u06a9\u0631\u06cc\u06ba \u060c \u06cc\u0627 \u0645\u0644\u0627\u062d\u0638\u06c1 \u06a9\u0631\u06cc\u06ba",
    "home-main-plans3": "hCaptcha \u0627\u0646\u0679\u0631\u067e\u0631\u0627\u0626\u0632 \u062d\u0644 \u06a9\u0631\u06c1 \u0627\u0631\u0636 \u06a9\u06cc \u0633\u0628 \u0633\u06d2 \u0628\u0691\u06cc \u06a9\u0645\u067e\u0646\u06cc\u0648\u06ba \u0645\u06cc\u06ba \u0633\u06d2 \u0644\u0627\u06a9\u06be\u0648\u06ba \u0635\u0627\u0631\u0641\u06cc\u0646 \u06a9\u0648 \u0644\u0627\u06a9\u06be\u0648\u06ba \u0648\u06cc\u0628 \u0633\u0627\u0626\u0679\u0648\u06ba \u06a9\u06cc \u062d\u0641\u0627\u0638\u062a \u06a9\u0631\u062a\u0627 \u06c1\u06d2\u06d4 \u0645\u0632\u06cc\u062f \u0645\u0639\u0644\u0648\u0645\u0627\u062a \u06a9\u06d2 \u0644\u0626\u06d2 \u06c1\u0645 \u0633\u06d2 \u0631\u0627\u0628\u0637\u06c1 \u06a9\u0631\u06cc\u06ba \u060c \u06cc\u0627 \u0645\u0644\u0627\u062d\u0638\u06c1 \u06a9\u0631\u06cc\u06ba",
    "home-main-plans4": "\u062a\u0641\u0635\u06cc\u0644\u0627\u062a \u06a9\u06d2 \u0644.",
    "learn-more-hcaptcha": "hCaptcha \u06a9\u06d2 \u0628\u0627\u0631\u06d2 \u0645\u06cc\u06ba \u0645\u0632\u06cc\u062f \u0645\u0639\u0644\u0648\u0645\u0627\u062a \u062d\u0627\u0635\u0644 \u06a9\u0631\u06cc\u06ba",
    "page-acc-faq-1": "\u0633\u0648\u0627\u0644: \u06a9\u06cc\u0627 \u0627\u06cc\u0686 \u06a9\u06cc\u067e\u0686\u0627 \u0633\u06cc\u06a9\u0634\u0646 508 + \u0688\u0628\u0644\u06cc\u0648 \u0633\u06cc \u0627\u06d2 \u062c\u06cc 2.0 \u06a9\u06d2 \u0645\u0637\u0627\u0628\u0642 \u06c1\u06d2\u061f \u062c\u0648\u0627\u0628: \u06c1\u0645\u0627\u0631\u0627 \u06cc\u0642\u06cc\u0646 \u06c1\u06d2 \u06a9\u06c1: \u06a9\u0633\u06cc \u0628\u06be\u06cc \u0642\u0633\u0645 \u06a9\u06cc \u062e\u0631\u0627\u0628\u06cc \u06a9\u06d2 \u062d\u0627\u0645\u0644 \u062a\u0645\u0627\u0645 \u0635\u0627\u0631\u0641 \u062c\u0648 \u0648\u06cc\u0628 \u06a9\u0648 \u062a\u0644\u0627\u0634 \u06a9\u0631\u0646\u06d2 \u0627\u0648\u0631 \u0641\u0627\u0631\u0645\u0648\u06ba \u067e\u0631 \u0645\u062a\u0646 \u062f\u0631\u062c \u06a9\u0631\u0646\u06d2 \u06a9\u06d2 \u0627\u06c1\u0644 \u06c1\u06cc\u06ba \u0648\u06c1 \u0631\u062c\u0633\u0679\u0631\u06cc\u0634\u0646 \u06a9\u06d2 \u0628\u0639\u062f \u0627\u06cc\u0686 \u0633\u06cc \u06a9\u06cc\u067e\u0686\u0627 \u06a9\u06d2 \u0630\u0631\u06cc\u0639\u06c1 \u0645\u062d\u0641\u0648\u0638 \u062e\u062f\u0645\u0627\u062a \u062a\u06a9 \u0631\u0633\u0627\u0626\u06cc \u062d\u0627\u0635\u0644 \u06a9\u0631\u0633\u06a9\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4 \u062a\u0627\u06c1\u0645 \u060c \u06cc\u06c1 \u0642\u0627\u0646\u0648\u0646\u06cc \u0645\u0634\u0648\u0631\u06c1 \u0646\u06c1\u06cc\u06ba \u06c1\u06d2: \u0622\u067e \u06a9\u0648 \u062e\u0648\u062f \u0627\u067e\u0646\u06cc \u062a\u0634\u062e\u06cc\u0635 \u06a9\u06cc \u062c\u0627\u0646\u06cc \u0686\u0627\u06c1\u0626\u06d2 \u060c \u0627\u067e\u0646\u06d2 \u0645\u062e\u0635\u0648\u0635 \u0639\u0645\u0644 \u06a9\u0648 \u0645\u062f\u0646\u0638\u0631 \u0631\u06a9\u06be\u062a\u06d2 \u06c1\u0648\u0626\u06d2 \u06cc\u06c1 \u06cc\u0642\u06cc\u0646\u06cc \u0628\u0646\u0627\u0626\u06cc\u06ba \u06a9\u06c1 \u06cc\u06c1 \u0622\u067e \u06a9\u06cc \u062a\u0639\u06cc\u0646\u0627\u062a\u06cc \u06a9\u0627 \u0645\u0639\u0627\u0645\u0644\u06c1 \u06c1\u06d2\u06d4",
    "page-acc-faq-2": "\u0633\u0648\u0627\u0644: \u06a9\u06cc\u0627 \u0622\u067e \u0622\u0688\u06cc\u0648 \u06a9\u06cc \u0637\u0631\u062d \u062f\u0648\u0633\u0631\u06d2 \u0642\u0627\u0628\u0644 \u0631\u0633\u0627\u0626 (a11y) \u0627\u062e\u062a\u06cc\u0627\u0631\u0627\u062a \u067e\u0631 \u06a9\u0627\u0645 \u06a9\u0631 \u0631\u06c1\u06d2 \u06c1\u06cc\u06ba\u061f \u062c\u0648\u0627\u0628: \u0645\u0627\u0636\u06cc \u0645\u06cc\u06ba \u0645\u0634\u06c1\u0648\u0631 \u0627\u062e\u062a\u06cc\u0627\u0631\u0627\u062a \u062c\u06cc\u0633\u06d2 \u0622\u0688\u06cc\u0648 \u06a9\u06cc\u067e\u0686\u0627 \u0628\u06c1\u062a \u0633\u0627\u0631\u06d2 A11y \u0635\u0627\u0631\u0641\u06cc\u0646 \u06a9\u06d2 \u0633\u0627\u062a\u06be \u0627\u0645\u062a\u06cc\u0627\u0632\u06cc \u0633\u0644\u0648\u06a9 \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba \u0627\u0648\u0631 \u062c\u062f\u06cc\u062f \u0645\u0634\u06cc\u0646 \u0633\u06cc\u06a9\u06be\u0646\u06d2 \u06a9\u06cc \u062a\u06a9\u0646\u06cc\u06a9 \u0633\u06d2 \u0622\u0633\u0627\u0646\u06cc \u0633\u06d2 \u06c1\u0627\u0631 \u062c\u0627\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4 \u0627\u0633 \u0646\u06d2 \u0645\u0648\u062c\u0648\u062f\u06c1 \u0622\u0688\u06cc\u0648 \u0686\u06cc\u0644\u0646\u062c\u0632 \u06a9\u0648 \u0632\u06cc\u0627\u062f\u06c1 \u0633\u06d2 \u0632\u06cc\u0627\u062f\u06c1 \u0645\u0634\u06a9\u0644 \u0628\u0646\u0646\u06d2 \u060c \u0634\u0648\u0631 \u0634\u0631\u0627\u0628\u06c1 \u06a9\u0631\u0646\u06d2 \u060c \u0639\u062c\u06cc\u0628 \u0648\u0642\u062a \u062f\u06cc\u0646\u06d2 \u060c \u063a\u06cc\u0631 \u0645\u0639\u0645\u0648\u0644\u06cc \u0627\u0644\u0641\u0627\u0638 \u06a9\u06d2 \u0627\u0645\u062a\u0632\u0627\u062c \u0648\u063a\u06cc\u0631\u06c1 \u067e\u0631 \u062d\u0645\u0644\u06c1 \u0622\u0648\u0631\u0648\u06ba \u06a9\u0648 \u0634\u06a9\u0633\u062a \u062f\u06cc\u0646\u06d2 \u067e\u0631 \u0645\u062c\u0628\u0648\u0631 \u06a9\u0631\u062f\u06cc\u0627 \u06c1\u06d2\u06d4 \u0686\u0646\u0627\u0646\u0686\u06c1 \u06c1\u0645 \u0627\u0633 \u0646\u0642\u0637\u06c1 \u0646\u0638\u0631 \u06a9\u06d2 \u0628\u0627\u0631\u06d2 \u0645\u06cc\u06ba \u06a9\u0645 \u067e\u0631\u062c\u0648\u0634 \u06c1\u06cc\u06ba\u06d4 \u0628\u0645\u0642\u0627\u0628\u0644\u06c1 \u0686\u06cc\u0644\u0646\u062c \u0633\u06d2 \u0645\u06a9\u0645\u0644 \u0637\u0648\u0631 \u067e\u0631 \u06af\u0631\u06cc\u0632 \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba \u060c \u0644\u06cc\u06a9\u0646 \u0627\u06af\u0631 A11y \u0628\u0631\u0627\u062f\u0631\u06cc \u06a9\u06cc \u0637\u0631\u0641 \u0633\u06d2 \u0645\u0637\u0627\u0644\u0628\u06c1 \u06c1\u06d2 \u062a\u0648 \u0627\u0633 \u067e\u0631 \u063a\u0648\u0631 \u06a9\u0631\u06cc\u06ba \u06af\u06d2\u06d4 \u062a\u0627\u06c1\u0645 \u060c \u06c1\u0645 \u0642\u0627\u0628\u0644 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u06d2 \u0645\u0639\u0627\u0645\u0644\u06d2 \u0645\u06cc\u06ba \u067e\u0631\u0627\u0626\u06cc\u0648\u06cc\u0633\u06cc \u067e\u0627\u0633 \u0645\u06cc\u06ba \u0628\u06c1\u062a \u062f\u0644\u0686\u0633\u067e\u06cc \u0631\u06a9\u06be\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4 \u06c1\u0645\u0627\u0631\u0627 \u06cc\u0642\u06cc\u0646 \u06c1\u06d2 \u06a9\u06c1 \u067e\u0631\u0627\u0626\u06cc\u0648\u06cc\u0633\u06cc \u067e\u0627\u0633 \u062c\u0627\u0631\u06cc \u06a9\u0631\u0646\u06d2 \u06a9\u06d2 \u0633\u0627\u062a\u06be \u06c1\u0645\u0627\u0631\u06d2 \u0645\u0648\u062c\u0648\u062f\u06c1 A11y \u0627\u067e\u0631\u0648\u0686 \u06a9\u0648 \u06cc\u06a9\u062c\u0627 \u06a9\u0631\u0646\u06d2 \u0633\u06d2 A11y \u0635\u0627\u0631\u0641\u06cc\u0646 \u0645\u062d\u0641\u0648\u0638 \u0637\u0631\u06cc\u0642\u06d2 \u0633\u06d2 \u0628\u0631\u0627\u0624\u0632 \u06a9\u0631\u0633\u06a9\u06cc\u06ba \u06af\u06d2 \u060c \u0627\u0633 \u0639\u0644\u0645 \u0645\u06cc\u06ba \u0645\u062d\u0641\u0648\u0638 \u06c1\u0648\u06ba \u06af\u06d2 \u06a9\u06c1 \u0627\u0646 \u06a9\u0627 \u0679\u0631\u06cc\u0641\u06a9 \u0632\u06cc\u0627\u062f\u06c1 \u0646\u062c\u06cc \u06c1\u06d2 \u060c \u062c\u0628\u06a9\u06c1 \u0628\u0648\u0679 \u0622\u067e\u0631\u06cc\u0679\u0631\u0632 \u06a9\u06d2 \u0630\u0631\u06cc\u0639\u06c1 \u0628\u062f\u0633\u0644\u0648\u06a9\u06cc \u067e\u0631 \u067e\u0627\u0628\u0646\u062f\u06cc \u06c1\u06d2 \u062c\u0648 \u0644\u0627\u0645\u062d\u0627\u0644\u06c1 \u0627\u0633 \u0648\u0642\u062a \u06c1\u0648\u062a\u0627 \u06c1\u06d2 \u062c\u0628 A11y \u0622\u067e\u0634\u0646 \u062f\u0633\u062a\u06cc\u0627\u0628 \u06c1\u0648\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4 \u06c1\u0645 \u0627\u0633 \u0646\u0626\u06cc \u0679\u06a9\u0646\u0627\u0644\u0648\u062c\u06cc \u06a9\u0648 \u0645\u0639\u06cc\u0627\u0631\u06cc \u0628\u0646\u0627\u0646\u06d2 \u0648\u0627\u0644\u06d2 IETF \u0648\u0631\u06a9\u0646\u06af \u06af\u0631\u0648\u067e \u0645\u06cc\u06ba \u0633\u0631\u06af\u0631\u0645 \u0634\u0631\u0627\u06a9\u062a \u062f\u0627\u0631 \u06c1\u06cc\u06ba\u06d4",
    "page-acc-faq-3": "\u0633\u0648\u0627\u0644: \u0631\u0627\u0632\u062f\u0627\u0631\u06cc \u06a9\u06d2 \u0628\u0627\u0631\u06d2 \u0645\u06cc\u06ba \u06a9\u06cc\u0627 \u062e\u06cc\u0627\u0644 \u06c1\u06d2\u061f \u06a9\u06cc\u0627 \u0627\u0646\u062f\u0631\u0627\u062c A11y \u0628\u0631\u0627\u0624\u0632\u0646\u06af \u0688\u06cc\u0679\u0627 \u06a9\u0648 \u06a9\u0633\u06cc \u0637\u0631\u062d \u0628\u06d2 \u0646\u0642\u0627\u0628 \u06a9\u0631\u062a\u0627 \u06c1\u06d2\u061f \u0622\u067e \u0627\u06cc \u0645\u06cc\u0644 \u06a9\u06d2 \u0633\u0627\u062a\u06be \u06a9\u06cc\u0627 \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba\u061f \u062c\u0648\u0627\u0628: hCaptcha \u06af\u0631\u0627\u0624\u0646\u0688 \u0627\u067e \u0633\u06d2 \u0631\u0627\u0632\u062f\u0627\u0631\u06cc \u06a9\u06d2 \u0644\u0626\u06d2 \u0688\u06cc\u0632\u0627\u0626\u0646 \u06a9\u06cc\u0627 \u06af\u06cc\u0627 \u06c1\u06d2\u06d4 \u06cc\u06c1 \u0631\u0648\u0627\u06cc\u062a\u06cc \u0627\u062e\u062a\u06cc\u0627\u0631\u0627\u062a \u062c\u06cc\u0633\u06d2 \u06a9\u06c1 \u0631\u06cc\u06a9\u0627\u0679\u0627 \u0633\u06d2 \u0628\u0627\u0644\u06a9\u0644 \u0645\u062e\u062a\u0644\u0641 \u06c1\u06d2 \u062c\u0633 \u06a9\u06cc \u0645\u0644\u06a9\u06cc\u062a \u0627\u0634\u062a\u06c1\u0627\u0631\u06cc \u0646\u06cc\u0679 \u0648\u0631\u06a9\u0633 \u06a9\u06d2 \u067e\u0627\u0633 \u06c1\u06d2 \u060c \u062c\u0646 \u06a9\u06d2 \u067e\u0627\u0633 \u0622\u067e \u06a9\u0648 \u0648\u06cc\u0628 \u06a9\u06d2 \u06af\u0631\u062f \u0679\u0631\u06cc\u06a9 \u06a9\u0631\u0646\u06d2 \u0627\u0648\u0631 \u0622\u067e \u06a9\u0648 \u0627\u06cc\u06a9 \u062d\u0642\u06cc\u0642\u06cc \u0634\u0646\u0627\u062e\u062a \u06a9\u06d2 \u0633\u0627\u062a\u06be \u0645\u0646\u0633\u0644\u06a9 \u06a9\u0631\u0646\u06d2 \u06a9\u06cc \u062a\u0631\u063a\u06cc\u0628 \u06c1\u06d2\u06d4 \u06c1\u0645 \u06a9\u0628\u06be\u06cc \u0628\u06be\u06cc \u0642\u0627\u0628\u0644 \u0631\u0633\u0627\u0626\u06cc \u0627\u06cc \u0645\u06cc\u0644\u0632 \u06cc\u0627 \u0645\u0639\u0644\u0648\u0645\u0627\u062a A11y \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u0645\u06cc\u06ba \u0633\u06c1\u0648\u0644\u062a \u06a9\u0627\u0631\u06cc \u0627\u0648\u0631 \u0628\u062f\u0633\u0644\u0648\u06a9\u06cc \u06a9\u06cc \u0631\u0648\u06a9 \u062a\u06be\u0627\u0645 \u06a9\u06d2 \u0639\u0644\u0627\u0648\u06c1 \u06a9\u0633\u06cc \u0627\u0648\u0631 \u0645\u0642\u0635\u062f \u06a9\u06d2 \u0644\u0626\u06d2 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u0646\u06c1\u06cc\u06ba \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4 \u06c1\u0645\u0627\u0631\u06cc \u0631\u0627\u0632\u062f\u0627\u0631\u06cc \u06a9\u06cc \u067e\u0627\u0644\u06cc\u0633\u06cc \u0645\u06cc\u06ba \u062c\u0627\u0645\u0639 \u0627\u0648\u0631 \u0645\u0633\u062a\u0646\u062f \u062c\u0648\u0627\u0628\u0627\u062a \u06c1\u06cc\u06ba \u06a9\u06c1 \u06c1\u0645 \u0688\u06cc\u0679\u0627 \u06a9\u0648 \u06a9\u0633 \u0637\u0631\u062d \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba \u060c \u0644\u06cc\u06a9\u0646 \u0645\u062e\u062a\u0635\u0631 \u062c\u0648\u0627\u0628 \u06cc\u06c1 \u06c1\u06d2 \u06a9\u06c1 \u06c1\u0645\u06cc\u06ba \u0622\u067e \u06a9\u06cc \u0628\u0631\u0627\u0626\u0648\u0632\u0646\u06af \u06c1\u0633\u0679\u0631\u06cc \u06a9\u06d2 \u062d\u0627\u0645\u0644 \u0634\u062e\u0635 \u06a9\u06cc \u062d\u06cc\u062b\u06cc\u062a \u0633\u06d2 \u0645\u0646\u0633\u0644\u06a9 \u06a9\u0631\u0646\u06d2 \u0645\u06cc\u06ba \u06a9\u0648\u0626\u06cc \u062f\u0644\u0686\u0633\u067e\u06cc \u0646\u06c1\u06cc\u06ba \u06c1\u06d2\u06d4 \u06c1\u0645 \u0641\u06cc \u0627\u0644\u062d\u0627\u0644 \u0622\u067e \u06a9\u06d2 \u0627\u06cc \u0645\u06cc\u0644 \u067e\u062a\u06d2 \u06a9\u0648 \u062a\u06cc\u0632\u06cc \u0633\u06d2 \u062e\u0627\u0631\u062c \u06a9\u0631\u0646\u06d2 \u06a9\u06d2 \u0644\u0626\u06d2 \u0627\u06cc\u06a9 \u06a9\u0631\u06cc\u067e\u0679\u0648\u06af\u0631\u0627\u0641\u06a9 \u062d\u0644 \u067e\u0631 \u0628\u06be\u06cc \u06a9\u0627\u0645 \u06a9\u0631 \u0631\u06c1\u06d2 \u06c1\u06cc\u06ba\u06d4 \u062c\u0628 \u062a\u06a9 \u06a9\u06c1 \u06c1\u0645\u0627\u0631\u06d2 \u067e\u0631\u0627\u0626\u06cc\u0648\u06cc\u0633\u06cc \u067e\u0627\u0633 \u06a9\u0627\u0645 \u06a9\u06cc \u062a\u06a9\u0645\u06cc\u0644 \u06a9\u0631\u062a\u06d2 \u06c1\u0648\u0626\u06d2 \u060c \u0628\u062f\u0633\u0644\u0648\u06a9\u06cc \u0633\u06d2 \u0628\u0686\u0646\u06d2 \u06a9\u06cc \u0627\u067e\u0646\u06cc \u0635\u0644\u0627\u062d\u06cc\u062a \u06a9\u0648 \u0628\u0631\u0642\u0631\u0627\u0631 \u0631\u06a9\u06be\u062a\u06d2 \u06c1\u0648\u0626\u06d2\u06d4",
    "page-acc-link-1": "\u0627\u06af\u0631 \u0622\u067e \u0642\u0627\u0628\u0644 \u0631\u0633\u0627 \u0635\u0627\u0631\u0641 \u06c1\u06cc\u06ba \u062a\u0648 \u060c \u0628\u0631\u0627\u06c1 \u06a9\u0631\u0645 \u06cc\u06c1\u0627\u06ba \u0633\u0627\u0626\u0646 \u0627\u067e \u06a9\u0631\u06cc\u06ba\u06d4",
    "page-acc-subhead": "\u0633\u06cc\u06a9\u06cc\u0648\u0631\u0679\u06cc \u06a9\u0648 \u0628\u0631\u0642\u0631\u0627\u0631 \u0631\u06a9\u06be\u062a\u06d2 \u06c1\u0648\u0626\u06d2 \u062a\u0645\u0627\u0645 \u0635\u0627\u0631\u0641\u06cc\u0646 \u06a9\u06d2 \u0644\u0626\u06d2 \u06c1\u0645\u0648\u0627\u0631 \u0631\u0633\u0627\u0626\u06cc \u06a9\u0648 \u06cc\u0642\u06cc\u0646\u06cc \u0628\u0646\u0627\u0646\u0627",
    "page-acc-summary": "\u062e\u0644\u0627\u0635\u06c1",
    "page-acc-summary-contents-1": "hCaptcha \u0628\u0648\u0679 \u06a9\u0648 \u0644\u0648\u06af\u0648\u06ba \u0633\u06d2 \u0627\u0644\u06af \u06a9\u0631\u06a9\u06d2 \u0631\u0648\u06a9\u0646\u06d2 \u06a9\u06d2 \u0644\u0626\u06d2 \u0688\u06cc\u0632\u0627\u0626\u0646 \u06a9\u06cc\u0627 \u06af\u06cc\u0627 \u06c1\u06d2\u06d4 \u0628\u0635\u0631\u06cc \u0679\u06cc\u0633\u0679 \u0627\u0633 \u06a9\u06d2 \u0644\u0626\u06d2 \u0627\u06cc\u06a9 \u0622\u0633\u0627\u0646 \u0679\u0648\u0644 \u06c1\u06cc\u06ba \u060c \u0644\u06cc\u06a9\u0646 \u06c1\u0631 \u06a9\u0648\u0626\u06cc \u0627\u06cc\u06a9 \u0628\u0635\u0631\u06cc \u0686\u06cc\u0644\u0646\u062c \u062d\u0644 \u0646\u06c1\u06cc\u06ba \u06a9\u0631\u0633\u06a9\u062a\u0627 \u06c1\u06d2\u06d4 \u0627\u0633 \u0648\u062c\u06c1 \u0633\u06d2 \u06c1\u0645 \u0646\u06d2 \u0627\u06cc\u06a9 \u0622\u0633\u0627\u0646 \u060c \u0628\u06d2 \u062a\u06a9\u0644\u06cc\u0641 \u0688\u06cc\u0632\u0627\u0626\u0646 \u062a\u06cc\u0627\u0631 \u06a9\u06cc\u0627 \u06c1\u06d2 \u062a\u0627\u06a9\u06c1 \u0627\u067e\u0646\u06cc \u062e\u062f\u0645\u062a \u06a9\u0627 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u0631\u062a\u06d2 \u06c1\u0648\u0626\u06d2 \u067e\u0628\u0644\u0634\u0631\u0648\u06ba \u06a9\u0648 \u0645\u06a9\u0645\u0644 \u0633\u06cc\u06a9\u0634\u0646 508 \u0627\u0648\u0631 \u0688\u0628\u0644\u06cc\u0648 \u0633\u06cc \u0627\u06d2 \u062c\u06cc 2.0 \u06a9\u06cc \u062a\u0639\u0645\u06cc\u0644 \u06a9\u06d2 \u0633\u0627\u062a\u06be \u0633\u0628 \u06a9\u06d2 \u0644\u0626\u06d2 \u0642\u0627\u0628\u0644 \u0631\u0633\u0627 \u062a\u062d\u0641\u0638 \u06a9\u0648 \u0628\u0631\u0642\u0631\u0627\u0631 \u0631\u06a9\u06be\u0646\u06d2 \u062f\u06cc\u06ba\u06d4",
    "page-acc-summary-contents-2": "\u06cc\u06c1 \u06a9\u0633 \u0637\u0631\u062d \u06a9\u0627\u0645 \u06a9\u0631\u062a\u0627 \u06c1\u06d2: \u067e\u06c1\u0644\u06d2 \u060c \u0642\u0627\u0628\u0644 \u0631\u0633\u0627\u0626\u06cc \u0635\u0627\u0631\u0641 \u0627\u0633 \u06cc\u0648 \u0622\u0631 \u0627\u06cc\u0644 \u067e\u0631 \u0633\u0627\u0626\u0646 \u0627\u067e \u06a9\u0631\u062a\u0627 \u06c1\u06d2 \u060c \u062c\u0648 hCaptcha \u0648\u06cc\u062c\u06cc\u0679 \u0627\u0646\u0641\u0627\u0631\u0645\u06cc\u0634\u0646 \u067e\u06cc\u062c \u0645\u06cc\u06ba \u0644\u0646\u06a9 \u06c1\u06d2\u06d4 \u0627\u0646\u06c1\u06cc\u06ba \u0627\u06cc\u06a9 \u0627\u0646\u06a9\u0631\u067e\u0679\u0688 \u06a9\u0648\u06a9\u06cc \u062f\u06cc \u06af\u0626\u06cc \u06c1\u06d2 \u062c\u0648 \u0631\u0648\u0632\u0627\u0646\u06c1 \u06a9\u0626\u06cc \u0628\u0627\u0631 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u06cc \u062c\u0627\u0633\u06a9\u062a\u06cc \u06c1\u06d2 \u060c \u0644\u06cc\u06a9\u0646 \u0644\u0627\u06af \u0627\u0646 \u06a9\u06d2 \u0630\u0631\u06cc\u0639\u06d2 \u06c1\u0631 24 \u06af\u06be\u0646\u0679\u0648\u06ba \u0645\u06cc\u06ba \u0627\u0633\u06d2 \u062a\u0627\u0632\u06c1 \u062f\u0645 \u06a9\u0631\u0646\u0627 \u0686\u0627\u06c1\u0626\u06d2\u06d4 \u062c\u0628 \u06a9\u0633\u06cc \u0633\u0627\u0626\u0679 \u067e\u0631 \u06a9\u0633\u06cc \u0642\u0627\u0628\u0644 \u0631\u0633\u0627\u0626\u06cc \u0635\u0627\u0631\u0641 \u06a9\u0648 \u0686\u06cc\u0644\u0646\u062c \u067e\u06cc\u0634 \u06a9\u06cc\u0627 \u062c\u0627\u062a\u0627 \u06c1\u06d2 \u062a\u0648 \u0648\u06c1 hCaptcha \u0633\u0631\u0648\u0633 \u06a9\u0627 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u0631\u062a\u06d2 \u06c1\u0648\u0626\u06d2 \u062e\u0648\u062f \u0628\u062e\u0648\u062f \u06af\u0632\u0631 \u062c\u0627\u0626\u06d2 \u06af\u06cc\u06d4",
    "page-acc-title": "hCaptcha \u0631\u0633\u0627\u0626\u06cc \u06a9\u06cc \u0645\u0639\u0644\u0648\u0645\u0627\u062a",
    "page-acc-tr-body": "\u0633\u0648\u0627\u0644: \u0645\u06cc\u06ba \u06a9\u0648\u06a9\u06cc \u06a9\u0648 \u062a\u0631\u062a\u06cc\u0628 \u062f\u06cc\u0646\u06d2 \u06a9\u06d2 \u0628\u0639\u062f \u0628\u06be\u06cc \u0627\u06cc\u06a9 \u0686\u06cc\u0644\u0646\u062c \u062f\u06cc\u06a9\u06be \u0631\u06c1\u0627 \u06c1\u0648\u06ba\u06d4 \u0627\u0633 \u06a9\u06cc \u0648\u062c\u06c1 \u06a9\u06cc\u0627 \u06c1\u06d2\u061f \u062c\u0648\u0627\u0628: \u06cc\u06c1 \u0639\u0627\u0645 \u0637\u0648\u0631 \u067e\u0631 \u06a9\u0633\u06cc \u062c\u0627\u0631\u062d\u0627\u0646\u06c1 \u0627\u0634\u062a\u06c1\u0627\u0631\u06cc \u0628\u0644\u0627\u06a9\u0631 \u06cc\u0627 \u0627\u06cc\u0646\u0679\u06cc \u06a9\u0648\u06a9\u06cc \u062a\u0648\u0633\u06cc\u0639 \u060c \u06cc\u0627 \u06a9\u0631\u0627\u0633 \u0633\u0627\u0626\u0679 \u06a9\u0648\u06a9\u06cc\u0632 \u06a9\u0648 \u0631\u0648\u06a9\u0646\u06d2 \u0648\u0627\u0644\u06cc \u062a\u0631\u062a\u06cc\u0628 \u06a9\u06cc \u0648\u062c\u06c1 \u0633\u06d2 \u06c1\u0648\u062a\u0627 \u06c1\u06d2 \u060c \u0627\u0633 \u0645\u0639\u0627\u0645\u0644\u06d2 \u0645\u06cc\u06ba hcaptcha.com \u06a9\u06d2 \u0644\u0626\u06d2 \u06a9\u0648\u06a9\u06cc \u062c\u0648 \u06a9\u0633\u06cc \u0645\u062e\u062a\u0644\u0641 \u0633\u0627\u0626\u0679 \u067e\u0631 hCaptcha \u062c\u0627\u0648\u0627 \u0627\u0633\u06a9\u0631\u067e\u0679 \u06a9\u06d2 \u0630\u0631\u06cc\u0639\u06c1 \u0633\u06cc\u0679 \u06cc\u0627 \u0686\u06cc\u06a9 \u06a9\u06cc \u062c\u0627\u062a\u06cc \u06c1\u06d2\u06d4 \u060c \u062c\u06cc\u0633\u06d2 \u0622\u067e \u062a\u0634\u0631\u06cc\u0641 \u0644\u06d2 \u062c\u0627\u0631\u06c1\u06d2 \u06c1\u06cc\u06ba\u06d4 hCaptcha \u062a\u06a9 \u0631\u0633\u0627\u0626 \u06a9\u06cc \u06a9\u0648\u06a9\u06cc\u0632 \u062a\u0645\u0627\u0645 \u0645\u0634\u06c1\u0648\u0631 \u0628\u0631\u0627\u0624\u0632\u0631\u0632 \u0627\u0648\u0631 \u0627\u0634\u062a\u06c1\u0627\u0631\u0627\u062a \u06a9\u0648 \u0627\u0646 \u06a9\u06cc \u0645\u0639\u06cc\u0627\u0631\u06cc \u062a\u0631\u062a\u06cc\u0628\u0627\u062a \u06a9\u06d2 \u0633\u0627\u062a\u06be \u06a9\u0627\u0645 \u06a9\u0631\u062a\u06cc \u06c1\u06cc\u06ba \u060c \u0644\u06c1\u0630\u0627 \u0639\u0627\u0645 \u0637\u0648\u0631 \u067e\u0631 \u0646\u0627\u06a9\u0627\u0645\u06cc \u0627\u06cc\u0646\u0679\u06cc \u0627\u06cc\u0688\u0648\u0628\u0644\u0627\u06a9 \u0627\u0633\u06a9\u0631\u067e\u0679 \u06cc\u0627 \u0627\u0633\u06cc \u0637\u0631\u062d \u06a9\u06d2 \u0627\u0635\u0648\u0644\u0648\u06ba \u06a9\u0648 \u0645\u062e\u0635\u0648\u0635 \u0633\u0627\u0626\u0679\u0648\u06ba \u06a9\u0648 \u0646\u0634\u0627\u0646\u06c1 \u0628\u0646\u0627\u0646\u06d2 \u06a9\u06cc \u0648\u062c\u06c1 \u0633\u06d2 \u06c1\u0648\u062a\u06cc \u06c1\u06d2\u06d4 \u062d\u0644: 1. \u0648\u0627\u0626\u0679 \u0644\u0633\u0679 hcaptcha.com \u0627\u0648\u0631 * .hcaptcha.com \u06a9\u0648\u06a9\u06cc\u0632 \u0622\u067e \u06a9\u06d2 \u0627\u0634\u062a\u06c1\u0627\u0631 \u0628\u0644\u0627\u06a9\u0631 \u06cc\u0627 \u0628\u0631\u0627\u0624\u0632\u0631 \u0633\u06cc\u06a9\u06cc\u0648\u0631\u0679\u06cc \u062a\u0648\u0633\u06cc\u0639 \u0645\u06cc\u06ba\u06d4 2. \u0627\u06af\u0631 \u0622\u067e \u0628\u06c1\u0627\u062f\u0631 \u0628\u0631\u0627\u0624\u0632\u0631 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u0631\u0631\u06c1\u06d2 \u06c1\u06cc\u06ba \u060c \u062c\u0633 \u0645\u06cc\u06ba (\u0627\u067e\u0631\u06cc\u0644 2020 \u062a\u06a9) \u06a9\u0633\u06cc \u0642\u0633\u0645 \u06a9\u06cc \u06a9\u0648\u06a9\u06cc \u0648\u0627\u0626\u0679 \u0644\u0633\u0679 \u0646\u0638\u0631 \u0646\u06c1\u06cc\u06ba \u0622\u062a\u06cc \u06c1\u06d2 \u062a\u0648 \u060c \u062a\u0631\u062c\u06cc\u062d\u0627\u062a -> \u0634\u06cc\u0644\u0688\u0632 -> \u06a9\u0648\u06a9\u06cc\u0632 \u067e\u0631 \u062c\u0627\u0626\u06cc\u06ba \u0627\u0648\u0631 \u062a\u0645\u0627\u0645 \u06a9\u0648\u06a9\u06cc\u0632 \u06a9\u06cc \u0627\u062c\u0627\u0632\u062a \u062f\u06cc\u06ba \u0645\u0646\u062a\u062e\u0628 \u06a9\u0631\u06cc\u06ba\u06d4 If. \u0627\u06af\u0631 \u0622\u067e \u062d\u0627\u0644 \u06c1\u06cc \u0645\u06cc\u06ba \u062c\u0627\u0631\u06cc \u06a9\u0631\u062f\u06c1 OS X 10.15 \u06cc\u0627 iOS 13 \u067e\u0631 \u0633\u0641\u0627\u0631\u06cc \u06a9\u0627 \u0628\u06c1\u062a \u06c1\u06cc \u062a\u0627\u0632\u06c1 \u062a\u0631\u06cc\u0646 \u0648\u0631\u0698\u0646 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u0631\u0631\u06c1\u06d2 \u06c1\u06cc\u06ba\u06d4 4 \u060c \u0627\u06cc\u067e\u0644 \u0646\u06d2 \u0627\u0628\u06be\u06cc \u062a\u06cc\u0633\u0631\u06cc \u067e\u0627\u0631\u0679\u06cc \u06a9\u06d2 \u06a9\u0648\u06a9\u06cc\u0632 \u0633\u06d2 \u0645\u062a\u0639\u0644\u0642 \u0633\u0641\u0627\u0631\u06cc \u06a9\u06d2 \u0637\u0631\u0632 \u0639\u0645\u0644 \u06a9\u0648 \u062a\u0628\u062f\u06cc\u0644 \u06a9\u0631\u062f\u06cc\u0627 \u06c1\u06d2 \u060c \u0627\u0648\u0631 \u0627\u0646 \u0633\u0628\u06be\u06cc \u06a9\u0648 \u0628\u0637\u0648\u0631 \u0688\u06cc\u0641\u0627\u0644\u0679 \u0645\u0633\u062f\u0648\u062f \u06a9\u0631\u062f\u06cc\u0627 \u06c1\u06d2\u06d4 \u06c1\u0645 \u0627\u06cc\u06a9 \u062d\u0644 \u067e\u0631 \u0639\u0645\u0644 \u062f\u0631\u0622\u0645\u062f \u06a9\u0631 \u0631\u06c1\u06d2 \u06c1\u06cc\u06ba \u060c \u0644\u06cc\u06a9\u0646 \u0627\u0633 \u062f\u0648\u0631\u0627\u0646 \u0628\u0631\u0627\u06c1 \u06a9\u0631\u0645 \u0633\u0641\u0627\u0631\u06cc \u062a\u0631\u062c\u06cc\u062d\u0627\u062a \u060c \u0631\u0627\u0632\u062f\u0627\u0631\u06cc \u06a9\u06d2 \u0633\u06cc\u06a9\u0634\u0646 \u067e\u0631 \u062c\u0627\u0626\u06cc\u06ba \u060c \u0627\u0648\u0631 \u0648\u06cc\u0628 \u0633\u0627\u0626\u0679 \u0633\u06d2 \u0628\u0627\u062e\u0628\u0631 \u0631\u06c1\u0646\u06d2 \u06a9\u06cc \u0646\u0634\u0627\u0646 \u0632\u062f \u06a9\u0631\u06cc\u06ba: \u0631\u0633\u0627\u0626\u06cc \u06a9\u06d2 \u06a9\u0648\u06a9\u06cc \u06a9\u0648 \u062a\u0648\u0642\u0639 \u06a9\u06d2 \u0645\u0637\u0627\u0628\u0642 \u06a9\u0627\u0645 \u06a9\u0631\u0646\u06d2 \u06a9\u06d2 \u0644 cross \u06a9\u0631\u0627\u0633 \u0633\u0627\u0626\u0679 \u0633\u06d2 \u0628\u0627\u062e\u0628\u0631 \u0631\u06c1\u0646\u06d2 \u0633\u06d2 \u0631\u0648\u06a9\u06cc\u06ba\u06d4 \u0633\u0648\u0627\u0644: \u0645\u06cc\u06ba \u0627\u06cc\u06a9 \u0633\u06d2 \u0632\u06cc\u0627\u062f\u06c1 \u0688\u06cc\u0648\u0627\u0626\u0633\u0632 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u0631\u062a\u0627 \u06c1\u0648\u06ba\u06d4 \u06a9\u06cc\u0627 \u0645\u062c\u06be\u06d2 \u0645\u062a\u0639\u062f\u062f \u0628\u0627\u0631 \u0633\u0627\u0626\u0646 \u0627\u067e \u06a9\u0631\u0646\u06d2 \u06a9\u06cc \u0636\u0631\u0648\u0631\u062a \u06c1\u06d2\u061f \u062c\u0648\u0627\u0628: \u0646\u06c1\u06cc\u06ba \u060c \u0628\u0631\u0627\u06c1 \u06a9\u0631\u0645 \u06a9\u0648\u06a9\u06cc \u06a9\u0648 \u062a\u0631\u062a\u06cc\u0628 \u062f\u06cc\u0646\u06d2 \u06a9\u06d2 \u0644 each \u0622\u067e \u06a9\u06d2 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u0631\u062f\u06c1 \u06c1\u0631 \u0622\u0644\u06d2 \u067e\u0631 \u0627\u0633\u06cc \u0627\u06cc \u0645\u06cc\u0644 \u0644\u0627\u06af \u0627\u0646 \u0644\u0646\u06a9 \u067e\u0631 \u06a9\u0644\u06a9 \u06a9\u0631\u06cc\u06ba\u06d4",
    "page-acc-tr-title": "\u0642\u0627\u0628\u0644 \u0631\u0633\u0627\u0626\u06cc \u0635\u0627\u0631\u0641\u06cc\u0646 \u06a9\u06d2 \u0644\u0626\u06d2: \u0633\u0648\u0627\u0644 \u0648 \u062c\u0648\u0627\u0628 \u0627\u0648\u0631 \u062f\u0634\u0648\u0627\u0631\u06cc \u062d\u0644 \u06a9\u0631\u0646\u06d2 \u06a9\u06d2 \u0631\u06c1\u0646\u0645\u0627",
    "priv-hc": "\u0646\u062c\u06cc \u0645\u0639\u0644\u0648\u0645\u0627\u062a \u06a9\u06cc \u062d\u0641\u0627\u0638\u062a\u06cc",
    "priv-hc-text": "\u0627\u0634\u062a\u06c1\u0627\u0631\u06cc \u0646\u06cc\u0679 \u0648\u0631\u06a9 \u0622\u067e \u06a9\u0648 \u0627\u0646 \u06a9\u06cc \u0645\u0635\u0646\u0648\u0639\u0627\u062a \u06a9\u06d2 \u0637\u0648\u0631 \u067e\u0631 \u062f\u06cc\u06a9\u06be\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4 \u06c1\u0645 \u0646\u06c1\u06cc\u06ba \u06a9\u0631\u062a\u06d2\u06d4 \u06c1\u0645\u06cc\u06ba \u0627\u0633 \u06a9\u06cc \u067e\u0631\u0648\u0627\u06c1 \u0646\u06c1\u06cc\u06ba \u06c1\u06d2 \u06a9\u06c1 \u0635\u0627\u0631\u0641 \u06a9\u0648\u0646 \u06c1\u06cc\u06ba \u060c \u0635\u0631\u0641 \u0627\u0633 \u06a9\u06d2 \u06a9\u06c1 \u0648\u06c1 \u0628\u062f\u062a\u0645\u06cc\u0632\u06cc \u06a9\u0627 \u0634\u06a9\u0627\u0631 \u06c1\u0648\u06ba\u06d4 \u06c1\u0645 \u067e\u0631\u0627\u0626\u06cc\u0648\u06cc\u0633\u06cc \u067e\u0627\u0633 \u062c\u06cc\u0633\u06d2 \u0627\u0642\u062f\u0627\u0645\u0627\u062a \u06a9\u06cc \u062d\u0645\u0627\u06cc\u062a \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba \u060c \u0627\u0648\u0631 \u0631\u0627\u0632\u062f\u0627\u0631\u06cc \u0627\u0648\u0631 \u0633\u0644\u0627\u0645\u062a\u06cc \u06a9\u0648 \u0645\u062a\u0648\u0627\u0632\u0646 \u0628\u0646\u0627\u0646\u06d2 \u06a9\u06d2 \u0644\u0626\u06d2 \u0633\u062e\u062a \u0645\u062d\u0646\u062a \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba\u06d4",
    "sec-hc": "\u0633\u06cc\u06a9\u06cc\u0648\u0631\u0679\u06cc \u0633\u0628 \u0633\u06d2 \u067e\u06c1\u0644\u06d2",
    "sec-hc-text": "hCaptcha \u0646\u0627\u0634\u0631 \u0645\u06cc\u06ba \u0645\u0631\u06a9\u0648\u0632 \u06c1\u06d2\u06d4 \u06c1\u0645 \u0633\u0627\u0626\u0679\u0648\u06ba \u06a9\u0648 \u0627\u0635\u0644 \u0688\u06cc\u0679\u0627 \u062f\u06cc\u0646\u06d2 \u06a9\u06d2 \u0644\u0626\u06d2 \u06a9\u0627\u0645 \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba \u0627\u0648\u0631 \u0627\u0646\u06c1\u06cc\u06ba \u0641\u06cc\u0635\u0644\u06c1 \u06a9\u0631\u0646\u06d2 \u062f\u06cc\u06ba \u06a9\u06c1 \u0627\u0633 \u06a9\u0627 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u06cc\u0633\u06d2 \u06a9\u0631\u06cc\u06ba\u06d4 \u062f\u0631\u0633\u062a\u06af\u06cc \u0622\u067e \u06a9\u06d2 \u0627\u0648\u0631 \u06c1\u0645\u0627\u0631\u06d2 \u0645\u0641\u0627\u062f \u0645\u06cc\u06ba \u06c1\u06d2\u06d4 \u062f\u06cc\u06af\u0631 \u06a9\u06cc\u067e\u0686\u0627 \u0627\u06cc\u0688\u0648\u0631\u0679\u0627\u0626\u0632\u0646\u06af \u06a9\u0645\u067e\u0646\u06cc\u0627\u06ba \u0641\u0631\u0627\u06c1\u0645 \u06a9\u0631\u062a\u06cc \u06c1\u06cc\u06ba\u06d4 \u06c1\u0631 \u0627\u06cc\u06a9 \u0628\u0648\u0679 \u062c\u0633 \u06a9\u0648 \u0627\u0646\u06c1\u0648\u06ba \u0646\u06d2 \u0645\u0633\u062f\u0648\u062f \u06a9\u06cc\u0627 \u06c1\u06d2 \u0627\u0633 \u0633\u06d2 \u0627\u0646 \u06a9\u06cc \u0622\u0645\u062f\u0646\u06cc \u06a9\u0645 \u06c1\u0648\u0633\u06a9\u062a\u06cc \u06c1\u06d2\u06d4",
    "what-is-hcaptcha": "hCaptcha \u06a9\u06cc\u0627 \u06c1\u06d2\u061f",
    "what-is-hcaptcha-text": "hCaptcha \u0622\u067e \u06a9\u06cc \u067e\u0633\u0646\u062f\u06cc\u062f\u06c1 \u0648\u06cc\u0628 \u062e\u062f\u0645\u0627\u062a \u06a9\u0648 \u0627\u0646\u0633\u0627\u0646\u0648\u06ba \u06a9\u06d2 \u0644\u0626\u06d2 \u0622\u0633\u0627\u0646 \u0627\u0648\u0631 \u0645\u0634\u06cc\u0646\u0648\u06ba \u06a9\u06d2 \u0644\u0626\u06d2 \u0645\u0634\u06a9\u0644 \u06c1\u06cc\u06ba \u06a9\u06c1 \u0622\u0633\u0627\u0646 \u0633\u0648\u0627\u0644\u0627\u062a \u067e\u0648\u0686\u06be \u06a9\u0631 \u0628\u0679\u0633 \u060c \u0627\u0633\u067e\u0627\u0645 \u0627\u0648\u0631 \u0628\u062f\u0633\u0644\u0648\u06a9\u06cc \u06a9\u0648 \u0628\u0631\u0642\u0631\u0627\u0631 \u0631\u06a9\u06be\u0646\u06d2 \u0645\u06cc\u06ba \u0645\u062f\u062f \u06a9\u0631\u062a\u0627 \u06c1\u06d2\u06d4",
    "why-hc": "HCaptcha \u06a9\u06cc\u0648\u06ba\u061f",
    "why-hc-text": "\u062c\u0628 \u0628\u06be\u06cc \u0622\u067e \u06a9\u0633\u06cc \u0686\u06cc\u0644\u0646\u062c \u06a9\u0627 \u062c\u0648\u0627\u0628 \u062f\u06cc\u062a\u06d2 \u06c1\u06cc\u06ba \u062a\u0648 \u060c \u06c1\u0645 \u06cc\u06c1 \u0633\u0645\u062c\u06be\u0646\u06d2 \u06a9\u06d2 \u0644\u0626\u06d2 \u0646\u0641\u06cc\u0633 \u0645\u0627\u0688\u0644 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u0631\u062a\u06d2 \u06c1\u06cc\u06ba \u06a9\u06c1 \u0622\u06cc\u0627 \u0622\u067e \u0627\u0646\u0633\u0627\u0646 \u06c1\u06cc\u06ba \u06cc\u0627 \u0645\u0634\u06cc\u0646\u06d4 \u0627\u0633 \u06a9\u06d2 \u0628\u0639\u062f \u062c\u0633 \u0648\u06cc\u0628 \u0633\u0627\u0626\u0679 \u067e\u0631 \u0622\u067e \u062a\u0634\u0631\u06cc\u0641 \u0644\u0627\u0626\u06d2 \u06c1\u06cc\u06ba \u0648\u06c1 \u0627\u0633 \u0646\u062a\u0627\u0626\u062c \u06a9\u0648 \u0627\u0633\u062a\u0639\u0645\u0627\u0644 \u06a9\u0631\u062a\u06d2 \u06c1\u0648\u0626\u06d2 \u0627\u067e\u0646\u06cc \u0633\u0627\u0626\u0679 \u067e\u0631 \u0627\u067e\u0646\u06d2 \u062a\u062c\u0631\u0628\u06d2 \u06a9\u0648 \u0628\u06c1\u062a\u0631 \u0628\u0646\u0627\u0646\u06d2 \u0645\u06cc\u06ba \u0645\u062f\u062f \u062f\u06cc\u062a\u06cc \u06c1\u06d2\u06d4"
}