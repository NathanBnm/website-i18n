{
    "acc-opt": "\u0632\u0649\u064a\u0627\u0631\u06d5\u062a \u0642\u0649\u0644\u0649\u0634 \u062a\u0627\u0644\u0644\u0627\u0646\u0645\u0649\u0644\u0649\u0631\u0649",
    "acc-opt-but": "\u0642\u0648\u0644\u0627\u064a\u0644\u0649\u0642 \u062a\u0627\u0644\u0644\u0627\u0634\u0644\u0627\u0631 \u06be\u06d5\u0642\u0642\u0649\u062f\u0649\u0643\u0649 \u062a\u06d5\u067e\u0633\u0649\u0644\u0627\u062a\u0644\u0627\u0631",
    "acc-opt-text": "\u0643\u06c6\u0631\u06c8\u0634 \u0642\u06c7\u06cb\u06cb\u0649\u062a\u0649\u06ad\u0649\u0632 \u0626\u0627\u062c\u0649\u0632\u0645\u06c7 \u064a\u0627\u0643\u0649 \u0628\u0648\u0644\u0645\u0649\u0633\u0627 hCaptchas \u0646\u0649 \u062a\u0627\u0645\u0627\u0645\u0644\u0649\u064a\u0627\u0644\u0645\u0627\u064a\u0633\u0649\u0632\u061f",
    "accessibility-link": "\u0642\u0648\u0644\u0627\u064a\u0644\u0649\u0642",
    "appreciate-in": "\u0643\u0649\u0631\u06af\u06c8\u0632\u06af\u06d5\u0646\u0644\u0649\u0631\u0649\u06ad\u0649\u0632\u0646\u0649 \u0642\u06d5\u062f\u0649\u0631\u0644\u06d5\u064a\u0645\u0649\u0632. \u06be\u06d5\u0631 \u0642\u0627\u0646\u062f\u0627\u0642 \u0645\u06d5\u0633\u0649\u0644\u0649\u0646\u0649 \u0628\u0649\u0632\u06af\u06d5 \u0628\u0649\u0644\u062f\u06c8\u0631\u06c8\u06ad \u060c \u0628\u0649\u0632 \u062a\u06d0\u0632\u062f\u0649\u0646 \u06be\u06d5\u0644 \u0642\u0649\u0644\u0649\u0645\u0649\u0632.",
    "fb-but": "\u0628\u0649\u0632\u06af\u06d5 \u062a\u06d5\u0643\u0644\u0649\u067e-\u067e\u0649\u0643\u0649\u0631 \u0626\u06d5\u06cb\u06d5\u062a\u0649\u06ad",
    "feedback-text": "\u0633\u0649\u0632 \u0643\u06c6\u0631\u06af\u06d5\u0646 hCaptcha \u06be\u06d5\u0642\u0642\u0649\u062f\u06d5 \u062a\u06d5\u0643\u0644\u0649\u067e \u0628\u06d0\u0631\u0649\u06ad",
    "home-ent-1": "\u0643\u0627\u0631\u062e\u0627\u0646\u0649\u0645\u0649\u0632\u0646\u0649\u06ad \u0628\u0649\u062e\u06d5\u062a\u06d5\u0631\u0644\u0649\u0643 \u062a\u06d5\u0645\u0649\u0646\u0627\u062a\u0649\u0646\u0649 \u0626\u0649\u0632\u062f\u06d5\u06cb\u0627\u062a\u0627\u0645\u0633\u0649\u0632\u061f",
    "home-ent-2": "\u064a\u06d0\u06ad\u0649 \u0645\u06d5\u062e\u0633\u06c7\u0633 \u062a\u0648\u0631 \u0628\u06d0\u0643\u0649\u062a\u0649\u0645\u0649\u0632\u0646\u0649 \u0632\u0649\u064a\u0627\u0631\u06d5\u062a \u0642\u0649\u0644\u0649\u06ad: BotStop.com",
    "home-intro-body": "\u0628\u0648\u062a\u06c7\u0644\u0643\u0649\u0644\u0627\u0631\u0646\u0649 \u0633\u0627\u0642\u0644\u0627\u067e \u0642\u06d0\u0644\u0649\u0634 \u0626\u06c8\u0686\u06c8\u0646 \u062a\u06c7\u062a\u0642\u06c7\u0686 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u06cb\u0627\u062a\u0627\u0645\u0633\u0649\u0632\u061f hCaptcha \u0626\u0627\u0628\u0648\u0646\u062a\u0644\u0627\u0631\u0646\u0649\u06ad \u0645\u06d5\u062e\u067e\u0649\u064a\u06d5\u062a\u0644\u0649\u0643\u0649\u0646\u0649 \u0642\u0648\u063a\u062f\u0627\u064a\u062f\u06c7 \u060c \u062a\u0648\u0631 \u0628\u06d0\u0643\u06d5\u062a\u0644\u06d5\u0631\u0646\u0649 \u0645\u06c7\u0643\u0627\u067e\u0627\u062a\u0644\u0627\u064a\u062f\u06c7 \u06cb\u06d5 \u0634\u0649\u0631\u0643\u06d5\u062a\u0644\u06d5\u0631\u0646\u0649\u06ad \u0633\u0627\u0646\u0644\u0649\u0642 \u0645\u06d5\u0644\u06c7\u0645\u0627\u062a\u0644\u0649\u0631\u0649\u063a\u0627 \u0628\u06d5\u0644\u06af\u06d5 \u0642\u0648\u064a\u06c7\u0634\u0649\u063a\u0627 \u064a\u0627\u0631\u062f\u06d5\u0645 \u0628\u06d0\u0631\u0649\u062f\u06c7. \u0626\u06c7 reCAPTCHA \u0646\u0649\u06ad \u0686\u06c8\u0634\u06c8\u0634 \u0626\u0648\u0631\u0646\u0649: \u0628\u0649\u0631 \u0646\u06d5\u0686\u0686\u06d5 \u0645\u0649\u0646\u06c7\u062a \u0626\u0649\u0686\u0649\u062f\u06d5 \u0626\u0627\u0644\u0645\u0627\u0634\u062a\u06c7\u0631\u0627\u0644\u0627\u064a\u0633\u0649\u0632.",
    "home-intro-head": "\u062a\u06d0\u062e\u0649\u0645\u06c7 \u0643\u06c6\u067e \u0628\u0648\u062a\u06c7\u0644\u0643\u0649\u0644\u0627\u0631\u0646\u0649 \u062a\u0648\u062e\u062a\u0649\u062a\u0649\u06ad. \u0626\u0649\u0634\u0644\u06d5\u062a\u0643\u06c8\u0686\u0649\u0646\u0649\u06ad \u0645\u06d5\u062e\u067e\u0649\u064a\u06d5\u062a\u0644\u0649\u0643\u0649\u0646\u0649 \u0642\u0648\u063a\u062f\u0627\u0634\u0646\u0649 \u0628\u0627\u0634\u0644\u0627\u06ad.",
    "home-main-1": "\u0634\u06d5\u062e\u0633\u0649\u064a. \u0628\u0649\u062e\u06d5\u062a\u06d5\u0631. \u062a\u06d0\u062e\u0649\u0645\u06c7 \u062a\u06d0\u0632. \u0628\u0649\u0632 \u0626\u06c7\u0646\u0649 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u0645\u0649\u0632.",
    "home-main-2": "\u0645\u06d5\u062e\u067e\u0649\u064a\u06d5\u062a\u0644\u0649\u0643\u0646\u0649 \u0645\u06d5\u0631\u0643\u06d5\u0632\u0644\u06d5\u0634\u062a\u06c8\u0631\u062f\u0649",
    "home-main-2c": "\u0626\u06d0\u0644\u0627\u0646 \u062a\u0648\u0631\u0649 \u0632\u0649\u064a\u0627\u0631\u06d5\u062a\u0686\u0649\u0644\u0649\u0631\u0649\u06ad\u0649\u0632\u0646\u0649 \u0645\u06d5\u06be\u0633\u06c7\u0644\u0627\u062a \u062f\u06d5\u067e \u0642\u0627\u0631\u0627\u064a\u062f\u06c7. HCaptcha \u0626\u06c8\u0686\u06c8\u0646 \u0645\u06d5\u06be\u0633\u06c7\u0644\u0627\u062a \u0626\u06c7\u0644\u0627\u0631 \u062a\u06d5\u0645\u0649\u0646\u0644\u0649\u06af\u06d5\u0646 \u062e\u0649\u0632\u0645\u06d5\u062a. \u0626\u0627\u0628\u0648\u0646\u062a\u0644\u0649\u0631\u0649\u06ad\u0649\u0632\u0646\u0649\u06ad \u0643\u0649\u0645 \u0626\u0649\u0643\u06d5\u0646\u0644\u0649\u0643\u0649 \u0628\u0649\u0644\u06d5\u0646 \u0643\u0627\u0631\u0649\u0645\u0649\u0632 \u064a\u0648\u0642 \u060c \u067e\u06d5\u0642\u06d5\u062a \u0626\u06c7\u0644\u0627\u0631\u0646\u0649\u06ad \u064a\u0627\u0645\u0627\u0646 \u063a\u06d5\u0631\u06d5\u0632\u0644\u0649\u0643 \u0628\u0648\u0644\u06c7\u0634\u0649 \u0645\u06c7\u0645\u0643\u0649\u0646. \u0645\u06d5\u062e\u067e\u0649\u064a\u06d5\u062a\u0644\u0649\u0643 \u0626\u06c6\u062a\u0643\u0649\u0644\u0649\u06af\u06d5 \u0626\u0648\u062e\u0634\u0627\u0634 \u062a\u06d5\u0634\u06d5\u0628\u0628\u06c7\u0633\u0644\u0627\u0631\u0646\u0649 \u0642\u0648\u0644\u0644\u0627\u064a\u0645\u0649\u0632 \u060c \u0634\u06d5\u062e\u0633\u0649\u064a \u06cb\u06d5 \u0628\u0649\u062e\u06d5\u062a\u06d5\u0631\u0644\u0649\u0643\u0646\u0649 \u062a\u06d5\u06ad\u067e\u06c7\u06ad\u0644\u0627\u0634\u062a\u06c7\u0631\u06c7\u0634 \u0626\u06c8\u0686\u06c8\u0646 \u062a\u0649\u0631\u0649\u0634\u0649\u0645\u0649\u0632.",
    "home-main-3": "\u0628\u0649\u062e\u06d5\u062a\u06d5\u0631\u0644\u0649\u0643 \u0628\u0649\u0631\u0649\u0646\u0686\u0649",
    "home-main-3c": "\u0626\u06d0\u0646\u0649\u0642\u0644\u0649\u0642 \u0633\u0649\u0632\u0646\u0649\u06ad \u06cb\u06d5 \u0628\u0649\u0632\u0646\u0649\u06ad \u0645\u06d5\u0646\u067e\u06d5\u0626\u06d5\u062a\u0649\u0645\u0649\u0632\u062f\u06d5. hCaptcha \u0646\u06d5\u0634\u0631\u0649\u064a\u0627\u062a\u0646\u0649 \u0626\u0627\u0633\u0627\u0633 \u0642\u0649\u0644\u063a\u0627\u0646. \u0628\u0649\u0632 \u0633\u0649\u0632\u06af\u06d5 \u06be\u06d5\u0642\u0649\u0642\u0649\u064a \u0633\u0627\u0646\u0644\u0649\u0642 \u0645\u06d5\u0644\u06c7\u0645\u0627\u062a \u0628\u06d0\u0631\u0649\u0634 \u0626\u06c8\u0686\u06c8\u0646 \u062a\u0649\u0631\u0649\u0634\u0649\u0645\u0649\u0632 \u06be\u06d5\u0645\u062f\u06d5 \u0626\u06c7\u0646\u0649 \u0642\u0627\u0646\u062f\u0627\u0642 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u0634\u0646\u0649 \u0642\u0627\u0631\u0627\u0631 \u0642\u0649\u0644\u0649\u0645\u0649\u0632. \u0628\u0627\u0634\u0642\u0627 \u062a\u06c7\u062a\u0642\u06c7\u0686\u0644\u0627\u0631 \u062a\u06d5\u0631\u0649\u067e\u0649\u062f\u0649\u0646 \u062a\u06d5\u0645\u0649\u0646\u0644\u06d5\u0646\u06af\u06d5\u0646",
    "home-main-3c2": "\u0626\u06d0\u0644\u0627\u0646 \u0634\u0649\u0631\u0643\u06d5\u062a\u0644\u0649\u0631\u0649. \u0628\u06c7 \u0626\u06c7\u0644\u0627\u0631\u0646\u0649\u06ad \u062a\u0648\u0633\u06c7\u06cb\u0627\u0644\u063a\u0627\u0646 \u06be\u06d5\u0631 \u0628\u0649\u0631 \u0628\u0648\u062a\u0646\u0649\u06ad \u0643\u0649\u0631\u0649\u0645\u0649\u0646\u0649 \u0626\u0627\u0632\u0627\u064a\u062a\u0649\u0634\u0649 \u0645\u06c7\u0645\u0643\u0649\u0646\u0644\u0649\u0643\u0649\u062f\u0649\u0646 \u062f\u06d0\u0631\u06d5\u0643 \u0628\u06d0\u0631\u0649\u062f\u06c7.",
    "home-main-4": "\u062a\u06d0\u062e\u0649\u0645\u06c7 \u064a\u0627\u062e\u0634\u0649 \u0626\u0649\u0634\u0644\u06d5\u062a\u0643\u06c8\u0686\u0649 \u062a\u06d5\u062c\u0631\u0649\u0628\u0649\u0633\u0649",
    "home-main-4c": "\u0645\u06c7\u0631\u06d5\u0643\u0643\u06d5\u067e \u0645\u0627\u0634\u0649\u0646\u0627 \u0626\u06c6\u06af\u0649\u0646\u0649\u0634 \u0645\u0648\u062f\u06d0\u0644\u0644\u0649\u0631\u0649 hCaptcha \u062a\u06d5\u0631\u0649\u067e\u0649\u062f\u0649\u0646 \u0626\u0649\u0646\u0633\u0627\u0646\u0649\u064a\u06d5\u062a\u0646\u0649 \u0628\u06d5\u0644\u06af\u0649\u0644\u06d5\u0634\u062a\u06d5 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u0644\u0649\u062f\u06c7. \u0628\u06c7 \u0626\u0627\u0631\u0642\u0649\u0644\u0649\u0642 \u0626\u0627\u0628\u0648\u0646\u062a\u0644\u0627\u0631\u063a\u0627 \u0628\u06d5\u0644\u06af\u06d5 \u0642\u0648\u064a\u06c7\u0634 \u0626\u06c8\u0686\u06c8\u0646 \u067e\u0627\u064a\u062f\u0649\u0644\u0649\u0642 \u0645\u0627\u0631\u0643\u0627 \u0633\u0627\u0646\u0644\u0649\u0642 \u0645\u06d5\u0644\u06c7\u0645\u0627\u062a\u0644\u0649\u0631\u0649\u0646\u0649 \u0626\u0649\u0634\u0644\u06d5\u067e\u0686\u0649\u0642\u0649\u0631\u0649\u0634 \u0628\u0649\u0644\u06d5\u0646 \u0628\u0649\u0631 \u06cb\u0627\u0642\u0649\u062a\u062a\u0627 \u060c \u0626\u0627\u0628\u0648\u0646\u062a\u0644\u0649\u0631\u0649\u06ad\u0649\u0632\u062f\u0649\u0646 \u0628\u0649\u0632\u0627\u0631 \u0628\u0648\u0644\u0649\u062f\u0649\u063a\u0627\u0646 \u06cb\u06d5\u0632\u0649\u067e\u0649\u0644\u06d5\u0631\u0646\u0649 \u062a\u06d5\u0644\u06d5\u067e \u0642\u0649\u0644\u0649\u0634\u0649\u0645\u0649\u0632 \u06cb\u06d5 \u0626\u06c7\u0644\u0627\u0631\u0646\u0649\u06ad \u06cb\u0627\u0642\u062a\u0649\u06ad\u0649\u0632\u0646\u0649 \u0626\u0627\u0632 \u0633\u06d5\u0631\u067e \u0642\u0649\u0644\u0649\u0634\u0649\u0645\u0649\u0632 \u0643\u06d0\u0631\u06d5\u0643.",
    "home-main-plans": "hCaptcha \u06be\u06d5\u0631 \u0642\u0627\u0646\u062f\u0627\u0642 \u0686\u0648\u06ad\u0644\u06c7\u0642\u062a\u0649\u0643\u0649 \u0646\u06d5\u0634\u0631\u0649\u064a\u0627\u062a\u0686\u0649\u0644\u0627\u0631 \u0626\u06c8\u0686\u06c8\u0646 \u06be\u06d5\u0642\u0633\u0649\u0632. \u0642\u0627\u0646\u062f\u0627\u0642\u0644\u0627 \u0628\u0648\u0644\u0645\u0649\u0633\u06c7\u0646 \u060c \u062a\u06d0\u062e\u0649\u0645\u06c7 \u0643\u06c6\u067e \u0643\u0648\u0646\u062a\u0631\u0648\u0644 \u0642\u0649\u0644\u0649\u0634\u0642\u0627 \u0626\u06d0\u06be\u062a\u0649\u064a\u0627\u062c\u0644\u0649\u0642 \u0643\u0627\u0631\u062e\u0627\u0646\u0627 \u06cb\u06d5 \u0633\u06c7\u067e\u0649\u0644\u0627\u0631 \u0626\u06c8\u0686\u06c8\u0646 \u0628\u0649\u0632\u0646\u0649\u06ad \u0643\u0627\u0631\u062e\u0627\u0646\u0649\u0646\u0649\u06ad \u06be\u06d5\u0644 \u0642\u0649\u0644\u0649\u0634 \u0686\u0627\u0631\u0649\u0633\u0649 \u0628\u0627\u0631.",
    "home-main-plans2": "hCaptcha \u0643\u0627\u0631\u062e\u0627\u0646\u0627 \u06be\u06d5\u0644 \u0642\u0649\u0644\u0649\u0634 \u0686\u0627\u0631\u0649\u0633\u0649 \u064a\u06d5\u0631\u0634\u0627\u0631\u0649\u062f\u0649\u0643\u0649 \u0628\u0649\u0631 \u0642\u0649\u0633\u0649\u0645 \u0686\u0648\u06ad \u0634\u0649\u0631\u0643\u06d5\u062a\u0644\u06d5\u0631 \u0626\u06c8\u0686\u06c8\u0646 \u0646\u06d5\u0686\u0686\u06d5 \u0626\u0648\u0646 \u0645\u0649\u0644\u064a\u0648\u0646 \u062a\u0648\u0631\u0628\u06d0\u0643\u06d5\u062a\u062a\u0649\u0643\u0649 \u0645\u0649\u0644\u064a\u0648\u0646\u0644\u0649\u063a\u0627\u0646 \u0626\u0627\u0628\u0648\u0646\u062a\u0644\u0627\u0631\u0646\u0649 \u0642\u0648\u063a\u062f\u0627\u064a\u062f\u06c7. \u062a\u06d0\u062e\u0649\u0645\u06c7 \u0643\u06c6\u067e \u0626\u06c7\u0686\u06c7\u0631\u063a\u0627 \u0626\u06d0\u0631\u0649\u0634\u0649\u0634 \u0626\u06c8\u0686\u06c8\u0646 \u0628\u0649\u0632 \u0628\u0649\u0644\u06d5\u0646 \u0626\u0627\u0644\u0627\u0642\u0649\u0644\u0649\u0634\u0649\u06ad \u064a\u0627\u0643\u0649 \u0632\u0649\u064a\u0627\u0631\u06d5\u062a \u0642\u0649\u0644\u0649\u06ad",
    "home-main-plans3": "hCaptcha \u0643\u0627\u0631\u062e\u0627\u0646\u0627 \u06be\u06d5\u0644 \u0642\u0649\u0644\u0649\u0634 \u0686\u0627\u0631\u0649\u0633\u0649 \u064a\u06d5\u0631\u0634\u0627\u0631\u0649\u062f\u0649\u0643\u0649 \u0628\u0649\u0631 \u0642\u0649\u0633\u0649\u0645 \u0686\u0648\u06ad \u0634\u0649\u0631\u0643\u06d5\u062a\u0644\u06d5\u0631 \u0626\u06c8\u0686\u06c8\u0646 \u0646\u06d5\u0686\u0686\u06d5 \u0626\u0648\u0646 \u0645\u0649\u0644\u064a\u0648\u0646 \u062a\u0648\u0631\u0628\u06d0\u0643\u06d5\u062a\u062a\u0649\u0643\u0649 \u0645\u0649\u0644\u064a\u0648\u0646\u0644\u0649\u063a\u0627\u0646 \u0626\u0627\u0628\u0648\u0646\u062a\u0644\u0627\u0631\u0646\u0649 \u0642\u0648\u063a\u062f\u0627\u064a\u062f\u06c7. \u062a\u06d0\u062e\u0649\u0645\u06c7 \u0643\u06c6\u067e \u0626\u06c7\u0686\u06c7\u0631\u063a\u0627 \u0626\u06d0\u0631\u0649\u0634\u0649\u0634 \u0626\u06c8\u0686\u06c8\u0646 \u0628\u0649\u0632 \u0628\u0649\u0644\u06d5\u0646 \u0626\u0627\u0644\u0627\u0642\u0649\u0644\u0649\u0634\u0649\u06ad \u064a\u0627\u0643\u0649 \u0632\u0649\u064a\u0627\u0631\u06d5\u062a \u0642\u0649\u0644\u0649\u06ad",
    "home-main-plans4": "\u062a\u06d5\u067e\u0633\u0649\u0644\u0627\u062a\u0644\u0627\u0631 \u0626\u06c8\u0686\u06c8\u0646.",
    "learn-more-hcaptcha": "HCaptcha \u06be\u06d5\u0642\u0642\u0649\u062f\u0649\u0643\u0649 \u062a\u06d5\u067e\u0633\u0649\u0644\u0627\u062a\u0644\u0627\u0631",
    "page-acc-faq-1": "\u0633\u0648\u0626\u0627\u0644: HCaptcha \u0628\u06c6\u0644\u06c8\u0645\u0649 508 + WCAG 2.0 \u0645\u0627\u0633 \u0643\u06d0\u0644\u06d5\u0645\u062f\u06c7\u061f \u062c\u0627\u06cb\u0627\u0628: \u0628\u0649\u0632 \u0634\u06c7\u0646\u0649\u06ad\u063a\u0627 \u0626\u0649\u0634\u0649\u0646\u0649\u0645\u0649\u0632: \u06be\u06d5\u0631 \u0642\u0627\u0646\u062f\u0627\u0642 \u0634\u06d5\u0643\u0649\u0644\u062f\u0649\u0643\u0649 \u062a\u0648\u0633\u0627\u0644\u063a\u06c7\u063a\u0627 \u0626\u06c7\u0686\u0631\u0649\u063a\u0627\u0646 \u0626\u0627\u0628\u0648\u0646\u062a\u0644\u0627\u0631 \u062a\u0648\u0631 \u0643\u06c6\u0631\u06d5\u0644\u06d5\u064a\u062f\u0649\u063a\u0627\u0646 \u06cb\u06d5 \u062c\u06d5\u062f\u06cb\u06d5\u0644\u06af\u06d5 \u062a\u06d0\u0643\u0649\u0633\u062a \u0643\u0649\u0631\u06af\u06c8\u0632\u06d5\u0644\u06d5\u064a\u062f\u0649\u063a\u0627\u0646 \u0628\u0627\u0631\u0644\u0649\u0642 \u0626\u0627\u0628\u0648\u0646\u062a\u0644\u0627\u0631 \u062a\u0649\u0632\u0649\u0645\u0644\u0627\u062a\u0642\u0627\u0646\u062f\u0649\u0646 \u0643\u06d0\u064a\u0649\u0646 hCaptcha \u062a\u06d5\u0631\u0649\u067e\u0649\u062f\u0649\u0646 \u0642\u0648\u063a\u062f\u0649\u0644\u0649\u062f\u0649\u063a\u0627\u0646 \u0645\u06c7\u0644\u0627\u0632\u0649\u0645\u06d5\u062a\u0644\u06d5\u0631\u06af\u06d5 \u0626\u06d0\u0631\u0649\u0634\u06d5\u0644\u06d5\u064a\u062f\u06c7. \u0642\u0627\u0646\u062f\u0627\u0642\u0644\u0627 \u0628\u0648\u0644\u0645\u0649\u0633\u06c7\u0646 \u060c \u0628\u06c7 \u0642\u0627\u0646\u06c7\u0646\u0649\u064a \u0645\u06d5\u0633\u0644\u0649\u06be\u06d5\u062a \u0626\u06d5\u0645\u06d5\u0633: \u0626\u06c6\u0632\u0649\u06ad\u0649\u0632\u0646\u0649\u06ad \u0626\u06d5\u0645\u06d5\u0644\u0649\u064a\u0644\u06d5\u0634\u062a\u06c8\u0631\u06c8\u0634\u0649\u06ad\u0649\u0632\u0646\u0649 \u0626\u0648\u064a\u0644\u0649\u0634\u0649\u067e \u060c \u0626\u06c6\u0632\u0649\u06ad\u0649\u0632\u0646\u0649\u06ad \u0628\u0627\u06be\u0627\u0644\u0649\u0634\u0649\u06ad\u0649\u0632\u063a\u0627 \u0643\u0627\u067e\u0627\u0644\u06d5\u062a\u0644\u0649\u0643 \u0642\u0649\u0644\u0649\u0634\u0649\u06ad\u0649\u0632 \u0643\u06d0\u0631\u06d5\u0643.",
    "page-acc-faq-2": "\u0633\u0648\u0626\u0627\u0644: \u0633\u0649\u0632 \u0626\u0627\u06cb\u0627\u0632\u063a\u0627 \u0626\u0648\u062e\u0634\u0627\u0634 \u0628\u0627\u0634\u0642\u0627 \u0642\u0648\u0644\u0627\u064a\u0644\u0649\u0642 (a11y) \u062a\u0627\u0644\u0644\u0627\u0646\u0645\u0649\u0644\u0649\u0631\u0649\u0646\u0649 \u0626\u0649\u0634\u0644\u06d5\u06cb\u0627\u062a\u0627\u0645\u0633\u0649\u0632\u061f \u062c\u0627\u06cb\u0627\u0628: \u0626\u0649\u0644\u06af\u0649\u0631\u0649 \u0626\u0627\u06cb\u0627\u0632\u0644\u0649\u0642 \u062a\u06c7\u062a\u0642\u06c7\u0686\u0642\u0627 \u0626\u0648\u062e\u0634\u0627\u0634 \u0626\u0627\u0645\u0645\u0649\u0628\u0627\u0628 \u062a\u0627\u0644\u0644\u0627\u0634\u0644\u0627\u0631 \u0646\u06c7\u0631\u063a\u06c7\u0646 1111 \u0626\u0649\u0634\u0644\u06d5\u062a\u0643\u06c8\u0686\u0649\u0644\u06d5\u0631\u0646\u0649 \u0643\u06d5\u0645\u0633\u0649\u062a\u0649\u062f\u06c7 \u06cb\u06d5 \u0632\u0627\u0645\u0627\u0646\u0649\u06cb\u0649 \u0645\u0627\u0634\u0649\u0646\u0627 \u0626\u06c6\u06af\u0649\u0646\u0649\u0634 \u062a\u06d0\u062e\u0646\u0649\u0643\u0649\u0633\u0649 \u062a\u06d5\u0631\u0649\u067e\u0649\u062f\u0649\u0646 \u0626\u0627\u0633\u0627\u0646\u0644\u0627 \u0645\u06d5\u063a\u0644\u06c7\u067e \u0628\u0648\u0644\u0649\u062f\u06c7. \u0628\u06c7 \u0646\u06c6\u06cb\u06d5\u062a\u062a\u0649\u0643\u0649 \u0626\u0627\u06cb\u0627\u0632 \u062e\u0649\u0631\u0649\u0633\u0644\u0649\u0631\u0649\u0646\u0649 \u062a\u06d0\u062e\u0649\u0645\u06c7 \u0642\u0649\u064a\u0649\u0646\u0644\u0627\u0634\u062a\u06c7\u0631\u06c7\u06cb\u06d5\u062a\u062a\u0649 \u060c \u0634\u0627\u06cb\u0642\u06c7\u0646 \u060c \u063a\u06d5\u0644\u0649\u062a\u06d5 \u06cb\u0627\u0642\u0649\u062a \u060c \u0626\u0627\u062f\u06d5\u062a\u062a\u0649\u0646 \u062a\u0627\u0634\u0642\u0649\u0631\u0649 \u0633\u06c6\u0632 \u0628\u0649\u0631\u0649\u0643\u0645\u0649\u0633\u0649 \u0642\u0627\u062a\u0627\u0631\u0644\u0649\u0642\u0644\u0627\u0631 \u06be\u06c7\u062c\u06c7\u0645\u0686\u0649\u0644\u0627\u0631\u0646\u0649 \u0645\u06d5\u063a\u0644\u06c7\u0628 \u0642\u0649\u0644\u062f\u0649. \u0634\u06c7\u06ad\u0627 \u0628\u0649\u0632 \u0628\u06c7 \u062e\u0649\u0644 \u0626\u06c7\u0633\u06c7\u0644 \u0628\u0649\u0644\u06d5\u0646 \u0631\u0649\u0642\u0627\u0628\u06d5\u062a\u062a\u0649\u0646 \u067e\u06c8\u062a\u06c8\u0646\u0644\u06d5\u064a \u0633\u0627\u0642\u0644\u0649\u0646\u0649\u0634\u0642\u0627 \u0626\u0627\u0646\u0686\u06d5 \u0642\u0649\u0632\u0649\u0642\u0645\u0627\u064a\u0645\u0649\u0632 \u060c \u0626\u06d5\u0645\u0645\u0627 a11y \u062c\u06d5\u0645\u0626\u0649\u064a\u0649\u062a\u0649\u062f\u0649\u0646 \u0626\u06d0\u06be\u062a\u0649\u064a\u0627\u062c \u0628\u0648\u0644\u0633\u0627 \u0626\u06c7\u0646\u0649 \u0626\u0648\u064a\u0644\u0649\u0634\u0649\u0645\u0649\u0632. \u0642\u0627\u0646\u062f\u0627\u0642\u0644\u0627 \u0628\u0648\u0644\u0645\u0649\u0633\u06c7\u0646 \u060c \u0628\u0649\u0632 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u0634\u0686\u0627\u0646\u0644\u0649\u0642\u0649 \u0642\u06d0\u067e\u0649\u0646\u0649\u06ad \u0645\u06d5\u062e\u067e\u0649\u064a\u06d5\u062a\u0644\u0649\u0643 \u0626\u06c6\u062a\u0643\u0649\u0644\u0649\u06af\u06d5 \u0628\u06d5\u0643 \u0642\u0649\u0632\u0649\u0642\u0649\u0645\u0649\u0632. \u0626\u0649\u0634\u0649\u0646\u0649\u0645\u0649\u0632\u0643\u0649 \u060c \u0628\u0649\u0632\u0646\u0649\u06ad \u06be\u0627\u0632\u0649\u0631\u0642\u0649 a11y \u0626\u06c7\u0633\u06c7\u0644\u0649\u0645\u0649\u0632\u0646\u0649 \u0645\u06d5\u062e\u067e\u0649\u064a\u06d5\u062a\u0644\u0649\u0643 \u067e\u0627\u0633\u0648\u0646\u0649 \u062a\u0627\u0631\u0642\u0649\u062a\u0649\u0634 \u0628\u0649\u0644\u06d5\u0646 \u0628\u0649\u0631\u0644\u06d5\u0634\u062a\u06c8\u0631\u06af\u06d5\u0646\u062f\u06d5 \u060c a11y \u0626\u0649\u0634\u0644\u06d5\u062a\u0643\u06c8\u0686\u0649\u0644\u06d5\u0631\u0646\u0649\u06ad \u0628\u0649\u062e\u06d5\u062a\u06d5\u0631 \u0632\u0649\u064a\u0627\u0631\u06d5\u062a \u0642\u0649\u0644\u0649\u0634\u0649\u063a\u0627 \u064a\u0648\u0644 \u0642\u0648\u064a\u0649\u062f\u06c7 \u060c \u0626\u06c7\u0644\u0627\u0631\u0646\u0649\u06ad \u0626\u06d0\u0642\u0649\u0645\u0649 \u062a\u06d0\u062e\u0649\u0645\u06c7 \u0634\u06d5\u062e\u0633\u0649\u064a \u0628\u0648\u0644\u0649\u062f\u06c7 \u060c \u0634\u06c7\u0646\u0649\u06ad \u0628\u0649\u0644\u06d5\u0646 \u0628\u0649\u0631 \u06cb\u0627\u0642\u0649\u062a\u062a\u0627 \u060c a11y \u062a\u0627\u0644\u0644\u0627\u0646\u0645\u0649\u0644\u0649\u0631\u0649 \u0628\u0648\u0644\u063a\u0627\u0646\u062f\u0627 \u0645\u06c7\u0642\u06d5\u0631\u0631\u06d5\u0631 \u064a\u06c8\u0632 \u0628\u06d0\u0631\u0649\u062f\u0649\u063a\u0627\u0646 \u0645\u0627\u0634\u0649\u0646\u0627 \u062a\u0649\u062c\u0627\u0631\u06d5\u062a\u0686\u0649\u0644\u0649\u0631\u0649\u0646\u0649\u06ad \u062e\u0648\u0631\u0644\u0649\u0646\u0649\u0634\u0649\u0646\u0649 \u0686\u06d5\u0643\u0644\u06d5\u064a\u062f\u06c7. \u0628\u0649\u0632 \u0628\u06c7 \u064a\u06d0\u06ad\u0649 \u062a\u06d0\u062e\u0646\u0649\u0643\u0649\u0646\u0649 \u0642\u06d0\u0644\u0649\u067e\u0644\u0627\u0634\u062a\u06c7\u0631\u0649\u062f\u0649\u063a\u0627\u0646 IETF \u062e\u0649\u0632\u0645\u06d5\u062a \u06af\u06c7\u0631\u06c7\u067e\u067e\u0649\u0633\u0649\u0646\u0649\u06ad \u0626\u0627\u0643\u062a\u0649\u067e \u0642\u0627\u062a\u0646\u0627\u0634\u0642\u06c7\u0686\u0649\u0644\u0649\u0631\u0649\u0645\u0649\u0632.",
    "page-acc-faq-3": "\u0633\u0648\u0626\u0627\u0644: \u0634\u06d5\u062e\u0633\u0649\u064a \u0633\u0649\u0631\u0686\u06c7\u061f \u062a\u0649\u0632\u0649\u0645\u0644\u0627\u0634 \u0645\u06d5\u0644\u06c7\u0645 \u062c\u06d5\u06be\u06d5\u062a\u062a\u0649\u0646 a11y \u0643\u06c6\u0631\u06c8\u0634 \u0633\u0627\u0646\u0644\u0649\u0642 \u0645\u06d5\u0644\u06c7\u0645\u0627\u062a\u0644\u0649\u0631\u0649\u0646\u0649 \u0626\u0627\u0634\u0643\u0627\u0631\u0649\u0644\u0627\u0645\u062f\u06c7\u061f \u0626\u06d0\u0644\u06d0\u0643\u062a\u0631\u0648\u0646\u0644\u06c7\u0642 \u062e\u06d5\u062a\u0646\u0649 \u0642\u0627\u0646\u062f\u0627\u0642 \u0642\u0649\u0644\u0649\u0633\u0649\u0632\u061f \u062c\u0627\u06cb\u0627\u0628: hCaptcha \u0628\u0627\u0634\u062a\u0649\u0646-\u0626\u0627\u062e\u0649\u0631 \u0634\u06d5\u062e\u0633\u0649\u064a \u0645\u06d5\u062e\u067e\u0649\u064a\u06d5\u062a\u0644\u0649\u0643 \u0626\u06c8\u0686\u06c8\u0646 \u0644\u0627\u064a\u0649\u06be\u06d5\u0644\u06d5\u0646\u06af\u06d5\u0646. \u0626\u06c7 \u0626\u06d0\u0644\u0627\u0646 \u062a\u0648\u0631\u0649\u063a\u0627 \u062a\u06d5\u06cb\u06d5 \u0628\u0648\u0644\u063a\u0627\u0646 reCAPTCHA \u063a\u0627 \u0626\u0648\u062e\u0634\u0627\u0634 \u0626\u06d5\u0646\u0626\u06d5\u0646\u0649\u06cb\u0649 \u062a\u0627\u0644\u0644\u0627\u0634\u0644\u0627\u0631\u063a\u0627 \u0626\u0648\u062e\u0634\u0649\u0645\u0627\u064a\u062f\u06c7 \u060c \u0626\u06c7\u0644\u0627\u0631 \u0633\u0649\u0632\u0646\u0649 \u062a\u0648\u0631 \u0626\u06d5\u062a\u0631\u0627\u067e\u0649\u062f\u0627 \u0626\u0649\u0632 \u0642\u0648\u063a\u0644\u0627\u067e \u060c \u06be\u06d5\u0642\u0649\u0642\u0649\u064a \u0633\u0627\u0644\u0627\u06be\u0649\u064a\u06d5\u062a \u0628\u0649\u0644\u06d5\u0646 \u0628\u0627\u063a\u0644\u0627\u064a\u062f\u06c7. \u0628\u0649\u0632 \u06be\u06d5\u0631\u06af\u0649\u0632\u0645\u06c7 \u0626\u06d0\u0644\u06d0\u0643\u062a\u0631\u0648\u0646\u0644\u06c7\u0642 \u062e\u06d5\u062a \u064a\u0627\u0643\u0649 \u0626\u06c7\u0686\u06c7\u0631\u0644\u0627\u0631\u0646\u0649 a11y \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u0634\u0643\u06d5 \u0642\u06c7\u0644\u0627\u064a\u0644\u0649\u0642 \u064a\u0627\u0631\u0649\u062a\u0649\u0634 \u06cb\u06d5 \u0642\u0627\u0644\u0627\u064a\u0645\u0649\u0642\u0627\u0646 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u0634\u0646\u0649\u06ad \u0626\u0627\u0644\u062f\u0649\u0646\u0649 \u0626\u06d0\u0644\u0649\u0634\u062a\u0649\u0646 \u0628\u0627\u0634\u0642\u0627 \u0645\u06d5\u0642\u0633\u06d5\u062a\u062a\u06d5 \u0626\u0649\u0634\u0644\u06d5\u062a\u0645\u06d5\u064a\u0645\u0649\u0632. \u0628\u0649\u0632\u0646\u0649\u06ad \u0645\u06d5\u062e\u067e\u0649\u064a\u06d5\u062a\u0644\u0649\u0643 \u062a\u06c8\u0632\u06c8\u0645\u0649\u0645\u0649\u0632\u062f\u06d5 \u0633\u0627\u0646\u0644\u0649\u0642 \u0645\u06d5\u0644\u06c7\u0645\u0627\u062a\u0644\u0627\u0631\u0646\u0649 \u0642\u0627\u0646\u062f\u0627\u0642 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u062f\u0649\u063a\u0627\u0646\u0644\u0649\u0642\u0649\u0645\u0649\u0632\u063a\u0627 \u0626\u06d5\u062a\u0631\u0627\u067e\u0644\u0649\u0642 \u06cb\u06d5 \u0646\u0648\u067e\u06c7\u0632\u0644\u06c7\u0642 \u062c\u0627\u06cb\u0627\u0628\u0644\u0627\u0631 \u0628\u0627\u0631 \u060c \u0626\u06d5\u0645\u0645\u0627 \u0642\u0649\u0633\u0642\u0627 \u062c\u0627\u06cb\u0627\u0628 \u0634\u06c7\u0643\u0649 \u060c \u0633\u0649\u0632\u0646\u0649 \u0643\u06c6\u0631\u06c8\u0634 \u062a\u0627\u0631\u0649\u062e\u0649\u06ad\u0649\u0632 \u0628\u0649\u0644\u06d5\u0646 \u0628\u0649\u0631 \u0626\u0627\u062f\u06d5\u0645 \u0633\u06c8\u067e\u0649\u062a\u0649\u062f\u06d5 \u0628\u0627\u063a\u0644\u0627\u0634\u0642\u0627 \u0642\u0649\u0632\u0649\u0642\u0645\u0627\u064a\u0645\u0649\u0632. \u0628\u0649\u0632 \u06be\u0627\u0632\u0649\u0631 \u0626\u06d0\u0644\u06d0\u0643\u062a\u0631\u0648\u0646\u0644\u06c7\u0642 \u062e\u06d5\u062a \u0626\u0627\u062f\u0631\u06d0\u0633\u0649\u06ad\u0649\u0632\u0646\u0649 \u062a\u06d0\u0632\u0644\u0649\u0643\u062a\u06d5 \u062a\u0627\u0634\u0644\u0649\u06cb\u06d0\u062a\u0649\u0634 \u0626\u06c8\u0686\u06c8\u0646 \u0634\u0649\u0641\u0649\u0631\u0644\u0649\u0642 \u06be\u06d5\u0644 \u0642\u0649\u0644\u0649\u0634 \u0686\u0627\u0631\u0649\u0633\u0649 \u0626\u06c8\u0633\u062a\u0649\u062f\u06d5 \u0626\u0649\u0632\u062f\u0649\u0646\u0649\u06cb\u0627\u062a\u0649\u0645\u0649\u0632. \u06be\u0627\u0644\u0628\u06c7\u0643\u0649 \u060c \u064a\u06d5\u0646\u0649\u0644\u0627 \u062e\u0648\u0631\u0644\u0649\u0646\u0649\u0634\u0646\u0649\u06ad \u0626\u0627\u0644\u062f\u0649\u0646\u0649 \u0626\u06d0\u0644\u0649\u0634 \u0626\u0649\u0642\u062a\u0649\u062f\u0627\u0631\u0649\u0645\u0649\u0632\u0646\u0649 \u0633\u0627\u0642\u0644\u0627\u067e \u060c \u0645\u06d5\u062e\u067e\u0649\u064a\u06d5\u062a\u0644\u0649\u0643 \u0633\u0627\u0642\u0644\u0627\u0634 \u062e\u0649\u0632\u0645\u0649\u062a\u0649\u0645\u0649\u0632\u0646\u0649 \u062a\u0648\u0644\u06c7\u0642\u0644\u0627\u064a\u0645\u0649\u0632.",
    "page-acc-link-1": "\u0626\u06d5\u06af\u06d5\u0631 \u0626\u0649\u0634\u0644\u06d5\u062a\u0643\u0649\u0644\u0649 \u0628\u0648\u0644\u0649\u062f\u0649\u063a\u0627\u0646 \u0626\u0649\u0634\u0644\u06d5\u062a\u0643\u06c8\u0686\u0649 \u0628\u0648\u0644\u0633\u0649\u06ad\u0649\u0632 \u060c \u0628\u06c7 \u064a\u06d5\u0631\u06af\u06d5 \u062a\u0649\u0632\u0649\u0645\u0644\u0649\u062a\u0649\u06ad.",
    "page-acc-subhead": "\u0628\u0649\u062e\u06d5\u062a\u06d5\u0631\u0644\u0649\u0643\u0646\u0649 \u0642\u0648\u063a\u062f\u0627\u0634 \u0628\u0649\u0644\u06d5\u0646 \u0628\u0649\u0631 \u06cb\u0627\u0642\u0649\u062a\u062a\u0627 \u0628\u0627\u0631\u0644\u0649\u0642 \u0626\u0649\u0634\u0644\u06d5\u062a\u0643\u06c8\u0686\u0649\u0644\u06d5\u0631\u0646\u0649\u06ad \u064a\u0648\u0686\u06c7\u0642\u0633\u0649\u0632 \u0632\u0649\u064a\u0627\u0631\u06d5\u062a \u0642\u0649\u0644\u0649\u0646\u0649\u0634\u0649\u063a\u0627 \u0643\u0627\u067e\u0627\u0644\u06d5\u062a\u0644\u0649\u0643 \u0642\u0649\u0644\u0649\u0634",
    "page-acc-summary": "\u062e\u06c7\u0644\u0627\u0633\u06d5",
    "page-acc-summary-contents-1": "hCaptcha \u0628\u0648\u062a\u0644\u0627\u0631\u0646\u0649 \u0643\u0649\u0634\u0649\u0644\u06d5\u0631\u062f\u0649\u0646 \u067e\u06d5\u0631\u0642\u0644\u06d5\u0646\u062f\u06c8\u0631\u06c8\u0634 \u0626\u0627\u0631\u0642\u0649\u0644\u0649\u0642 \u062a\u0648\u062e\u062a\u0649\u062a\u0649\u0634 \u0626\u06c8\u0686\u06c8\u0646 \u0644\u0627\u064a\u0649\u06be\u06d5\u0644\u06d5\u0646\u06af\u06d5\u0646. \u0643\u06c6\u0631\u06c8\u0634 \u0633\u0649\u0646\u0649\u0642\u0649 \u0628\u06c7\u0646\u0649\u06ad \u0626\u06c8\u0686\u06c8\u0646 \u0642\u06c7\u0644\u0627\u064a\u0644\u0649\u0642 \u0642\u0648\u0631\u0627\u0644 \u060c \u0626\u06d5\u0645\u0645\u0627 \u06be\u06d5\u0645\u0645\u06d5 \u0626\u0627\u062f\u06d5\u0645 \u0643\u06c6\u0631\u06c8\u0634 \u0642\u0649\u064a\u0649\u0646\u0686\u0649\u0644\u0649\u0642\u0649\u0646\u0649 \u06be\u06d5\u0644 \u0642\u0649\u0644\u0627\u0644\u0645\u0627\u064a\u062f\u06c7. \u0628\u06c7 \u0633\u06d5\u06cb\u06d5\u0628\u062a\u0649\u0646 \u0628\u0649\u0632 \u0626\u0627\u062f\u062f\u0649\u064a \u060c \u0626\u0627\u0632\u0627\u0628\u0633\u0649\u0632 \u062a\u0627\u0644\u0644\u0627\u0634\u0646\u0649 \u0644\u0627\u064a\u0649\u06be\u06d5\u0644\u06d5\u067e \u0686\u0649\u0642\u062a\u06c7\u0642 \u060c \u0645\u06c7\u0644\u0627\u0632\u0649\u0645\u0649\u062a\u0649\u0645\u0649\u0632\u0646\u0649 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u067e \u0646\u06d5\u0634\u0631\u0649\u064a\u0627\u062a\u0686\u0649\u0644\u0627\u0631\u0646\u0649\u06ad 508-\u0628\u06c6\u0644\u06d5\u0643 \u06cb\u06d5 WCAG 2.0 \u0628\u06d5\u0644\u06af\u0649\u0644\u0649\u0645\u0649\u0644\u0649\u0631\u0649\u06af\u06d5 \u0645\u0627\u0633 \u0643\u06d0\u0644\u0649\u062f\u0649\u063a\u0627\u0646 \u0628\u0627\u0631\u0644\u0649\u0642 \u0643\u0649\u0634\u0649\u0644\u06d5\u0631\u0646\u0649\u06ad \u0642\u0648\u0644\u0627\u064a\u0644\u0649\u0642\u0644\u0649\u0642\u0649\u0646\u0649 \u0633\u0627\u0642\u0644\u0627\u067e \u0642\u0627\u0644\u0627\u0644\u0627\u064a\u0645\u0649\u0632.",
    "page-acc-summary-contents-2": "\u0626\u06c7\u0646\u0649\u06ad \u0626\u0649\u0634\u0644\u06d5\u0634 \u0626\u06c7\u0633\u06c7\u0644\u0649: \u0628\u0649\u0631\u0649\u0646\u0686\u0649\u062f\u0649\u0646 \u060c \u0626\u0649\u0634\u0644\u06d5\u062a\u0643\u0649\u0644\u0649 \u0628\u0648\u0644\u0649\u062f\u0649\u063a\u0627\u0646 \u0626\u0649\u0634\u0644\u06d5\u062a\u0643\u06c8\u0686\u0649\u0644\u06d5\u0631 hCaptcha \u0643\u0649\u0686\u0649\u0643 \u0642\u0648\u0631\u0627\u0644 \u0626\u06c7\u0686\u06c7\u0631 \u0628\u06d0\u062a\u0649\u06af\u06d5 \u0626\u06c7\u0644\u0627\u0646\u063a\u0627\u0646 \u0628\u06c7 URL \u062f\u0627 \u062a\u0649\u0632\u0649\u0645\u0644\u0649\u062a\u0649\u062f\u06c7. \u0626\u06c7\u0644\u0627\u0631\u063a\u0627 \u0634\u0649\u0641\u0649\u0631\u0644\u0627\u0646\u063a\u0627\u0646 \u0633\u0627\u0642\u0644\u0627\u0646\u0645\u0649\u0644\u0627\u0631 \u0628\u06d0\u0631\u0649\u0644\u0649\u062f\u06c7 \u060c \u0626\u06c7\u0646\u0649 \u0643\u06c8\u0646\u062f\u06d5 \u0628\u0649\u0631 \u0642\u0627\u0646\u0686\u06d5 \u0642\u06d0\u062a\u0649\u0645 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u0634\u0643\u06d5 \u0628\u0648\u0644\u0649\u062f\u06c7 \u060c \u0626\u06d5\u0645\u0645\u0627 \u0686\u0648\u0642\u06c7\u0645 \u06be\u06d5\u0631 24 \u0633\u0627\u0626\u06d5\u062a\u062a\u06d5 \u0643\u0649\u0631\u0649\u0634 \u0626\u0627\u0631\u0642\u0649\u0644\u0649\u0642 \u064a\u06d0\u06ad\u0649\u0644\u0627\u0634 \u0643\u06d0\u0631\u06d5\u0643. HCaptcha \u0645\u06c7\u0644\u0627\u0632\u0649\u0645\u0649\u062a\u0649\u0646\u0649 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u067e \u06be\u06d5\u0631 \u0642\u0627\u0646\u062f\u0627\u0642 \u062a\u0648\u0631 \u0628\u06d0\u0643\u06d5\u062a\u062a\u0649\u0643\u0649 \u0632\u0649\u064a\u0627\u0631\u06d5\u062a\u0686\u0649\u0644\u06d5\u0631\u06af\u06d5 \u0631\u0649\u0642\u0627\u0628\u06d5\u062a \u0626\u0648\u062a\u062a\u06c7\u0631\u0649\u063a\u0627 \u0642\u0648\u064a\u06c7\u0644\u063a\u0627\u0646\u062f\u0627 \u060c \u0626\u06c7\u0644\u0627\u0631 \u0626\u0627\u067e\u062a\u0648\u0645\u0627\u062a\u0649\u0643 \u0626\u06c6\u062a\u06c8\u067e \u0643\u06d0\u062a\u0649\u062f\u06c7.",
    "page-acc-title": "hCaptcha \u0632\u0649\u064a\u0627\u0631\u06d5\u062a \u0626\u06c7\u0686\u06c7\u0631\u0649",
    "page-acc-tr-body": "\u0633\u0648\u0626\u0627\u0644: \u0645\u06d5\u0646 \u0633\u0627\u0642\u0644\u0627\u0646\u0645\u0649\u0644\u0627\u0631\u0646\u0649 \u062a\u06d5\u06ad\u0634\u0649\u06af\u06d5\u0646\u062f\u0649\u0646 \u0643\u06d0\u064a\u0649\u0646 \u064a\u06d5\u0646\u0649\u0644\u0627 \u0631\u0649\u0642\u0627\u0628\u06d5\u062a\u0646\u0649 \u0643\u06c6\u0631\u06c8\u06cb\u0627\u062a\u0649\u0645\u06d5\u0646. \u0628\u06c7\u0646\u0649 \u0646\u06d0\u0645\u06d5 \u0643\u06d5\u0644\u062a\u06c8\u0631\u06c8\u067e \u0686\u0649\u0642\u0649\u0631\u0649\u06cb\u0627\u062a\u0649\u062f\u06c7\u061f \u062c\u0627\u06cb\u0627\u0628: \u0628\u06c7 \u0626\u0627\u062f\u06d5\u062a\u062a\u06d5 \u062a\u0627\u062c\u0627\u06cb\u06c7\u0632\u0686\u0649\u0644\u0649\u0642 \u0626\u06d0\u0644\u0627\u0646 \u0686\u06d5\u0643\u0644\u0649\u0645\u0649\u0633\u0649 \u064a\u0627\u0643\u0649 \u0633\u0627\u0642\u0644\u0627\u0646\u0645\u0649\u0644\u0627\u0631\u063a\u0627 \u0642\u0627\u0631\u0634\u0649 \u0643\u06d0\u06ad\u06d5\u064a\u062a\u0649\u0634 \u064a\u0627\u0643\u0649 \u062a\u0648\u0631 \u0628\u06d0\u0643\u06d5\u062a \u0633\u0627\u0642\u0644\u0627\u0646\u0645\u0649\u0644\u0649\u0631\u0649\u0646\u0649 \u0686\u06d5\u0643\u0644\u06d5\u064a\u062f\u0649\u063a\u0627\u0646 \u062a\u06d5\u06ad\u0634\u06d5\u0643\u0646\u0649 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u0634\u062a\u0649\u0646 \u0628\u0648\u0644\u0649\u062f\u06c7 \u060c \u0628\u06c7 \u0626\u06d5\u06be\u06cb\u0627\u0644\u062f\u0627 hcaptcha.com \u0646\u0649\u06ad \u0633\u0627\u0642\u0644\u0627\u0646\u0645\u0649\u0633\u0649 \u0628\u0627\u0634\u0642\u0627 \u062a\u0648\u0631 \u0628\u06d0\u0643\u06d5\u062a\u062a\u06d5 hCaptcha JavaScript \u062a\u06d5\u0631\u0649\u067e\u0649\u062f\u0649\u0646 \u062a\u06d5\u06ad\u0634\u0649\u0644\u0649\u062f\u06c7 \u064a\u0627\u0643\u0649 \u062a\u06d5\u0643\u0634\u06c8\u0631\u06c8\u0644\u0649\u062f\u06c7. \u0633\u0649\u0632 \u0632\u0649\u064a\u0627\u0631\u06d5\u062a \u0642\u0649\u0644\u063a\u0627\u0646\u063a\u0627 \u0626\u0648\u062e\u0634\u0627\u0634. hCaptcha \u0642\u0648\u0644\u0627\u064a\u0644\u0649\u0642 \u0633\u0627\u0642\u0644\u0627\u0646\u0645\u0649\u0644\u0649\u0631\u0649 \u0626\u06c6\u0644\u0686\u06d5\u0645\u0644\u0649\u0643 \u062a\u06d5\u06ad\u0634\u06d5\u0643\u0644\u0649\u0631\u0649 \u0628\u0649\u0644\u06d5\u0646 \u0628\u0627\u0631\u0644\u0649\u0642 \u062f\u0627\u06ad\u0644\u0649\u0642 \u062a\u0648\u0631\u0643\u06c6\u0631\u06af\u06c8\u0686 \u06cb\u06d5 \u0626\u06d0\u0644\u0627\u0646 \u0686\u06d5\u0643\u0644\u0649\u06af\u06c8\u0686\u0649\u0644\u06d5\u0631 \u0628\u0649\u0644\u06d5\u0646 \u0626\u0649\u0634\u0644\u06d5\u064a\u062f\u06c7 \u060c \u0634\u06c7\u06ad\u0627 \u0626\u0627\u062f\u06d5\u062a\u062a\u06d5 \u0645\u06d5\u063a\u0644\u06c7\u0628\u0649\u064a\u06d5\u062a \u062a\u0648\u0633\u06c7\u0644\u06c7\u0634\u0642\u0627 \u0642\u0627\u0631\u0634\u0649 \u0642\u0648\u0644\u064a\u0627\u0632\u0645\u0627 \u064a\u0627\u0643\u0649 \u0626\u0627\u0644\u0627\u06be\u0649\u062f\u06d5 \u062a\u0648\u0631 \u0628\u06d0\u0643\u06d5\u062a\u0644\u06d5\u0631\u0646\u0649 \u0646\u0649\u0634\u0627\u0646 \u0642\u0649\u0644\u063a\u0627\u0646 \u0645\u06c7\u0634\u06c7\u0646\u0649\u06ad\u063a\u0627 \u0626\u0648\u062e\u0634\u0627\u0634 \u0642\u0627\u0626\u0649\u062f\u0649\u0644\u06d5\u0631 \u0633\u06d5\u06cb\u06d5\u0628\u0649\u062f\u0649\u0646 \u0628\u0648\u0644\u0649\u062f\u06c7. \u06be\u06d5\u0644 \u0642\u0649\u0644\u0649\u0634 \u0686\u0627\u0631\u0649\u0633\u0649: 1. Whitelist hcaptcha.com \u06cb\u06d5 * .hcaptcha.com \u0626\u06d0\u0644\u0627\u0646\u0644\u0649\u0631\u0649 \u064a\u0627\u0643\u0649 \u062a\u0648\u0631\u0643\u06c6\u0631\u06af\u06c8 \u0628\u0649\u062e\u06d5\u062a\u06d5\u0631\u0644\u0649\u0643 \u0643\u06d0\u06ad\u06d5\u064a\u062a\u0649\u0644\u0645\u0649\u0633\u0649\u062f\u0649\u0643\u0649 \u0633\u0627\u0642\u0644\u0627\u0646\u0645\u0649\u0644\u0627\u0631. 2. \u0626\u06d5\u06af\u06d5\u0631 \u0633\u0649\u0632 \u0628\u0627\u062a\u06c7\u0631 \u062a\u0648\u0631\u0643\u06c6\u0631\u06af\u06c8\u0646\u0649 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u06cb\u0627\u062a\u0642\u0627\u0646 \u0628\u0648\u0644\u0633\u0649\u06ad\u0649\u0632 \u060c (2020-\u064a\u0649\u0644\u0649 4-\u0626\u0627\u064a\u063a\u0649\u0686\u06d5) \u06be\u06d0\u0686\u0642\u0627\u0646\u062f\u0627\u0642 \u0633\u0627\u0642\u0644\u0627\u0646\u0645\u0649\u0644\u0627\u0631\u0646\u0649\u06ad \u0626\u0627\u0642 \u062a\u0649\u0632\u0649\u0645\u0644\u0649\u0643\u0649 \u064a\u0648\u0642\u062a\u06d5\u0643 \u0642\u0649\u0644\u0633\u0649\u06ad\u0649\u0632 \u060c \u0645\u0627\u064a\u0649\u0644\u0644\u0649\u0642 -> \u0642\u0627\u0644\u0642\u0627\u0646 -> \u0633\u0627\u0642\u0644\u0627\u0646\u0645\u0649\u0644\u0627\u0631\u063a\u0627 \u0643\u0649\u0631\u0649\u067e \u060c \u0628\u0627\u0631\u0644\u0649\u0642 \u0633\u0627\u0642\u0644\u0627\u0646\u0645\u0649\u0644\u0627\u0631\u063a\u0627 \u064a\u0648\u0644 \u0642\u0648\u064a\u06c7\u06ad. 3. \u0626\u06d5\u06af\u06d5\u0631 \u0633\u0649\u0632 \u064a\u06d0\u0642\u0649\u0646\u062f\u0627 \u0626\u06d0\u0644\u0627\u0646 \u0642\u0649\u0644\u0649\u0646\u063a\u0627\u0646 OS X 10.15 \u064a\u0627\u0643\u0649 iOS 13. 4 \u062f\u06d5 Safari \u0646\u0649\u06ad \u0626\u06d5\u06ad \u064a\u06d0\u06ad\u0649 \u0646\u06d5\u0634\u0631\u0649\u0646\u0649 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u06cb\u0627\u062a\u0642\u0627\u0646 \u0628\u0648\u0644\u0633\u0649\u06ad\u0649\u0632 \u060c \u0626\u0627\u0644\u0645\u0627 \u0634\u0649\u0631\u0643\u0649\u062a\u0649 Safari \u0646\u0649\u06ad \u0626\u06c8\u0686\u0649\u0646\u0686\u0649 \u062a\u06d5\u0631\u06d5\u067e \u0633\u0627\u0642\u0644\u0627\u0646\u0645\u0649\u0644\u0649\u0631\u0649\u063a\u0627 \u0645\u06c7\u0646\u0627\u0633\u0649\u06cb\u06d5\u062a\u0644\u0649\u0643 \u06be\u06d5\u0631\u0649\u0643\u0649\u062a\u0649\u0646\u0649 \u0626\u06c6\u0632\u06af\u06d5\u0631\u062a\u0649\u067e \u060c \u0633\u06c8\u0643\u06c8\u062a\u062a\u0649\u0643\u0649 \u06be\u0627\u0644\u06d5\u062a\u062a\u06d5 \u06be\u06d5\u0645\u0645\u0649\u0633\u0649\u0646\u0649 \u062a\u0648\u0633\u0649\u062f\u0649. \u0628\u0649\u0632 \u0628\u0649\u0631 \u06be\u06d5\u0644 \u0642\u0649\u0644\u0649\u0634 \u0686\u0627\u0631\u0649\u0633\u0649\u0646\u0649 \u064a\u0648\u0644\u063a\u0627 \u0642\u0648\u064a\u06c7\u06cb\u0627\u062a\u0649\u0645\u0649\u0632 \u060c \u0626\u06d5\u0645\u0645\u0627 \u0634\u06c7\u0646\u0649\u06ad \u0628\u0649\u0644\u06d5\u0646 \u0628\u0649\u0631 \u06cb\u0627\u0642\u0649\u062a\u062a\u0627 Safari \u0645\u0627\u064a\u0649\u0644\u0644\u0649\u0642\u0649 \u060c \u0645\u06d5\u062e\u067e\u0649\u064a\u06d5\u062a\u0644\u0649\u0643 \u0628\u06c6\u0644\u0649\u0643\u0649\u0646\u0649 \u0632\u0649\u064a\u0627\u0631\u06d5\u062a \u0642\u0649\u0644\u0649\u06ad \u06cb\u06d5 \u062a\u0648\u0631\u0628\u06d0\u0643\u06d5\u062a \u0626\u0649\u0632 \u0642\u0648\u063a\u0644\u0627\u0634\u0646\u0649 \u062a\u06d5\u0643\u0634\u06c8\u0631\u06c8\u067e \u0628\u06d0\u0642\u0649\u06ad: \u062a\u0648\u0631 \u0628\u06d0\u0643\u06d5\u062a \u06be\u0627\u0644\u0642\u0649\u063a\u0627\u0646 \u0626\u0649\u0632 \u0642\u0648\u063a\u0644\u0627\u0634\u0646\u0649\u06ad \u0626\u0627\u0644\u062f\u0649\u0646\u0649 \u0626\u06d0\u0644\u0649\u067e \u060c \u0633\u0627\u0642\u0644\u0627\u0646\u0645\u0649\u0644\u0627\u0631\u0646\u0649\u06ad \u0643\u06c8\u062a\u0643\u06d5\u0646\u062f\u06d5\u0643 \u0626\u0649\u0634\u0644\u0649\u0634\u0649\u06af\u06d5 \u0643\u0627\u067e\u0627\u0644\u06d5\u062a\u0644\u0649\u0643 \u0642\u0649\u0644\u0649\u06ad. \u0633\u0648\u0626\u0627\u0644: \u0645\u06d5\u0646 \u0643\u06c6\u067e \u0626\u06c8\u0633\u0643\u06c8\u0646\u0649\u0646\u0649 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u0645\u06d5\u0646. \u0643\u06c6\u067e \u0642\u06d0\u062a\u0649\u0645 \u062a\u0649\u0632\u0649\u0645\u0644\u0649\u062a\u0649\u0634\u0649\u0645 \u0643\u06d0\u0631\u06d5\u0643\u0645\u06c7\u061f \u062c\u0627\u06cb\u0627\u0628: \u064a\u0627\u0642. \u0633\u0627\u0642\u0644\u0627\u0646\u0645\u0649\u0644\u0627\u0631\u0646\u0649 \u062a\u06d5\u06ad\u0634\u06d5\u0634 \u0626\u06c8\u0686\u06c8\u0646 \u0626\u0649\u0634\u0644\u06d5\u062a\u0643\u06d5\u0646 \u06be\u06d5\u0631 \u0628\u0649\u0631 \u0626\u06c8\u0633\u0643\u06c8\u0646\u0649\u062f\u06d5 \u0633\u0649\u0632\u06af\u06d5 \u0626\u06d5\u06cb\u06d5\u062a\u0649\u0644\u06af\u06d5\u0646 \u0626\u06d0\u0644\u06d0\u0643\u062a\u0631\u0648\u0646\u0644\u06c7\u0642 \u062e\u06d5\u062a \u0643\u0649\u0631\u0649\u0634 \u0626\u06c7\u0644\u0627\u0646\u0645\u0649\u0633\u0649\u0646\u0649 \u0686\u06d0\u0643\u0649\u06ad.",
    "page-acc-tr-title": "\u0642\u0648\u0644\u0627\u064a\u0644\u0649\u0642 \u0626\u0649\u0634\u0644\u06d5\u062a\u0643\u06c8\u0686\u0649\u0644\u06d5\u0631 \u0626\u06c8\u0686\u06c8\u0646: \u0633\u0648\u0626\u0627\u0644-\u062c\u0627\u06cb\u0627\u0628 \u06cb\u06d5 \u0645\u06d5\u0633\u0649\u0644\u0649\u0644\u06d5\u0631\u0646\u0649 \u06be\u06d5\u0644 \u0642\u0649\u0644\u0649\u0634 \u0642\u0648\u0644\u0644\u0627\u0646\u0645\u0649\u0633\u0649",
    "priv-hc": "\u0645\u06d5\u062e\u067e\u0649\u064a\u06d5\u062a\u0644\u0649\u0643\u0646\u0649 \u0645\u06d5\u0631\u0643\u06d5\u0632\u0644\u06d5\u0634\u062a\u06c8\u0631\u062f\u0649",
    "priv-hc-text": "\u0626\u06d0\u0644\u0627\u0646 \u062a\u0648\u0631\u0649 \u0633\u0649\u0632\u0646\u0649 \u0626\u06c7\u0644\u0627\u0631\u0646\u0649\u06ad \u0645\u06d5\u06be\u0633\u06c7\u0644\u0627\u062a\u0649 \u062f\u06d5\u067e \u0642\u0627\u0631\u0627\u064a\u062f\u06c7. \u0628\u0649\u0632 \u0642\u0649\u0644\u0645\u0627\u064a\u0645\u0649\u0632. \u0628\u0649\u0632 \u0626\u0627\u0628\u0648\u0646\u062a\u0644\u0627\u0631\u0646\u0649\u06ad \u0643\u0649\u0645 \u0626\u0649\u0643\u06d5\u0646\u0644\u0649\u0643\u0649 \u0628\u0649\u0644\u06d5\u0646 \u0643\u0627\u0631\u0649\u0645\u0649\u0632 \u064a\u0648\u0642 \u060c \u067e\u06d5\u0642\u06d5\u062a \u0626\u06c7\u0644\u0627\u0631\u0646\u0649\u06ad \u064a\u0627\u0645\u0627\u0646 \u063a\u06d5\u0631\u06d5\u0632\u0644\u0649\u0643 \u0628\u0648\u0644\u06c7\u0634\u0649 \u0645\u06c7\u0645\u0643\u0649\u0646. \u0645\u06d5\u062e\u067e\u0649\u064a\u06d5\u062a\u0644\u0649\u0643 \u0626\u06c6\u062a\u0643\u0649\u0644\u0649\u06af\u06d5 \u0626\u0648\u062e\u0634\u0627\u0634 \u062a\u06d5\u0634\u06d5\u0628\u0628\u06c7\u0633\u0644\u0627\u0631\u0646\u0649 \u0642\u0648\u0644\u0644\u0627\u064a\u0645\u0649\u0632 \u060c \u0645\u06d5\u062e\u067e\u0649\u064a\u06d5\u062a\u0644\u0649\u0643 \u06cb\u06d5 \u0628\u0649\u062e\u06d5\u062a\u06d5\u0631\u0644\u0649\u0643\u0646\u0649 \u062a\u06d5\u06ad\u067e\u06c7\u06ad\u0644\u0627\u0634\u062a\u06c7\u0631\u06c7\u0634 \u0626\u06c8\u0686\u06c8\u0646 \u062a\u0649\u0631\u0649\u0634\u0649\u0645\u0649\u0632.",
    "sec-hc": "\u0628\u0649\u062e\u06d5\u062a\u06d5\u0631\u0644\u0649\u0643 \u0628\u0649\u0631\u0649\u0646\u0686\u0649",
    "sec-hc-text": "hCaptcha \u0646\u06d5\u0634\u0631\u0649\u064a\u0627\u062a\u0646\u0649 \u0626\u0627\u0633\u0627\u0633 \u0642\u0649\u0644\u063a\u0627\u0646. \u0628\u0649\u0632 \u062a\u0648\u0631 \u0628\u06d0\u0643\u06d5\u062a\u0644\u06d5\u0631\u06af\u06d5 \u06be\u06d5\u0642\u0649\u0642\u0649\u064a \u0633\u0627\u0646\u0644\u0649\u0642 \u0645\u06d5\u0644\u06c7\u0645\u0627\u062a \u0628\u06d0\u0631\u0649\u0634 \u06cb\u06d5 \u0626\u06c7\u0646\u0649 \u0642\u0627\u0646\u062f\u0627\u0642 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u0634\u0646\u0649 \u0642\u0627\u0631\u0627\u0631 \u0642\u0649\u0644\u0649\u0634 \u0626\u06c8\u0686\u06c8\u0646 \u062a\u0649\u0631\u0649\u0634\u0649\u0645\u0649\u0632. \u0626\u06d0\u0646\u0649\u0642\u0644\u0649\u0642 \u0633\u0649\u0632\u0646\u0649\u06ad \u06cb\u06d5 \u0628\u0649\u0632\u0646\u0649\u06ad \u0645\u06d5\u0646\u067e\u06d5\u0626\u06d5\u062a\u0649\u0645\u0649\u0632\u062f\u06d5. \u0628\u0627\u0634\u0642\u0627 \u0631\u06d5\u0633\u0649\u0645\u0644\u06d5\u0631\u0646\u0649 \u0626\u06d0\u0644\u0627\u0646 \u0634\u0649\u0631\u0643\u06d5\u062a\u0644\u0649\u0631\u0649 \u062a\u06d5\u0645\u0649\u0646\u0644\u06d5\u064a\u062f\u06c7. \u0626\u06c7\u0644\u0627\u0631 \u062a\u0648\u0633\u06c7\u06cb\u0627\u0644\u063a\u0627\u0646 \u06be\u06d5\u0631 \u0628\u0649\u0631 \u0645\u0627\u0634\u0649\u0646\u0627 \u0626\u06c7\u0644\u0627\u0631\u0646\u0649\u06ad \u0643\u0649\u0631\u0649\u0645\u0649\u0646\u0649 \u0626\u0627\u0632\u0627\u064a\u062a\u0649\u0634\u0649 \u0645\u06c7\u0645\u0643\u0649\u0646.",
    "what-is-hcaptcha": "HCaptcha \u062f\u06d0\u06af\u06d5\u0646 \u0646\u06d0\u0645\u06d5\u061f",
    "what-is-hcaptcha-text": "hCaptcha \u0633\u0649\u0632 \u064a\u0627\u0642\u062a\u06c7\u0631\u0649\u062f\u0649\u063a\u0627\u0646 \u062a\u0648\u0631 \u0645\u06c7\u0644\u0627\u0632\u0649\u0645\u06d5\u062a\u0644\u0649\u0631\u0649\u0646\u0649\u06ad \u0626\u0649\u0646\u0633\u0627\u0646\u0644\u0627\u0631\u063a\u0627 \u0626\u0627\u0633\u0627\u0646 \u06cb\u06d5 \u0645\u0627\u0634\u0649\u0646\u0627 \u0626\u06c8\u0686\u06c8\u0646 \u0642\u0649\u064a\u0649\u0646 \u0628\u0648\u0644\u063a\u0627\u0646 \u0626\u0627\u062f\u062f\u0649\u064a \u0633\u0648\u0626\u0627\u0644\u0644\u0627\u0631\u0646\u0649 \u0633\u0648\u0631\u0627\u0634 \u0626\u0627\u0631\u0642\u0649\u0644\u0649\u0642 \u0628\u0648\u062a\u06c7\u0644\u0643\u0627 \u060c \u0626\u06d5\u062e\u0644\u06d5\u062a \u062e\u06d5\u062a \u06cb\u06d5 \u062e\u0648\u0631\u0644\u0627\u0634\u062a\u0649\u0646 \u0633\u0627\u0642\u0644\u0649\u0646\u0649\u0634\u0649\u063a\u0627 \u064a\u0627\u0631\u062f\u06d5\u0645 \u0628\u06d0\u0631\u0649\u062f\u06c7.",
    "why-hc": "HCaptcha \u0646\u06d0\u0645\u0649\u0634\u0642\u0627\u061f",
    "why-hc-text": "\u06be\u06d5\u0631 \u0642\u06d0\u062a\u0649\u0645 \u0631\u0649\u0642\u0627\u0628\u06d5\u062a\u0643\u06d5 \u062c\u0627\u06cb\u0627\u0628 \u0628\u06d5\u0631\u06af\u06d5\u0646\u062f\u06d5 \u060c \u0628\u0649\u0632 \u0645\u06c7\u0631\u06d5\u0643\u0643\u06d5\u067e \u0645\u0648\u062f\u06d0\u0644\u0644\u0627\u0631\u0646\u0649 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u067e \u0633\u0649\u0632\u0646\u0649\u06ad \u0626\u0627\u062f\u06d5\u0645 \u064a\u0627\u0643\u0649 \u0645\u0627\u0634\u0649\u0646\u0627 \u0626\u0649\u0643\u06d5\u0646\u0644\u0649\u0643\u0649\u06ad\u0649\u0632\u0646\u0649 \u0628\u0649\u0644\u0649\u0645\u0649\u0632. \u0633\u0649\u0632 \u0632\u0649\u064a\u0627\u0631\u06d5\u062a \u0642\u0649\u0644\u063a\u0627\u0646 \u062a\u0648\u0631 \u0628\u06d0\u0643\u06d5\u062a \u0626\u0627\u0646\u062f\u0649\u0646 \u0628\u06c7 \u0646\u06d5\u062a\u0649\u062c\u0649\u0646\u0649 \u0626\u0649\u0634\u0644\u0649\u062a\u0649\u067e \u060c \u062a\u0648\u0631 \u0628\u06d0\u0643\u06d5\u062a\u062a\u0649\u0643\u0649 \u062a\u06d5\u062c\u0631\u0649\u0628\u0649\u06ad\u0649\u0632\u0646\u0649 \u064a\u0627\u062e\u0634\u0649\u0644\u0627\u0634\u0642\u0627 \u064a\u0627\u0631\u062f\u06d5\u0645 \u0628\u06d0\u0631\u0649\u062f\u06c7."
